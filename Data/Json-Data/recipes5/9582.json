        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275881/homemade-vanilla-coffee-creamer/",
        "name": "Homemade Vanilla Coffee Creamer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7840809.jpg",
