    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16769/maple-glazed-sweet-potatoes/",
        "name": "Maple Glazed Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1746526.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "1999-11-14T04:25:35.000Z",
        "description": "Sweet potatoes made with brown sugar and maple syrup. A family favorite that everyone enjoys. If you're not cooking for a vegetarian crowd, you can top these with marshmallows as well.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "2\u2009\u00bd sweet potatoes",
            "3 tablespoons butter",
            "\u00bc cup maple syrup",
            "2 tablespoons brown sugar",
            "2 tablespoons flaked coconut",
            "\u2153 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place sweet potatoes in a large saucepan with water to cover them. Bring the water to a boil, reduce heat, cover and let simmer 25 to 30 minutes until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Use non-aerosol non stick spray to grease a 8x8 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the sweet potatoes are cool, peel and cut them into large chunks. Arrange them in the prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, melt butter, syrup, sugar and coconut together over a low heat. Cook for 3 to 5 minutes, stirring constantly. Sprinkle mixture over sweet potatoes. Top mixture with pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 5 to 7 minutes, until top is lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
