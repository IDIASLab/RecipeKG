    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275212/mango-chutney-glazed-pork-loin-roast/",
        "name": "Mango-Chutney Glazed Pork Loin Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mango-Chutney Glazed Pork Loin Roast"
        },
        "datePublished": "2019-08-15T05:04:57.000Z",
        "description": "A little of this...a little of that, this delicious pork loin roast recipe has just a touch of India, but appeals to everyone! Serve with jasmine rice or potatoes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (2 1/2 pound) center-cut boneless pork loin roast",
            "1 tablespoon tandoori paste, or to taste",
            "12 ounces mild shredded mango chutney",
            "2 tablespoons apricot jam",
            "1 tablespoon dehydrated onion flakes",
            "2 cups water",
            "2 slices pineapple, cubed",
            "1 tablespoon dried cranberries",
            "1 tablespoon chopped candied orange peel"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rub outside of roast with tandoori paste and place into the bottom of a baking dish. Spread mango chutney on top, followed by apricot jam. Sprinkle dried onion over roast. Pour water around the sides and throw pineapples and cranberries into the water. Top roast with candied orange peel. Cover the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until an instant-read thermometer inserted into the center of the roast reads 160 degrees F (70 degrees C), 1 1/2 to 2 hours, removing the lid for the last 15 to 30 minutes.\n"
            }
        ],
