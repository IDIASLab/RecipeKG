        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss edamame, olive oil, and salt together in a large bowl. Spread in a single layer in a shallow baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until edamame are crispy, 20 to 30 minutes.\n"
            }
