    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276167/baked-parmesan-cauliflower/",
        "name": "Baked Parmesan Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7152822.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-09-27T22:33:31.000Z",
        "description": "Orange is the new black. Cauliflower is the new kale. I was not previously a cauliflower fan but I've crossed over with this delicious recipe that's great for Thanksgiving!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "nonstick cooking spray",
            "\u00be cup grated Parmesan cheese",
            "\u00bd teaspoon dried thyme",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon dried basil",
            "\u00bc teaspoon garlic powder",
            "kosher salt and freshly ground black pepper to taste",
            "1 head cauliflower, chopped into bite-size pieces",
            "2 tablespoons olive oil",
            "2 tablespoons chopped fresh parsley leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a cooling rack with nonstick cooking spray; place on a baking sheet and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Parmesan cheese, thyme, oregano, basil, garlic powder, salt, and pepper in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cauliflower onto the prepared cooling rack. Drizzle with olive oil and sprinkle with Parmesan cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender and easily pierced with a knife, about 15 minutes. Remove from the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until cauliflower is golden brown, 2 to 3 minutes. Remove from the oven, garnish with parsley, and serve immediately.\n"
            }
        ],
