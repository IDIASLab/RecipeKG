        "recipeYield": "3 cups",
        "recipeIngredient": [
            "1 cup bourbon whiskey",
            "\u00bd cup brown sugar",
            "1 cup ketchup",
            "2 teaspoons Worcestershire sauce",
            "\u00bc cup white vinegar",
            "1 tablespoon lemon juice",
            "\u00bd teaspoon minced garlic",
            "1 pinch dry mustard",
