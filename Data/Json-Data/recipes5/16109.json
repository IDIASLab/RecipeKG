        "name": "Instant Pot&reg; Cheesy Mexican Lentils and Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7002554.jpg",
            "width": 0,
