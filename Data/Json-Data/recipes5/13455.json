    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283584/cheesy-hot-crab-dip/",
        "name": "Cheesy Hot Crab Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheesy Hot Crab Dip"
        },
        "datePublished": "2020-01-15T10:16:30.000Z",
        "description": "Who doesn't love warm crab dip! It will have you wanting more. Serve with your favorite type of chips or bread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup shredded Parmesan cheese",
            "2 teaspoons hot sauce",
            "1 teaspoon seafood seasoning (such as Old Bay\u00ae)",
            "1 teaspoon garlic powder",
            "1 pinch ground black pepper",
            "1 (16 ounce) can crabmeat",
            "1 medium green bell pepper, diced",
            "1 medium red bell pepper, diced",
            "\u00bd cup diced sweet onion",
            "2 tablespoons shredded Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, 1/2 cup Parmesan cheese, hot sauce, seafood seasoning, garlic powder, and black pepper in a bowl; stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain crab; pick out and discard any shells. Add crab to cream cheese mixture, along with bell peppers and onion; mix thoroughly. Transfer into a baking dish and sprinkle with remaining Parmesan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly, about 20 minutes. Turn on the broiler and broil until the top is a nice golden brown color, about 5 minutes.\n"
            }
        ],
