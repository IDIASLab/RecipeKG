    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277420/heavenly-vietnamese-beef-fried-rice/",
        "name": "Heavenly Vietnamese Beef Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Heavenly Vietnamese Beef Fried Rice"
        },
        "datePublished": "2019-11-27T09:08:06.000Z",
        "description": "This Vietnamese beef fried rice is a quick meal-in-one. Garnish with cilantro and a wedge of lime.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00be cups water",
            "1 cup jasmine rice",
            "1 cube beef bouillon",
            "5 kaffir lime leaves",
            "14 ounces lean ground beef",
            "1 cup torn cilantro leaves",
            "1 cup torn mint leaves",
            "\u00bd cup sliced green onions",
            "\u00bc cup lime juice",
            "\u00bc cup chopped green chile peppers",
            "4 tablespoons low-sodium soy sauce, divided",
            "1 (12 ounce) package firm tofu, drained and cut into 1-inch cubes",
            "2 tablespoons vegetable oil",
            "1 large red onion, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring 1 3/4 cups water to a boil. Add rice, beef bouillon, and lime leaves. Boil for 2 minutes. Reduce heat to low and cook, covered, for 10 minutes. Turn off heat and let rice stand for 10 minutes. Spread onto a platter and let cool completely, at least 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In the meantime, marinate beef with cilantro, mint, green onions, lime juice, chile peppers, and 2 tablespoons soy sauce for up to 1 hour. Marinate tofu in the remaining soy sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a wok over high heat. Add red onion; cook for 1 minute, then add marinated beef. Saute until browned, 3 to 5 minutes. Discard lime leaves from the cooled rice and toss rice into the wok. Stir-fry for 2 minutes. Taste and adjust seasonings as needed. Add tofu, stir gently, and fry for 3 minutes more. Serve hot.\n"
            }
        ],
