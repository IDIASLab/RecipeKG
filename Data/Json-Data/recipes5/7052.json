    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258241/slow-cooker-chicken-francese/",
        "name": "Slow Cooker Chicken Francese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8191871.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T07:10:54.000Z",
        "description": "Tender lemony buttered chicken to serve on top of your favorite pasta or rice.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT4H18M",
        "totalTime": "P0DT5H48M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds skinless, boneless chicken breasts, cut into strips",
            "1\u2009\u00bc cups white wine, divided",
            "1\u2009\u00bc cups chicken broth, divided",
            "\u00be cup fresh lemon juice, divided",
            "\u00be cup all-purpose flour",
            "1 teaspoon dried oregano",
            "1 teaspoon dried basil",
            "\u00bd teaspoon ground black pepper",
            "\u00bc teaspoon salt",
            "1 cup grated Parmesan cheese",
            "2 tablespoons butter",
            "2 cubes  chicken bouillon",
            "3 cloves garlic, minced",
            "2 teaspoons butter, melted",
            "1 teaspoon chopped fresh parsley",
            "\u00bd cup heavy whipping cream",
            "2 tablespoons cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine chicken strips, 1/2 cup white wine, 1/2 cup chicken broth, and 1/2 cup lemon juice in a large resealable bag. Seal and place in the refrigerator for 1 hour to marinate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, oregano, basil, pepper, and salt in a bowl. Remove chicken strips from the marinade and dredge in the flour mixture. Discard marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour Parmesan cheese onto a plate. Roll chicken strips in Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 2 tablespoons butter in a skillet over medium-high heat. Cook chicken strips in batches until browned, 1 to 2 minutes per side. Transfer to a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour remaining 3/4 cup white wine, 3/4 cup chicken broth, 1/4 cup lemon juice, bouillon cubes, and garlic over chicken in the slow cooker. Cover and cook on Low until chicken is no longer pink in the center, 4 to 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer chicken to a plate using a slotted spoon. Drizzle 2 teaspoons melted butter on top. Cover to keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour liquid from the slow cooker into a pot; add parsley. Bring to a boil. Reduce heat to low and stir in heavy cream and cornstarch. Simmer until sauce thickens, about 5 minutes. Pour sauce over chicken.\n"
            }
        ],
