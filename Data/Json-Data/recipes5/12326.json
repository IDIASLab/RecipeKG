        "recipeIngredient": [
            "10 large dried red chile peppers, seeded",
            "3 peppers dried red chile peppers, seeded",
            "3 tablespoons chopped shallot",
            "2 tablespoons chopped garlic",
            "1 tablespoon chopped galangal",
            "1 tablespoon chopped fresh cilantro",
            "2 teaspoons finely chopped lemongrass",
            "2 teaspoons salt",
            "1 teaspoon kaffir lime leaves",
            "1 teaspoon ground black pepper",
            "1 teaspoon ground cumin",
            "1 teaspoon ground coriander",
            "\u00bd teaspoon ground cardamom",
            "1 tablespoon shrimp paste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak 10 large dried red chile peppers in water for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine soaked peppers, dried peppers, shallot, garlic, galangal, cilantro, lemongrass, salt, lime leaves, pepper, cumin, coriander, and cardamom in a large mortar and grind with a pestle. Add shrimp paste and continue to grind until paste is smooth.\n"
            }
        ],
