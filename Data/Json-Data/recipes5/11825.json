    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277091/spicy-seafood-soup/",
        "name": "Spicy Seafood Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Seafood Soup"
        },
        "datePublished": "2019-11-08T06:42:12.000Z",
        "description": "I fell in love with Nigerian fish pepper soup and made my own variety of fish soup using everyday foods from the local grocer.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 bowls",
        "recipeIngredient": [
            "1 pound catfish fillets",
            "1 pound uncooked medium shrimp, peeled and deveined",
            "2 tablespoons garlic powder",
            "2 tablespoons paprika",
            "1 tablespoon Cajun seasoning",
            "1 tablespoon curry powder",
            "1 teaspoon olive oil",
            "1 cup fresh corn kernels",
            "1 head bok choy, leaves and stalks separated",
            "\u00bd cup chopped onion",
            "\u00bd cup chopped green bell pepper",
            "1 serrano pepper, chopped",
            "2 tablespoons minced garlic",
            "1 cup chopped tomatoes",
            "4 cups water",
            "2 tablespoons chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season catfish and shrimp with garlic powder, paprika, Cajun seasoning, and curry powder; set aside in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a 6-quart pot over medium heat for 2 minutes; add olive oil. Add corn, bok choy, onion, bell pepper, serrano pepper, and garlic. Stir-fry until onion becomes tender, about 5 minutes. Add tomatoes and cook for 5 minutes. Pour in water and bring to a boil. Reduce heat to medium-low, cover, and cook for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add catfish and shrimp to the soup. Cover and cook until flesh is opaque, 6 to 7 minutes more. Garnish soup with cilantro.\n"
            }
        ],
