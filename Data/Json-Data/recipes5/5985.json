    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236707/spicy-grilled-chicken-with-mango-salsa/",
        "name": "Spicy Grilled Chicken with Mango Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8004321.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2019-07-10T07:05:14.000Z",
        "description": "Spicy grilled chicken with fresh colorful salsa is perfect for any summer cookout.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H16M",
        "totalTime": "P0DT0H31M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 (4 ounce) boneless, skinless chicken breasts",
            "2 tablespoons chicken seasoning",
            "\u00bc cup Mazola\u00ae Corn Oil",
            "3 tablespoons fresh lemon or lime juice",
            "2 cups diced fresh mango",
            "1 cup diced red bell pepper",
            "3 tablespoons minced red onion",
            "1 tablespoon fresh lemon or lime juice",
            "2 tablespoons minced fresh cilantro",
            "Salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill to medium heat, or between 350 degrees  to 450 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Trim excess fat from chicken, rinse and pat dry. If necessary, pound chicken to 1/2-inch thickness.  Place chicken into a 1-gallon size resealable plastic bag. Add seasoning, oil and lemon juice to the bag. Seal bag and turn to thoroughly coat chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill chicken over direct high heat for 6 to 8 minutes. Turn chicken and continue to cook for 6 to 8 minutes until cooked through. Transfer to a serving plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine salsa ingredients in a bowl; stir and season to taste with salt and pepper. (Can be made up to 8 hours ahead and refrigerated until ready to serve.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top grilled chicken with mango salsa and serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Mazola&reg;"
        ],
