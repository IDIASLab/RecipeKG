    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17839/shoofly-pie-iii/",
        "name": "Shoofly Pie III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F741831.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-02-26T20:48:05.000Z",
        "description": "A recipe my maternal grandmother used to make. She lived to be 96. Maybe the pie helped! This is best when served with whipped cream or vanilla ice cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 8-inch pie",
        "recipeIngredient": [
            "1 prepared 8 inch pie crust",
            "1 cup all-purpose flour",
            "\u2154 cup packed brown sugar",
            "\u00bc teaspoon salt",
            "5 tablespoons butter",
            "\u2154 cup hot water",
            "5 tablespoons molasses",
            "1 tablespoon dark brown sugar",
            "\u00bd teaspoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together flour, 2/3 cup brown sugar, and salt. Cut in butter or margarine until mixture resembles rice. Reserve 3 tablespoons of mixture for sprinkling top of pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, mix together hot water, molasses, dark brown sugar, and baking soda. Stir molasses mixture into remaining crumb mixture. Pour into pastry shell. Sprinkle with reserved crumb mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 35 to 40 minutes, until top springs back when pressed. Cool on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Vintage Pie Recipes"
        ],
