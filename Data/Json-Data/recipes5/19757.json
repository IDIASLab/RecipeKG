    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/214743/grilled-sweet-potatoes/",
        "name": "Grilled Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1122921.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2020-03-05T05:07:22.000Z",
        "description": "I don't really like the traditional sweet potato casserole, but my son loves sweet potatoes, so I try to come up with different ways to cook them. This spring I tried grilling them. These are now a favorite of mine and my sons.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 large sweet potatoes, cut into 1-inch cubes",
            "2 tablespoons butter",
            "2 tablespoons brown sugar",
            "1 teaspoon ground cumin",
            "\u00bc teaspoon cayenne pepper, or more to taste",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat, and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the sweet potato cubes in half, and place each half in the center of a rectangle of aluminum foil. Top each portion with about half of the butter. Sprinkle about half of the brown sugar, cumin, and cayenne pepper over each portion. Season with salt and pepper. Fold the corners of the aluminum foil together and seal to form a pouch around each portion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on the preheated grill, turning pouches every 10 minutes, until the potatoes are tender and cooked through, 20 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
