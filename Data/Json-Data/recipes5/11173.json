    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23151/my-little-secret/",
        "name": "My Little Secret",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7162891.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-10-18T17:22:11.000Z",
        "description": "This is something I just threw together one day for a pot luck at work. It's a delicious, creamy potato salad bursting with flavor. The ingredient measures may be altered to taste, and to create the desired consistency.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "10 cups",
        "recipeIngredient": [
            "2 pounds new red potatoes",
            "3  large eggs eggs",
            "\u00be pound thick cut bacon",
            "\u00bd cup sliced black olives, drained",
            "\u00bd cup diced green onions, divided",
            "1 cup mayonnaise",
            "1 cup sour cream",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil. Boil potatoes until tender but still firm, about 15 minutes. Drain, cool and dice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place eggs in a medium saucepan and cover with cold water. Bring water to a boil and immediately remove from heat. Cover and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool, peel and dice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain and crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together the potatoes, eggs, bacon, olives, 1/4 cup green onions, mayonnaise, sour cream, salt and pepper. Increase quantities of mayonnaise and sour cream as necessary to create desired consistency. Garnish with remaining green onions.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Potato Salad Recipes",
            "Red Potato Salad Recipes"
