    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273841/tortellini-alla-fini/",
        "name": "Tortellini alla Fini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9401170.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-05-11T03:17:30.000Z",
        "description": "This is my own version of tortellini alla fini. Rich Alfredo sauce with ham, peas, and tortellini. It is yum-yum goodness!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages fresh cheese tortellini",
            "6 tablespoons butter",
            "1 pint heavy cream",
            "1 teaspoon ground nutmeg",
            "1 teaspoon ground black pepper",
            "3 cups freshly shredded Parmesan cheese",
            "2 cups diced cooked ham",
            "2 cups cooked peas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil; stir in tortellini and return to a boil. Cook uncovered, stirring occasionally, until the tortellini float to the top and the filling is hot, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt butter in a saucepan over medium heat. Add cream and stir frequently until heated through. Season with nutmeg and pepper. Slowly mix in Parmesan cheese, stirring constantly, and allow to thicken. Mix in ham and peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain cooked tortellini and return to the pot. Pour sauce over tortellini and toss to combine.\n"
            }
        ],
        "recipeCategory": [
            "Pasta and Noodles",
            "Pasta by Shape Recipes",
            "Tortellini Recipes"
        ],
