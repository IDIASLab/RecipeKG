    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24765/moms-baked-beans-ii/",
        "name": "Mom's Baked Beans II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F98278.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T04:56:17.000Z",
        "description": "I'm not sure how it started but we have to have this dish at EVERY holiday meal, or there's mutiny!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H20M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "8 to 10 servings",
        "recipeIngredient": [
            "4 slices bacon",
            "1 onion, diced",
            "2 (28 ounce) cans baked beans",
            "3 tablespoons molasses",
            "2 tablespoons prepared mustard",
            "\u00bc teaspoon salt",
            "\u00bd cup diced tomatoes",
            "\u00be cup brown sugar",
            "1 tablespoon dry mustard",
            "\u00bd cup chopped cooked ham"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon and onions in a large, deep skillet. Cook over medium high heat until evenly brown. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine beans, molasses, mustard, salt, tomatoes, brown sugar, dry mustard, ham, onions and crumbed bacon.  Mix well and transfer to a 2 quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven, covered, for 1 hour.  Uncover and bake for 1 hour more.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Beans and Peas",
            "Baked Bean Recipes"
