        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266515/lisas-creamy-clam-chowder/",
        "name": "Lisa's Creamy Clam Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5607024.jpg",
