        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8498/tangy-chicken-breasts/",
        "name": "Tangy Chicken Breasts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F565455.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-21T19:27:19.000Z",
        "description": "The chicken is marinated for six hours, and then broiled.  Serve with wild rice or noodles.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT8H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 skinless, boneless chicken breast halves",
            "1 cup Worcestershire sauce",
            "1 cup vegetable oil",
            "1 cup lemon juice",
            "1 teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken in a single layer in a bowl or pan.  Mix together Worcestershire sauce, oil, lemon juice, and garlic; pour over chicken.   Cover.  Refrigerate for at least 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take chicken out of marinade, and place on broiler pan or grill.  Broil until done, approximately 7 minutes on each side.\n"
            }
        ],
