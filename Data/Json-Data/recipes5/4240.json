        "mainEntityOfPage": "https://www.allrecipes.com/recipe/139467/hollys-hummus/",
        "name": "Holly's Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6784311.jpg",
