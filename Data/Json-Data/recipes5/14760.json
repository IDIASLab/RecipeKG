        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216165/mayan-hot-chocolate/",
        "name": "Mayan Hot Chocolate",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5413970.jpg",
