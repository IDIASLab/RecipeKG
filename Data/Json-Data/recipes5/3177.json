    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23933/crab-and-cheese-spread/",
        "name": "Crab and Cheese Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F40563.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-07-10T06:52:32.000Z",
        "description": "Serve this creamy cheese and crab spread on crackers, or use it as a dip for breadsticks and veggies. You can also use shrimp instead of the crab.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT1H15M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "1 tablespoon lemon juice",
            "1 teaspoon hot pepper sauce",
            "2 green onions, finely chopped",
            "1 (6 ounce) can crabmeat, drained and flaked",
            "1 (8 ounce) jar cocktail sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, blend the cream cheese, lemon juice and hot pepper sauce. Stir in the green onions and crabmeat. Chill in the refrigerator at least 1 hour before serving.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to serve, top with cocktail sauce.\n"
            }
        ],
