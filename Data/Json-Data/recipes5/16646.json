    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10517/cookie-pralines/",
        "name": "Cookie Pralines",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F801522.jpg",
            "width": 3872,
            "height": 2592
        },
        "datePublished": "1998-02-15T21:37:56.000Z",
        "description": "Delicious pecan cookies.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "2 1/2 dozen",
        "recipeIngredient": [
            "1 cup packed dark brown sugar",
            "1 tablespoon all-purpose flour",
            "\u00bc teaspoon salt",
            "1 egg white",
            "1 teaspoon vanilla extract",
            "2 cups chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 275 degrees F (135 degrees C). Grease a baking sheet or line it with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk sugar, flour, and salt; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In separate bowl, beat egg white until stiff. Fold sugar mixture into stiffened egg white. Add pecans and vanilla.  Stir gently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto prepared baking sheet. Bake in preheated oven until browned, 25 to 30 minutes. Cool 5 minutes on baking sheet, then remove to wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Praline Recipes"
        ],
