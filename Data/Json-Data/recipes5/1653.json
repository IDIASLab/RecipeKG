    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273706/chocolate-covered-frozen-bananas/",
        "name": "Chocolate-Covered Frozen Bananas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F507209.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-05-18T03:47:31.000Z",
        "description": "You will sure be surprised how quick and easy these chocolate-covered frozen bananas are to make at home.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "6 bananas",
        "recipeIngredient": [
            "6 firm bananas, unpeeled",
            "6 popsicle sticks",
            "2 cups chocolate chips",
            "2 tablespoons vegetable shortening",
            "\u00bd cup shredded coconut, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut 1 inch off the bottom of each banana and gently insert a popsicle stick into each one. Carefully remove and discard banana peels. Place bananas on the prepared baking sheet and freeze until solid, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chocolate chips and shortening in the top of a double boiler over simmering water. Stir frequently, scraping down the sides with a rubber spatula to avoid scorching, until chocolate is melted, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip frozen bananas into the melted chocolate and roll in coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Eat immediately or return to the freezer until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
        ],
