    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277213/vegetarian-stuffed-mushrooms/",
        "name": "Vegetarian Stuffed Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7581994.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-07T07:34:56.000Z",
        "description": "Easy vegetarian stuffed mushroom recipe for the holidays.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (8 ounce) package mushrooms, stems removed",
            "4 tablespoons Italian-style salad dressing",
            "4 tablespoons butter",
            "1 onion, chopped",
            "3 cloves garlic, minced",
            "1 (10 ounce) package frozen chopped spinach, thawed and drained",
            "1 (8 ounce) package cream cheese",
            "4 tablespoons grated Parmesan cheese",
            "4 tablespoons dry bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush both sides of each mushroom cap with Italian dressing. Arrange mushrooms on a baking sheet, gill-sides up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, about 12 minutes. Drain any juice that has formed in the mushrooms. Leave the oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a pot over medium-high heat. Saute onion and garlic in the hot butter until soft, 5 to 7 minutes. Stir spinach, cream cheese, Parmesan cheese, and 3 tablespoons bread crumbs into the onion mixture until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide spinach mixture over mushroom caps; sprinkle with remaining bread crumbs. Return mushrooms to the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping is golden brown and cheese is melted, about 10 minutes.\n"
            }
        ],
