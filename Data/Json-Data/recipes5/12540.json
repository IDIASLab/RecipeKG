        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277653/easy-chicken-cordon-bleu-casserole/",
        "name": "Easy Chicken Cordon Bleu Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Chicken Cordon Bleu Casserole"
        },
        "datePublished": "2019-12-12T06:57:35.000Z",
        "description": "Cheesy chicken Cordon Bleu casserole.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound cooked penne pasta",
            "1 cup cubed cooked chicken",
            "1 cup cubed fully cooked ham",
            "1\u2009\u00bd cups milk",
            "1 (8 ounce) package cream cheese",
            "2 tablespoons dried minced onion",
            "2 cups shredded Swiss cheese",
            "1 pinch cayenne pepper, or to taste",
            "salt and ground black pepper to taste",
            "1 cup panko bread crumbs",
            "\u00bd cup salted butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss pasta with chicken and ham in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk and cream cheese in a saucepan over medium to low heat until combined, about 5 minutes. Pour over casserole. Cover with Swiss cheese. Season with cayenne, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until melted and bubbly, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine panko and melted butter in a bowl; top casserole with mixture. Turn on the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until browned, 3 to 4 minutes.\n"
            }
        ],
