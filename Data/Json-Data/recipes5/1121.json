        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273211/coconut-cookies/",
        "name": "Coconut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7053591.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-05-01T03:12:39.000Z",
        "description": "These low-carb and gluten-free cookies with coconut oil can be baked or left unbaked. If baking, this recipe produces a crisp cookie. Adjust temperature and time to your liking.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "2 cups unsweetened shredded coconut",
            "1 cup almond flour",
            "\u2153 cup maple syrup",
            "3\u2009\u00bd tablespoons melted coconut oil",
            "\u00bd teaspoon vanilla extract",
            "\u215b teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together coconut, almond flour, maple syrup, coconut oil, vanilla extract, and salt in a bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop cookie dough in tablespoon-sized mounds onto the prepared baking sheets. Cover and refrigerate or freeze until use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "If baking cookies, preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cookies to 2 ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned, 5 to 10 minutes.\n"
            }
        ],
