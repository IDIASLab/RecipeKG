    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276676/gluten-free-snickerdoodles/",
        "name": "Gluten-Free Snickerdoodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7128690.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-10-25T12:46:14.000Z",
        "description": "These gluten-free snickerdoodles cookies are easy and fast, but they are also super yummy and soft.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 cookies",
        "recipeIngredient": [
            "2 eggs",
            "\u00bc cup melted coconut oil",
            "\u00bc cup coconut sugar",
            "1 teaspoon apple cider vinegar",
            "1 cup white rice flour",
            "3 tablespoons cinnamon sugar, divided",
            "\u00bc teaspoon baking soda",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs, oil, coconut sugar, and vinegar together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, 2 tablespoons cinnamon sugar, baking soda, and salt together in another bowl. Slowly incorporate into egg mixture; don't worry if dough seems like batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by tablespoonfuls onto the prepared baking sheet quickly. Sprinkle tops with remaining cinnamon sugar as desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 12 minutes. Remove from the oven and let cool for 5 minutes before enjoying.\n"
            }
        ],
