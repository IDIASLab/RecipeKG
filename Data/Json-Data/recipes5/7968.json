    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274525/hot-cauliflower-wings/",
        "name": "Hot Cauliflower Wings",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7418387.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-12T08:09:54.000Z",
        "description": "Spicy and crispy on the outside, tender and juicy on the inside. Serve these cauliflower wings with a vegan ranch or blue cheese dressing.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 small head cauliflower, broken into small florets",
            "1 cup all-purpose flour",
            "1 teaspoon paprika",
            "salt and ground black pepper to taste",
            "2 cups almond milk, or as needed",
            "\u00bd cup bread crumbs, or as needed",
            "2 ounces vegan margarine (such as Earth Balance\u00ae)",
            "1 (8 ounce) bottle hot pepper sauce (such as Frank's RedHot\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Line a cookie sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, paprika, salt, and pepper together. Add enough almond milk to create a slightly thick, soupy texture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread crumbs in a shallow bowl. Dip cauliflower pieces one at a time into the batter, then roll in bread crumbs. Place on the prepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until starting to brown, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While 'wings' are baking, melt margarine and whisk together with hot sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 425 degrees F (220 degrees C). Dip 'wings' into the sauce and place back on the cookie sheet; continue baking for 10 to 15 minutes. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Cauliflower",
            "Roasted"
        ],
