        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21655/simple-baked-beans/",
        "name": "Simple Baked Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7761108.jpg",
