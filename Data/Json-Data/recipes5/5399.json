        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228760/easy-extremely-garlic-shrimp/",
        "name": "Easy Extremely Garlic Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7633231.jpg",
