    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/79252/apricot-cherry-bars-with-oatmeal-crumble-topping/",
        "name": "Apricot-Cherry Bars With Oatmeal Crumble Topping",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1900428.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T04:59:05.000Z",
        "description": "Apricot-Cherry Bars With Oatmeal Crumble Topping are delectable, bake-ahead and foolproof.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "50 bite-sized bars",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 cup old-fashioned oats",
            "\u00be cup light brown sugar, firmly packed",
            "\u00bc teaspoon salt",
            "8 tablespoons butter",
            "2 cups sweetened, flaked coconut",
            "2 cups sliced almonds",
            "2 cups  dried cherries",
            "2 cups dried apricots, coarsely chopped",
            "1\u2009\u00bd (14 ounce) cans sweetened condensed milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Adjust oven rack to lower-middle position; heat oven to 325 degrees. Spray a 13-by-9-inch Pyrex baking pan with vegetable cooking spray. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix flour, oats, brown sugar and salt. Stir in butter with a fork until well mixed and clumps have formed. Spread 1 1/2 packed cups of oat mixture over the bottom of the pan, pressing to form a thin crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix coconut, almonds, cherries, apricots and milk in a large bowl. Pour over crust, using a rubber spatula to evenly distribute and press down. Sprinkle remaining oat mixture over dried fruit filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until lightly golden, about 30 minutes. Cool to room temperature. (You can cover and store at room temperature up to 2 days.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, cut into generous 1 1/2-inch squares.\n"
            }
        ],
