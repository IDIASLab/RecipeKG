    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273030/leftover-ham-and-veggie-soup/",
        "name": "Leftover Ham and Veggie Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Leftover Ham and Veggie Soup"
        },
        "datePublished": "2019-04-26T10:42:56.000Z",
        "description": "I created this soup using leftover ham and veggies I had on hand. It's quick and easy. Hope you like it.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "1 large onion, diced",
            "3 large russet potatoes, diced",
            "2 cups diced cooked ham",
            "2 cups diced carrots",
            "1 cup diced celery",
            "2 tablespoons dried parsley",
            "2 tablespoons Italian seasoning",
            "2 dried bay leaves",
            "3 (14.5 ounce) cans chicken broth",
            "1 (15 ounce) can corn, drained",
            "1 (15 ounce) can peas, drained",
            "2 tablespoons butter",
            "ground black pepper to taste",
            "\u00bd cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil over medium heat in a large stockpot. Add onion; cook and stir until softened and translucent, 3 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes, ham, carrots, celery, parsley, and Italian seasoning into pot. Cook and stir until vegetables begin to soften, about 5 minutes. Add bay leaves, chicken broth, corn, and peas and bring to a boil, about 5 minutes. Reduce heat to medium and cook until vegetables have cooked down and are tender, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove bay leaves; add butter and pepper to taste. Top with shredded Cheddar cheese.\n"
            }
        ],
