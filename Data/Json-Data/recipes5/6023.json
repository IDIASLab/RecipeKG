        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237085/maple-salad-dressing/",
        "name": "Maple Salad Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1128972.jpg",
