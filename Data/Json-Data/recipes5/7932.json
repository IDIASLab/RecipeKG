        "name": "Ajoblanco (Cold Spanish Almond Soup)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5607430.jpg",
            "width": 1707,
            "height": 960
