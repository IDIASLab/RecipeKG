    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8953/easy-mexican-fried-chicken/",
        "name": "Easy Mexican Fried Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F828131.jpg",
            "width": 4288,
            "height": 3216
        },
        "datePublished": "1999-04-26T22:03:44.000Z",
        "description": "Great with Mexican rice and fried beans with cheese melted on top, if desired! Sort of 'On-The-Border Southern Fried Chicken'.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 skinless, boneless chicken breast halves",
            "1 quart buttermilk",
            "2 (1.25 ounce) packages taco seasoning mix",
            "3 tablespoons olive oil",
            "1\u2009\u00bd cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a resealable plastic bag combine the chicken, buttermilk and 1 packet of taco seasoning. Seal and shake to mix together. Refrigerate and let marinate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, heat the olive oil over medium low heat. Mix flour and the other packet of taco seasoning in a shallow dish or plate. Remove chicken from refrigerator and remove chicken from marinade. Discard marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat chicken with flour mixture and fry in skillet until cooked through and juices run clear, 15 to 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Fried Chicken Recipes"
        ],
