    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276782/udon-noodle-stir-fry/",
        "name": "Udon Noodle Stir-Fry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5019690.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-10-30T23:24:12.000Z",
        "description": "Perfect for a weeknight dinner, this udon noodle stir-fry comes together quickly and really delivers on taste with tender-crisp veggies, edamame, and a soy and maple syrup sauce.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 (9 ounce) packages udon noodles",
            "\u00bd cup soy sauce",
            "4 tablespoons water",
            "4 tablespoons rice vinegar",
            "4 tablespoons maple syrup",
            "2 teaspoons Sriracha sauce, or to taste",
            "2 large cloves garlic, minced",
            "1 tablespoon grated fresh ginger ",
            "2 teaspoons cornstarch",
            "2 tablespoons vegetable oil",
            "4 cups fresh broccoli florets",
            "4 cups sliced carrots",
            "1 cup shelled edamame",
            "4 green onions, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook udon in boiling water, stirring occasionally, until noodles are tender yet firm to the bite, 10 to 12 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While noodles are cooking, whisk together soy sauce, water, rice vinegar, maple syrup, Sriracha sauce, garlic, ginger, and cornstarch in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large wok over high heat. Add broccoli and carrots; stir-fry until tender-crisp, 3 to 5 minutes. Add noodles, soy sauce mixture, and edamame. Stir-fry for another minute, using a fork to pull noodles apart. Remove from heat and mix in green onions.\n"
            }
        ],
