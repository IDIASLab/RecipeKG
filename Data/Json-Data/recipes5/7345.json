        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263215/salmon-tartare/",
        "name": "Salmon Tartare",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5221629.jpg",
            "width": 3888,
            "height": 2592
