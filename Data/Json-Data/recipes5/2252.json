    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274066/indian-green-curry/",
        "name": "Indian Green Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Indian Green Curry"
        },
        "datePublished": "2019-06-12T04:52:42.000Z",
        "description": "A variation on a Thai green curry, this is a spicy dish that my boyfriend said 'tastes as good as a take-away.' Best served with rice, naan, and mango chutney.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon butter",
            "2 teaspoons olive oil",
            "2 green chile peppers, finely sliced, or to taste",
            "1 clove garlic, finely sliced ",
            "1 teaspoon whole black peppercorns",
            "2 cardamom pods",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground turmeric",
            "1 teaspoon ground cumin",
            "2 medium skinless, boneless chicken breasts, diced",
            "1 medium potato, diced",
            "1 large green bell pepper, diced",
            "1 red onion, diced",
            "1 (14 ounce) can coconut milk",
            "2 tablespoons plain yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter over medium heat in a large wok; pour in olive oil. Add chile peppers, garlic, peppercorns, cardamom pods, cinnamon, turmeric, and cumin. Add chicken; stir-fry in the mixture until fully covered and starting to brown, about 5 minutes. Add potato, pepper, and onion; continue to stir-fry for 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir coconut milk into the chicken mixture. Reduce heat to low and let simmer, stirring occasionally, until sauce has thickened, about 30 minutes. Remove from heat; stir in yogurt.\n"
            }
        ],
