        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273799/garlic-butter-shrimp/",
        "name": "Garlic Butter Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5300634.jpg",
