    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/62892/spicy-bbq-chicken/",
        "name": "Spicy BBQ Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F38884.jpg",
            "width": 2848,
            "height": 2132
        },
        "datePublished": "2020-03-05T04:58:21.000Z",
        "description": "The traditional BBQ sauce for grilled chicken (or ribs!) that everyone has had at some point -- very tasty with a little extra personality. You will not regret cooking!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "\u00bc cup onion, finely chopped",
            "1 clove garlic, minced",
            "\u00be cup ketchup",
            "\u2153 cup vinegar",
            "1 tablespoon Worcestershire sauce",
            "2 teaspoons brown sugar",
            "1 teaspoon dry mustard",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon black pepper",
            "\u00bc (5 ounce) bottle hot pepper sauce",
            "1 (3 pound) chicken, cut into pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a skillet over medium heat and cook the onion and garlic until tender. Mix in ketchup, vinegar, Worcestershire sauce, brown sugar, dry mustard, salt, pepper and hot sauce. Bring to a boil. Reduce heat to low and simmer 10 minutes, stirring occasionally. Remove from heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly oil grill grate. Place chicken on grill. Brush constantly with the sauce and cook 8 to 15 minutes on each side, depending on size of piece, until juices run clear. Discard any remaining sauce.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken"
        ],
