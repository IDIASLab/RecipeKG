    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234173/mouth-watering-chicken-dish/",
        "name": "Mouth-Watering Chicken Dish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1068859.jpg",
            "width": 1586,
            "height": 1583
        },
        "datePublished": "2019-07-10T07:04:51.000Z",
        "description": "I made this for a family whose father was a chef. He thought this was incredible!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H45M",
        "totalTime": "P0DT2H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "8 skinless chicken thighs",
            "1 lemon, juiced",
            "1 teaspoon garlic salt, or to taste",
            "1 tablespoon Italian seasoning",
            "6 red potatoes, halved",
            "1 tablespoon water, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle olive oil into the bottom of a 9x13-inch baking dish. Arrange chicken thighs in the baking dish; drizzle about half the lemon juice onto chicken. Sprinkle half the garlic salt and half the Italian seasoning over chicken. Arrange potatoes around the perimeter of the baking dish, framing the chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in the preheated oven for 45 minutes. Reduce heat to to 300 degrees F (150 degrees C) and continue cooking chicken, turning every 30 minutes and seasoning with lemon juice, garlic salt, Italian seasoning, and water on the second turn, until chicken is very tender, 2 to 3 hours.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Thigh Recipes"
        ],
