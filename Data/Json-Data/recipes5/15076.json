    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278203/baked-shrimp-dip/",
        "name": "Baked Shrimp Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Baked Shrimp Dip"
        },
        "datePublished": "2020-02-16T11:24:19.000Z",
        "description": "This baked shrimp easy dip is perfect for entertaining when served with pita triangles or French bread.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 pound frozen fully cooked salad shrimp, thawed",
            "\u00bc cup finely chopped fresh parsley",
            "salt and ground black pepper to taste",
            "\u00bd cup ketchup, or more as needed",
            "2 tablespoons prepared horseradish, or more as needed",
            "\u00bd cup mayonnaise",
            "3 tablespoons seasoned bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat. Saute shrimp in the hot oil with parsley, salt, and pepper until just heated through, 3 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ketchup and horseradish together in a bowl for cocktail sauce, adding more ketchup or horseradish to adjust spiciness. Stir in mayonnaise and mix well. Fold in cooked shrimp until well mixed. Pour into a small, shallow baking dish. Sprinkle bread crumbs evenly over the top of the mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are bubbly, about 20 minutes.\n"
            }
        ],
