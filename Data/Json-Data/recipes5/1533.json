        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273431/no-bake-creamy-raspberry-pie/",
        "name": "No-Bake Creamy Raspberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6651068.jpg",
            "width": 3231,
            "height": 3024
        },
        "datePublished": "2019-05-14T03:19:01.000Z",
        "description": "This delicious, quick, and easy no-bake raspberry cream pie is nice and refreshing on a hot summer day!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "2 9-inch pies",
        "recipeIngredient": [
            "\u2154 cup water",
            "1 (3 ounce) package raspberry-flavored gelatin mix",
            "2 cups ice cubes",
            "3\u2009\u00bd cups whipped topping",
            "2\u2009\u00bd cups fresh raspberries, divided",
            "2 (9 inch)  prepared graham cracker crusts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a medium saucepan. Stir raspberry gelatin into boiling water until dissolved. Remove from heat. Add ice cubes; stir until mixture is cooled. Remove remaining ice cubes that did not melt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir whipped topping into gelatin mixture until thoroughly combined. Fold in 1 1/2 cups raspberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture evenly between the 2 graham cracker crusts. Decorate with remaining raspberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate pies at least 2 hours to overnight.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes"
