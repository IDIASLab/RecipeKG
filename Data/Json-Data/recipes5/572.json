    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272603/coconut-water-lime-rice-pilaf/",
        "name": "Coconut Water-Lime Rice Pilaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6585984.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2019-04-06T05:55:12.000Z",
        "description": "A wonderful, coconut water and rice side dish for baked coconut shrimp.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 tablespoons butter, divided",
            "1 cup uncooked jasmine rice",
            "2 cups coconut water",
            "1 teaspoon salt",
            "2 limes, juiced, divided",
            "\u00bd cup shredded carrots",
            "2 tablespoons chopped green onions",
            "2 tablespoons chopped fresh cilantro",
            "1 lime, quartered"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 1-quart covered baking dish with 1 tablespoon butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt remaining 2 tablespoons butter in a large, deep skillet over medium-high heat. Stir in jasmine rice and cook until slightly golden, 3 to 4 minutes. Pour in coconut water. Season with salt. Mix in juice of 1 lime. Reduce to a simmer and stir occasionally until liquid begins to be absorbed, about 5 minutes. Transfer to the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, covered, in the preheated oven until the coconut water is completely absorbed, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the cover and add carrots, green onions, cilantro, and remaining lime juice. Fluff with a fork; cover and let stand for 5 minutes. Serve with lime wedges.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Rice Side Dish Recipes",
            "Pilaf"
        ],
