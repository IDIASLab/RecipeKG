    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221912/jules-zesty-7-layer-dip/",
        "name": "Jules' Zesty 7 Layer Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F947157.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-07-10T07:03:18.000Z",
        "description": "This dip is different from all of the ones online. I'm always asked to make it and it's always all eaten. I usually cut the recipe in half unless it's for a large group.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "2 tablespoons sour cream",
            "1 tablespoon mayonnaise",
            "1 tablespoon Worcestershire sauce",
            "1 tablespoon lemon juice",
            "1 teaspoon hot pepper sauce (such as Tabasco\u00ae)",
            "1 cup salsa, or as desired",
            "1 tomato, chopped",
            "1 green bell pepper, chopped",
            "5 green onions, chopped",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese, sour cream, mayonnaise, Worcestershire sauce, lemon juice, and hot pepper sauce in a bowl until smoothly combined; spread mixture in an even layer over a large plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread salsa in a layer over the cream cheese mixture. Combine tomato, green pepper, and green onions in a bowl and spread the vegetables over the salsa. Cover the dip with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until serving time; remove from refrigerator about 20 minutes before serving to allow cream cheese mixture to soften.\n"
            }
        ],
