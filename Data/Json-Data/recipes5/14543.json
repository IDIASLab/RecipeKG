    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262300/mexican-bean-soup-with-chicken/",
        "name": "Mexican Bean Soup with Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mexican Bean Soup with Chicken"
        },
        "datePublished": "2020-02-01T07:52:02.000Z",
        "description": "Mayocoba beans, Roma tomatoes, serrano chiles, roasted chicken breast, and cilantro come together in a simple, flavorful Mexican soup.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H15M",
        "totalTime": "P0DT10H35M",
        "recipeYield": "1 pot of soup",
        "recipeIngredient": [
            "1 pound mayocoba beans (such as Rancho Gordo\u00ae)",
            "water to cover",
            "2 tablespoons vegetable oil, divided",
            "1 white onion, chopped",
            "4 Roma (plum) tomatoes, chopped",
            "1 serrano chile pepper, minced",
            "2 rotisserie chicken breasts, diced",
            "1 quart chicken stock",
            "\u00bd bunch fresh cilantro, chopped",
            "sea salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put mayocoba beans into a large container with enough cool water to cover by several inches. Let beans soak 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain and rinse beans thoroughly; put into a large stockpot. Add enough water to cover beans by 2 inches. Bring to a boil, reduce heat to medium-low, and cover the pot. Simmer until beans are soft in the center, about 90 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon oil in a large skillet over medium heat. Add onion; cook and stir until tender, about 5 minutes. Add tomatoes and serrano pepper; simmer until softened slightly, about 5 minutes. Reduce heat to medium-low and continue cooking until liquid begins to thicken, about 10 minutes more. Season tomato mixture with salt and remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in another large skillet over medium heat. Add chicken meat and cook, stirring occasionally, until evenly browned, 5 to 7 minutes. Stir in the tomato mixture and simmer 10 minutes. Pour in chicken stock. Bring soup to a boil and simmer until flavors combine, 15 minutes more. Stir into the pot of beans. Add cilantro and salt. Bring to a simmer and cook until beans are completely softened, about 30 minutes more.\n"
            }
        ],
