        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254899/scrambled-peanut-butter-eggs/",
        "name": "Scrambled Peanut Butter Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3877359.jpg",
            "width": 3446,
            "height": 3450
