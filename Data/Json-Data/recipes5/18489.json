        "mainEntityOfPage": "https://www.allrecipes.com/recipe/77978/moms-little-smokies/",
        "name": "Mom's Little Smokies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F334598.jpg",
