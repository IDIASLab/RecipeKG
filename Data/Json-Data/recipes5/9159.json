        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16657/instant-potato-casserole/",
        "name": "Instant Potato Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F350036.jpg",
