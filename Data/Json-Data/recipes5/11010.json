    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276358/chocolate-peanut-butter-cup-cupcakes/",
        "name": "Chocolate Peanut Butter Cup Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Peanut Butter Cup Cupcakes"
        },
        "datePublished": "2019-10-12T07:03:13.000Z",
        "description": "Yummy cupcakes with mini peanut butter cups in the center!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "24 cupcakes",
        "recipeIngredient": [
            "1 package chocolate cake mix",
            "1\u2009\u00bc cups water",
            "3 eggs",
            "\u00bd cup peanut butter",
            "\u2153 cup canola oil",
            "24 miniature peanut butter cups",
            "\u00bd cup heavy whipping cream",
            "\u00bd cup semisweet chocolate chips",
            "\u00bd cup peanut butter",
            "20 miniature peanut butter cups, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 muffin tins with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cake mix, water, eggs, peanut butter, and oil in a large bowl. Beat on low speed until blended. Increase speed to high and beat until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill muffin cups halfway with batter. Push a peanut butter cup into the center of each cupcake so that it is level with the batter. Add an additional tablespoon of batter to each cupcake, covering the peanut butter cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are firm and pulling away from the sides of the pan, 15 to 18 minutes. Cool for 10 minutes before transferring to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place heavy cream and chocolate chips in a small microwave-safe bowl. Heat for 1 1/2 minutes, 30 seconds at a time, stirring after each interval. Stir in peanut butter until frosting is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each cupcake into the frosting, turn in a circular motion, and pull up and out quickly. Sprinkle with chopped peanut butter cups immediately.\n"
            }
        ],
