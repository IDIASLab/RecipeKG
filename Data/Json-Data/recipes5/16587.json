        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10127/make-ahead-peanut-butter-cookies/",
        "name": "Make Ahead Peanut Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F971111.jpg",
