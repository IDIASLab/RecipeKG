    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275298/earl-grey-tea-biscuits/",
        "name": "Earl Grey Tea Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F679916.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-08-17T06:06:21.000Z",
        "description": "Delicious, airy biscuits with the great taste of Earl Grey tea in them. Enjoy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "10 biscuits",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "1 tablespoon baking powder",
            "2 bag Earl Grey tea, ground to a powder",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground nutmeg",
            "\u00be cup butter, softened",
            "\u00bd cup powdered sugar",
            "\u2154 cup strong brewed Earl Grey tea"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder, powdered tea, salt, and nutmeg together in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix butter and powdered sugar together in a small bowl until smooth. Cut butter-sugar mixture into the flour mixture until it resembles coarse crumbs. Stir in brewed tea until a soft dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough briefly on a floured surface. Roll out into a circle about 1/2 inch thick. Cut out biscuits using a round cutter and transfer to the baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 12 to 15 minutes.\n"
            }
        ],
