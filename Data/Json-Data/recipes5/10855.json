        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255010/soft-hard-boiled-eggs/",
        "name": "Soft Hard-Boiled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3850888.jpg",
