    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273343/tex-mex-savory-popcorn/",
        "name": "Tex-Mex Savory Popcorn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6656280.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-05-09T02:35:21.000Z",
        "description": "A delicious popcorn that combines all of your favorite Tex-Mex flavors!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup popcorn kernels",
            "3 tablespoons canola oil",
            "\u00bd teaspoon salt",
            "3 tablespoons butter",
            "\u00bd lime, zested and juiced",
            "1 teaspoon paprika",
            "1 teaspoon chili powder",
            "1 teaspoon garlic powder",
            "1 teaspoon onion powder",
            "1 teaspoon ground cumin",
            "1 pinch cayenne pepper",
            "2 ounces Cotija cheese, finely grated"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place popcorn, oil, and salt in a 4-quart pan. Cover with a lid; make sure lid is not too tight so steam can escape.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the pan over high heat. Move pan back and forth when kernels begin to pop. Continue shaking the pan until popping sounds slow down. Remove from heat. Stir in any salt that accumulated on the sides and transfer popcorn to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add butter to the hot pan to melt. Drizzle melted butter over the popcorn. Drizzle lime juice and zest over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine paprika, chili powder, garlic powder, onion powder, cumin, and cayenne pepper in a bowl. Sprinkle seasonings and Cotija cheese over the popcorn; toss to mix.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Popcorn Recipes"
        ],
