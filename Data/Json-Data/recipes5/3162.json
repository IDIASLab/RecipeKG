        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23051/crab-dip-ii/",
        "name": "Crab Dip II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8227.jpg",
