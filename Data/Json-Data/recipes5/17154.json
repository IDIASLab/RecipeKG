    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17854/pistachio-nut-bundt-cake/",
        "name": "Pistachio Nut Bundt Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F30140.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-01-21T20:26:48.000Z",
        "description": "A green Bundt cake. This is perfect for Christmas or St. Patrick's day.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 - 9 inch Bundt cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "2 (3.4 ounce) packages instant pistachio pudding mix",
            "1 cup sour cream",
            "\u00bd cup vegetable oil",
            "4 eggs",
            "\u00bd cup packed brown sugar",
            "\u00bd cup chopped walnuts",
            "1 teaspoon ground cinnamon",
            "\u00bc cup confectioners' sugar for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour a 9 inch Bundt pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the cake mix and instant pudding. Add the sour cream, oil and eggs, mix well. Pour half of the batter into the prepared pan. Combine the brown sugar, walnuts and cinnamon, sprinkle over the batter in the pan. Cover with the remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 hour in the preheated oven, until cake springs back when lightly touched. Cool for 15 minutes in pan before inverting onto a wire rack to cool completely. When cake is cooled, dust with confectioners' sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pistachio Dessert Recipes"
        ],
