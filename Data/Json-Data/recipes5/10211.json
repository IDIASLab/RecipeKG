    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276424/beet-relish/",
        "name": "Beet Relish",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Beet Relish"
        },
        "datePublished": "2019-10-04T11:35:53.000Z",
        "description": "Just the right hint of sweetness in this relish.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 pints beet relish",
        "recipeIngredient": [
            "6 cups cooked and finely chopped beets",
            "4 cups white sugar",
            "2 cups white vinegar",
            "1 tablespoon lemon juice",
            "1 teaspoon salt",
            "\u00bd cup water",
            "2 (.25 ounce) packages unflavored gelatin (such as Knox\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Inspect 4 pint-sized jars for cracks and rings for rust, discarding any defective ones. Immerse in simmering water until relish is ready. Wash new, unused lids and rings in warm soapy water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine beets, sugar, vinegar, lemon juice, and salt in a large pot. Bring to a boil over medium-high heat, reduce heat, and simmer for about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir water and gelatin together in a cup and add to the beets. Boil for 7 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack relish into hot, sterilized jars, filling to within 1/4 inch of the top. Run a clean knife or thin spatula around the insides of the jars to remove any air bubbles. Wipe rims with a moist paper towel to remove any residue. Top with lids and screw rings on tightly. Store in a cool place.\n"
            }
        ],
