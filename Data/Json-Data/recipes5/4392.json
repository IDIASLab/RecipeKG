        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149939/spicy-green-beans/",
        "name": "Spicy Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F696817.jpg",
