    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273484/loaded-baked-cauliflower/",
        "name": "Loaded Baked Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F685042.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-05-18T04:00:11.000Z",
        "description": "A great low-carb substitute for the classic side dish, the loaded baked potato. Top with a dollop of sour cream if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "1 8-inch baking dish",
        "recipeIngredient": [
            "6 cups fresh cauliflower florets",
            "2 tablespoons sour cream, or more to taste",
            "1 tablespoon butter",
            "salt and ground black pepper to taste",
            "1 (8 ounce) package shredded Cheddar cheese",
            "2 tablespoons real bacon bits",
            "1 tablespoon chopped fresh chives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a pot of water to a rolling boil. Add cauliflower and boil until fork-tender, 5 to 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sour cream, butter, salt, and pepper to the cauliflower. Mash to desired texture, adding more sour cream for a creamier consistency.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cauliflower mixture to an 8-inch baking dish. Top with Cheddar cheese and bacon bits.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and bubbly, 10 to 15 minutes. Sprinkle with chives and serve.\n"
            }
        ],
