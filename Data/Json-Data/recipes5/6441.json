    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242129/debs-red-pepper-pesto/",
        "name": "Deb's Red Pepper Pesto",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2408665.jpg",
            "width": 1230,
            "height": 1230
        },
        "datePublished": "2019-07-10T07:06:15.000Z",
        "description": "This is a great summer recipe when basil and fresh corn is abundant! It is quick and easy and perfect to bring to a summer gathering.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 bunches fresh basil, stems removed",
            "1 teaspoon extra-virgin olive oil, or as needed",
            "2 cloves garlic, minced, or more to taste",
            "2 ounces pine nuts",
            "3 roasted red bell peppers, seeded and peeled, or more to taste",
            "1 lemon, juiced",
            "1 tablespoon balsamic vinegar, or more to taste",
            "1 tablespoon grated Parmesan cheese, or to taste",
            "salt and ground black pepper to taste",
            "3 tablespoons extra-virgin olive oil, or more as desired",
            "2 tablespoons cooked corn kernels, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place basil in a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat the bottom of a skillet with about 1 teaspoon olive oil and heat over medium; cook and stir garlic until lightly browned, 2 to 3 minutes. Add cooked garlic to basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a separate skillet over medium-high heat; add pine nuts and cook until toasted and fragrant, 3 to 5 minutes. Add toasted pine nuts and roasted red bell pepper to basil mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Process basil mixture until evenly combined and chunky.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lemon juice, balsamic vinegar, Parmesan cheese, salt, and pepper into red bell pepper mixture; process, adding the 3 tablespoons olive oil a little at a time, until desired consistency is reached. Add corn and process just until mixed.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces"
        ],
