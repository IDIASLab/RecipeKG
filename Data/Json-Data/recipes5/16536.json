    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9763/chocolate-chip-cookie-bars/",
        "name": "Chocolate Chip Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8260697.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Is it a bar, or is it a cookie?",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 cups all-purpose flour",
            "1 teaspoon vanilla extract",
            "1 cup packed brown sugar",
            "1 pinch salt",
            "1 cup semisweet chocolate chips",
            "\u00be cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat together the butter, vanilla and sugar until light and fluffy. Sift together the salt and flour and mix into the butter mixture.  Fold in the chocolate chips and nuts.  Mix until well blended. Press batter into a 15x10 inch jelly roll pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 20 minutes or until golden brown.  Cut into bars and let cool in pan on wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Chip Cookie Recipes"
        ],
