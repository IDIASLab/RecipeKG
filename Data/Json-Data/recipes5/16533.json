        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9747/almond-kisses/",
        "name": "Almond Kisses",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F38877.jpg",
