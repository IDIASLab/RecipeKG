    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10206/oatmeal-and-everything-bars/",
        "name": "Oatmeal and Everything Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F192722.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-12-12T20:06:15.000Z",
        "description": "This recipe contains oatmeal, caramel and chocolate, and makes really chewy, delicious bars.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 (14 ounce) package individually wrapped caramels",
            "3 tablespoons milk",
            "\u00be cup margarine",
            "1 cup all-purpose flour",
            "1 cup rolled oats",
            "\u00be cup packed brown sugar",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel the plastic wrapping from the caramels and melt them in a saucepan over medium heat along with the milk. ( If using a microwave, use a  large, microwave-safe bowl, melt the caramels and milk in the microwave at medium power for 2 to 3 minutes, checking every 20 to 30 seconds.)  Stir and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the margarine and combine with flour, oats, brown sugar, baking soda and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press slightly  more than half the oat mixture into a greased 11 x 7 inch pan.  Sprinkle chocolate chips on top of the mixture, then pour the caramel over the chips.  Top with remaining oat mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 minutes; cool.  Cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes"
        ],
