        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12502/frosty-pumpkin-pie/",
        "name": "Frosty Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F128904.jpg",
