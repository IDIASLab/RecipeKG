    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261250/zesty-broccoli-chicken-casserole/",
        "name": "Zesty Broccoli Chicken Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Zesty Broccoli Chicken Casserole"
        },
        "datePublished": "2019-08-02T05:50:04.000Z",
        "description": "Cheese, broccoli mix, cream of chicken and cream of mushroom soups, rice, salsa, and chicken, with Panko seasoned bread crumbs.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 cups cooked rice",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (16 ounce) package frozen stir-fry vegetables, thawed",
            "2 cooked chicken breasts, shredded",
            "2 cups shredded Mexican cheese blend",
            "1 cup salsa",
            "\u00bc cup margarine",
            "1 cup Italian seasoned bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine rice, cream of chicken soup, and cream of mushroom soup in a large bowl. Mix in vegetables, shredded chicken, 1 cup Mexican cheese blend, and salsa. Transfer chicken mixture to a baking pan. Sprinkle remaining cheese evenly on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a skillet over medium heat. Add bread crumbs, stirring constantly until the crumbs turn golden brown, about 5 minutes. Pour bread crumbs evenly over the chicken mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through, about 45 minutes.\n"
            }
        ],
