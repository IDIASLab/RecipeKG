        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257013/lemon-cinnamon-pancakes/",
        "name": "Lemon Cinnamon Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4560510.jpg",
            "width": 3264,
            "height": 1836
        },
        "datePublished": "2019-07-10T07:10:37.000Z",
        "description": "This is a modification of a recipe I learned in New Zealand... sweet and delicious pancakes with cinnamon and lemon.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P0DT0H16M",
        "recipeYield": "4 pancakes",
        "recipeIngredient": [
            "3 tablespoons butter, melted",
            "1 cup milk",
            "1 egg",
            "2 tablespoons honey",
            "1 cup all-purpose flour",
            "1 tablespoon ground cinnamon",
            "1 lemon, halved",
            "1 tablespoon confectioners' sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large skillet over medium heat, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour melted butter into a bowl. Add milk, egg, and honey; blend well. Stir in flour and cinnamon until batter is just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of batter into the buttered skillet. Cook until bubbles form and the edges are dry, 3 to 4 minutes. Flip and cook until browned on the other side, 2 to 3 minutes. Repeat with remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze lemon juice over pancakes. Sprinkle confectioners' sugar on top.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
