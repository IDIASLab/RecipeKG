        "recipeIngredient": [
            "5 large sweet potatoes, thinly sliced",
            "cooking spray",
            "3 teaspoons roasted garlic-pepper seasoning, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread sweet potato slices evenly on 2 cookie sheets. Coat evenly with cooking spray; sprinkle 1/2 of the garlic-pepper seasoning on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Flip, spray on the second side, and sprinkle remaining seasoning on top. Continue baking until tender, about 15 minutes more.\n"
            }
        ],
