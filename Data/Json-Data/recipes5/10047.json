        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276126/grilled-stuffed-mushrooms/",
        "name": "Grilled Stuffed Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grilled Stuffed Mushrooms"
        },
        "datePublished": "2019-09-29T05:11:25.000Z",
        "description": "My grandmother used to make these grilled stuffed mushrooms for special occasions and all of us kids loved them.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 mushroom caps",
        "recipeIngredient": [
            "6 slices bacon",
            "2 cups ricotta cheese",
            "2 cups mozzarella cheese",
            "4 green onions, chopped",
            "1 egg",
            "1 teaspoon garlic powder",
            "1 teaspoon ground black pepper",
            "6 mushroom caps"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and finely chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cooked bacon, ricotta cheese, mozzarella cheese, green onions, egg, garlic powder, and pepper together in a large bowl. Load mushroom caps evenly with mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook mushrooms on the preheated grill until cheese is melted and slightly brown, 8 to 10 minutes.\n"
            }
        ],
