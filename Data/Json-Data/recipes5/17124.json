    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17491/mouth-watering-ribs/",
        "name": "Mouth Watering Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F323231.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-03-12T00:55:17.000Z",
        "description": "These ribs are cooked in a barbeque sauce which is out of this world! It contains simple ingredients, yet it gives the ribs great flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds pork spareribs",
            "1 onion, chopped",
            "1 cup ketchup",
            "2 tablespoons cider vinegar",
            "2 tablespoons lemon juice",
            "1 tablespoon prepared mustard",
            "2 tablespoons brown sugar",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ribs in a large pot over high heat with water to cover. Boil for 45 minutes. Remove ribs, allow to cool slightly, then cut into individual ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate medium bowl, combine the onion, ketchup, vinegar, lemon juice, mustard, brown sugar, salt and ground black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange ribs in a 10x15 inch baking dish. Pour sauce over ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 45 minutes to 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes",
            "Spare Ribs"
        ],
