    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233144/cheesy-sausage-mushroom-quiche/",
        "name": "Cheesy Sausage Mushroom Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8247667.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-09-24T07:13:06.000Z",
        "description": "Savory quiche perfect for breakfast or dinner.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9-inch quiche",
        "recipeIngredient": [
            "1 ready-to-use pie crust",
            "\u00bd pound sausage",
            "1 teaspoon vegetable oil, or as needed",
            "1 onion, diced",
            "6 eggs",
            "\u00bd cup milk",
            "\u00bc cup mayonnaise",
            "2 cups shredded pepperjack cheese",
            "1 cup diced mushrooms",
            "1 teaspoon minced garlic",
            "\u00bc cup shredded sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Press pie crust into a 9-inch pie dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir sausage in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat; cook and stir onion in the hot oil until softened, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs, milk, and mayonnaise together in a bowl until evenly mixed; stir sausage, onion, pepperjack cheese, mushrooms, and garlic into egg mixture. Pour mixture into the prepared pie crust; top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a knife inserted into the center of the quiche comes out clean, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Quiche"
        ],
