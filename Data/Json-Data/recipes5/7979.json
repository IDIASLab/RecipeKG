    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274536/steel-cut-oat-bars/",
        "name": "Steel-Cut Oat Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Steel-Cut Oat Bars"
        },
        "datePublished": "2019-07-12T21:50:26.000Z",
        "description": "You control the ingredients in this great grab-and-go snack. Use your favorite type of granola and any type of fruit or nuts.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup chopped pecans",
            "\u00bd cup steel cut oats",
            "\u00bd cup granola",
            "\u00bc cup coconut oil",
            "\u00bc cup chopped almonds",
            "\u00bc cup fresh raspberries",
            "\u00bc cup raisins",
            "1 medium banana, mashed",
            "1 egg white",
            "1 tablespoon peanut butter",
            "1 tablespoon honey",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc cup dark chocolate chips, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line a 9x13-inch baking dish with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pecans, oats, granola, coconut oil, almonds, raspberries, raisins, mashed banana, egg white, peanut butter, honey, vanilla extract, baking powder, and cinnamon in a large bowl. Mix until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer mixture to prepared baking dish and press into a thin layer with your hands; you may need to coat your hands with coconut oil so the mixture doesn't stick. Sprinkle chocolate chips on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Lower oven temperature to 200 degrees F (95 degrees C), and cook until golden brown, about 45 minutes. Let cool completely before slicing into bars.\n"
            }
        ],
