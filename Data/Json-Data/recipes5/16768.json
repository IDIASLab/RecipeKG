    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11419/marshmallow-brownies/",
        "name": "Marshmallow Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6757438.jpg",
            "width": 1441,
            "height": 960
        },
        "datePublished": "1999-08-27T21:28:45.000Z",
        "description": "Butterscotch chocolate marshmallow brownies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butterscotch chips",
            "\u00bd cup butter",
            "1\u2009\u00bd cups all-purpose flour",
            "\u2154 cup packed brown sugar",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "1 teaspoon vanilla extract",
            "2 eggs",
            "2 cups miniature marshmallows",
            "2 cups milk chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butterscotch morsels and butter in a large bowl in microwave.  Stir the mixture well and let it cool to lukewarm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the liquid mixture is cooling, mix flour, brown sugar, baking powder, salt, vanilla, and eggs into the butterscotch mixture; mix well.  Fold in marshmallows and chocolate morsels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into a lightly greased 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 25 minutes. Be careful not to overcook.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Blondie Recipes"
        ],
