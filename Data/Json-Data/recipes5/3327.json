        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25929/tropical-chicken-ii/",
        "name": "Tropical Chicken II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F139588.jpg",
