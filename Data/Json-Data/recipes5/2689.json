    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274453/one-pot-pasta-wonder/",
        "name": "One-Pot Pasta Wonder",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "One-Pot Pasta Wonder"
        },
        "datePublished": "2019-07-08T02:11:11.000Z",
        "description": "A delicious sausage and pasta dish with a little bit of spice (ZING!) all made in one pot.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "6 (3.5 ounce) links Italian sausage, or to taste",
            "2 cloves garlic, chopped",
            "\u00bd cup red wine",
            "1 (28 ounce) can diced tomatoes with spices",
            "1\u2009\u00bd cups chicken stock",
            "\u00bd teaspoon cayenne powder",
            "2 cups bow-tie pasta (farfalle)",
            "\u00bd cup chopped mushrooms",
            "\u00bc cup chopped fresh parsley",
            "\u00bc cup chopped fresh basil",
            "\u00bc cup grated Romano cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium-high heat. Add sausages and brown on all sides, 3 to 5 minutes. Add garlic; saute for a few seconds, until fragrant, but not burned. Add wine and cook, covered, until wine has reduced, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add diced tomatoes, chicken stock, and cayenne powder. Stir well, cover, and simmer for 5 minutes. Stir in uncooked pasta until well combined, cover, and cook for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add mushrooms, parsley, and basil. Cook, covered, until pasta is tender yet firm to the bite, 5 to 7 minutes. Remove from heat, transfer to a serving dish, and toss with Romano cheese. Serve immediately.\n"
            }
        ],
