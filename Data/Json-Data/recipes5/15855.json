    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237920/bacon-tacos/",
        "name": "Bacon Tacos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3777338.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-02-26T05:51:33.000Z",
        "description": "Bacon is delicious and so are tacos. Together they make an amazing meal! Serve all ingredients a la carte.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "1 cup water",
            "\u00bd cup white rice",
            "5 slices bacon",
            "1 (14 ounce) can black beans",
            "2 teaspoons taco seasoning mix, divided",
            "1 teaspoon vegetable oil, or as needed",
            "5 flour tortillas",
            "2 tomatoes, diced",
            "1 green bell pepper, chopped",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and rice to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until the rice is tender and liquid has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned and crispy, about 10 minutes. Drain the bacon slices on paper towels, retaining the drippings in the skillet. Crumble bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine beans and 1 teaspoon taco seasoning mix in a saucepan over low heat; cook and stir until warmed, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir the cooked rice and 1 teaspoon taco seasoning mix in the reserved bacon drippings until evenly coated, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush vegetable oil over each tortilla and arrange tortillas on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake tortillas in the preheated oven until warmed, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place black bean mixture, rice mixture, bacon, tomatoes, green bell peppers, and Cheddar cheese each in separate bowls; serve with tortillas.\n"
            }
        ],
