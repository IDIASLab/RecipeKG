        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8576/montana-grilled-chicken/",
        "name": "Montana Grilled Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F824751.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-05-02T03:57:59.000Z",
        "description": "I developed this after moving to Montana, hence the name.  It's a family favorite!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (2 to 3 pound) whole chicken, cut into pieces",
            "\u00bd cup lemon juice",
            "\u00bc cup light Italian-style salad dressing",
            "1 clove garlic, minced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix lemon juice, dressing, and garlic thoroughly in a covered container large enough to hold all the chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wash and pat dry chicken parts.  Place in container with marinade, and mix to completely coat.  Cover.  Place in refrigerator overnight, turning a couple of times to coat.  Must  marinate for at least 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain chicken.  Grill over medium heat till juices run clear.\n"
            }
        ],
