        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9523/vegan-peanut-butter-fudge/",
        "name": "Vegan Peanut Butter Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1462267.jpg",
            "width": 2048,
            "height": 1152
        },
        "datePublished": "1999-09-03T03:23:47.000Z",
        "description": "This is my favorite fudge recipe. It seems that the corn syrup keeps it from crystallizing or getting too hard.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pound",
        "recipeIngredient": [
            "2 cups packed brown sugar",
            "\u215b teaspoon salt",
            "\u00be cup soy milk",
            "2 tablespoons light corn syrup",
            "4 tablespoons peanut butter",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Lightly grease one 9x5x2 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 2-quart pot over very low heat, mix together the brown sugar, salt, soy milk, corn syrup, peanut butter and vanilla. Cook until hot and brown sugar is dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Quickly pour into pan and refrigerate. Cut into squares and store in semi-airtight container in refrigerator.\n"
            }
        ],
