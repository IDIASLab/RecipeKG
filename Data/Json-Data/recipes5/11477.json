    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276770/pumpkin-bread-trifle/",
        "name": "Pumpkin Bread Trifle",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Bread Trifle"
        },
        "datePublished": "2019-10-26T05:55:17.000Z",
        "description": "Layered dessert of pumpkin bread, pumpkin pudding, and cream cheese. Better on the second day! I used the Delicious Pumpkin Bread recipe on this website.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT4H20M",
        "recipeYield": "1 trifle",
        "recipeIngredient": [
            "1 loaf pumpkin bread, cut into 2-inch cubes",
            "3 cups milk",
            "2 (3.5 ounce) packages instant pumpkin pudding mix (such as Jell-O\u00ae)",
            "1 (16 ounce) package frozen whipped topping (such as Cool Whip\u00ae), divided",
            "1 (8 ounce) package cream cheese, softened",
            "1 cup powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Layer 1/2 of the pumpkin bread cubes in the bottom of a glass trifle bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk and pudding mixes together in a bowl. Let stand until thickened, about 5 minutes. Fold in 1 cup whipped topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend remaining whipped topping, cream cheese, and powdered sugar together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1/2 of the pudding mixture over the bread cubes. Cover the pudding with a layer of the cream cheese mixture. Repeat layers with remaining ingredients. Refrigerate for at least 4 hours before serving, or up to overnight.\n"
            }
        ],
