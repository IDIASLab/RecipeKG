    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282891/vegetarian-black-eyed-pea-dip/",
        "name": "Vegetarian Black-Eyed Pea Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegetarian Black-Eyed Pea Dip"
        },
        "datePublished": "2020-02-18T18:33:26.000Z",
        "description": "One-pan, warm, vegetarian black-eyed pea dip. Enjoy with your favorite chips, crackers, or breads.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon oil",
            "1 small yellow onion, chopped",
            "\u00bd bunch green onions, sliced, white parts and tops separated",
            "1 small jalapeno pepper, seeded and minced",
            "1 (15 ounce) can black-eyed peas, drained",
            "1 teaspoon smoked paprika",
            "1 teaspoon Creole seasoning",
            "\u00bd (8 ounce) package cream cheese, softened",
            "2 tablespoons sour cream, or more to taste",
            "1 cup shredded Cheddar-Monterey Jack cheese blend"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in an oven-proof skillet over medium heat; stir in yellow onion, white parts of the green onions, and jalapeno. Cook and stir until the onions have softened and turned translucent, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in black-eyed peas, paprika, and Creole seasoning; reduce heat to low. Stir in cream cheese and sour cream until melted. Top with shredded cheese blend.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place in the preheated oven and broil until cheese is melted and golden, about 4 minutes. Garnish with green onion tops.\n"
            }
        ],
