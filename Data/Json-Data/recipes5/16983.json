        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15395/incredible-chicken/",
        "name": "Incredible Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4478778.jpg",
