        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14634/johnny-appleseed-meatballs/",
        "name": "Johnny Appleseed Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3529452.jpg",
            "width": 4128,
            "height": 2322
        },
        "datePublished": "1999-10-11T18:13:24.000Z",
        "description": "The apples add a subtle sweetness to this delicious and simple dish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "18 to 20 meatballs",
        "recipeIngredient": [
            "1 pound ground beef",
            "\u00bc cup dried bread crumbs",
            "\u00bd cup chopped, peeled apple",
            "1 egg",
            "1 teaspoon onion salt",
            "2 tablespoons vegetable oil",
            "1 cup water",
            "1 tablespoon brown sugar",
            "1 (.75 ounce) packet dry brown gravy mix"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine ground beef with chopped apple, bread crumbs, egg and onion salt. Divide into 18 to 20 meatballs. Roll each one between your hands to shape into a ball.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet and add oil. Cook the meatballs until browned; pour off fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in water, brown sugar and gravy mix. Cover and simmer for 15 minutes\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Meatball Appetizer Recipes"
        ],
