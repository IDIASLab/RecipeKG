        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244269/supremely-spicy-hummus/",
        "name": "Supremely Spicy Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2916630.jpg",
            "width": 1328,
