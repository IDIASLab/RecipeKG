    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273222/dairy-free-chocolate-oat-cookies/",
        "name": "Dairy-Free Chocolate-Oat Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Dairy-Free Chocolate-Oat Cookies"
        },
        "datePublished": "2019-05-01T03:29:56.000Z",
        "description": "These cookies are made with almond milk for a dairy-free alternative to chocolate chip cookies, which can be made vegan easily with the substitution of eggs. Light, fluffy, and not overly sweet.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "30 cookies",
        "recipeIngredient": [
            "3 cups rolled oats",
            "1 cup unsweetened almond milk",
            "1\u2009\u00bd cups unbleached all-purpose flour",
            "\u00bd cup whole wheat flour",
            "1 teaspoon baking soda",
            "1 teaspoon sea salt",
            "1 cup coconut oil",
            "\u00be cup honey",
            "1 tablespoon honey",
            "\u00bd cup whole cane sugar (such as Sucanat\u00ae)",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 cup dairy-free dark chocolate chips (such as Callebaut\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats and almond milk in a bowl and let soak for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, sift together all-purpose flour, whole wheat flour, baking soda, and sea salt in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together coconut oil, honey, and sugar in a large bowl using an electric mixer. Add eggs and vanilla extract; stir. Stir in oat and flour mixtures. Fold in dark chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookies by spoonfuls onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 12 to 15 minutes.\n"
            }
        ],
