        "name": "Macaroni and Cheese with Sausage, Peppers and Onions",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7525657.jpg",
            "width": 0,
            "height": 0
