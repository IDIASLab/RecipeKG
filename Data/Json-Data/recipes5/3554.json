    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/50848/grilled-lemon-herb-pork-chops/",
        "name": "Grilled Lemon Herb Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F439692.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "2019-07-10T06:53:38.000Z",
        "description": "These are SO SIMPLE, yet so good - they taste just like steak! The chops can also be broiled in the oven.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup lemon juice",
            "2 tablespoons vegetable oil",
            "4 cloves garlic, minced",
            "1 teaspoon salt",
            "\u00bc teaspoon dried oregano",
            "\u00bc teaspoon pepper",
            "6 (4 ounce) boneless pork loin chops"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large resealable bag, combine lemon juice, oil, garlic, salt, oregano, and pepper. Place chops in bag, seal, and refrigerate 2 hours or overnight. Turn bag frequently to distribute marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat. Remove chops from bag, and transfer remaining marinade to a saucepan. Bring marinade to a boil, remove from heat, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly oil the grill grate. Grill pork chops for 5 to 7 minutes per side, basting frequently with boiled marinade, until done.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes",
            "Boneless"
        ],
