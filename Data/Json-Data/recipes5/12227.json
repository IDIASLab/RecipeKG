    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275825/chile-verde-quiche/",
        "name": "Chile Verde Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7103492.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-24T21:32:52.000Z",
        "description": "A Southwestern quiche with roasted green chile for the crust! Make a bunch the day before for a breakfast/brunch party.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 9-inch quiche",
        "recipeIngredient": [
            "cooking spray",
            "8 canned whole green chiles, or more to taste",
            "7 eggs",
            "\u00bd cup chopped onions",
            "3 tablespoons heavy cream",
            "\u00be teaspoon garlic salt",
            "\u00bd teaspoon ground cumin",
            "\u00bc teaspoon ground black pepper",
            "\u00bc pound sharp Cheddar cheese, cubed",
            "2 tomatoes, sliced, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Spray a pie plate with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut chiles lengthwise into strips. Line strips over the bottom of pie plate to form a crust, placing pointed ends toward the center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a bowl and mix in onions, cream, garlic salt, cumin, and pepper. Fold in Cheddar cheese. Layer tomatoes over the chile 'crust,' and pour egg mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until quiche is set, 30 to 35 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Quiche"
        ],
