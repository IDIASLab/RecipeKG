    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273023/easy-slow-cooker-cheesy-potato-soup-with-ham/",
        "name": "Easy Slow Cooker Cheesy Potato Soup with Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6559810.jpg",
            "width": 960,
            "height": 1443
        },
        "datePublished": "2019-04-25T08:01:27.000Z",
        "description": "This rich, thick, and creamy soup is a great way to use up leftover holiday ham! Made in your slow cooker, and using shortcuts like frozen hash brown potatoes, this delicious soup requires very little effort on your part.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT4H30M",
        "totalTime": "P0DT4H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (28 ounce) package frozen hash brown potatoes, thawed",
            "2 cups diced cooked ham",
            "\u00bd cup diced onion",
            "2 stalks celery, chopped",
            "4 cups chicken broth",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon pepper",
            "\u00bc teaspoon garlic powder",
            "1 (8 ounce) package cream cheese, softened",
            "1 (8 ounce) package shredded Cheddar cheese",
            "2 tablespoons chopped green onions, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine hash browns, ham, onion, celery, chicken broth, salt, pepper, and garlic powder in a slow cooker. Stir well to combine. Place the lid on the slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low heat until celery is soft, 4 to 5 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in cream cheese and Cheddar cheese. Stir until cheese is melted. Cover and cook on Low for an additional 30 minutes, stirring occasionally. Serve topped with chopped green onions as a garnish.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Potato Soup Recipes"
        ],
