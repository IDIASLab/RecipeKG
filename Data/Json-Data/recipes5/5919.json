        "recipeIngredient": [
            "1 egg yolk",
            "1 cup fat-free mayonnaise",
            "2 tablespoons diced cucumber",
            "\u00bd lemon, juiced",
            "1 tablespoon diced onion",
            "1\u2009\u00bd teaspoons white vinegar",
            "1 teaspoon mustard",
            "1 small garlic clove, pressed"
