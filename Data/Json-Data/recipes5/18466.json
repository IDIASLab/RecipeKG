    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/76705/wonderful-short-ribs/",
        "name": "Wonderful Short Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F813530.jpg",
            "width": 3431,
            "height": 2279
        },
        "datePublished": "2020-03-05T04:58:54.000Z",
        "description": "This tender meat that falls right off the bone is great served over rice!  It is great even as leftovers!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (28 ounce) can tomato sauce",
            "3 tablespoons lemon juice",
            "4 teaspoons Worcestershire sauce",
            "2 tablespoons dried parsley",
            "1 teaspoon dried thyme",
            "2 bay leaves",
            "2 tablespoons brown sugar",
            "2 teaspoons salt",
            "1 teaspoon crushed red pepper flakes",
            "1 medium onion, cut into rings",
            "3 pounds beef short ribs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pan over medium-high heat, stir in tomato sauce, lemon juice, and Worcestershire sauce. Stir in parsley, thyme, bay leaves, brown sugar, salt, and red pepper flakes. Add onions and short ribs, and stir together until the mixture comes to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover, reduce heat to medium low, and simmer; stirring occasionally and skimming fat from surface, until meat is tender, about 2 1/2 hours. Remove bay leaves before serving.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Rib Recipes"
        ],
