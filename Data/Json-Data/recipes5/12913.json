    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277812/garlic-roasted-potatoes/",
        "name": "Garlic Roasted Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7623215.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-30T22:46:11.000Z",
        "description": "What could be better than roasted new potatoes with garlic? Yummy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound small new potatoes, halved",
            "1 head garlic, cloves separated but not peeled",
            "2 tablespoons olive oil",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange potatoes and garlic cloves in a 9x13-inch pan. Drizzle with oil and sprinkle with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until potatoes are tender and garlic cloves are soft, 30 to 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven. Squeeze garlic cloves to release roasted garlic onto the potatoes; discard garlic skins. Mix potatoes and garlic together.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Roasted Potato Recipes"
        ],
