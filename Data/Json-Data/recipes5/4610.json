        "mainEntityOfPage": "https://www.allrecipes.com/recipe/176120/redeye-bloody-mary/",
        "name": "RedEye Bloody Mary",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F180156.jpg",
