    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275382/taco-salad-pizza/",
        "name": "Taco Salad Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6978022.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-02-26T05:52:21.000Z",
        "description": "When you can't decide between pizza and tacos, make both! Save time by starting with a pre-made frozen CAULIPOWER&reg; crust.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "1 pizza",
        "recipeIngredient": [
            "1 CAULIPOWER\u00ae Three Cheese Pizza",
            "cooking spray",
            "\u00bd pound ground beef",
            "\u00bd cup black beans, rinsed and drained",
            "\u2153 cup water",
            "\u00bc cup salsa",
            "2 tablespoons taco seasoning",
            "\u00bc cup shredded pepper Jack cheese",
            "\u00bc cup sour cream",
            "1 tablespoon Mexican-style hot sauce",
            "1 cup shredded lettuce",
            "1 Roma tomato, diced",
            "1 jalapeno pepper, sliced",
            "2 tablespoons chopped fresh cilantro",
            "\u00bc cup tortilla strips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a skillet with cooking spray. Cook ground beef on medium-high heat until browned, 5 to 7 minutes. Drain off grease. Add black beans, water, salsa, and taco seasoning and cook for about 2 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unwrap pizza and place on a baking sheet. Spread meat and bean mixture evenly over the crust. Add pepper Jack cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While pizza is baking, combine sour cream and hot sauce in a small bowl. Transfer to a small zip-top bag and place in the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow pizza to cool for about 5 minutes, then top with lettuce, tomato, jalapeno, cilantro, and tortilla strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Snip off a corner of the zip-top bag and drizzle the sour cream and hot sauce mixture over the pizza.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pizza Recipes",
            "Beef"
        ],
