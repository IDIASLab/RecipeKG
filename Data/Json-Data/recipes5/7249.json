    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261087/popcorn-chicken/",
        "name": "Popcorn Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5108805.jpg",
            "width": 2150,
            "height": 2150
        },
        "datePublished": "2019-07-10T07:11:26.000Z",
        "description": "Bite-sized pieces of chicken that are sure to be a family favorite. I love to serve it with homemade mashed potatoes and green beans.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H28M",
        "recipeYield": "30 bite-sized pieces",
        "recipeIngredient": [
            "3 cups flour",
            "2 teaspoons garlic powder",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "2 eggs",
            "2 tablespoons lemon juice",
            "oil for frying",
            "3 skinless, boneless chicken breasts, cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, garlic powder, salt, and black pepper together in a bowl. Beat eggs and lemon juice together in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 1/2 to 2 inches of oil in a large pot over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip the chicken pieces into the flour mixture, then the egg mixture, and again into the flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Test the oil to make sure it is hot enough: a small amount of flour dropped in should sizzle. Place 10 pieces of chicken into the hot oil; cook until golden brown, about 8 minutes. Remove from the oil and drain on a paper towel lined plate and season with salt. Repeat with remaining chicken.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Fried Chicken Recipes"
        ],
