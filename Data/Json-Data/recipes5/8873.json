        "name": "Teriyaki Grilled Beef Tongue with Pineapple",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6929795.jpg",
            "width": 3648,
