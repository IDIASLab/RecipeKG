    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229641/mini-crab-cakes/",
        "name": "Mini Crab Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559352.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2019-07-10T07:03:57.000Z",
        "description": "Crab cakes made with Greek nonfat yogurt are mini bites of bliss. A fun, festive appetizer!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "1 (16 ounce) can fancy lump crabmeat, well drained and picked through for cartilage",
            "1 egg, lightly beaten",
            "1 cup Dannon Oikos Plain Greek Nonfat Yogurt",
            "\u00bc cup finely chopped red bell pepper",
            "\u00bc cup thinly sliced green onion",
            "1 teaspoon finely grated zest of one lemon",
            "Fresh cracked pepper to taste",
            "1 cup plain or panko breadcrumbs, divided",
            "Cooking spray",
            "2 tablespoons chopped parsley",
            "2 tablespoons chopped basil",
            "2 tablespoons lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together crabmeat, egg, 1/3 cup yogurt, red pepper, green onion, lemon zest fresh cracked pepper, and 1/2 cup breadcrumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form mini crab cakes using a tablespoon or small ice cream scoop, and coat the crab cakes in remaining breadcrumbs, placing them onto a parchment lined baking sheet. Spray lightly with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a 400 degrees F preheated oven for 12-15 minutes or until lightly golden.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining yogurt, lemon juice, chopped parsley and basil. Season with cracked black pepper. Serve sauce with crab cakes.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Dannon Oikos&reg;"
        ],
