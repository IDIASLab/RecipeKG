        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71243/pinto-bean-spread/",
        "name": "Pinto Bean Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559853.jpg",
