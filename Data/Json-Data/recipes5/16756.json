        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11346/seashell-sandies/",
        "name": "Seashell Sandies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3721573.jpg",
            "width": 2410,
            "height": 2410
        },
        "datePublished": "1999-03-08T19:44:44.000Z",
        "description": "Thin and crisp cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 1/2 dozen",
        "recipeIngredient": [
            "\u00be cup butter",
            "\u00be cup sifted confectioners' sugar",
            "\u00bc cup packed brown sugar",
            "1 egg",
            "1\u2009\u00bd teaspoons vanilla extract",
            "2\u2009\u00bc cups all-purpose flour",
            "\u00bd cup toasted pecans, finely ground",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl mix the butter or margarine, confectioners' sugar, brown sugar, egg and vanilla together until light and well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine the flour, ground pecans and salt. Add the dry ingredients to the wet ingredients and mix until a soft dough is formed. Wrap dough in plastic and refrigerate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll chilled dough to 1/4 inch thickness and cut into desired shapes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookies on an ungreased baking sheet and bake at 350 degrees F (175 degrees C) for 7 to 9 minutes or until light golden brown.\n"
            }
        ],
