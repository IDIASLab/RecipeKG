        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258681/mamas-milkshake/",
        "name": "Mama's Milkshake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4528643.jpg",
