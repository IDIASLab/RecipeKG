    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236107/amazing-salmon-marinade/",
        "name": "Amazing Salmon Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1127896.jpg",
            "width": 2499,
            "height": 2499
        },
        "datePublished": "2019-07-10T07:05:07.000Z",
        "description": "This marinade is great for grilled salmon. It works well with any cut of salmon. However, I typically use salmon fillets. I like king salmon best, but it works well on any type of fresh salmon. This marinade is enough for 3 pounds of salmon.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u2153 cup loosely packed fresh basil leaves",
            "3 tablespoons honey",
            "1 lemon, juiced",
            "1 tablespoon vegetable oil",
            "1 tablespoon soy sauce",
            "1 tablespoon chopped shallot",
            "1 teaspoon Worcestershire sauce",
            "\u00bd teaspoon grated fresh ginger ",
            "3 pounds salmon fillets"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Make a shallow pan from a double layer of aluminum foil. Place foil pan in a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange salmon fillets in the prepared foil pan, skin-side down. Stir basil, honey, lemon, oil, soy sauce, shallot, Worcestershire sauce, and ginger together in a bowl; pour over salmon. Let salmon marinate for 30 minutes to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium-low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slide salmon in the foil pan off the baking sheet onto the preheated grill. Cover grill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook until salmon is easily flaked with a fork, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Salmon",
            "Salmon Fillet Recipes"
        ],
