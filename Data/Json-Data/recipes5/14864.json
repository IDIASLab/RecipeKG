    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278322/celery-soup/",
        "name": "Celery Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Celery Soup"
        },
        "datePublished": "2020-02-10T23:52:53.000Z",
        "description": "Dairy-free, delicious celery soup! Serve plain--absolutely delicious--or with a dollop of sour cream, or a little Parmesan or other shredded cheese.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 tablespoons olive oil, divided, or to taste",
            "1 large onion, chopped",
            "2 carrots, peeled and chopped, or more to taste",
            "1 leek, thinly sliced",
            "4 cloves garlic, peeled and chopped",
            "6 cups diced celery",
            "1 quart chicken bone broth",
            "1\u2009\u00bd pounds baby yellow potatoes, peeled",
            "1 small bunch fresh parsley",
            "1 teaspoon dried thyme, or to taste",
            "\u00bd teaspoon salt-free seasoning blend (such as Penzeys Mural of Flavor), or to taste",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons olive oil in a frying pan over medium heat. Add onion, carrots, and leek; cook until soft, about 5 minutes. Add garlic; cook for 1 minute. Transfer to a soup pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining olive oil in the same frying. Saute celery to release some of the moisture, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer celery to the soup pot. Add chicken broth and potatoes. Cut parsley into the pot using kitchen scissors. Season soup with thyme, seasoning blend, salt, and pepper. Increase heat to medium-high and bring to a rolling simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat and simmer until carrots, celery, and potatoes are soft enough to easily puree, about 30 minutes. Avoid cooking vegetables until mushy. Remove from heat and cool for about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree soup with an immersion blender. Simmer over low heat until heated through, 10 to 15 minutes. Serve immediately or let flavors meld overnight and serve the next day.\n"
            }
        ],
