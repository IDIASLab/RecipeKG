        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10392/martha-washington-cookies/",
        "name": "Martha Washington Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F742930.jpg",
            "width": 2048,
