    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275247/orange-guajillo-glazed-sweet-potatoes/",
        "name": "Orange-Guajillo Glazed Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1005836.jpg",
            "width": 1083,
            "height": 1080
        },
        "datePublished": "2019-08-15T05:10:23.000Z",
        "description": "These sweet and spicy, roasted, Mexican-inspired sweet potatoes add a different and delicious twist to a favorite side dish.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup red guajillo chile Mexican cooking sauce (such as Herdez\u00ae)",
            "\u00bc cup orange marmalade",
            "1 tablespoon honey",
            "\u00bd teaspoon red pepper flakes, or to taste",
            "\u00bd teaspoon ground cinnamon",
            "salt and freshly ground black pepper to taste",
            "3 medium sweet potatoes, peeled and cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Cover a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix guajillo sauce, orange marmalade, honey, red pepper flakes, cinnamon, salt, and pepper together in a medium saucepan over low heat. Stir until sauce is smooth and heated through, 3 to 5 minutes. Remove from heat. Toss in sweet potatoes and cover thoroughly with glaze. Pour onto the prepared baking pan and cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Remove foil and increase oven temperature to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue to bake until tender, about 10 minutes more.\n"
            }
        ],
