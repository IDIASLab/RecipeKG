    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221077/cucumber-with-sour-cream/",
        "name": "Cucumber with Sour Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F994154.jpg",
            "width": 1536,
            "height": 1536
        },
        "datePublished": "2019-07-10T07:03:14.000Z",
        "description": "A marinated cucumber salad that is best made with fresh garden cucumbers when available.  Also good as a dip if you chop the cucumbers instead of slicing them.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT2H15M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "6 tablespoons vegetable oil",
            "2 tablespoons vinegar",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2 cucumbers, peeled and sliced",
            "\u00be cup mayonnaise",
            "3 tablespoons heavy whipping cream",
            "3 tablespoons minced fresh onion",
            "2 tablespoons sour cream",
            "1 teaspoon lemon juice",
            "\u00bd teaspoon dried basil",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk vegetable oil, vinegar, 1/2 teaspoon salt, and 1/2 teaspoon pepper together in a large bowl until the oil emulsifies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold the cucumbers into the marinade until evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate cucumbers in refrigerator for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain cucumbers and discard marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk mayonnaise, cream, onion, sour cream, lemon juice, and basil together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold drained cucumbers into mayonnaise mixture; season with salt and pepper to serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Cucumber Salad Recipes"
        ],
