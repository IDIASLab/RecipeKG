        "name": "Grandma's Porcupine Meatballs in the Instant Pot&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7079361.jpg",
