    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21421/thanksgiving-yams/",
        "name": "Thanksgiving Yams",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1746695.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2000-09-10T05:47:17.000Z",
        "description": "Mother's great yams were always a hit at Thanksgiving. In America, yams are actually a type of sweet potato. Gingersnap cookies and marshmallows make this dish a real treat.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "6 cups",
        "recipeIngredient": [
            "2 (29 ounce) cans sweet potatoes",
            "2 tablespoons butter",
            "1 tablespoon lemon juice",
            "\u00bd (16 ounce) package gingersnap cookies",
            "1 (16 ounce) package miniature marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan over medium heat, mix the sweet potatoes, butter and lemon juice. Break the gingersnap cookies into the mixture. Stirring occasionally, cook until thick, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the mixture with desired quantity of marshmallows. Remove from heat and cover 5 minutes, or until marshmallows have melted. Serve warm.\n"
            }
        ],
