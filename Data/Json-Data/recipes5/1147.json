    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273135/cheesy-cauliflower-bake/",
        "name": "Cheesy Cauliflower Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheesy Cauliflower Bake"
        },
        "datePublished": "2019-05-01T23:06:29.000Z",
        "description": "This cheesy baked casserole uses cauliflower in place of noodles so you can enjoy a decadent dish with fewer calories!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cooking spray",
            "2 tablespoons salt",
            "1 large head cauliflower, cut into small florets",
            "2 cups low-fat cottage cheese",
            "2 cups low-fat sour cream",
            "\u00bc cup melted butter",
            "\u00bc cup green onions, finely chopped",
            "2 cloves garlic, minced",
            "1 teaspoon Worcestershire sauce",
            "1 dash hot pepper sauce (such as Tabasco\u00ae)",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u2153 cup grated Parmesan cheese",
            "\u2153 cup chopped blanched almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Coat a baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil and season with 2 tablespoons salt. Add cauliflower and cook until crisp-tender, about 5 minutes. Drain well and place between several layers of paper towels to dry. Transfer cauliflower to a large mixing bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine, cottage cheese, sour cream, butter, green onions, garlic, Worcestershire sauce, hot sauce, salt, and pepper in a bowl. Stir cheese mixture into the bowl with the cooked cauliflower and transfer to the prepared baking dish. Spoon Parmesan cheese on top. Cover with an oven-proof lid or aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat a small skillet over medium heat and add almonds. Cook, stirring constantly, until lightly toasted, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle almonds on top of cheesy cauliflower bake and return to the oven for 10 minutes more.\n"
            }
        ],
