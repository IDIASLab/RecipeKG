        "name": "Crock Pot&reg; Party Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4553045.jpg",
            "width": 960,
            "height": 960
