    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254799/fig-thumbprint-cookies/",
        "name": "Fig Thumbprint Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4537787.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2019-07-10T07:10:06.000Z",
        "description": "These cookies are a great way to use fresh figs, and kids love them!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "40 cookies",
        "recipeIngredient": [
            "2\u2009\u2154 cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups confectioners' sugar",
            "\u00bd cup unsalted butter",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "\u00bd pound fresh figs",
            "\u2153 cup honey",
            "\u00bc teaspoon ground cinnamon",
            "\u215b teaspoon ground nutmeg",
            "1 tablespoon lemon juice, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking soda, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter with an electric mixer in a large bowl until fluffy. Beat eggs in, one at a time, until combined; stir in vanilla extract. Fold in flour mixture until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine figs, honey, cinnamon, and nutmeg together in a food processor; pulse until smooth. Stir in lemon juice until filling is slightly thinner than jam.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1-inch balls; place on a non-stick baking sheet. Make thumbprints in batter with a wet finger. Spoon 1 teaspoon of filling into each thumbprint.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, 10 to 12 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Fig Dessert Recipes"
        ],
