    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/169660/moms-shredded-elk-sandwiches/",
        "name": "Mom's Shredded Elk Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3742516.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2020-03-05T05:06:02.000Z",
        "description": "Reminiscent of sloppy joes, this recipe can be used with elk or venison - it takes away that gamey taste!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT5H0M",
        "totalTime": "P0DT5H20M",
        "recipeYield": "10 sandwiches",
        "recipeIngredient": [
            "1\u2009\u00bd cups ketchup",
            "3 tablespoons brown sugar",
            "1 tablespoon dry mustard powder",
            "1 tablespoon lemon juice",
            "1 tablespoon liquid smoke flavoring",
            "1 teaspoon onion powder",
            "1 teaspoon garlic powder",
            "2 teaspoons celery salt",
            "2 teaspoons ground black pepper",
            "2 teaspoons Worcestershire sauce",
            "\u215b teaspoon ground nutmeg",
            "3 drops hot pepper sauce",
            "1 (4 pound) elk roast, cut in half",
            "10 hamburger buns, split"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the ketchup, brown sugar, mustard powder, lemon juice, liquid smoke, onion powder, garlic powder, celery salt, black pepper, Worcestershire sauce, nutmeg, and hot pepper sauce in a bowl. Place the elk roast into a large slow cooker, and pour the sauce overtop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover, and cook on High until the meat shreds easily with a fork, about 5 hours. Shred, and serve on the hamburger buns.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Game Meats",
            "Elk"
        ],
