    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9325/spiced-ham-loaf/",
        "name": "Spiced Ham Loaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F766894.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1998-12-10T01:41:43.000Z",
        "description": "This was my grandfather's contribution to the Christmas table.  A nice twist on the meatloaf recipe.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 pound ground ham",
            "1\u2009\u00bd pounds lean ground beef",
            "1 egg",
            "1 cup milk",
            "1 cup bread crumbs",
            "\u00bd teaspoon salt",
            "ground black pepper to taste",
            "\u00bd cup packed brown sugar",
            "\u00bd cup water",
            "1 teaspoon ground mustard",
            "\u00bc cup distilled white vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine ham, beef, egg, milk, bread crumbs, salt, and pepper.  Form into loaf.   Place in a casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, water, mustard, and vinegar in a small saucepan over medium high heat.  Bring to boil, and boil for 10 minutes.  Pour over loaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) until done, about 1 hour, basting occasionally.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Pork Meatloaf Recipes"
