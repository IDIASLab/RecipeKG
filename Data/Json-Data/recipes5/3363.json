        "mainEntityOfPage": "https://www.allrecipes.com/recipe/30105/mediterranean-chickpea-salad-i/",
        "name": "Mediterranean Chickpea Salad I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F850108.jpg",
