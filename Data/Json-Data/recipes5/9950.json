    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276109/spicy-ranch-cauliflower-crackers/",
        "name": "Spicy Ranch Cauliflower Crackers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7071107.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-09-26T21:33:08.000Z",
        "description": "A spicy keto cracker made with cauliflower instead of flour! The perfect low-carb cracker. Store in an airtight container.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "18 crackers",
        "recipeIngredient": [
            "1 (12 ounce) package frozen riced cauliflower",
            "cheese cloth",
            "1 egg",
            "1 tablespoon dry ranch salad dressing mix",
            "\u215b teaspoon cayenne pepper, or more to taste",
            "1 cup shredded Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place riced cauliflower in a microwave-safe bowl. Microwave, covered, for 3 to 4 minutes. Transfer to a cheesecloth-lined strainer and allow to cool for 15 minutes. Squeeze moisture out of the cooled riced cauliflower.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine riced cauliflower, egg, ranch mix, and cayenne pepper in a bowl and mix well. Stir in Parmesan cheese until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop mixture with a small cookie scoop onto the prepared cookie sheet and flatten with a small hand rolling pin, a cup, or your hand to approximately 1/16-inch thickness. The thinner the dough, the crispier the cracker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crackers in the preheated oven for 10 minutes, flip, and bake for an additional 10 minutes. Cool on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Cracker Recipes"
        ],
