        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241388/strawberry-delight-pie/",
        "name": "Strawberry Delight Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2131306.jpg",
