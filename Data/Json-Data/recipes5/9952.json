        "recipeIngredient": [
            "aluminum foil",
            "1 (12 ounce) package cauliflower pizza crust",
            "2 tablespoons olive oil",
            "\u2153 cup thinly shaved Parmesan cheese",
            "1\u2009\u00bd teaspoons everything bagel seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Line a rimmed baking sheet with aluminum foil, and place a cooling rack on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush both sides of the pizza crust with olive oil and place on top of the cooling rack. Sprinkle with Parmesan cheese and everything bagel seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 17 minutes. Cut into bite-sized pieces.\n"
            }
