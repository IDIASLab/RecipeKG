    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277443/low-carb-ricotta-cheesecake/",
        "name": "Low-Carb Ricotta Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Low-Carb Ricotta Cheesecake"
        },
        "datePublished": "2019-12-17T02:44:56.000Z",
        "description": "Crustless ricotta cheesecake made with Splenda&reg; sweetener created by a low-carber who just had to have dessert once in a while.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT5H45M",
        "recipeYield": "1 9 1/2-inch cheesecake",
        "recipeIngredient": [
            "4 eggs",
            "\u2154 cup granular sucralose sweetener (such as Splenda\u00ae)",
            "1 (15 ounce) container ricotta cheese",
            "2 (8 ounce) packages cream cheese, softened",
            "4 tablespoons sour cream",
            "2 tablespoons lemon juice",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Wrap the outside of a 9 1/2-inch springform pan with foil to prevent water seeping in while baking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a bowl using an electric mixer until light and doubled in volume. Add sucralose sweetener and mix on medium speed to combine. Beat in ricotta cheese until well combined. Reduce speed to medium-low, add cream cheese, and mix well. Blend in sour cream, lemon juice, and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared pan; lower pan into a larger baking dish filled with 2 inches of hot water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully transfer to the preheated oven and bake for 90 minutes. Turn oven off; do not open the door. Leave cheesecake in the oven for 1 hour more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and let cool on the counter for 1 hour. Refrigerate until thoroughly chilled, at least 2 hours.\n"
            }
        ],
