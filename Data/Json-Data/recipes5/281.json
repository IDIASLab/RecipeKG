    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269484/sheet-pan-nachos/",
        "name": "Sheet Pan Nachos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6114451.jpg",
            "width": 2610,
            "height": 2610
        },
        "datePublished": "2019-04-04T04:14:55.000Z",
        "description": "A very simple sheet pan recipe, these nachos are guaranteed to be a hit at your next get-together.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 pound ground beef (80% lean)",
            "\u00bc cup water",
            "1 tablespoon chili powder",
            "1 teaspoon ground cayenne pepper",
            "salt and ground black pepper to taste",
            "1 (14.5 ounce) package tortilla chips",
            "1 (4 ounce) can sliced black olives",
            "1 (12 ounce) jar pickled jalapeno pepper slices",
            "2 cups shredded Cheddar cheese",
            "1 (8 ounce) container sour cream",
            "1 cup salsa"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler. Line a sheet pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes. Mix in water, chili powder, cayenne, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lay out tortilla chips on the sheet pan. Top with ground beef mixture. Cover with olives, jalapeno slices, and Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil until cheese is bubbly and golden brown, 3 to 5 minutes. Serve with sour cream and salsa for dipping.\n"
            }
        ],
