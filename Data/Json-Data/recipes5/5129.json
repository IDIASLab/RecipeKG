    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220708/lemon-filled-cupcakes/",
        "name": "Lemon-Filled Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7653999.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T07:03:08.000Z",
        "description": "My family requests that I make these cupcakes every time I come home. For all lemon lovers!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "2 dozen cupcakes",
        "recipeIngredient": [
            "1 (18.25 ounce) package lemon cake mix",
            "1 cup water",
            "\u2153 cup vegetable oil",
            "3 eggs",
            "\u00bc cup vegetable shortening",
            "\u00bc cup butter",
            "\u00bc cup fresh lemon juice, or to taste",
            "2 cups confectioners' sugar",
            "4 ounces white chocolate, chopped",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup butter, softened",
            "1 teaspoon vanilla extract",
            "4 cups confectioners' sugar",
            "3 tablespoons orange juice",
            "1 tablespoon grated lemon zest, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line 24 muffin cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat lemon cake mix, water, vegetable oil, and eggs in a large bowl for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling them about 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cupcakes in the preheated oven until very lightly browned and a toothpick inserted into the middle of a cupcake comes out clean, 18 to 22 minutes. Cool cupcakes thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut vegetable shortening and 1/4 cup butter together in a bowl with a pastry cutter until well mixed. Beat lemon juice into shortening mixture until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 2 cups confectioners' sugar into shortening mixture, about 1/2 cup at a time, until the filling is smooth. Set filling aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place white chocolate into a microwave-safe bowl; heat in microwave on High for 15 seconds and stir chocolate. Continue heating in 15-second intervals, stirring between heating until white chocolate is melted and smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For frosting, beat cream cheese and 1/2 cup butter together in a bowl until smooth and creamy. Stir in melted white chocolate and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually beat in 4 cups confectioners' sugar, alternating with drizzles of orange juice, to make a smooth and creamy frosting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut holes through the top of each cupcake. Holes should be deep enough to put filling inside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon lemon filling into the holes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pipe the cream cheese frosting on top of cupcakes; sprinkle with lemon zest.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
