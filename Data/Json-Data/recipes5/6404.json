    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241657/portobello-mushroom-pizzas/",
        "name": "Portobello Mushroom Pizzas",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Portobello Mushroom Pizzas"
        },
        "datePublished": "2019-07-10T07:06:10.000Z",
        "description": "This recipe is light and quick and requires only a few basic ingredients. If you love pizza like we do, but are looking for a healthier alternative, give this recipe a try. You can experiment with different ingredients and add your own toppings.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H23M",
        "totalTime": "P0DT0H48M",
        "recipeYield": "4 mushroom pizzas",
        "recipeIngredient": [
            "olive oil cooking spray",
            "2 tablespoons olive oil",
            "1 onion, chopped",
            "4 tomatoes, diced",
            "1 leek, chopped",
            "4 cloves garlic, chopped",
            "1 pinch salt",
            "1 tablespoon fresh lemon juice",
            "5 leaves fresh basil, chopped",
            "4 large portobello mushroom caps",
            "\u00bc cup grated Parmigiano-Reggiano cheese  ",
            "1 tablespoon chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a saucepan over medium heat. Cook and stir onion in hot oil until slightly browned, about 5 minutes. Add tomatoes, leek, garlic, and salt; saute until tomatoes are soft, about 5 minutes more. Sprinkle lemon juice over the vegetable mixture. Stir in basil; heat until wilted, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange portobello mushroom caps onto the prepared baking sheet. Fill mushroom caps with onion mixture. Sprinkle Parmigiano-Reggiano cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until mushrooms are heated through and the Parmigiano-Reggiano cheese has melted, about 12 minutes. Sprinkle with parsley.\n"
            }
        ],
