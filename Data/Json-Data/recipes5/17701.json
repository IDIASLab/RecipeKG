    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25803/carrot-cake-cookies/",
        "name": "Carrot Cake Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2780917.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2020-03-05T04:56:43.000Z",
        "description": "This is a cake like cookie that tastes like carrot cake, but in cookie   form.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "3 dozen cookies",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "1 cup brown sugar",
            "2 eggs",
            "1 (8 ounce) can crushed pineapple, drained",
            "\u00be cup shredded carrots",
            "1 cup raisins",
            "2 cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 tablespoons ground cinnamon",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease cookie sheets or line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and brown sugar until smooth. Beat in the eggs one at a time, stir in the crushed pineapple, carrots and raisins. Combine the flour, baking powder, baking soda, salt and cinnamon; stir into the carrot mixture. Mix in the walnuts if desired. Drop by rounded spoonfuls onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 to 20 minutes in the preheated oven, until bottoms begin to brown and cookies are set. Allow cookies to cool for a few minutes on the cookie sheets before removing to wire racks to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
