    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277151/cheesy-shrimp-and-grits/",
        "name": "Cheesy Shrimp and Grits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7541914.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-12T03:45:05.000Z",
        "description": "Shrimp and grits with cheese.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 cups water",
            "salt and ground black pepper to taste",
            "1 cup stone-ground grits",
            "2 cups shredded sharp Cheddar cheese",
            "3 tablespoons butter",
            "1 pound shrimp, peeled and deveined",
            "6 slices bacon, chopped",
            "1 cup thinly sliced scallions",
            "2 tablespoons chopped fresh parsley",
            "1 large clove garlic, minced",
            "4 teaspoons lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a pot. Add salt and pepper. Stir in grits; reduce heat and let simmer until water is absorbed, 20 to 25 minutes. Remove from heat and stir in Cheddar cheese and butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse shrimp and pat dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining bacon grease in the skillet over medium heat. Cook shrimp until they are bright pink on the outside and the meat is opaque, 3 to 4 minutes. Add bacon, scallions, parsley, garlic, and lemon juice. Saute for 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon grits into a serving bowl. Add shrimp mixture and mix well. Serve immediately.\n"
            }
        ],
