        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277450/greek-grilled-beyond-meat-burgers/",
        "name": "Greek Grilled Beyond Meat&reg; Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7339557.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-03T19:28:50.000Z",
        "description": "Plant-based Beyond Meat&reg; burgers with a Greek flair.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 burgers",
        "recipeIngredient": [
            "1 large cucumber",
            "1 cup Greek yogurt",
            "2 teaspoons minced garlic",
            "1 teaspoon lemon juice",
            "1 teaspoon dried dill",
            "\u00bd teaspoon salt",
            "1 tablespoon Greek seasoning (such as Cavender's\u00ae)",
            "4 Beyond Meat\u00ae burger patties",
            "4 hamburger buns",
            "8 leaves lettuce",
            "1 small red onion, sliced",
            "1 small tomato, sliced",
            "12 pitted Kalamata olives, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grate cucumber and place in a colander to drain for 10 minutes. Press out any remaining liquid with paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together cucumber, yogurt, garlic, lemon juice, dill, and salt in a small bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle Greek seasoning on both sides of burger patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place patties on the preheated grill and cook for 4 minutes. Flip and grill for 4 more minutes. Place burgers on buns and top with lettuce, onion, tomato, olives, and reserved cucumber sauce.\n"
            }
        ],
