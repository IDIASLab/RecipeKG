        "mainEntityOfPage": "https://www.allrecipes.com/recipe/157825/slow-cooker-ham-and-beans/",
        "name": "Slow Cooker Ham and Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F592321.jpg",
