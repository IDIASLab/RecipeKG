    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/150353/roasted-garlic-flat-iron-steak/",
        "name": "Roasted Garlic Flat Iron Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1016661.jpg",
            "width": 1015,
            "height": 1018
        },
        "datePublished": "2020-02-06T03:50:02.000Z",
        "description": "These steaks, seasoned with a delicious hunk of garlic, are guaranteed to melt in your mouth and have the family begging for more.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 steaks",
        "recipeIngredient": [
            "15 cloves garlic",
            "1 tablespoon olive oil",
            "\u00bd cup olive oil",
            "4 (8 ounce) flat iron steaks",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange garlic cloves on a baking sheet. Drizzle 1 tablespoon olive oil over garlic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake garlic in preheated oven until golden brown, 15 to 20 minutes, stirring once to prevent burning. Remove from oven and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cooled garlic and 1/2 cup olive oil in a food processor; blend until mixture is a paste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season steaks with salt and pepper. Rub garlic paste on both sides of each steak.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a large skillet over medium heat. Cook the steaks until the internal temperature reaches 135 degrees to 140 degrees F (60 degrees C), about 5 minutes per side. Remove the whole pan from the heat and let stand until steaks reach an internal temperature of 145 degrees F (63 degrees C), or your desired degree of doneness.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks",
            "Flat Iron Steak Recipes"
        ],
