    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274014/fall-macaroni-casserole/",
        "name": "Fall Macaroni Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fall Macaroni Casserole"
        },
        "datePublished": "2019-06-06T03:06:37.000Z",
        "description": "I thought this up around some smoked turkey sausage I purchased on sale last week and apples from my backyard. Top with Parmesan cheese to taste.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd (12 ounce) package elbow macaroni",
            "1 tablespoon olive oil",
            "13 ounces smoked turkey sausage, chopped",
            "\u00bd onion, diced",
            "1 clove garlic, minced",
            "2 small apples - peeled, cored, and diced",
            "1 bunch dandelion greens, chopped",
            "5 ounces Asiago cheese, diced",
            "1 teaspoon grated Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 8 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a pan over medium heat. Add sausage and sear for 1 minute. Add onion, then garlic; cook and stir until starting to brown, 3 to 5 minutes. Stir in apples and cook until browned, about 2 minutes. Add dandelion greens and cook until wilted, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sausage mixture with macaroni and Asiago cheese in a serving dish. Top with Parmesan cheese.\n"
            }
        ],
