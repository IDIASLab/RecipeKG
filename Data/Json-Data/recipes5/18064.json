    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/46781/hazelnut-chocolate-chunk-muffins/",
        "name": "Hazelnut Chocolate Chunk Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hazelnut Chocolate Chunk Muffins"
        },
        "datePublished": "2020-03-05T04:57:48.000Z",
        "description": "Sweet, delicately-textured muffins, with bran, wheat germ, and crunchy whole hazelnuts!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1 cup oil for frying",
            "\u00be cup hazelnuts",
            "1 tablespoon all-purpose flour",
            "2 cups all-purpose flour",
            "\u00bc cup wheat germ",
            "\u2154 cup brown sugar",
            "\u00bc cup oat bran",
            "3 tablespoons hazelnut butter",
            "1 egg",
            "1 tablespoon baking powder",
            "1 cup milk",
            "\u00bd cup semisweet chocolate chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C). Line a 12 cup muffin pan with paper cup liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil for frying in a medium, deep skillet over medium-high heat. In a small bowl, toss the hazelnuts in 1 tablespoon flour to lightly coat. Spoon the hazelnuts into the hot oil, and fry until golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix the 2 cups flour, wheat germ, brown sugar, oat bran, hazelnut butter, and egg. Thoroughly mix in baking powder. Stir in the milk, and fold in the chocolate chips. Evenly pour into the lined muffin pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 25 to 30 minutes in the preheated oven, until a toothpick inserted in the center of a muffin comes out clean.\n"
            }
        ],
