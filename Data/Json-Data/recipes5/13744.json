        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9162/moms-traditional-creamed-onions/",
        "name": "Mom's Traditional Creamed Onions",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7649310.jpg",
