        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222372/lemony-salmon-burgers/",
        "name": "Lemony Salmon Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2023895.jpg",
