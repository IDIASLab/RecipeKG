    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273777/skillet-baked-ziti/",
        "name": "Skillet Baked Ziti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F917702.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-05-23T05:02:59.000Z",
        "description": "Skillet ziti cooks up twice as fast as oven-baked version. It's creamier and cheesier!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound ziti pasta",
            "1 pound ground beef",
            "1 large sweet onion, chopped",
            "4 cloves garlic, chopped",
            "1 ounce fresh basil, chopped",
            "salt to taste",
            "freshly ground black pepper to taste",
            "1 (26 ounce) jar tomato and basil pasta sauce",
            "1 (8 ounce) container sour cream",
            "\u00bd cup shredded Parmesan cheese",
            "1 (8 ounce) package shredded Italian cheese blend"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook ziti in the boiling water, stirring occasionally until tender yet firm to the bite, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine beef, onion, garlic, basil, salt, and pepper in a large skillet over medium-high heat. Cook and stir until beef is no longer pink, 5 to 10 minutes. Stir in pasta sauce and cook until thoroughly heated, about 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain cooked pasta and add to the skillet. Toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sour cream and Parmesan cheese in a bowl. Stir into pasta mixture in the skillet. Sprinkle Italian cheese blend over pasta mixture. Cover the skillet, reduce heat to medium, and cook until cheese melts, about 5 minutes more.\n"
            }
        ],
