        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78132/dinner-party-salad/",
        "name": "Dinner Party Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F889371.jpg",
