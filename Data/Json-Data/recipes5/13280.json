    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277902/cream-cheese-spinach-casserole/",
        "name": "Cream Cheese Spinach Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cream Cheese Spinach Casserole"
        },
        "datePublished": "2020-01-08T02:42:32.000Z",
        "description": "A delicious and easy to make creamed spinach dish. Great for holidays or simply dinner for the family. This recipe yields a large quantity for holiday gatherings. Half the recipe if you're just serving your family.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "cooking spray",
            "1 tablespoon olive oil, or as needed",
            "2 cups chopped onions",
            "salt and ground black pepper to taste",
            "1\u2009\u00bd pounds cooked chicken breast, shredded",
            "2 (9 ounce) packages fresh spinach, chopped",
            "2 (8 ounce) tubs whipped cream cheese",
            "3 cups shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Spray a medium or large casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat. Add onions and a generous pinch of salt and pepper; saute until onions are translucent, about 5 minutes. Transfer to a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cooked chicken to the onions. Mix in spinach and cream cheese until combined; you can add spinach a little bit at a time to make it easier. Add 1/2 of the mozzarella cheese and stir until everything is thoroughly incorporated and cream cheese has no clumps remaining. Transfer to the prepared casserole dish and sprinkle with remaining mozzarella.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese has melted and filling is bubbling, about 30 minutes. Let cool for 10 minutes before serving.\n"
            }
        ],
