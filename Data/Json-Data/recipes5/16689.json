    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10844/apple-oatmeal-bars/",
        "name": "Apple Oatmeal Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1254278.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "1998-04-07T17:30:40.000Z",
        "description": "My husband loves these. He calls them Apple Crisp. The recipe came from his Mother.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "1 cup packed brown sugar",
            "1 cup rolled oats",
            "1 cup butter",
            "4 tablespoons butter",
            "6 cups thinly sliced apples"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, salt and baking soda.  Add brown sugar and oatmeal, mix well.  Cut in 1 cup butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread half of the crumb mixture in a buttered 9 x 13 pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put apples on top of bottom layer.  Cover with remaining crumb mixture and dot with 4 tablespoons butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C)  for 40 to 45 minutes.  It is great served with ice cream!\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
