    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277115/easy-apple-bread-pudding/",
        "name": "Easy Apple Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7291396.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-08T05:45:15.000Z",
        "description": "An easy-to-make and oh-so-yummy apple bread pudding. Perfect for dessert or just as an afternoon snack with coffee or tea. Serve with a scoop of vanilla ice cream, whipped cream, or some powdered sugar on top, if desired.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x2-inch bread pudding",
        "recipeIngredient": [
            "1 tablespoon butter, or as needed",
            "2 medium apples - peeled, cored, and cubed",
            "3 teaspoons honey, divided",
            "1 whole clove",
            "6 slices bread, cubed",
            "1 cup milk",
            "2 eggs",
            "2 drops vanilla extract",
            "\u00bc teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Butter a 9x2-inch round baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place apples into a bowl and cover with hot water. Add 1 teaspoon honey and clove. Let soak for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread cubes into a bowl and add remaining honey, milk, eggs, and vanilla extract. Mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take apples out of water and let excess drip off; add apples and cinnamon to the bread mixture and mix slowly. Pour mixture into the prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown on top, 30 to 35 minutes. Serve warm.\n"
            }
        ],
