        "recipeIngredient": [
            "1 cup frozen unsweetened raspberries",
            "\u00be cup gold tequila",
            "\u00be cup fresh orange juice",
            "\u00bd cup fresh lemon juice",
            "\u00bc cup brandy-based orange liqueur (such as Grand Marnier\u00ae)",
            "2 tablespoons superfine sugar",
            "4 mint leaves",
            "4 ice cubes"
