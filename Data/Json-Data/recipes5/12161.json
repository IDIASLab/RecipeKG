    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277332/gluten-free-sausage-balls/",
        "name": "Gluten-Free Sausage Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4532683.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-21T03:00:22.000Z",
        "description": "Gluten-free sausage balls that are also very economical.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 sausage balls",
        "recipeIngredient": [
            "2 cups rice flour",
            "2 teaspoons baking powder",
            "1 teaspoon salt",
            "2 cups boiling water",
            "2 pounds sausage",
            "1 pound shredded cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine rice flour, baking powder, and salt in a large bowl. Pour boiling water over the mixture and mix together with a whisk or electric mixer. Add sausage and mix together with an electric mixer or potato masher. Stir in cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape sausage mixture into small balls using your hands and set onto a baking sheet or a baking rack with small holes (preferred). If using a baking rack, place a drip tray underneath to catch drippings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until sausage balls are cooked through, 25 to 30 minutes. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C).\n"
            }
        ],
