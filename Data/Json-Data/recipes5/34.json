    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267825/paella-soup/",
        "name": "Paella Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Paella Soup"
        },
        "datePublished": "2019-04-04T04:13:54.000Z",
        "description": "A delicious soup with a Spanish paella flair that's sure to warm you up this winter.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 cup chopped celery",
            "1 cup chopped onion",
            "\u00be cup chopped carrots",
            "1 red bell pepper, chopped, or more to taste",
            "1 cup cubed fully cooked ham",
            "1\u2009\u00bd quarts chicken broth",
            "\u00bc teaspoon saffron threads, or more to taste",
            "1 cup frozen petite peas",
            "3\u2009\u2153 cups water",
            "2 (5.6 ounce) packages Spanish rice mix (such as La Preferida\u00ae)",
            "1 (10 ounce) can diced tomatoes and green chiles (such as RO*TEL\u00ae)",
            "2 pounds frozen cooked shrimp, thawed and tails removed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a saucepan over medium-high heat. Saute celery, onion, carrots, and red pepper until soft, about 10 minutes. Add ham; cook and stir for 2 minutes. Add chicken broth and saffron. Bring to a boil; reduce to a simmer. Add peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, bring water to a boil in a separate pot. Add rice mixes and stir well. Bring back to a boil; reduce heat, cover, and simmer for 3 minutes. Remove from heat and let stand, covered, for 8 minutes. Add diced tomatoes and green chiles; stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add rice mixture to the soup. Remove soup from heat and let cool for 5 minutes. Add shrimp and let stand until warmed through.\n"
            }
        ],
