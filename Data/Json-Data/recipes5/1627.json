    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273474/breakfast-bowl/",
        "name": "Breakfast Bowl",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Breakfast Bowl"
        },
        "datePublished": "2019-05-16T22:26:39.000Z",
        "description": "All your breakfast favorites put together. This is sort of a take off of Denny's&reg; Farmer's Scramble (which they no longer make, unfortunately). I love breakfast, and you can make this bowl almost any way you want to.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "1 bowl",
        "recipeIngredient": [
            "1 medium potato, grated",
            "1 pinch salt, or more to taste",
            "1 teaspoon olive oil, or as needed",
            "\u215b pound pork sausage, or to taste",
            "\u215b cup chopped onion",
            "\u215b cup chopped green bell pepper",
            "1 egg, or more to taste",
            "1 (2.5 ounce) package country gravy mix",
            "2 cups water",
            "\u215b cup shredded Cheddar cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mold grated potato into a patty shape. Season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-low heat. Cook potato patty until browned on both sides, about 5 minutes. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat another skillet over medium-high heat. Cook and stir sausage in the hot skillet until browned and crumbly, 5 to 7 minutes. Set sausage aside on a plate, reserving grease in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat reserved sausage grease over medium-high heat. Add onion and bell pepper and saute until softened, 3 to 5 minutes. Transfer to a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the same skillet over medium-high heat. Whisk egg in a bowl and pour into the skillet; cook and stir until set, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour gravy mix into a small saucepan. Whisk in water. Cook over medium heat, stirring frequently, until gravy comes to a boil. Reduce heat and simmer for 1 minute. Remove from heat; gravy will thicken upon standing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cooked hash brown potato patty in the bottom of a serving bowl. Top with egg, sausage, onion and pepper mixture, and gravy, in that order.\n"
            }
        ],
