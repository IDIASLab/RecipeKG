        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8897/cranberry-chicken-iii/",
        "name": "Cranberry Chicken III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F848868.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1999-06-27T02:47:10.000Z",
        "description": "Why wait for Thanksgiving to enjoy cranberries? Chicken with cranberry sauce makes a great summertime treat.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2\u2009\u00bd pounds cut up chicken pieces",
            "salt and pepper to taste",
            "1 (8 ounce) can jellied cranberry sauce",
            "\u00bc cup corn syrup   ",
            "2 tablespoons lemon juice",
            "2 tablespoons melted butter",
            "\u00bd teaspoon dried rosemary, crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat grill to medium. Rinse chicken and pat dry. Season with salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Sauce: In a medium bowl combine the cranberry sauce, corn syrup, lemon juice, butter/margarine and rosemary. Mix well. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill chicken pieces, bone side up, on uncovered grill over medium hot coals for 20 minutes. Turn chicken and grill for another 20 to 30 minutes or until tender and juices run clear, brushing often with the sauce during the last 15 minutes of cooking.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken"
        ],
