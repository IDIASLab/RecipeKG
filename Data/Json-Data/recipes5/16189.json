        "name": "Roasted Cauliflower with Parmesan",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7819788.jpg",
            "width": 0,
            "height": 0
