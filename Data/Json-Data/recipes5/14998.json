        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15044/spruced-up-mashed-potatoes/",
        "name": "Spruced Up Mashed Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F306105.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-07-16T02:18:44.000Z",
        "description": "This recipe includes cauliflower, garlic and a carrot mashed with the potatoes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "5 pounds potatoes, peeled and quartered",
            "3 cups cauliflower florets",
            "4 cloves garlic",
            "1 large carrots, chopped",
            "\u00bd cup skim milk",
            "3 tablespoons butter",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1 tablespoon chopped fresh parsley, for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil.  Add potatoes, cauliflower, garlic and carrots. Cook until potatoes are tender but still firm, about 15 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the milk, butter, salt and pepper. Beat until smooth. Sprinkle with parsley and serve.\n"
            }
        ],
        "recipeCategory": [
