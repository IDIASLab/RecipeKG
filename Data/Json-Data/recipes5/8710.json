    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274890/pork-tenderloin-with-roasted-lemon-and-orzo/",
        "name": "Pork Tenderloin with Roasted Lemon and Orzo",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pork Tenderloin with Roasted Lemon and Orzo"
        },
        "datePublished": "2019-08-08T01:25:30.000Z",
        "description": "Easy meal ready in under an hour.  Great lemon taste!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 lemon, halved and thinly sliced",
            "2 (1 pound) pork tenderloins",
            "4 teaspoons vegetable oil",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bd cup uncooked orzo pasta",
            "1 tablespoon vegetable oil",
            "2 cloves garlic, chopped",
            "2 bunches Swiss chard, chopped",
            "\u215b cup lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss lemon slices and pork tenderloins with 4 teaspoons oil in a large bowl. Place pork on the prepared baking sheet, surrounded with lemon slices. Season pork with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until slightly pink in the center, about 30 minutes. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "While pork is roasting, bring a large pot of lightly salted water to a boil. Cook orzo in the boiling water, stirring occasionally until tender yet firm to the bite, about 11 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon oil in a large saucepan over medium heat. Add garlic and cook until fragrant, about 2 minutes. Add chard; cover, and cook until tender, about 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cooked orzo and roasted lemon slices to the chard mixture; toss to combine. Serve with the pork.\n"
            }
        ],
