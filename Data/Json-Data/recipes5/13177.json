    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277879/spicy-party-mix/",
        "name": "Spicy Party Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7634218.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-01-04T03:27:30.000Z",
        "description": "I make this spicy hot party mix during the holidays when it is cold out. It really warms you up.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "5 tablespoons butter",
            "1 tablespoon Worcestershire sauce",
            "1 tablespoon hot pepper sauce (such as Frank's RedHot\u00ae)",
            "2 teaspoons seasoned salt",
            "1\u2009\u00bd teaspoons garlic powder",
            "1 teaspoon onion powder",
            "1 teaspoon cayenne pepper",
            "\u215b teaspoon celery seed",
            "3 cups crispy corn cereal squares (such as Corn Chex\u00ae)",
            "3 cups crispy wheat cereal squares (such as Wheat Chex\u00ae)",
            "3 cups mini pretzels"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan. Whisk in Worcestershire sauce and hot sauce until well blended. Whisk in seasoned salt, garlic powder, onion powder, cayenne, and celery seed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine corn cereal squares, wheat cereal squares, and pretzels in a large oven-safe bowl or roasting pan. Drizzle the sauce over the mix with a spoon, stirring frequently, until well coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every 15 minutes, until hot and crunchy, about 1 hour and 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn off the oven, stir the mix, and leave the oven door cracked open with the mix still inside. Let cool inside for 45 to 60 minutes. Store in airtight containers.\n"
            }
        ],
