    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275052/instant-pot-italian-chicken/",
        "name": "Instant Pot\u00ae Italian Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Instant Pot\u00ae Italian Chicken"
        },
        "datePublished": "2019-06-23T06:31:45.000Z",
        "description": "Super easy Instant Pot\u00ae Italian chicken recipe. Serve over rice or mashed potatoes.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon salted butter",
            "1 tablespoon minced garlic",
            "2 (5 ounce) frozen skinless, boneless chicken breasts",
            "1 (4.5 ounce) can sliced mushrooms, drained",
            "2 (.7 ounce) packages dry Italian-style salad dressing mix",
            "1 cup chicken broth",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (8 ounce) package cream cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Turn on a multi-functional pressure cooker (such as Instant Pot&reg;) and select Saute function. Add butter to melt; then add garlic and saute for about 1 minutes. Add chicken, mushrooms, and 1 package of salad dressing mix. Pour broth over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Close and lock the lid. Select high pressure according to manufacturer's instructions; set timer for 15 minutes. Allow 10 to 15 minutes for pressure to build.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Release pressure using the natural-release method according to manufacturer's instructions for 5 minutes. Take chicken out and shred with 2 forks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Select Saute function. Add cream of chicken soup, remaining Italian dressing mix packet, and cream cheese; stir to mix. Add shredded chicken back in. Heat through, stirring occasionally, for 5 minutes. Serve.\n"
            }
        ],
