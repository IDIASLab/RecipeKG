        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24055/sinful-banana-pudding/",
        "name": "Sinful Banana Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F16965.jpg",
