    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213497/oh-so-good-pork-chops/",
        "name": "Oh So Good Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7777374.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-05T05:07:10.000Z",
        "description": "This recipe has been handed down for several generations. It has a delicious sweet and savory gravy that tastes wonderful over mashed potatoes!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 tablespoons vegetable oil",
            "6 pork chops",
            "1 cup water",
            "1 (.75 ounce) packet dry brown gravy mix",
            "2 tablespoons brown sugar",
            "2 tablespoons vinegar",
            "\u215b teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the vegetable oil in a large skillet over medium heat, and brown the pork chops on both sides, 5 to 8 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the water, brown gravy mix, brown sugar, vinegar, and garlic powder in a bowl until the sugar is dissolved and the mixture is smooth. Pour the gravy mixture over the browned pork chops. Cover the skillet, reduce heat to low, and simmer until the chops are tender, about 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes"
        ],
