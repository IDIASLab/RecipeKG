    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275163/skinny-macaroni-and-cheese/",
        "name": "Skinny Macaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Skinny Macaroni and Cheese"
        },
        "datePublished": "2019-08-08T05:45:10.000Z",
        "description": "Looking for a low-fat, delicious-tasting, macaroni and cheese? Well I've got the one for you. This skinny one uses fat-free milk, making it a lot more wholesome than your normal mac 'n cheese!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "1 (16 ounce) package elbow macaroni",
            "4 cups water",
            "1\u2009\u2153 cups non-fat powdered milk",
            "\u00be cup butter",
            "\u00bd cup all-purpose flour",
            "2 teaspoons Worcestershire sauce",
            "2 teaspoons salt",
            "9 ounces shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water and powdered milk together in a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter over medium heat in a saucepan. Add milk mixture, flour, Worcestershire sauce, and salt. Stir until thick, 3 to 4 minutes. Stir in Cheddar cheese until melted, 2 to 3 minutes more. Pour sauce over cooked macaroni and pour mixture into a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly, about 30 minutes.\n"
            }
        ],
