        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9081/brandied-candied-sweet-potatoes-with-brown-sugar/",
        "name": "Brandied Candied Sweet Potatoes with Brown Sugar",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F684816.jpg",
