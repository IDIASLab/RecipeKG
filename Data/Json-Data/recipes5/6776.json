        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254690/no-fuss-hummus/",
        "name": "No-Fuss Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3797872.jpg",
            "width": 2199,
