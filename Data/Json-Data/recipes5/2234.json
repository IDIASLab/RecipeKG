        "name": "Instant Pot&reg; Vegan Steel-Cut Oats with Apple and Cinnamon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6662272.jpg",
            "width": 1800,
