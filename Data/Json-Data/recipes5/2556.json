        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277691/mangonada-jell-o-shots/",
        "name": "Mangonada Jell-O&reg; Shots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6785851.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-06-30T12:43:06.000Z",
        "description": "If you like the popular Mexican treat, you will love these! Tastes like a mango margarita in jelly form.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT4H20M",
        "recipeYield": "20 2-ounce shots",
        "recipeIngredient": [
            "\u00bc cup water",
            "2 (.25 ounce) packages unflavored gelatin",
            "2 cups mango puree",
            "\u00bd cup white sugar",
            "1 tablespoon lemon juice",
            "1 cup vodka",
            "1 teaspoon chamoy sauce, or to taste",
            "1 teaspoon chile-lime seasoning (such as Tajin\u00ae), or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat water and gelatin in a small saucepan over medium-high heat. Whisk until dissolved, about 5 minutes. Add mango puree, sugar, and lemon juice; whisk until dissolved, about 5 minutes. Take off of heat to cool slightly, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk vodka into cooled mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add a few drops of chamoy and a sprinkle of Tajin&reg; to twenty 2-ounce cups. Fill evenly with mango gelatin and sprinkle with more Tajin&reg;. Let set in a refrigerator for 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Latin American",
            "Mexican"
