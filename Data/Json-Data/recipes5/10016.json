    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276079/daniel-fast-granola/",
        "name": "Daniel Fast Granola",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1921901.jpg",
            "width": 1610,
            "height": 1218
        },
        "datePublished": "2019-09-29T03:37:31.000Z",
        "description": "Homemade granola for the Daniel fast. Can eat as a snack or as cereal.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT3H5M",
        "totalTime": "P0DT3H50M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "\u00bc cup pineapple juice",
            "\u00bc cup coconut oil",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon sea salt",
            "3 cups rolled oats",
            "\u00bc cup sunflower seeds",
            "\u00bd cup pitted dates",
            "\u00bc cup roasted almonds",
            "1\u2009\u00bc cups quick-cooking oats",
            "\u00bd cup raisins",
            "\u00bd cup dried cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 200 degrees F (95 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pineapple juice and coconut oil together in a microwave-safe bowl or jar and melt in a microwave, about 1 minute. Add vanilla extract, cinnamon, and salt; stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour rolled oats and sunflower seeds into a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dates and almonds into an electric blender with pineapple juice mixture. Pulse for about 15 seconds to combine. Pour into oat mixture and stir using a large spoon. Stir in quick-cooking oats. Pour onto the prepared baking sheet evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 2 1/2 hours. Remove from the oven and add raisins and cranberries. Continue to bake until golden, about 30 minutes more. Let cool for 30 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Cereals",
            "Granola Recipes"
        ],
