    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282471/cheeseburger-stuffed-peppers/",
        "name": "Cheeseburger-Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheeseburger-Stuffed Peppers"
        },
        "datePublished": "2020-02-05T04:18:27.000Z",
        "description": "Keto cheeseburger-stuffed bell peppers without rice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 stuffed pepper halves",
        "recipeIngredient": [
            "4 medium bell peppers",
            "1 tablespoon olive oil",
            "1 (12 ounce) package frozen riced cauliflower",
            "1 pound lean ground beef",
            "1 medium red onion, chopped",
            "\u00bd cup reduced-sugar ketchup",
            "1 tablespoon yellow mustard, or to taste",
            "1 tablespoon dill pickle relish, or to taste",
            "1\u2009\u00bd cups shredded reduced-fat Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice peppers in half lengthwise. Remove and discard core and seeds. Place peppers in a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, 30 to 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the peppers are baking, heat olive oil in a large skillet over medium-high heat. Add riced cauliflower and cook, stirring often, until warmed through, about 5 minutes. Transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook ground beef and onion in the same skillet over medium-high heat until browned and crumbly, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add beef to the riced cauliflower. Stir in ketchup, mustard, and relish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove peppers from the oven and fill with beef mixture. Top with Cheddar cheese. Return to the oven and bake until filling is hot and cheese is melted, about 15 minutes.\n"
            }
        ],
