        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/63340/brussels-sprouts-with-mushrooms/",
        "name": "Brussels Sprouts with Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F293021.jpg",
