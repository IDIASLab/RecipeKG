    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283208/gullah-red-rice/",
        "name": "Gullah Red Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9034841.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-01T21:40:56.000Z",
        "description": "Charleston red rice is one of Charleston, South Carolina's Gullah dishes. Being from the Sea Islands of Charleston, this is a go-to dish passed down!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 slices bacon, chopped",
            "1 green bell pepper, chopped",
            "1 small onion, chopped",
            "2 link (4\" long x 1-1/8\" dia)s smoked sausage links, sliced",
            "1 (8 ounce) can tomato sauce (such as Hunt's\u00ae)",
            "1 (8 ounce) can tomato paste (such as Hunt's\u00ae)",
            "1 cup water",
            "2 cups uncooked parboiled (converted) rice",
            "1 tablespoon white sugar",
            "1 teaspoon salt, or to taste",
            "1 teaspoon ground black pepper",
            "1 teaspoon onion powder",
            "1 teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a pot and cook over medium-high heat, turning occasionally, until evenly browned, 7 to 10 minutes. Add bell pepper and onion to bacon grease and saute over medium heat until soft, about 5 minutes. Add sausage and cook and stir until browned, 8 to 10 minutes. Remove mixture from the pan to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour tomato sauce and tomato paste into the same pot, along with 1 cup water. Bring to a boil; add rice, sugar, salt, pepper, onion powder, and garlic powder. Bring back to a boil; reduce heat and let simmer over medium-low heat for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the rice mixture to a 9x13-inch baking pan. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until liquid has evaporated and rice is tender, about 25 minutes.\n"
            }
        ],
