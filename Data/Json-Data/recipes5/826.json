    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258179/hearty-caldo-de-res-mexican-beef-soup/",
        "name": "Hearty Caldo de Res (Mexican Beef Soup)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hearty Caldo de Res (Mexican Beef Soup)"
        },
        "datePublished": "2019-04-19T05:49:51.000Z",
        "description": "Step-by-step way to make this soup and it tastes really good too. Serve with lemon wedges, lime wedges, jalapenos, and/or torts.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT2H25M",
        "totalTime": "P0DT3H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound bone-in beef shanks",
            "1 pound boneless beef shanks",
            "1 pound beef oxtail, cut into pieces",
            "1 large onion, chopped",
            "2 cups beef broth",
            "2 large tomatoes, chopped",
            "salt and ground black pepper to taste",
            "4 large potatoes, cut into chunks",
            "1 yuca root - peeled, halved, and sliced",
            "4 ears corn, cut into chunks",
            "2 large carrots, cut into chunks",
            "water to cover",
            "2 cups chopped fresh cilantro",
            "1 head cabbage, cut into chunks",
            "5 zucchini, cut into chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine bone-in beef shanks, boneless beef shanks, oxtail, and onion in a large pot over medium-high heat. Cook and stir until beef is browned, 45 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir beef broth, tomatoes, salt, and pepper into the pot. Simmer gently over medium heat, partially covered, until tomatoes break down, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir potatoes, yucca, corn, and carrot into the pot carefully with a wooden spoon. Add cilantro. Pour in enough water to cover vegetables; bring to a boil. Reduce heat and simmer, stirring carefully to avoiding mashing the vegetables, until potatoes are tender, 20 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cabbage and zucchini into the pot. Simmer until zucchini is soft, 15 to 20 minutes. Discard bones before serving.\n"
            }
        ],
