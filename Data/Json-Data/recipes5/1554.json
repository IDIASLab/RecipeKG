    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273131/easy-baked-pork-chops/",
        "name": "Easy Baked Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9164524.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-05-14T07:57:43.000Z",
        "description": "Try this easy recipe to prepare flavorful, juicy baked pork chops in the oven without marinating.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 pork chops",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "1 small onion, coarsely chopped",
            "4 pork chops",
            "3 tablespoons maple syrup",
            "3 tablespoons spicy brown mustard",
            "4 cloves garlic, minced, or more to taste",
            "1 teaspoon red pepper",
            "1 teaspoon chili powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 9x13-inch baking pan with oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scatter onions evenly into the baking pan. Place pork chops on top of onions in the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine maple syrup, mustard, garlic, red pepper, chili powder, and salt together in a small bowl. Pour mixture evenly over pork chops. Cover baking dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chops are no longer pink in the center and an instant-read thermometer inserted into the center reads 160 degrees F (71 degrees C), 1 to 1 1/2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes",
            "Baked"
        ],
