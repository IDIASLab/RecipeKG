        "mainEntityOfPage": "https://www.allrecipes.com/recipe/152710/hodies-sloppy-joes/",
        "name": "Hodie's Sloppy Joes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F54091.jpg",
