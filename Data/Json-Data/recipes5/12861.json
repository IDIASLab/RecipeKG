        "mainEntityOfPage": "https://www.allrecipes.com/recipe/89683/german-tomato-pie/",
        "name": "German Tomato Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7525659.jpg",
