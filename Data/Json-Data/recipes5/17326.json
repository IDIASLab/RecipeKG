        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21853/sweet-potatoes-and-apples/",
        "name": "Sweet Potatoes and Apples",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970413.jpg",
            "width": 3456,
