    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228347/bennos-bacon-potato-salad/",
        "name": "Benno's Bacon Potato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970178.jpg",
            "width": 2439,
            "height": 2419
        },
        "datePublished": "2019-07-10T07:03:44.000Z",
        "description": "This potato salad is one of my tried and true favorites. Friends and family love it!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc teaspoon salt",
            "4 cups water, or enough to cover",
            "1 pound red potatoes, scrubbed and cut into quarters ",
            "\u00bd yellow onion, diced",
            "2 slices thick-cut bacon, cut into 1-inch squares",
            "2 stalks celery, diced",
            "\u00bc cup mild yellow pepper rings, drained",
            "\u00bd cup mayonnaise",
            "1 tablespoon Dijon mustard",
            "1 teaspoon prepared horseradish",
            "2 tablespoons juice from yellow pepper rings",
            "\u00bd teaspoon lemon juice",
            "Creole seasoning (such as Tony Chachere's\u00ae), or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir salt into water in a large pot and add potatoes; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes. Drain and cool. Slice potatoes and place in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir yellow onion, bacon, celery, and pepper rings in a skillet until bacon is browned and celery begins to soften, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mayonnaise, Dijon mustard, horseradish, pepper juice, lemon juice, and Creole seasoning together in a bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add bacon mixture and dressing to potatoes; stir to combine. Refrigerate for 30 minutes before serving.\n"
            }
        ],
