    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274509/kielbasa-pasta-sauce/",
        "name": "Kielbasa Pasta Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F556609.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-07-10T21:14:34.000Z",
        "description": "I whipped up this spaghetti sauce one afternoon and was blown away by the flavor. The roasted red peppers complement the kielbasa and give the sauce a slightly smoky flavor. Serve over spaghetti.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd pound kielbasa sausage, chopped",
            "1 (8 ounce) package sliced fresh mushrooms",
            "1 (8 ounce) jar roasted red peppers, drained and chopped",
            "\u00bd medium onion, diced",
            "3 cloves garlic, minced",
            "1 (15 ounce) can tomato sauce",
            "1 (14.5 ounce) can diced tomatoes",
            "4 ounces tomato paste",
            "1 tablespoon Italian seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook kielbasa sausage in a deep nonstick skillet or frying pan over medium heat for 1 minute. Add mushrooms, roasted red peppers, onion, and garlic. Stir and cook until onion is tender, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain grease from the sausage mixture and return to the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together tomato sauce, diced tomatoes, tomato paste, and Italian seasoning in a medium bowl and pour over the sausage mixture in the skillet. Stir and simmer, covered, over medium-low heat until heated through, about 10 minutes.\n"
            }
        ],
