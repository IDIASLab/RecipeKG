        "recipeIngredient": [
            "\u00be cup apple cider vinegar",
            "\u00bc cup olive oil",
            "3 tablespoons honey",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1 pinch celery seed",
            "4\u2009\u00bd leaves basil, chopped",
