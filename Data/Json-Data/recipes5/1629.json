    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273477/breakfast-potato-boats/",
        "name": "Breakfast Potato Boats",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8063944.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-05-16T22:35:45.000Z",
        "description": "These breakfast potato boats are a perfect combination of baked potato and yummy eggs and bacon.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 large potatoes",
            "1 slice bacon",
            "2 tablespoons butter, divided",
            "2 pinches chopped fresh parsley, or to taste",
            "salt and ground black pepper to taste",
            "2 eggs",
            "\u2153 cup shredded Cheddar cheese",
            "1 teaspoon chopped fresh parsley, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 450 degrees F (230 degrees C). Prick potatoes several times with a fork and place onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until potatoes are easily pierced with a fork, 50 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While potatoes bake, place bacon in a skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon on paper towels. Let cool 10 minutes. Crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove potatoes from the oven and let cool 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice tops off of potatoes and scoop out insides to form a deep bowl. Add 1/2 tablespoon butter, 1 pinch parsley, salt, and pepper to each potato.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crack an egg into each potato. Top each with an additional 1/2 tablespoon butter. Divide bacon and Cheddar cheese between the 2 potato bowls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake potatoes in the preheated oven until eggs are set, 15 to 20 minutes. Sprinkle each with 1/2 teaspoon parsley.\n"
            }
        ],
