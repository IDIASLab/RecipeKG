        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10239/applesauce-oatmeal-cookies/",
        "name": "Applesauce Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3490188.jpg",
            "width": 3264,
