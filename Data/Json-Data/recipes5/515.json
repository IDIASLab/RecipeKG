        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272534/ruby-chocolate-cheesecake-dip/",
        "name": "Ruby Chocolate Cheesecake Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6502199.jpg",
            "width": 1440,
