        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273874/smoked-salsa/",
        "name": "Smoked Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6747553.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-06-06T02:15:45.000Z",
        "description": "Tomatoes, onions, and peppers are smoked long and slow in a smoker grill, resulting in a robust, homemade salsa.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT3H0M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 tomatoes, diced",
            "2 onions, diced",
            "1 green bell pepper, diced",
            "1 red bell pepper, diced",
            "1 yellow bell pepper, diced",
            "1 orange bell pepper, diced",
            "3 jalapeno peppers, diced",
            "1 lime, juiced",
            "\u00bc teaspoon garlic powder, or to taste",
            "salt and ground black pepper to taste",
            "1 bunch fresh cilantro, finely chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat a smoker grill to 250 degrees F (120 degrees C). Add wood chips according to manufacturer's directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, onions, bell peppers, and jalapeno peppers in an aluminum grill pan. Place on the grate of the preheated smoker grill; smoke for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove vegetables from the smoker and place in a blender. Blend to desired consistency. Mix in lime juice, garlic powder, salt, and pepper. Add fresh cilantro for decoration.\n"
            }
        ],
