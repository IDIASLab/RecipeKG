    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268524/sausage-dip/",
        "name": "Sausage Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F431489.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-02-26T05:52:12.000Z",
        "description": "I love dips and quesos so I combined all of my favorite recipes into one and this is what I came up with!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 8-inch dish",
        "recipeIngredient": [
            "2 pounds shredded Cheddar cheese",
            "1 (16 ounce) container sour cream",
            "3 tomatoes, diced",
            "\u00bd cup mayonnaise",
            "1 (4 ounce) can diced jalapeno peppers",
            "1 pound ground hot Italian sausage",
            "1 lime, juiced",
            "1 (1 ounce) package taco seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Cheddar cheese, sour cream, tomatoes, mayonnaise, and jalapeno peppers in a medium bowl and mix until well blended. Pour into an 8x8-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until melted, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, cook and stir sausage in a hot skillet until browned and crumbly, 5 to 7 minutes. Drain excess grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cooked sausage, lime juice, and taco seasoning into the hot cheese mixture. Serve immediately or keep warm in a small slow cooker.\n"
            }
        ],
