        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228309/pork-chop-cocktail/",
        "name": "Pork Chop Cocktail",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4463715.jpg",
            "width": 4896,
            "height": 4896
