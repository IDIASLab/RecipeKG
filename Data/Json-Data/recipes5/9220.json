    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275619/pesto-pasta-chicken-bake/",
        "name": "Pesto Pasta Chicken Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pesto Pasta Chicken Bake"
        },
        "datePublished": "2019-08-30T22:32:53.000Z",
        "description": "Creamy and delicious and nice enough to serve to guests.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 3 1/2-quart baking casserole",
        "recipeIngredient": [
            "1 (16 ounce) package farfalle (bow tie) pasta",
            "cooking spray",
            "2\u2009\u00bd cups chopped, cooked chicken",
            "2 (10 ounce) cans condensed cream of chicken soup",
            "1 (12 ounce) jar marinated artichoke hearts, drained and chopped",
            "1 cup grated Parmesan cheese",
            "1 (7 ounce) container pesto",
            "\u00be cup chopped, drained oil-packed sun-dried tomatoes",
            "1 (4 ounce) jar diced pimento peppers",
            "2 tablespoons grated Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook bow-tie pasta at a boil, stirring occasionally, until tender yet firm to the bite, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 3 1/2-quart baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain pasta and place in a large bowl. Add cooked chicken, cream of chicken soup, artichoke hearts, 1 cup Parmesan cheese, pesto, sun-dried tomatoes, and pimento peppers; mix to combine. Spoon pasta mixture into the prepared baking dish. Sprinkle with additional Parmesan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until sauce is bubbly, 35 to 40 minutes.\n"
            }
        ],
