        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20980/fiery-pork-skewers/",
        "name": "Fiery Pork Skewers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F424488.jpg",
            "width": 4288,
