        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24189/mock-pate-de-foie-gras/",
        "name": "Mock Pate de Foie Gras",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1000417.jpg",
            "width": 1940,
            "height": 1936
        },
        "datePublished": "2019-07-10T06:52:34.000Z",
        "description": "Spread this rich liver sausage appetizer on crackers. The eye pleasing presentation is certain to impress.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "1 party ball",
        "recipeIngredient": [
            "1\u2009\u00bd cups pecan halves or pieces",
            "1 (8 ounce) package cream cheese, softened",
            "8 ounces braunschweiger liver sausage",
            "2 tablespoons minced onion",
            "1\u2009\u00bd tablespoons Worcestershire sauce",
            "1 tablespoon fresh lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pecans on a medium baking sheet. Bake in the preheated oven 5 minutes, or until toasted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together cream cheese, braunschweiger liver sausage, onion, Worcestershire sauce and lemon juice. Form the mixture into a ball.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place toasted pecans in a pie pan. Place cream cheese mixture ball into the pie pan, and roll to coat with pecans. Cover with plastic wrap. Chill in the refrigerator until serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Pate Recipes"
        ],
