    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270678/grilled-honey-lemon-salmon-with-basil/",
        "name": "Grilled Honey-Lemon Salmon with Basil",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grilled Honey-Lemon Salmon with Basil"
        },
        "datePublished": "2019-07-10T07:13:16.000Z",
        "description": "Light and warm grilled salmon with a hint of honey, lemon, and basil. Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "5 salmon fillets",
        "recipeIngredient": [
            "5 salmon fillets, rinsed",
            "\u00bd cup white vinegar",
            "1 teaspoon ground ginger",
            "1 teaspoon garlic powder",
            "ground black pepper to taste",
            "kosher salt to taste",
            "cooking spray",
            "\u2153 cup honey",
            "3 tablespoons lemon juice",
            "2 tablespoons minced fresh basil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place salmon fillets in a plastic container and add vinegar, ginger, garlic powder, black pepper, and salt. Lightly massage into salmon for about 1 minute. Spray both sides of fish with cooking spray. Refrigerate fish while you prepare the rest of the ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix honey, lemon juice, and basil together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat a clean outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill salmon fillets on the hot grill. Cook the first side for 3 to 5 minutes, depending on thickness of the fish. Turn fish and lightly baste with honey-lemon mixture. Grill another 3 minutes and turn so honey side is down. Baste top of salmon with honey mixture and grill until just warmed on this side, approximately 1 minute more.\n"
            }
        ],
