        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19519/tequila-cocktail-sauce/",
        "name": "Tequila Cocktail Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2408195.jpg",
