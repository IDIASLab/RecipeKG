        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32555/hot-spiced-cranberry-cider/",
        "name": "Hot Spiced Cranberry Cider",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3955.jpg",
