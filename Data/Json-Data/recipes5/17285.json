        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21046/tennessee-rubdown/",
        "name": "Tennessee Rubdown",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F849584.jpg",
            "width": 1639,
            "height": 1639
