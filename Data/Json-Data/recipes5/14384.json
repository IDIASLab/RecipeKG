        "name": "Blackened Corn and Jicama Pico de Gallo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8119393.jpg",
            "width": 0,
