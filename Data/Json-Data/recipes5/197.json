        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268969/quick-mushroom-rice/",
        "name": "Quick Mushroom Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1965636.jpg",
