        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26542/cantaloupe-salad/",
        "name": "Cantaloupe Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F833189.jpg",
