    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278603/peking-chicken-hotdish/",
        "name": "Peking Chicken Hotdish",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Peking Chicken Hotdish"
        },
        "datePublished": "2011-01-19T17:41:31.000Z",
        "description": "Easy Chinese hotdish to take with you anywhere!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "2 cups uncooked white rice",
            "2\u2009\u00bd cups water",
            "1 (10.75 ounce) can  condensed golden mushroom soup",
            "1 pound cooked chicken, chopped",
            "1 medium red bell pepper, chopped",
            "\u00bd cup chopped onion",
            "\u00bc cup soy sauce",
            "1 tablespoon cornstarch",
            "\u00bc cup hoisin sauce",
            "2 tablespoons rice vinegar",
            "\u00bd tablespoon red pepper flakes",
            "\u00bd cup peanuts, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix rice, water, and mushroom soup together in a 9x13-inch baking dish. Top with chicken, bell pepper, and onion. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk soy sauce and cornstarch together in a bowl. Mix in hoisin sauce, rice vinegar, and red pepper flakes. Uncover baking dish and pour sauce over casserole. Top with peanuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue baking, uncovered, until heated through, about 20 minutes more.\n"
            }
        ],
