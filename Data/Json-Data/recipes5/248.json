        "name": "Instant Pot&reg; Wheat Berries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6009135.jpg",
            "width": 2592,
