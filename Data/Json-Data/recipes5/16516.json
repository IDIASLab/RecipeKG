        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9675/walnut-cookies-i/",
        "name": "Walnut Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F423871.jpg",
            "width": 1784,
