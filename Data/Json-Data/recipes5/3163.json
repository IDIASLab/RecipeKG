    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23170/honeyed-beef-stew/",
        "name": "Honeyed Beef Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F504440.jpg",
            "width": 2288,
            "height": 1712
        },
        "datePublished": "2000-10-12T12:17:14.000Z",
        "description": "A melt in the mouth meal guests ask for again and again. So easy to make .",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT2H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "2 pounds lean beef chuck, trimmed and cut into 1 inch cubes",
            "1 large onion, chopped",
            "1 green bell pepper, chopped",
            "3 strips celery, chopped",
            "2 large carrots, peeled and sliced",
            "2 tablespoons honey",
            "2 tablespoons fresh lemon juice",
            "2 teaspoons yellow mustard",
            "\u00bc cup ketchup",
            "2 cups water",
            "salt to taste",
            "freshly ground pepper, to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pot over medium/high heat, heat the olive oil and quickly brown the beef pieces on all sides. Place the onion, green pepper, celery, and carrots into the pot and fry for 5 minutes; stirring occasionally.  Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, combine the honey, lemon juice, mustard, ketchup, water, salt and pepper. Mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the meat and vegetables to a 2 quart casserole dish. Pour the ketchup mixture over the meat and vegetables.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated oven for 2 to 2 1/2 hours or until meat is tender.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Beef"
