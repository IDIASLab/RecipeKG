    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270755/magic-vegan-stuffed-peppers/",
        "name": "Magic Vegan Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Magic Vegan Stuffed Peppers"
        },
        "datePublished": "2020-02-26T05:52:15.000Z",
        "description": "Delicious, hearty, easy, and vegan friendly. They're magic stuffed peppers, not kidding!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "3 stuffed peppers",
        "recipeIngredient": [
            "1 cup water",
            "\u00bd cup brown rice",
            "1 medium ear of corn",
            "cooking spray",
            "2 cloves garlic, finely chopped",
            "1 (8 ounce) can tomato sauce",
            "\u00bd cup chopped green onions",
            "1 (4 ounce) can black olives, drained",
            "\u00bd (1 ounce) package taco seasoning mix",
            "2 tablespoons hot pepper sauce (such as Tabasco\u00ae)",
            "3 red bell peppers, stemmed and seeded"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water and brown rice in a rice cooker. Cook according to manufacturer's instructions, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, husk corn and peel off the silk. Wrap in a damp paper towel and microwave for 5 minutes. Let cool for at least 1 minute. Cut kernels from the cob.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a large skillet with cooking spray. Saute garlic until light brown, about 1 minute. Mix in brown rice, corn, tomato sauce, green onions, olives, taco seasoning, and hot sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon mixture into the bell peppers. Prop peppers up in a loaf pan so they don't fall over.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through, about 40 minutes.\n"
            }
        ],
