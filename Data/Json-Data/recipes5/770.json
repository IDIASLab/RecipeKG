        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21155/quick-and-simple-broccoli-and-cheese/",
        "name": "Quick and Simple Broccoli and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5325525.jpg",
            "width": 3024,
