    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/102321/easy-baked-ham/",
        "name": "Easy Baked Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F49337.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2020-03-05T05:00:02.000Z",
        "description": "My family does this every year with our Christmas Eve ham and it never fails to be anything but delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (4 pound) fully-cooked spiral cut ham",
            "1 (20 ounce) can sliced pineapple, drained with juice reserved",
            "1 cup dark brown sugar",
            "1 liter ginger ale"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove as much of the ham from the bone as you can. Layer one third of the ham in the bottom of a 6 quart casserole dish. Top with a few pineapple rings and 1/4 of the brown sugar. Repeat the layers, ending with pineapple rings on top, but don't sprinkle the last 1/4 of brown sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour any juice left in the pineapple can over the ham. Then pour in enough of the ginger ale to fill the dish about half way. Sprinkle the remaining brown sugar over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover with aluminum foil and bake for 20 minutes in the preheated oven. Uncover and cook for an additional 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Ham",
            "Whole"
        ],
