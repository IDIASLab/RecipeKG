    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7361/cream-cheese-frosting-without-powdered-sugar/",
        "name": "Cream Cheese Frosting without Powdered Sugar",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F602452.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-02T18:17:01.000Z",
        "description": "This is an alternative to the classic recipe that calls for powdered sugar. We use it to frost cocktail cake. Try it on applesauce, spice, banana, or carrot cakes too.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "2 tablespoons unsalted butter, melted and cooled",
            "2 tablespoons light brown sugar",
            "1 teaspoon vanilla extract",
            "1 teaspoon almond extract",
            "1 tablespoon honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend together cream cheese, butter, brown sugar, vanilla, and almond extract with an electric mixer until starting to become stiff. Pour in honey. Beat together until mixture is whipped, making sure not to overmix as it will collapse.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use immediately or store in a sealed container in the refrigerator until use.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frostings and Icings",
            "Cream Cheese"
        ],
