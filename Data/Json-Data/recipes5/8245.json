    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274892/roasted-lemon-pasta/",
        "name": "Roasted Lemon Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Lemon Pasta"
        },
        "datePublished": "2019-07-25T06:28:47.000Z",
        "description": "Pasta like you've never had it before with roasted lemons.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 large lemons",
            "1 (16 ounce) package whole wheat penne",
            "3 cloves garlic, minced",
            "3 tablespoons olive oil",
            "2 tablespoons finely chopped fresh basil",
            "1 pinch salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap lemons in aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lemons in the preheated oven and roast until tender, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While lemons are roasting, bring a large pot of lightly salted water to a boil. Add penne and cook, stirring occasionally, until tender yet firm to the bite, about 11 minutes. Drain pasta and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove lemons from the oven and let cool slightly. Remove foil, slice each lemon in half, and squeeze roasted lemon pulp into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet. Add garlic and cook until fragrant, about 1 minute. Add lemon pulp and cook for 2 to 3 minutes. Add cooked pasta, basil, salt and pepper; toss well. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Pasta and Noodles",
            "Noodle Recipes"
        ],
