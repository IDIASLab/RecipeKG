    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275072/jewels-roasted-corn-salsa/",
        "name": "Jewel's Roasted Corn Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6886213.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-08-08T20:10:55.000Z",
        "description": "This dish is inspired by my favorite salsa at Chipotle&reg;. You can use it to top tacos or burritos, or just scoop it up with tortilla chips.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 ears fresh sweet corn, husked and silks removed",
            "\u00bd red onion, diced",
            "\u00bd cup roughly chopped fresh cilantro",
            "2 fresh jalapeno peppers, diced",
            "3 tablespoons freshly squeezed lime juice",
            "1 clove garlic, minced",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place corn on the preheated grill and cook, turning often, until charred all over, about 10 minutes. Remove to a plate and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place red onion, cilantro, jalapeno peppers, lime juice, garlic, and salt in a bowl. Cut corn kernels from the cobs and add to the onion mixture. Mix well, gently breaking up any large corn chunks with the spoon. Discard cobs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and refrigerate for at least 2 hours.\n"
            }
        ],
