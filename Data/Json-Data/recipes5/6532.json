    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244610/juicy-chicken-breast-marinade/",
        "name": "Juicy Chicken Breast Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2613934.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-07-10T07:06:26.000Z",
        "description": "This is a great basic chicken marinade recipe that will give you moist, delicious chicken breasts every time!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P0DT6H26M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 chicken breasts",
            "1 cup safflower oil",
            "\u00bc cup lemon juice",
            "3 tablespoons minced garlic",
            "3 tablespoons Italian seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pound chicken breasts on a work surface to at least 1/2-inch thickness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk safflower oil, lemon juice, garlic, and Italian seasoning together in a bowl and pour into a resealable plastic bag. Add chicken, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator for 6 to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook chicken on preheated grill until longer pink in the center, about 3 minutes per side. Transfer chicken to a plate and let sit for 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
