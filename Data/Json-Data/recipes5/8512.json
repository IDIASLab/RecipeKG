        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour hot, strong coffee into two mugs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 1 ounce Irish cream liqueur, 1 ounce hazelnut liqueur, and 1 ounce Irish whiskey into each mug.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a dollop of whipped cream in each mug; sprinkle whipped cream with nutmeg.\n"
            }
