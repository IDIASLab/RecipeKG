    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/96335/brandied-pumpkin-pie/",
        "name": "Brandied Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Brandied Pumpkin Pie"
        },
        "datePublished": "2020-03-05T04:59:52.000Z",
        "description": "This is the best pumpkin pie I have ever had. All you have to do is try it once and you're sure to be hooked, just like my family is.  I found this recipe a few years ago, and we have enjoyed it ever since! Serve with whipped topping.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 prepared, unbaked pie crust, thawed",
            "2 eggs, slightly beaten",
            "1 (15 ounce) can pumpkin",
            "1 (12 fluid ounce) can evaporated milk",
            "\u2154 cup brown sugar",
            "\u00bc cup brandy",
            "\u00bc teaspoon salt",
            "2 tablespoons ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the eggs, pumpkin, milk, brown sugar, brandy, salt, cinnamon, ginger, cloves, and nutmeg in a large bowl until smooth. Pour the mixture into the pie crusts. Cover the outside edge of each pie with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 25 minutes; remove foil. Continue to bake until a knife inserted near the center of a pie comes out clean, about 25 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool completely on a wire rack before serving. Store unused portion in refrigerator.\n"
            }
        ],
