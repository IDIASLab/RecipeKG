        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234997/citrus-bacon-green-beans/",
        "name": "Citrus-Bacon Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1378000.jpg",
            "width": 2448,
            "height": 3264
