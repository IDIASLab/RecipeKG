    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277047/curry-red-lentil-soup/",
        "name": "Curry Red Lentil Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6364596.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-20T01:15:38.000Z",
        "description": "Clean, delicious, comforting red lentil soup! You can prep dry ingredients and spices, and add whatever veggies you have on hand...I made it with cauliflower, carrots, and kale!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 teaspoons olive oil",
            "1 head cauliflower, chopped into small florets",
            "2 carrots, chopped",
            "2 cups boiling water",
            "1 cube vegetable bouillon",
            "1 (14 ounce) can reduced-fat coconut milk",
            "1 cup red lentils",
            "1 teaspoon garlic powder",
            "1 teaspoon dried onion flakes",
            "1 teaspoon curry powder",
            "1 teaspoon paprika",
            "1 teaspoon ground turmeric",
            "\u00bd teaspoon ground cumin",
            "1 bunch kale leaves, stems and inner ribs discarded, leaves coarsely chopped "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large pot over medium-high heat. Add cauliflower and carrots; saute until softened, 7 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour boiling water and vegetable bouillon into a bowl and stir until dissolved. Add broth and coconut milk to cauliflower mixture. Bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir lentils, garlic powder, onion flakes, curry, paprika, turmeric, and cumin into the pot. Cover and let simmer until lentils are fully cooked and tender, about 20 minutes. Stir in kale and let sit over low heat for 5 minutes. Remove from heat and serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Cauliflower"
        ],
