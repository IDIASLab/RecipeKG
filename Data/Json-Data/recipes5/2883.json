        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13920/broccoli-with-lemon-almond-butter/",
        "name": "Broccoli With Lemon Almond Butter",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F186939.jpg",
