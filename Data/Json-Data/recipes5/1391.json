    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247469/grown-up-macaroni-and-cheese/",
        "name": "Grown-Up Macaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grown-Up Macaroni and Cheese"
        },
        "datePublished": "2019-05-08T06:30:53.000Z",
        "description": "Creamy homestyle macaroni and cheese with ingredients for the discriminating palate. This is a casserole dish, best for the nights that the kids are out and you want to surprise someone with a special occasion comfort food.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "4 1/2 cups",
        "recipeIngredient": [
            "1 ounce dried porcini mushrooms",
            "1 (8 ounce) package small shell pasta",
            "\u00bc cup unsalted butter",
            "1 leek, finely chopped",
            "1 small shallot, finely chopped",
            "\u00bc cup all-purpose flour",
            "1 teaspoon Worcestershire sauce",
            "\u215b teaspoon ground black pepper",
            "1 cup milk, warmed",
            "1 tomato, chopped",
            "2 tofu hot dogs, sliced 1/8-inch thick",
            "6 ounces grated Cheddar cheese",
            "3 ounces Brie cheese, rind removed and cut into cubes",
            "2 ounces grated Asiago cheese",
            "\u00bc cup milk",
            "\u2153 cup bread crumbs",
            "1 teaspoon ground paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place porcini mushrooms in a small bowl. Cover with warm water. Let soak until rehydrated, 20 to 30 minutes. Drain and chop coarsely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook shell pasta in the boiling water, stirring occasionally until tender yet firm to the bite, 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large pot over medium heat. Add leek and shallot; cook and stir until softened, about 2 minutes. Reduce heat to low; whisk in flour, Worcestershire sauce, and pepper. Pour in 1 cup warmed milk slowly, stirring constantly, until incorporated. Simmer sauce until thickened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chopped mushrooms, tomato, and tofu hot dogs into the sauce; fold in pasta. Stir in Cheddar cheese, Brie cheese, and Asiago cheese until melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour pasta and cheese mixture into a 9-inch square casserole dish. Drizzle 1/4 cup milk on top if mixture seems too thick. Top with bread crumbs and paprika. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling in the center, about 20 minutes.\n"
            }
        ],
