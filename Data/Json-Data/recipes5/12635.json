    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277792/tilapia-with-roasted-potatoes/",
        "name": "Tilapia with Roasted Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tilapia with Roasted Potatoes"
        },
        "datePublished": "2019-12-19T02:25:39.000Z",
        "description": "This is my family's favorite tilapia dish, which is great because it's so easy to make! You can use any white fish you prefer in this recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "nonstick cooking spray",
            "8 medium red potatoes, cut into chunks",
            "1 medium yellow bell pepper, cut into 1-inch pieces",
            "4 cloves garlic, minced",
            "1 tablespoon olive oil",
            "1 teaspoon salt, divided",
            "\u00bd teaspoon ground black pepper",
            "2 cups cherry tomatoes, halved",
            "4 (4 ounce) fillets tilapia fillets",
            "1 teaspoon lemon zest",
            "1 medium lemon, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Spray a baking dish with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes, bell pepper, garlic, oil, and 1/2 teaspoon salt in the prepared dish; toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes. Add tomatoes. Place tilapia fillets on top of vegetables and sprinkle with remaining salt, pepper, and lemon zest. Continue to bake until fish flakes easily with a fork, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze lemon juice over fish and serve.\n"
            }
        ],
