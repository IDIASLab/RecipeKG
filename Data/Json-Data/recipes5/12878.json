    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220152/oven-fried-bananas/",
        "name": "Oven-Fried Bananas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7525703.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-28T03:50:09.000Z",
        "description": "A unique and delicious low-fat dessert! For a chocolatey twist, coat the bananas in crushed puffed cocoa cereal instead of the breadcrumbs. Serve while hot, on top of fat-free ice cream. Alternatively, you can melt 2-3 tablespoons of ice cream and use to dip bananas in.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "cooking spray",
            "\u00bc cup dry bread crumbs",
            "1 tablespoon granular no-calorie sucralose sweetener (such as Splenda\u00ae)",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground ginger",
            "1 pinch salt",
            "1 large banana, cut into slices"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper and spray with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bread crumbs, no-calorie sweetener, cinnamon, ginger, and salt in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray banana slices on both sides with the cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll banana slices in the bread crumb mixture to coat, and arrange slices on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly spray the banana slices again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crisp, 10 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
