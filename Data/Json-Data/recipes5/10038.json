    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276178/sugar-cookie-bites/",
        "name": "Sugar Cookie Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sugar Cookie Bites"
        },
        "datePublished": "2019-09-29T04:51:49.000Z",
        "description": "Tiny, cute, sugar cookie bites.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "48 cookie bites",
        "recipeIngredient": [
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd cup unsalted butter",
            "3 tablespoons powdered sugar",
            "\u00bc teaspoon kosher salt",
            "1 (16 ounce) package vanilla frosting",
            "1 drop red food coloring, or as desired",
            "1 drop green food coloring, or as desired",
            "1 tablespoon red and green sprinkles, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend flour, butter, powdered sugar, and salt in a food processor until combined. Place dough in a bowl and knead until it comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out onto waxed paper into a 1/2-inch thick square. Cut dough into 1/2-inch squares. Place bites onto a large cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until light brown and edges are set, 18 to 20 minutes. Remove from the oven and let cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide vanilla frosting into 3 small bowls. Dye 1 bowl red using red food coloring and 1 green using green food coloring; leave third bowl uncolored. Frost cooled cookies and top with sprinkles as desired.\n"
            }
        ],
