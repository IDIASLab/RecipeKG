        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16615/creamy-canned-chicken-spread/",
        "name": "Creamy Canned Chicken Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Creamy Canned Chicken Spread"
        },
        "datePublished": "2000-01-29T03:26:56.000Z",
        "description": "Melt-in-the-mouth creamy chicken spread great for parties or receptions.  Once you taste it, you'll want to eat the whole bowl!",
        "prepTime": "P0DT8H10M",
        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 envelope (1 tablespoon) unflavored gelatin",
            "3 tablespoons water",
            "\u00be cup mayonnaise",
            "1 (8 ounce) package cream cheese, softened",
            "1 onion, chopped",
            "1 cup celery, minced",
            "1 (5 ounce) can chicken chunks, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small pot, heat chicken soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine gelatin and water and stir it into the heated soup. Blend mayonnaise, cream cheese, onion, and celery into the soup mixture. Add chicken and continue mixing. Refrigerate overnight.\n"
            }
        ],
