    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/67020/no-bake-cranberry-sauce-pie/",
        "name": "No-Bake Cranberry Sauce Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7313973.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-20T07:50:08.000Z",
        "description": "Cranberry sauce never tasted so good! This colorful pie is a nice addition to any Thanksgiving or Christmas table. Your tastebuds will be in for a treat with its creamy, tart and sweet medley.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT3H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "1 tablespoon margarine",
            "1 teaspoon vanilla extract",
            "1 cup confectioners' sugar",
            "1 (9 inch) prepared graham cracker crust",
            "1 (3 ounce) package raspberry flavored Jell-O\u00ae mix",
            "1 cup boiling water",
            "1 (14.5 ounce) can whole berry cranberry sauce",
            "\u2153 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat the cream cheese, margarine, and vanilla extract together in a mixing bowl until smooth and fluffy. Sift in the confectioner's sugar and mix until thoroughly blended. Spread evenly over the bottom of the graham cracker pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk the gelatin and boiling water together in a small bowl until dissolved. Stir in the cranberry sauce and walnuts. Pour over the cream cheese layer in the pie crust. Refrigerate until set, 3 to 4 hours. Serve chilled.\n"
            }
        ],
