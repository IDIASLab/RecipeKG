    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272576/green-thai-shrimp-curry/",
        "name": "Green Thai Shrimp Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Green Thai Shrimp Curry"
        },
        "datePublished": "2019-04-05T00:44:24.000Z",
        "description": "This very tasty and easy Thai shrimp dish is great for entertaining. I served it over baked brown rice with raisins and crushed peanuts.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons peanut oil",
            "1 small onion, finely chopped",
            "3 cloves garlic, minced",
            "1 tablespoon minced fresh ginger",
            "\u00bc teaspoon hot pepper flakes, or more to taste",
            "2 tablespoons green curry paste (such as Thai Kitchen\u00ae)",
            "1 stalk celery, finely sliced",
            "1 small red bell pepper, halved and cut into slivers",
            "1 cup coconut milk (such as Chaokoh\u00ae)",
            "1 pound medium cleaned raw shrimp, or more to taste",
            "1 tablespoon fish sauce",
            "\u00bd cup chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour 1 tablespoon peanut oil into a large skillet over medium heat. Add onion, garlic, ginger, and hot pepper flakes. Stir and cook until onions are tender, about 5 minutes. Add remaining oil and green curry paste. Stir and cook about 1 more minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add celery and red bell pepper to the skillet with the onion mixture. Continue cooking and stirring until tender-crisp, 3 to 5 minutes. Pour in coconut milk. Cook over medium heat, stirring occasionally, until reduced by 75%, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add shrimp to the skillet with the curry and cook until they turn pink, about 2 minutes. Season with fish sauce and cilantro.\n"
            }
        ],
