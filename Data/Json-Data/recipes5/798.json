    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272877/grilled-eggplant-and-garlic-pasta/",
        "name": "Grilled Eggplant and Garlic Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grilled Eggplant and Garlic Pasta"
        },
        "datePublished": "2019-04-18T03:35:20.000Z",
        "description": "Try this recipe for grilled eggplant and garlic pasta anytime, using either an indoor or outdoor grill.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package spaghetti",
            "\u2153 cup olive oil",
            "\u00bd teaspoon garlic powder",
            "1 large eggplant, sliced",
            "1 tablespoon olive oil",
            "\u2153 red onion, sliced",
            "2 cloves garlic, minced, or more to taste",
            "1 (28 ounce) jar spaghetti sauce",
            "1 (14.5 ounce) can diced tomatoes, drained",
            "1 (6 ounce) can sliced black olives, drained",
            "4 ounces fresh mozzarella cheese, sliced, or to taste",
            "2 tablespoons shredded fresh basil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally, until tender yet firm to the bite, about 12 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1/3 cup olive oil and garlic powder in a small bowl. Brush both sides of eggplant slices with oil mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill eggplant slices on the preheated grill until soft and lightly browned, flipping halfway through, 8 to 10 minutes total. Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm 1 tablespoon olive oil in a skillet over medium heat. Add onion and cook until soft, about 5 minutes. Add minced garlic and cook until aromatic, 1 to 2 minutes more. Add spaghetti sauce and diced tomatoes; mix well. Add black olives and mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut cooled grilled eggplant slices into fourths and add to the sauce in the skillet. Mix until coated and warmed through, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sliced fresh mozzarella cheese just minutes before serving to allow cheese to melt. Serve sauce on spaghetti noodles and sprinkle with fresh basil.\n"
            }
        ],
