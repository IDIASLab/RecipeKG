    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78181/fondue-with-tequila/",
        "name": "Fondue with Tequila",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fondue with Tequila"
        },
        "datePublished": "2019-07-10T06:54:27.000Z",
        "description": "Something different but delicious. A fondue with avocado, cream, Mexican cotija cheese and a hint of tequila. Serve with shrimp or bread for dipping.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "6 tablespoons butter",
            "1 clove garlic, minced",
            "2 small onions, chopped",
            "2 tablespoons all-purpose flour",
            "1 avocado, peeled and pitted",
            "\u00bd cup cream",
            "1 cup milk",
            "3 tablespoons lemon juice",
            "2 tablespoons tequila",
            "\u00bd cup crumbled cotija cheese",
            "1 pinch salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a skillet over medium heat. Add the onions, and cook until tender, stirring occasionally. Add the garlic, and cook for about a minute. Whisk in the flour with a fork so no lumps form. Cook and stir with the fork until browned. Remove from heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the avocado into a blender or food processor, and pour in the milk, cream, lemon juice, salt and pepper. Puree until smooth. Pour into a heat proof bowl or casserole dish, and stir in the onion and garlic mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the oven for 20 minutes, or until thickened. Stir in the tequila and cheese, and return to the oven for about 5 minutes to melt the cheese. Stir and serve as a fondue.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Hot Cheese Dip Recipes"
        ],
