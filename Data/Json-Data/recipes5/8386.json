        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274994/blt-pizza-appetizer/",
        "name": "BLT Pizza Appetizer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6951290.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-31T00:33:02.000Z",
        "description": "This is a wonderful and easy appetizer pizza that is always a hit at parties. Store in the refrigerator until serving.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 pizza",
        "recipeIngredient": [
            "cooking spray",
            "2 (8 ounce) packages refrigerated crescent rolls",
            "1 cup mayonnaise",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc teaspoon garlic salt",
            "1 pound bacon strips, cooked and chopped",
            "2 large tomatoes, chopped and seeded",
            "3 green onions, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a cookie sheet with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unroll crescent rolls and pat onto the prepared pan. Let stand for 5 minutes; poke in several places using a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, about 10 minutes. Remove from the oven and let cool, at least 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat mayonnaise, cream cheese, and garlic salt together using an electric mixer in a mixing bowl. Spread onto cooled crescent rolls. Sprinkle bacon on top of cheese spread. Sprinkle tomatoes on top of bacon, followed by green onions.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry"
        ],
