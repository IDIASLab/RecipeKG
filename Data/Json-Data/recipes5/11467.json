        "mainEntityOfPage": "https://www.allrecipes.com/recipe/55091/savory-toasted-pumpkin-seeds/",
        "name": "Savory Toasted Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F490358.jpg",
