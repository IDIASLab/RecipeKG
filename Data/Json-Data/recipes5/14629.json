    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274404/chipotle-chili-dogs/",
        "name": "Chipotle Chili Dogs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6845156.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-02-01T07:52:22.000Z",
        "description": "Put some fireworks on your hot dogs with chipotle chili sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 chili dogs",
        "recipeIngredient": [
            "1 pound ground chuck",
            "1 white onion, diced",
            "2 chipotle chili peppers in adobo sauce, minced",
            "2 cloves garlic, minced",
            "3 (8 ounce) cans tomato sauce",
            "1 teaspoon chili powder",
            "12 hot dogs",
            "12 hot dog buns",
            "1 tablespoon butter, or as needed",
            "shredded Cheddar cheese",
            "2 tablespoons diced onion, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine ground chuck, diced onion, minced chipotle chili peppers, and garlic in a saucepan. Cook over medium heat until beef is browned and crumbly, about 5 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return beef mixture to saucepan; stir in tomato sauce and chili powder. Bring to a boil, reduce heat, and simmer 15 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook hot dogs according to package directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter hot dog buns and toast in a toaster oven, about 2 minutes. Place cooked hot dogs in the prepared buns. Spoon chili over hot dogs. Top with Cheddar cheese and onions. Serve immediately.\n"
            }
        ],
