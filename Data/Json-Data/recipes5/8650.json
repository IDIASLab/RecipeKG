    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275123/healthy-lentil-and-portobello-soup-in-the-slow-cooker/",
        "name": "Healthy Lentil and Portobello Soup in the Slow Cooker",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Healthy Lentil and Portobello Soup in the Slow Cooker"
        },
        "datePublished": "2019-08-04T20:56:51.000Z",
        "description": "This is a healthy, easy soup for the slow cooker! It's vegan to boot!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT5H10M",
        "totalTime": "P0DT5H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "4 medium carrots, chopped",
            "1 medium onion, chopped",
            "4 cloves garlic, minced",
            "2 cups dry lentils, rinsed and sorted",
            "2 (14.5 ounce) cans diced tomatoes",
            "2 (14.5 ounce) cans reduced-sodium vegetable broth",
            "4 portobello mushroom caps, cut into 1/2-inch slices",
            "2 teaspoons dried rosemary",
            "2 bay leaves",
            "salt and freshly ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat. Add carrots and onion. Cook and stir until onion is soft and translucent, about 5 minutes. Add garlic and cook until fragrant, about 30 seconds. Transfer mixture to a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer lentils, tomatoes, vegetable broth, portobello mushrooms, rosemary, and bay leaves on top of carrots and onions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook on Low until vegetables are tender, 5 to 6 hours.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas",
            "Lentil Soup Recipes"
        ],
