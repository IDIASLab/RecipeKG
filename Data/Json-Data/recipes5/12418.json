    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277550/cream-cheese-cookies/",
        "name": "Cream Cheese Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7516942.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-05T01:13:54.000Z",
        "description": "A light airy dessert that everyone in the family will enjoy and ask for more.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup butter, softened",
            "1 (18.25 ounce) box cake mix (any flavor)",
            "1 egg, at room temperature",
            "\u00bd teaspoon vanilla extract",
            "2 tablespoons powdered sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and butter in a large bowl; beat with an electric mixer until smooth and creamy. Add cake mix, egg, and vanilla extract and mix until batter is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop out 1-inch balls of dough and set them on the prepared baking sheet, 1 1/2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until slightly golden brown on top, 13 to 15 minutes. Leave on the baking sheet for 1 to 3 minutes, then transfer to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dust with powdered sugar if you like. Store cookies in an airtight container. They will keep for 3 to 5 days.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
