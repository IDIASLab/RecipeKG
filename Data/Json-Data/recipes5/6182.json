        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238707/sesame-seed-oil-hummus/",
        "name": "Sesame Seed Oil Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1315838.jpg",
