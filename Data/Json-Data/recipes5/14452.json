    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255018/sweet-potato-risotto/",
        "name": "Sweet Potato Risotto",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3850284.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-08-11T07:24:13.000Z",
        "description": "This savory, traditional Italian dish gets a delicious makeover with unsweetened coconut milk, Bruce's&reg; Yams Cut Sweet Potatoes, and zesty lime.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "5 cups chicken broth",
            "3 tablespoons olive oil",
            "\u00bd cup chopped white onion",
            "2 cups Arborio (short grain) rice",
            "\u00bc cup unsweetened coconut milk",
            "1 (15 ounce) can Bruce's\u00ae Yams Cut Sweet Potatoes in Syrup, drained, syrup reserved",
            "1 tablespoon apple cider vinegar",
            "2 tablespoons minced parsley",
            "\u00bd teaspoon sea salt, or more to taste",
            "\u00bd teaspoon ground white pepper",
            "2 tablespoons butter",
            "1 lime, zested"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring chicken stock to a simmer in a saucepan and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place oil in a separate (large) saucepan. Add onion and saute for 2 minutes until fragrant.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in rice and stir so that all grains of rice are coated. Add broth (1 cup at a time) and stir until liquid has been absorbed. After all broth has been absorbed, stir in coconut milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain the Bruce's&reg; Yams Cut Sweet Potatoes, reserving 1/4 cup syrup. Add syrup to rice and cook until rice starts to become light and creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in sweet potatoes (chunks will break down), cider vinegar, parsley, salt, and pepper. When incorporated, remove from heat and stir in butter (optional).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to a serving dish and sprinkle with freshly grated lime zest.\n"
            }
        ],
