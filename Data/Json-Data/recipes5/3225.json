        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24586/salmon-spread-ii/",
        "name": "Salmon Spread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F89491.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-07-10T06:52:37.000Z",
        "description": "Poached salmon elevates a simple cream cheese spread to new heights! Serve with crackers.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "1 pound salmon fillets",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup sour cream",
            "2 finely chopped green onions",
            "\u00bd teaspoon salt",
            "2 drops hot pepper sauce",
            "2 tablespoons fresh lemon juice",
            "1 tablespoon Worcestershire sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan of simmering water, poach the salmon filets 10 minutes, or until flaky and tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together cream cheese, sour cream, green onions, salt, hot pepper sauce, lemon juice and Worcestershire sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Flake salmon into the mixture. Cover and refrigerate 8 hours, or overnight, before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Cream Cheese Spreads Recipes"
        ],
