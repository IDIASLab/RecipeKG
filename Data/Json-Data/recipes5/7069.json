        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258401/easy-artichoke-crostini/",
        "name": "Easy Artichoke Crostini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4548044.jpg",
            "width": 2304,
            "height": 4096
        },
        "datePublished": "2019-07-10T07:10:56.000Z",
        "description": "A fantastic artichoke spread for crostini or baguettes that you can make in advance; it can be covered and kept at room temperature for up to 3 hours. It's always a hit at dinner parties!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H13M",
        "recipeYield": "8 crostini",
        "recipeIngredient": [
            "4 artichoke hearts in water, drained",
            "2 tablespoons olive oil",
            "1 teaspoon lemon juice, or more to taste",
            "salt and ground black pepper to taste",
            "1 loaf French baguette",
            "1 large clove garlic, peeled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place artichoke hearts in a bowl; mash with a fork. Stir in olive oil, lemon juice, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut baguette into 8 slices, about 1/2-inch thick; place slices on baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil both sides until toasted, 3 to 5 minutes. Slice garlic clove in half; rub on one side of each slice. Top bread slices with artichoke spread.\n"
            }
        ],
