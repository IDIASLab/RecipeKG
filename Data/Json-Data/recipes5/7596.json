        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269008/roasted-artichoke-greek-salad/",
        "name": "Roasted Artichoke Greek Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9413671.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T07:12:55.000Z",
        "description": "A roasted artichoke Greek salad.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup artichoke hearts, drained and quartered",
            "1 teaspoon olive oil",
            "sea salt to taste",
            "3 tablespoons olive oil",
            "2 tablespoons freshly squeezed lemon juice",
            "1 tablespoon chopped fresh oregano",
            "6 cups torn romaine lettuce",
            "\u00bd cup crumbled feta cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place artichokes on a baking sheet. Drizzle with 1 teaspoon olive oil and sprinkle with sea salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven on the top rack until golden, about 25 minutes. Remove from the oven and let cool at least 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While artichokes cool, whisk 3 tablespoons olive oil, lemon juice, oregano, and sea salt to taste together in a large bowl for dressing. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place romaine lettuce in a serving bowl. Add artichokes and feta cheese. Drizzle with dressing and toss to combine.\n"
            }
        ],
