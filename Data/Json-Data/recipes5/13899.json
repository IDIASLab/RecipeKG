    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254440/irish-stuffed-baked-potato/",
        "name": "Irish Stuffed Baked Potato",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3738078.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-01-30T05:21:03.000Z",
        "description": "A marriage of Irish colcannon and potato for a delicious version of the baked potato. It's a meal in itself or a side with meat.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 large baking potatoes",
            "1\u2009\u00bd cups finely chopped cabbage",
            "\u00bd cup finely chopped onion",
            "4 slices bacon, chopped",
            "\u00bd cup milk",
            "\u00bc cup butter",
            "\u00bc teaspoon ground black pepper",
            "salt to taste",
            "\u00be cup shredded Cheddar cheese, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scrub potatoes, pierce several times with a fork, and place on microwave-safe plate. Cook potatoes on High in microwave 5 minutes; turn over and cook until soft, about 5 minutes more. Scoop potato flesh into a bowl; reserve potato skins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir cabbage, onion, and bacon in a skillet over medium heat until vegetables are soft, about 10 minutes. Stir milk, butter, pepper, and salt into cabbage mixture; stir in potato flesh and 1/2 cup of Cheddar cheese. Cook and stir cabbage mixture, breaking up potato and mashing lightly, until milk is absorbed, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pan from heat; pile potato mixture into potato skins and place stuffed potatoes in a baking dish. Sprinkle potatoes with remaining Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is bubbly and potatoes are heated through, about 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Twice Baked Potato Recipes"
