        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32161/reuben-sandwich/",
        "name": "Reuben Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2839.jpg",
