        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267399/easy-green-gazpacho/",
        "name": "Easy Green Gazpacho",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5650789.jpg",
            "width": 2736,
            "height": 2736
        },
        "datePublished": "2019-04-04T04:13:38.000Z",
        "description": "We call this version green gazpacho cause it has a lot of green ingredients - cucumbers, green bell pepper, and avocado. My favorite lunch on a hot day.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cucumbers, peeled and chopped",
            "1 green bell pepper, chopped",
            "1 stalk 2 stalks celery, chopped",
            "1 avocado",
            "\u00bd onion, chopped",
            "1 clove garlic, chopped",
            "2 sprigs fresh cilantro",
            "2 tablespoons water, or more to taste",
            "1 tablespoon cold-pressed extra-virgin olive oil",
            "8 cherry tomatoes, quartered"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cucumber, bell pepper, and celery in a blender; blend until smooth. Add avocado and blend until smooth. Add onion, garlic, and cilantro and blend. With the blender running, add cold water until desired consistency is reached.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup into a container with a lid and refrigerate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in olive oil and garnish with cherry tomatoes before serving.\n"
            }
        ],
