        "name": "Cool-as-a-Cucumber Avocado Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2282014.jpg",
            "width": 2448,
            "height": 3264
