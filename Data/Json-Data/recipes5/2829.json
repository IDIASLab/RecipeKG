        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12179/cherry-cheesecake-pie/",
        "name": "Cherry Cheesecake Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F447016.jpg",
