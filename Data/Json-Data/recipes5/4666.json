    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/189313/taramousalata-greek-caviar-spread/",
        "name": "Taramousalata (Greek Caviar Spread)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Taramousalata (Greek Caviar Spread)"
        },
        "datePublished": "2019-07-10T07:01:39.000Z",
        "description": "Yummy Greek caviar spread that will impress your guests and leftovers freeze well! Serve chilled with pita or crunchy bread. Leftovers can be frozen and thawed in the refrigerator when you have a craving again.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "3 1/2 cups",
        "recipeIngredient": [
            "1 potato",
            "8 slices white sandwich bread",
            "2 cups milk",
            "\u00bd cup carp roe",
            "1 small onion, chopped",
            "\u00bc cup fresh lemon juice",
            "\u00bd cup olive oil, or more if needed",
            "2 Greek olives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 450 degrees F (230 degrees C). Prick the potato in several places with a fork and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the potato in the preheated oven easily pierced with a fork, 50 minutes to 1 hour. Cool completely in refrigerator. Peel and chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the bread slices in a shallow dish; pour the milk over the bread to cover completely. Allow to soak a few minutes before squeezing as much moisture as possible from the bread, discarding the milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend the bread, potato, carp roe, onion, lemon juice, and olive oil together in a food processor until fluffy, about 1 minute. Add more olive oil if needed to reach a desired consistency; chill. Garnish with the Greek olives to serve.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes"
        ],
