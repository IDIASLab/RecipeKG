    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9643/pineapple-coconut-bars/",
        "name": "Pineapple Coconut Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F18768.jpg",
            "width": 2856,
            "height": 1904
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Pineapple and coconut make this a special tropical treat. Serve them at your next luau",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "1 cup packed brown sugar",
            "2 eggs",
            "1 (8 ounce) can crushed pineapple",
            "\u00bd teaspoon rum",
            "\u00be cup all-purpose flour",
            "\u00be teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00be cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 9 inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl, cream shortening and brown sugar.  Add eggs, pineapple and rum, mix well. In separate bowl, mix together the flour, baking powder and salt.  Slowly add to egg mixture and beat until well blended. Blend in coconut and spread batter evenly in bottom of pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 25 to 30 minutes.  Let cool on wire rack before cutting into bars.\n"
            }
        ],
