    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276897/asparagus-pasta-bake/",
        "name": "Asparagus Pasta Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7146938.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-16T02:57:14.000Z",
        "description": "This asparagus pasta bake is a slightly fancier version of the baked ziti classic.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ziti pasta",
            "1 (15 ounce) container ricotta cheese",
            "1 (28 ounce) jar tomato sauce, divided",
            "2 cups finely shredded mozzarella cheese, divided",
            "\u2153 cup chopped fresh parsley",
            "\u00bd pound asparagus, trimmed",
            "3 tablespoons olive oil",
            "1 medium onion, chopped",
            "1 large red bell pepper, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil; cook ziti at a boil until tender yet firm to the bite, about 8 minutes. Drain and transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add ricotta cheese, 1 1/2 cups tomato sauce, 1 cup mozzarella cheese, and parsley to drained pasta. Mix and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop trimmed asparagus into 1/4-inch slices, separating stems and tips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a pan over medium-high heat. Add onion and saute for 5 minutes. Stir in asparagus stems; cook for another 3 minutes. Add bell pepper and cook, 2 to 3 minutes. Add asparagus tips and cook, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1 cup tomato sauce on the bottom of a 9x13-inch baking dish. Add 1/2 of the pasta mixture and smooth into an even layer. Sprinkle 1/2 of the vegetables on top. Top with 1/2 cup mozzarella cheese. Add remaining pasta in an even layer, followed by remaining vegetables. Cover with remaining tomato sauce. Sprinkle with remaining mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, on the middle rack of the preheated oven for 30 minutes. Move to the top rack and continue to bake until cheese is melted and bubbly, about 5 minutes more. Let stand for 5 to 10 minutes before serving.\n"
            }
        ],
