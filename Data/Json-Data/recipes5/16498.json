        "name": "Butter Balls I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7793208.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Rich , buttery flavored cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "\u00be cup butter",
            "1 cup packed brown sugar",
            "\u00be teaspoon baking powder",
            "1 egg",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F and grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add eggs, sugar and vanilla.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sifted dry ingredients.  Stir until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut dough into pieces about 2 inches by 2 inches.   Dip in white sugar.  Roll dough into palm of hand until round.  Dip in brown sugar and place on greased cookie sheet about 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F for about 10 minutes or until light brown.\n"
            }
        ],
