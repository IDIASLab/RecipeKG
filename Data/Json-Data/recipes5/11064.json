        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25892/quick-and-easy-spinach-casserole/",
        "name": "Quick and Easy Spinach Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1126466.jpg",
