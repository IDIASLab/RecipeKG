        "name": "Shrimp Scampi Over Rice from Knorr&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3185339.jpg",
            "width": 960,
            "height": 960
