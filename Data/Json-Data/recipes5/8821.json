    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275199/wassail/",
        "name": "Wassail",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Wassail"
        },
        "datePublished": "2019-08-12T21:17:35.000Z",
        "description": "This non-alcoholic apple cider punch simmered with honey, pineapple juice, and lemon juice and flavored with clove-spiced oranges is perfect for the holidays or any fall gathering.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "18 cups",
        "recipeIngredient": [
            "3 oranges",
            "3 tablespoons whole cloves, or as needed",
            "water as needed",
            "3 quarts apple cider",
            "2 (3 inch) cinnamon sticks",
            "5 cups pineapple juice",
            "\u00bd cup honey",
            "\u2153 cup lemon juice",
            "2 teaspoons lemon zest",
            "\u00bd teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stud oranges with whole cloves and place in a baking pan. Add water to cover bottom of the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until oranges have softened, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place apple cider and cinnamon sticks into a large saucepan; bring to a boil. Simmer, covered, for 5 minutes. Remove lid and add pineapple juice, honey, lemon juice, lemon zest, and ground nutmeg. Return to a simmer and cook for 5 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a punch bowl and float spiced oranges on top.\n"
            }
        ],
