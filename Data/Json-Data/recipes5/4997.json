        "mainEntityOfPage": "https://www.allrecipes.com/recipe/217910/broiled-sesame-cod/",
        "name": "Broiled Sesame Cod",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F836611.jpg",
            "width": 1600,
            "height": 1200
