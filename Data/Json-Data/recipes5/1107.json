    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270075/kifta-and-potatoes-with-tahini-sauce/",
        "name": "Kifta and Potatoes with Tahini Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1962852.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2019-04-30T08:34:48.000Z",
        "description": "One of my Middle Eastern husband's favorite meals. Serve with pita bread to dip in the tahini sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound ground lamb",
            "\u00bd large onion, minced",
            "\u00bd cup minced fresh parsley",
            "1\u2009\u00bc teaspoons salt",
            "\u00be teaspoon ground allspice",
            "\u00be teaspoon ground black pepper",
            "2 tablespoons vegetable oil, or as needed",
            "5 potatoes, peeled and sliced into rounds",
            "salt to taste",
            "2 cloves garlic, crushed and peeled",
            "2 lemons, juiced",
            "1\u2009\u00bd cups water",
            "\u00bd cup tahini",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "1 tablespoon olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine lamb, onion, parsley, salt, allspice, and pepper in a large bowl. Roll meat into 2 1/2-inch long pieces and arrange in a baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While meat is cooking, heat vegetable oil in a frying pan over medium heat. Add potatoes; fry until lightly golden, about 5 minutes. Set aside on paper towels to drain excess oil. Sprinkle with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove lamb from the oven; drain and discard any fat from meat. Leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine garlic, lemon juice, water, tahini, salt, and pepper in a food processor until smooth. Set tahini sauce aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange potatoes on top of lamb in the baking dish and pour tahini sauce on top. Drizzle with olive oil. Return the baking pan to the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lamb is no longer pink and an instant-read thermometer inserted into the center reads at least 150 degrees F (65 degrees C), about 30 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Lamb",
            "Ground"
        ],
