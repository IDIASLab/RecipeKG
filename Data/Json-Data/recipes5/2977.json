        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16062/italian-restaurant-style-dressing/",
        "name": "Italian Restaurant-Style Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6252219.jpg",
