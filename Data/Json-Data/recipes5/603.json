        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272918/chicken-and-veggie-quinoa-bowls/",
        "name": "Chicken and Veggie Quinoa Bowls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6532089.jpg",
            "width": 0,
