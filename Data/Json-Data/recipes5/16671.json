    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10677/date-drop-cookies-ii/",
        "name": "Date Drop Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8262109.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-03-12T23:50:52.000Z",
        "description": "Date cookies topped with a  walnut piece.  A simple confectioners' sugar icing may be drizzled over the cooled cookies if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 2 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bc cup shortening",
            "\u00be cup packed brown sugar",
            "1 egg",
            "\u00bd cup sour cream",
            "2\u2009\u00bd cups dates, pitted and chopped",
            "1 cup walnut halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pre-heat oven to 400 degrees F (205 degrees C).   Lightly grease two baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, baking soda and salt and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together shortening and brown sugar.  Beat in the egg and the sour cream.  Gradually blend in flour mixture.  Fold in dates.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by teaspoonfuls 1-1/2 inches apart onto baking sheets.  Press a walnut half into the center of each cookie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8-10 minutes until lightly colored.  Cool cookies on wire racks.\n"
            }
        ],
