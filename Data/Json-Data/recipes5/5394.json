    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228776/artichoke-hummus/",
        "name": "Artichoke Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3606052.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2019-07-10T07:03:47.000Z",
        "description": "A fun twist on traditional hummus using artichokes and white beans.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "3 cloves garlic, chopped",
            "2 tablespoons chopped fresh basil",
            "2 tablespoons lemon juice",
            "2 (15 ounce) cans cannellini beans, drained (reserve juice)",
            "1 tablespoon olive oil",
            "1 tablespoon water",
            "1 tablespoon tahini",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cumin",
            "1 (15 ounce) can artichoke hearts, drained and quartered"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chopped garlic in the preheated oven until softened, 5 to 7 minutes, to reduce bitterness. Transfer baked garlic to a food processor and add basil and lemon juice; pulse several times to mince basil leaves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cannellini beans, olive oil, water, tahini, salt, and cumin in the food processor and process until the hummus is smooth. If hummus is too thick, process 1 tablespoon reserved liquid from beans into the mixture. Add artichoke hearts to the hummus and process until smooth.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Hummus Recipes"
        ],
