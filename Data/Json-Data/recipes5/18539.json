    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/81141/cranberry-nut-tarts/",
        "name": "Cranberry Nut Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1792870.jpg",
            "width": 3128,
            "height": 2346
        },
        "datePublished": "2020-03-05T04:59:09.000Z",
        "description": "A great cranberry twist on traditional nut tassies. These delightful fall tarts are baked in a mini muffin pan to stunning perfection. Perfect for parties and cookie trays.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "24 tarts",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "1 (3 ounce) package cream cheese, softened",
            "1 cup all-purpose flour",
            "1 egg",
            "\u00be cup packed brown sugar",
            "1 tablespoon butter, melted",
            "1 teaspoon vanilla extract",
            "3 tablespoons chopped walnuts",
            "3 tablespoons chopped fresh cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the butter and cream cheese. Mix in the flour until well blended. Cover and refrigerate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the dough chills, make the filling. In a medium bowl, whisk together the egg, brown sugar, melted butter and vanilla. Stir in the cranberries and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (160 degrees C). Roll the dough into 1 inch balls. Press the balls into the cups of mini muffin pans so that the dough covers the bottom and sides. Fill each one with a generous teaspoon of the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 to 30 minutes in the preheated oven, until the edges of the tarts are lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Tarts",
            "Fruit Tart Recipes"
        ],
