    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20170/pumpkin-bread-vi/",
        "name": "Pumpkin Bread VI",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F133124.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-06-17T19:27:40.000Z",
        "description": "A wonderfully hearty pumpkin bread with just a kiss of orange flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 - 9x5 inch loaf",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 cup whole wheat flour",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon baking soda",
            "2\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon ground allspice",
            "\u00bc teaspoon ground nutmeg",
            "\u2153 cup vegetable oil",
            "\u00be cup brown sugar",
            "2 eggs",
            "\u2153 cup orange juice",
            "1 cup pumpkin puree"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together flour, salt, baking soda, baking powder, allspice and nutmeg. In a large bowl, beat together vegetable oil, brown sugar, eggs, orange juice and pumpkin. Stir flour mixture into pumpkin mixture until just combined. Pour batter into prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 50 to 60 minutes, until a toothpick inserted into center of the loaf comes out clean.\n"
            }
        ],
