    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263633/kale-and-mushroom-vegan-quiche/",
        "name": "Kale and Mushroom Vegan \"Quiche\"",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1039419.jpg",
            "width": 1920,
            "height": 1920
        },
        "datePublished": "2019-07-10T07:11:50.000Z",
        "description": "A great vegan alternative to quiche. So much flavor, I could eat it every day. Great when served with a light salad.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 9-inch quiche",
        "recipeIngredient": [
            "1 (9 inch) vegan pie crust",
            "2 tablespoons olive oil",
            "2 cloves garlic, diced",
            "1 (8 ounce) package button mushrooms, diced",
            "4 cups chopped kale",
            "1 medium red onion, diced",
            "1 (12 ounce) package extra-firm tofu, diced",
            "3 tablespoons dry vegan egg replacer",
            "1 tablespoon Dijon mustard",
            "1 tablespoon nutritional yeast",
            "1 tablespoon lemon juice",
            "1 teaspoon onion powder",
            "1 teaspoon garlic powder",
            "\u00bd teaspoon ground turmeric",
            "\u00bd teaspoon salt, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Place pie crust in a 9-inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat. Saute garlic until slightly toasted, about 2 minutes. Add mushrooms, kale, and onion; saute until leaves are wilted and onion is translucent, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tofu, egg replacer, Dijon mustard, nutritional yeast, lemon juice, onion powder, garlic powder, turmeric, and salt in a food processor. Blend until smooth. Pour into a bowl and fold in kale mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into the pie crust and level the top with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until center is firm and top is golden, 30 to 40 minutes. Cool for 10 minutes before serving.\n"
            }
        ],
