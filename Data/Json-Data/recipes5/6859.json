        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255950/citrus-baked-fish/",
        "name": "Citrus Baked Fish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4529846.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-07-10T07:10:25.000Z",
        "description": "Salmon works best in this citrus-flavored dish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "4 (3 ounce) fillets salmon fillets",
            "\u00bc cup lemon juice",
            "\u00bc cup orange juice",
            "\u00bc cup lime juice",
            "2 teaspoons melted butter",
            "1 teaspoon dried parsley",
            "\u00bd teaspoon ground paprika",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a baking dish lightly with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place salmon fillets in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lemon juice, orange juice, lime juice, butter, parsley, paprika, salt, and pepper together until well blended. Drizzle over salmon in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake salmon in the preheated oven until easily flaked with a fork, 10 to 15 minutes. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C).\n"
            }
        ],
