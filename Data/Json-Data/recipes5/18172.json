    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/52301/sunflower-threes/",
        "name": "Sunflower Threes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sunflower Threes"
        },
        "datePublished": "2020-03-05T04:58:03.000Z",
        "description": "Basically an oatmeal raisin cookie.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "\u00be cup vegetable shortening",
            "\u2153 cup maple syrup",
            "\u2153 cup brown sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bc teaspoon ground cinnamon",
            "2 cups rolled oats",
            "1 cup whole wheat flour",
            "\u2153 cup all-purpose flour",
            "\u00be cup sunflower seeds",
            "\u00be cup golden raisins",
            "\u2153 cup finely diced dried pineapple",
            "\u2153 cup pineapple juice",
            "\u00be teaspoon baking soda",
            "\u00bc teaspoon baking powder",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat shortening, maple syrup, brown sugar, vanilla extract, and cinnamon together with an electric mixer in a large bowl until combined. Stir oats, whole-wheat flour, all-purpose flour, sunflower seeds, golden raisins, dried pineapple, pineapple juice, baking soda, baking powder, and salt into the shortening mixture to make a sticky dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pinch off about 1 1/2 tablespoon of dough per cookie and roll into balls about 1 1/4 inch across; arrange on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned, 12 to 15 minutes.\n"
            }
        ],
