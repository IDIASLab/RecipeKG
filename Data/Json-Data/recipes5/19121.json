    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149032/carrot-sweet-potato-mash/",
        "name": "Carrot-Sweet Potato Mash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F296158.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T05:05:10.000Z",
        "description": "This side dish reminds me of the fall and all of the flavors that I look forward to cooking with.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (16 ounce) package baby carrots",
            "1 sweet potato, peeled and cubed",
            "\u00bc cup butter",
            "\u00bd cup applesauce, or as needed",
            "\u00bc cup brown sugar",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place carrots and potatoes into a large pot and cover with salted water. Bring to a boil over high heat, then reduce heat to medium-low, cover, and simmer until tender, about 20 minutes. Drain and allow to steam dry for a minute or two.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt the butter in a small saucepan over medium heat, then stir in the applesauce and brown sugar until dissolved. When the potatoes are ready, transfer to a serving bowl and mash until smooth. Then fold in the applesauce mixture and raisins.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Mashed Potato Recipes"
        ],
