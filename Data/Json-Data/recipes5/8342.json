    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274902/grain-free-granola-with-pumpkin-spice/",
        "name": "Grain-Free Granola with Pumpkin Spice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5510403.jpg",
            "width": 1078,
            "height": 988
        },
        "datePublished": "2019-07-29T20:35:13.000Z",
        "description": "Enjoy this sweet, pumpkin spice-flavored granola that is grain-free and filled with delicious seeds and nuts.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup pumpkin seeds",
            "1 cup sliced almonds",
            "1 cup sunflower seeds",
            "1 cup chopped walnuts",
            "1 cup shredded coconut",
            "\u00bd cup raisins",
            "1 tablespoon pumpkin pie spice",
            "\u00bc teaspoon salt",
            "\u2153 cup coconut oil",
            "\u00bc cup raw honey",
            "1 tablespoon pure maple syrup",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Line a large baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pumpkin seeds, almonds, sunflower seeds, walnuts, coconut, raisins, pumpkin pie spice, and salt in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine coconut oil, honey, maple syrup, and vanilla extract in a saucepan over low heat. Stir until melted, about 5 minutes. Pour over pumpkin seed mixture and mix very well. Spread granola across parchment paper and flatten out evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, watching to prevent burning, about 25 minutes. Remove from oven and let cool before indulging!\n"
            }
        ],
