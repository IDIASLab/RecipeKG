    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275935/sweet-potato-bowl/",
        "name": "Sweet Potato Bowl",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7048463.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-09-14T02:26:20.000Z",
        "description": "Use that spiralizer for your next meal and create this EB Sweet Potato Bowl!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 teaspoons olive oil, divided",
            "1 cup ground sausage",
            "2 large Eggland's Best Eggs",
            "1 large sweet potato",
            "Salt and pepper",
            "\u00bd cup salsa",
            "1 avocado, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon of olive oil in a medium pan over medium-high heat and brown the ground sausage, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the sausage cooks, fill a large pot with a few inches of water and place over medium-high heat until it just starts to simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crack Eggland's Best eggs individually into a ramekin or cup. Create a gentle whirlpool in the simmering water. Slowly pour the eggs one by one into the water and leave to cook for 3 minutes. Remove with a slotted spoon and drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the eggs poach, cut the sweet potato into noodles using a spiralizer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the other teaspoon of olive oil in a large skillet over medium heat. Add in the sweet potato noodles and cook until they just begin to soften, about 5 to 7 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the cooked sweet potato noodles into a large bowl. Drain the ground sausage and add to the sweet potato noodles. Toss with the salsa.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the noodles and sausage between two bowls, top each with diced avocado and a poached egg.\n"
            }
        ],
