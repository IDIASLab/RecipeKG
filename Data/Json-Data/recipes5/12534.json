    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277544/cauliflower-cheese-soup/",
        "name": "Cauliflower Cheese Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7486400.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-12-24T16:28:09.000Z",
        "description": "I love a good broccoli cheese soup - and cauliflower cheese soup is delicious too!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 head cauliflower",
            "1 tablespoon olive oil, or more as needed",
            "4 medium (blank)s carrots, diced",
            "4 stalks celery, diced",
            "1 small yellow onion, diced",
            "2 cloves garlic, finely diced",
            "4 cups chicken broth",
            "\u00bc cup butter",
            "\u00bc cup all-purpose flour",
            "\u00bd teaspoon salt, or to taste",
            "1 pinch ground black pepper to taste",
            "3 cups milk, at room temperature",
            "12 ounces freshly shredded sharp Cheddar cheese, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cauliflower into the bowl of a food processor and blend until finely chopped.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saucepan over medium-high heat until shimmering. Add carrots, celery, and onion. Cook for 2 to 3 minutes. Add garlic and continue to cook for 5 to 6 minutes. Pour in broth and cauliflower; increase heat to high. Bring to a boil; reduce heat to a simmer and cook until vegetables are tender, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt butter in a medium skillet over medium heat. Add flour and cook for 2 minutes. Season with salt and pepper. Whisk milk in slowly and bring to a boil. Remove from heat and add Cheddar cheese slowly until completely combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour broth from cauliflower mixture into an electric blender and puree to your desired consistency. Return to the saucepan. Pour cheese sauce into soup and stir well to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve in bowls and garnish with additional Cheddar cheese, if desired.\n"
            }
        ],
