        "mainEntityOfPage": "https://www.allrecipes.com/recipe/36992/california-style-israeli-salad/",
        "name": "California Style Israeli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559944.jpg",
