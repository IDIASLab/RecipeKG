    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277577/raspberry-chocolate-chip-cookies/",
        "name": "Raspberry Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F864735.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-07-20T18:07:45.000Z",
        "description": "I decided to get a little creative with a new flavor on cake mix cookies. The combination of raspberry and chocolate are delicious together. The cookie is very light and cake-like.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "3 1/2 dozen",
        "recipeIngredient": [
            "1 (15.25 ounce) package white cake mix",
            "4  large egg whites egg whites",
            "\u2153 cup canola oil",
            "2\u2009\u00bd tablespoons raspberry puree",
            "1 teaspoon raspberry extract",
            "2 drops red food coloring, or as desired",
            "\u00bc cup chocolate chips, or to taste",
            "1 cup powdered sugar",
            "1 tablespoon milk",
            "2 teaspoons light corn syrup",
            "\u00bd teaspoon raspberry extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cake mix, egg whites, oil, raspberry puree, raspberry extract, and red food coloring together in a bowl until well blended. Stir in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dough by tablespoonfuls onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set, about 8 minutes. Let cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine powdered sugar, milk, corn syrup, and raspberry extract together for glaze in a bowl until smooth. If too thick, add milk sparingly until desired consistency is reached. Glaze cooled cookies as desired.\n"
            }
        ],
