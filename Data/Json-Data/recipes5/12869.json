    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/150138/carrot-cauliflower-salad/",
        "name": "Carrot-Cauliflower Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Carrot-Cauliflower Salad"
        },
        "datePublished": "2019-12-28T03:50:08.000Z",
        "description": "Enjoy the bold taste of this easy-to-make cauliflower-carrot salad. It's great for picnics.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large head cauliflower, cut into florets",
            "1 cup mayonnaise",
            "1 (.7 ounce) package dry Italian salad dressing mix",
            "\u00bd cup grated Parmesan cheese",
            "1 large carrot, peeled and shredded"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the cauliflower in a steamer basket set in a saucepan over 1 inch of boiling water. Steam 5 minutes, until tender but still firm. Drain, cool slightly, and place into a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the mayonnaise with the Italian salad dressing and Parmesan cheese in a bowl. Add the carrots to the bowl with the cauliflower, and toss with the mayonnaise mixture until evenly coated. Cover, and refrigerate at least 2 hours or overnight to allow flavors to blend.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Carrot Salad Recipes"
        ],
