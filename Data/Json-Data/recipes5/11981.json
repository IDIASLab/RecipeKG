    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265810/spaghetti-with-zucchini-and-almonds/",
        "name": "Spaghetti with Zucchini and Almonds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F412219.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-11-15T05:15:49.000Z",
        "description": "A fantastic light and healthier pasta dish with zucchini that works very well as a balanced vegetarian main course. Serve with freshly grated Parmesan cheese.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "3 tablespoons blanched almonds",
            "\u2153 pound dried spaghetti",
            "2 tablespoons extra-virgin olive oil",
            "2 zucchini, grated",
            "1 clove garlic, crushed",
            "salt and ground black pepper to taste",
            "2 teaspoons butter",
            "2 tablespoons chopped fresh parsley",
            "1 lemon, zested",
            "1 squeeze lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a small pan over medium heat. Add almonds and toast, about 2 minutes. Let cool slightly and roughly chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally, until tender yet firm to the bite, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While pasta cooks, add olive oil to a saucepan over medium heat. Add zucchini, garlic, salt, and pepper and fry until tender, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reserve 1/4 cup of the pasta cooking water and drain. Add cooked spaghetti to the saucepan with the zucchini. Add butter, parsley, lemon zest, lemon juice, and toasted almonds and stir to coat.\n"
            }
        ],
