        "mainEntityOfPage": "https://www.allrecipes.com/recipe/68358/blue-cheese-with-capers-and-red-onion/",
        "name": "Blue Cheese with Capers and Red Onion",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6234505.jpg",
            "width": 1936,
            "height": 1945
