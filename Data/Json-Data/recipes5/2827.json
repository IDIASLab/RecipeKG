    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12113/french-peach-pie/",
        "name": "French Peach Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6681283.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1998-04-01T03:19:30.000Z",
        "description": "This is an easy impressive pie. It is also very good with fresh fruit.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell, baked",
            "1 (15 ounce) can sliced peaches, juice reserved",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "1 cup milk",
            "1 cup sour cream",
            "\u00bc teaspoon almond extract",
            "1 tablespoon cornstarch",
            "1 teaspoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Drain peaches, reserving 2/3 cup of syrup. Stir together pudding mix, milk, sour cream, and almond extract for 2 minutes, until very smooth. Pour filling into pie shell. Chill for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange peach slices in a nice pattern over custard in pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, mix together reserved peach syrup and corn starch.  Bring to a boil, and cook for 2 minutes.  Remove from heat, and stir in lemon juice.  Pour glaze over peaches.  Chill until set.  Serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes",
            "Pudding Pie Recipes"
        ],
