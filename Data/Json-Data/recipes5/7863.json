    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274175/zaatar-chicken-salad-with-lemon-tahini-dressing/",
        "name": "Za'atar Chicken Salad with Lemon-Tahini Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6720741.jpg",
            "width": 2219,
            "height": 2219
        },
        "datePublished": "2019-07-10T07:13:50.000Z",
        "description": "Experiment with Middle Eastern flavors in this nutritious dinner salad. Use purchased za'atar seasoning blend or use your own.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 tablespoons lemon juice",
            "2 tablespoons extra-virgin olive oil",
            "2 tablespoons za'atar seasoning",
            "1 clove garlic, minced",
            "1 teaspoon kosher salt",
            "\u00bd teaspoon freshly ground black pepper",
            "\u00bd pound chicken tenders",
            "6 cups torn, bite-sized romaine lettuce",
            "1 cup cherry tomatoes, halved",
            "1 green bell pepper, diced",
            "1 cucumber, peeled and diced",
            "\u00bd cup shredded carrot",
            "1 lemon, zested and juiced",
            "4 tablespoons tahini",
            "4 tablespoons water, or more as needed",
            "2 cloves garlic",
            "1 dried date, minced",
            "salt and ground black pepper to taste",
            "1 tablespoon chopped fresh chives",
            "1 tablespoon chopped fresh mint"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix 2 tablespoons lemon juice, olive oil, za'atar, garlic, salt, and pepper together in a gallon-sized plastic zip-top bag. Add chicken tenders and refrigerate, turning occasionally, 30 minutes to 12 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken tenders on the grate, reduce heat to medium, and cover. Grill until an instant-read thermometer inserted into the thickest part of a tender registers 165 degrees F (74 degrees C), 4 to 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from grill and let rest. Prepare the salad in the meantime; place romaine pieces in a large bowl and add tomatoes, bell pepper, cucumber, and carrot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine lemon zest and juice, tahini, water, garlic, and date in the bowl of a mini food processor or a blender. Pulse several times, adding water by tablespoons if necessary, until desired consistency is achieved. Season with salt and pepper. Pour dressing into the salad bowl and toss.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide salad between plates. Slice each chicken tender crosswise into 1/4-inch slices and place over the salads. Garnish with chives and mint.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Green Salad Recipes",
            "Romaine Lettuce Salad Recipes"
        ],
