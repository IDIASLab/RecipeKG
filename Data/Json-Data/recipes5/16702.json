        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups walnuts",
            "1 cup dates, pitted and chopped",
            "2 cups flaked coconut",
            "1 cup packed brown sugar",
            "2 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grind nuts and dates.  Mix 1 1/2 cup coconut and remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop up in tablespoons, shape into balls and roll in remaining coconut.  Bake on ungreased cookie sheet until coconut browns, 10 to 15 minutes, at 350 degrees F (175 degrees C) .\n"
            }
        ],
