    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15716/pineapple-cranberry-salad/",
        "name": "Pineapple Cranberry Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1095930.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-11-22T18:39:34.000Z",
        "description": "This is the best cranberry gelatin salad I've ever tasted. It has been served with our Thanksgiving dinner for many years.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 to 10 servings",
        "recipeIngredient": [
            "2 (3 ounce) packages raspberry flavored Jell-O\u00ae mix",
            "1 (16 ounce) can whole cranberry sauce",
            "\u00bd teaspoon orange zest",
            "\u2153 cup orange juice",
            "2 tablespoons lemon juice",
            "1 (20 ounce) can pineapple tidbits, drained",
            "\u00be cup sliced celery"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Drain and reserve syrup from pineapple, add water to equal 1 cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat, bring the pineapple juice to a boil, add gelatin and stir until dissolved. Remove from heat, add cranberry sauce, orange peel, orange juice and lemon juice. Blend together and cool until slightly thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in pineapple and celery, turn into mold or 8 inch square pan. Chill until firm.\n"
            }
        ],
        "recipeCategory": [
