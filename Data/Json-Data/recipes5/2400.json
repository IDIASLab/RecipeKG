    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273754/honey-dijon-chicken-thighs/",
        "name": "Honey-Dijon Chicken Thighs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4485351.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2019-06-20T10:50:10.000Z",
        "description": "I have a super yummy, simple weeknight meal to share with you! My honey-Dijon chicken thighs are packed with flavor.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "4 chicken thighs",
        "recipeIngredient": [
            "3 tablespoons honey",
            "2 tablespoons olive oil",
            "1 tablespoon Dijon mustard",
            "\u00bd teaspoon dried basil",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon salt",
            "ground black pepper to taste",
            "4 chicken thighs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk honey, olive oil, Dijon mustard, basil, oregano, salt, and pepper together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken thighs into a plastic zip-top bag; pour marinade over. Seal the bag and massage marinade into thighs until completely covered. Refrigerate for at least 1 hour and up to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the chicken thighs and excess marinade into a baking dish and cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Remove from the oven, uncover, and flip chicken thighs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and continue to bake until thighs are no longer pink in the center and an instant-read thermometer inserted near the bones reads 165 degrees F (74 degrees C), about 20 minutes more. Remove from the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil thighs in the preheated oven until crispy and golden, 2 to 5 minutes. Remove from the oven and serve.\n"
            }
        ],
