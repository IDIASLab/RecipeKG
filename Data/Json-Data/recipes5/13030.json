        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215083/creamy-chicken-spread/",
        "name": "Creamy Chicken Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4483117.jpg",
