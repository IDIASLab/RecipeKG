    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282738/lemon-pasta-with-salmon/",
        "name": "Lemon Pasta with Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9343101.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-23T20:06:06.000Z",
        "description": "This lemon pasta is a wonderful family recipe and a great way to use up leftover salmon. Simple and delicious. Serve with a green salad and fresh Italian bread.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package penne pasta",
            "3 tablespoons salted butter, divided",
            "3 tablespoons olive oil, divided",
            "1 large yellow onion, chopped",
            "3 cloves garlic, chopped, or more to taste",
            "1 pound cooked salmon, flaked",
            "1 large lemon, zested and juiced",
            "\u00bd cup chopped fresh Italian parsley",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add penne and cook, stirring occasionally, until tender yet firm to the bite, about 11 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat 2 tablespoons butter and 2 tablespoons oil in a large skillet over medium heat. Add onion to melted butter mixture and saute until translucent, about 5 minutes. Add garlic; cook and stir until fragrant, about 2 minutes. Add salmon and cook until heated through, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain pasta and add to the skillet. Stir in parsley and lemon zest. Add lemon juice and season with salt and pepper. Add remaining oil and butter and mix until glossy.\n"
            }
        ],
