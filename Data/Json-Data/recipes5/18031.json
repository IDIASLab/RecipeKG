        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/45614/caribbean-bbq-sauce/",
        "name": "Caribbean BBQ Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4571850.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-03-05T04:57:44.000Z",
        "description": "This is the most WONDERFUL BBQ sauce recipe I know. It is different (sort of), and the black rum cannot be substituted by another type of rum. It ruins the taste.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "2 1/2 cups",
        "recipeIngredient": [
            "1 teaspoon vegetable oil",
            "3 slices bacon, diced",
            "1 medium onion, finely chopped",
            "1 cup tomato sauce",
            "\u00bd cup black rum",
            "1 lemon, juiced",
            "\u2153 cup brown sugar",
            "1 dash chili sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place vegetable oil, bacon, and onion in a medium skillet over medium high heat. Cook until bacon is evenly brown and onion is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir tomato sauce and rum into the skillet with bacon and onion, and reduce heat. Simmer about 2 minutes. Mix in lemon juice, brown sugar, and chili sauce. Continue to simmer about 8 minutes.\n"
            }
        ],
