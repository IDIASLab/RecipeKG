        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash bananas in a medium bowl. Stir in oats, chocolate chips, and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by rounded teaspoonfuls onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 to 20 minutes.\n"
            }
