    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272857/dill-pickle-macaroni-and-cheese/",
        "name": "Dill Pickle Macaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6549107.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "2019-04-20T07:07:31.000Z",
        "description": "Dill pickle relish adds a bit of crunch and a ton of flavor to traditional, creamy baked macaroni and cheese.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 1 1/2-quart casserole dish",
        "recipeIngredient": [
            "cooking spray",
            "1\u2009\u00bd cups elbow macaroni",
            "3 tablespoons butter",
            "3 tablespoons all-purpose flour",
            "2 cups milk",
            "1 cup shredded extra-sharp Cheddar cheese",
            "1 cup shredded Monterey Jack cheese",
            "\u2153 cup dill pickle relish, well drained",
            "\u00bc cup minced onion",
            "1 teaspoon chile-garlic sauce (such as Sriracha\u00ae)",
            "salt and ground black pepper to taste",
            "1 slice bread",
            "1 teaspoon butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a 1 1/2-quart casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 7 minutes, making sure not to overcook. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the macaroni is cooking, melt 3 tablespoons butter over medium-low heat in a large saucepan. Blend in flour and cook, stirring constantly, until mixture becomes paste-like and light golden brown, about 5 minutes. Gradually whisk in milk and bring to a simmer over medium heat. Cook and stir until thickened, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in Cheddar cheese and Monterey Jack cheese; stir until melted. Add dill pickle relish, onion, Sriracha sauce, salt, and pepper to cheese mixture. Stir in cooked macaroni. Pour mixture into the prepared casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter both sides of bread and tear into large pieces. Blend in a food processor or electric blender until you have crumbs. Spread on top of casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling and browned on top, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
            "Macaroni and Cheese Recipes",
            "Baked Macaroni and Cheese Recipes"
        ],
