    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/132011/pumpkin-buns/",
        "name": "Pumpkin Buns",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2782788.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T05:04:05.000Z",
        "description": "This is a tea-time snack full of pumpkin flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "12 buns",
        "recipeIngredient": [
            "1 cup grated fresh pumpkin",
            "\u00bd cup soft brown sugar",
            "1 pinch salt",
            "1 cup whole wheat flour",
            "1 teaspoon vegetable oil",
            "vegetable oil for deep-frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the pumpkin, brown sugar, and salt in a small saucepan over medium-low heat; cook until the brown sugar dissolves. Remove from heat and allow to cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour into the cooled pumpkin mixture to form a dough. Add 1 teaspoon of vegetable oil to the dough; allow to rest 5 minutes. Roll the dough into 12 even-sized balls and flatten to about 2-inches in diameter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the remaining vegetable oil in a skillet over medium-hot heat. Fry the dough in the hot oil until lightly brown on each side. Drain on a plate lined with paper towels.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
