    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275902/cauliflower-rice-casserole-with-spinach-and-artichokes/",
        "name": "Cauliflower Rice Casserole with Spinach and Artichokes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4574531.jpg",
            "width": 3024,
            "height": 3652
        },
        "datePublished": "2019-09-13T12:00:50.000Z",
        "description": "A delicious cauliflower rice casserole full of protein and veggies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "3 cloves garlic, minced",
            "1 (10 ounce) package baby spinach leaves",
            "1 (10 ounce) jar artichoke hearts, drained and chopped",
            "32 ounces cauliflower rice",
            "2 cups cooked quinoa",
            "2 eggs",
            "\u00bc cup shredded mozzarella-style vegan cheese (such as Daiya\u00ae)",
            "2 tablespoons shredded mozzarella-style vegan cheese (such as Daiya\u00ae), or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saute pan over medium heat. Cook and stir garlic in the hot oil for 1 minute. Add spinach and cook until wilted, 5 to 7 minutes. Add artichokes and cook until heated through, about 5 minutes. Remove from heat and transfer mixture to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook cauliflower rice in the same skillet over medium heat until fragrant, about 5 minutes. Remove from heat and add to the spinach mixture with cooked quinoa; mix well. Add eggs, 1 at a time, until thoroughly incorporated. Stir in 1/4 cup vegan cheese. Transfer mixture to a 9x13-inch baking dish and sprinkle with 2 tablespoons vegan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil casserole in the preheated oven until cheese is gooey and golden brown, about 2 minutes.\n"
            }
        ],
