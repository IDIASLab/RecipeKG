    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260997/cinnamon-roll-waffle-breakfast-sandwich/",
        "name": "Cinnamon Roll Waffle Breakfast Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4569919.jpg",
            "width": 1050,
            "height": 990
        },
        "datePublished": "2019-09-27T03:37:41.000Z",
        "description": "This is one of the menu items I created to sell on my breakfast delivery business last year, and oh did it deliver! Super easy and super yummy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "5 sandwiches",
        "recipeIngredient": [
            "cooking spray",
            "1 tablespoon unsalted butter, or as needed",
            "5 eggs",
            "\u00bc teaspoon ground black pepper, or to taste",
            "5 slices country ham",
            "2 (17.5 ounce) cans refrigerated cinnamon roll dough with icing",
            "5 slices sharp Cheddar cheese, or to taste",
            "1 tablespoon orange zest, or to taste",
            "\u00bc teaspoon sea salt, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat waffle iron and spray with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over medium heat. Fry eggs to your desired doneness, 3 to 6 minutes. Season with sea salt and pepper. Transfer to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook ham in the same skillet until golden brown, about 2 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 cinnamon roll in the waffle iron; cook until golden brown, 3 to 5 minutes. Repeat with remaining dough to create 10 cinnamon roll waffles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 slice of Cheddar cheese on half of the cinnamon roll waffles; add 1 fried egg and 1 slice of ham. Cover each sandwich with another cinnamon roll waffle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the cream cheese icing from the cinnamon roll package with orange zest; mix until combined. Drizzle on top of the sandwiches.\n"
            }
        ],
