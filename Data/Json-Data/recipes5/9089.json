    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275536/lemon-grilled-chicken-thighs/",
        "name": "Lemon Grilled Chicken Thighs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6915563.jpg",
            "width": 2711,
            "height": 2247
        },
        "datePublished": "2019-08-27T07:44:40.000Z",
        "description": "Whip up an easy lemony-garlic marinade before you head out to work in the morning, and come home to chicken thighs that will grill up to perfection and please your whole family!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "8 chicken thighs",
        "recipeIngredient": [
            "1 lemon, zested and juiced",
            "\u00bc cup olive oil",
            "1 tablespoon grated garlic",
            "3 sprigs fresh parsley",
            "\u00bd teaspoon salt",
            "8 chicken thighs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place lemon zest and 1 tablespoon lemon juice in a large zip-top bag; reserve remaining lemon juice for another use. Add olive oil, garlic, parsley, and salt. Add chicken thighs, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator, 1 hour to overnight\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken on the preheated grill and cook, turning once, until no longer pink at the bone and the juices run clear, 8 to 9 minutes per side. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C). Remove from the grill and allow to rest 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Thigh Recipes"
        ],
