    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278235/fresh-shrimp-spring-rolls/",
        "name": "Fresh Shrimp Spring Rolls",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fresh Shrimp Spring Rolls"
        },
        "datePublished": "2020-02-14T04:39:31.000Z",
        "description": "Savory fresh shrimp spring rolls with tangy dipping sauce.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "8 spring rolls",
        "recipeIngredient": [
            "2 ounces uncooked bean threads (cellophane noodles)",
            "2 tablespoons oil, or more as needed",
            "8 ounces tofu, drained and sliced into strips",
            "8 rice paper sheets",
            "1 bunch fresh basil",
            "\u2153 cup fresh cilantro",
            "1 cup bean sprouts",
            "1 cucumber, peeled and julienned",
            "1 medium carrot, peeled and julienned",
            "1 cup shredded lettuce",
            "\u00bd cup chopped onion",
            "16 cooked small peeled and deveined shrimp",
            "2 ounces sweet chili sauce",
            "1 tablespoon soy sauce",
            "1 tablespoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cover noodles with boiling water for 20 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat. Add tofu strips; pan-fry until golden, 5 to 7 minutes per side. Remove from heat and let cool, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large bowl with hot water. Dip one sheet of rice paper in the water for 20 seconds, then lay flat. Place 2 basil leaves, 2 cilantro leaves, 3 to 4 bean sprouts, a small handful of cucumber, carrot, lettuce, and onion, 2 shrimp, and 1 strip of tofu in rows across the middle of the rice paper. Fold the wrapper at both ends and roll tightly. Repeat with remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chili sauce, soy sauce, and lemon juice in a small bowl for dipping sauce. Serve with spring rolls.\n"
            }
        ],
