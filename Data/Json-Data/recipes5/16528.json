    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9728/hazelnut-lace/",
        "name": "Hazelnut Lace",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hazelnut Lace"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Thin and crispy cookies folded into a triangle shape or a hollow roll. Use brandy in place of the hazelnut liqueur if you like!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00bd cup packed brown sugar",
            "\u00bd cup butter",
            "\u2153 cup light corn syrup",
            "1 cup ground hazelnuts",
            "\u00be cup all-purpose flour",
            "2 tablespoons hazelnut liqueur"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Line cookie sheets with greased foil, shiny side up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, combine brown sugar, butter, and syrup.  Cook and stir over medium heat until well blended.  Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the nuts, flour, and liqueur.  Mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoonfuls onto a greased, foil-lined cookie sheet   about 5 inches apart (bake only 4-5 at a time; cookies spread when baked).  Bake for 9-11 minutes; let stand 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from cookie sheet.  Pull up three sides to center to form a  triangle, or roll around the greased handle of a wooden spoon.  If cookies are difficult to remove from sheet, reheat them in oven about 30 seconds.  Cool on rack.\n"
            }
        ],
