    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24538/slow-cooker-spare-ribs/",
        "name": "Slow Cooker Spare Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F410372.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T04:56:14.000Z",
        "description": "Slow cooked pork spare ribs, or country style ribs, also great with boneless, skinless chicken breasts.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT8H15M",
        "totalTime": "P0DT8H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed tomato soup",
            "1 onion, chopped",
            "3 cloves garlic, minced",
            "1 tablespoon brown sugar",
            "1 tablespoon Worcestershire sauce",
            "2 tablespoons soy sauce",
            "2 pounds pork spareribs",
            "1 teaspoon cornstarch",
            "\u00bc cup cold water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place ribs in a large stock pot, and cover with water. Bring to a boil, and cook for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, mix together soup, onion, garlic, brown sugar, Worcestershire sauce, and soy sauce. Remove ribs from water, and transfer to a slow cooker. Pour sauce over ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover, and cook  on Low for 6 to 8 hours, or until ribs are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "If sauce is too thin when cooking time is done, drain sauce from ribs, and pour into a sauce pan. Combine 1 teaspoon cornstarch with a small amount of cold water, stir into sauce, and bring sauce to boil. Cook until sauce has reached desired thickness.\n"
            }
        ],
