        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10845/molasses-cookies-iii/",
        "name": "Molasses Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Molasses Cookies III"
        },
        "datePublished": "1998-04-30T01:58:01.000Z",
        "description": "This is my son Ben's favorite cookie. For a tender tangier cookie use buttermilk in place of the milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "\u2153 cup packed brown sugar",
            "2 tablespoons honey",
            "1 egg",
            "1 cup whole wheat pastry flour",
            "\u00bd cup wheat germ",
            "\u00bd cup dry milk powder",
            "2 tablespoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bd cup molasses",
            "\u00be teaspoon ground ginger",
            "\u00be teaspoon ground nutmeg",
            "1 teaspoon distilled white vinegar",
            "2 tablespoons water",
            "\u00bc cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat shortening ( or butter ) and sugar until light and fluffy.  Beat in molasses, egg and 3 tablespoons of flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together remaining flour and other dry ingredients.  Mix together the vinegar, water and buttermilk. Add alternately to the molasses mixture and beat batter until it is very smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoons of the mixture onto greased cookie sheets.   Bake at 350 degrees F (175 degrees C) for 12-15 minutes.\n"
            }
        ],
