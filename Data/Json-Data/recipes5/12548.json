    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282811/portobello-steaks/",
        "name": "Portobello Steaks",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7441173.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-13T05:58:46.000Z",
        "description": "These juicy portobello steaks are one of my favorite quick and easy dishes. These can be cooked on a skillet indoors or on a grill outdoor. Such a great plant-based alternative whether you are vegan or just looking for a meatless Monday recipe!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 steaks",
        "recipeIngredient": [
            "4 large portobello mushrooms",
            "3 tablespoons balsamic vinegar",
            "3 tablespoons vegan Worcestershire sauce",
            "2 tablespoons olive oil",
            "2 cloves garlic, minced",
            "1 teaspoon onion powder",
            "salt and ground black pepper to taste",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove stems and clean the mushrooms; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vinegar, Worcestershire, oil, garlic, onion powder, salt, and pepper together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mushrooms inside a large zip-top bag. Pour in the marinade and turn the bag so the mushrooms get evenly coated. Let sit in a refrigerator for a minimum of 5 minutes, or up to 12 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a large skillet with cooking spray and heat over high heat. Grill mushrooms in the hot pan until heated through and browned, about 4 minutes per side.\n"
            }
        ],
