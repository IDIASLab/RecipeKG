        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9218/sweet-potato-balls-appetizer/",
        "name": "Sweet Potato Balls Appetizer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F144774.jpg",
            "width": 250,
