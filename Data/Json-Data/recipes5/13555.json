    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261733/vegan-shepherds-pie/",
        "name": "Vegan Shepherd's Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Shepherd's Pie"
        },
        "datePublished": "2020-01-16T08:51:15.000Z",
        "description": "Yummy vegan shepherd's pie, a Southern favorite!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H48M",
        "totalTime": "P0DT0H58M",
        "recipeYield": "1 8-inch pie",
        "recipeIngredient": [
            "olive oil cooking spray",
            "1 (10 ounce) package sliced fresh mushrooms",
            "1 yellow onion, chopped",
            "2 (14 ounce) cans Italian-style diced tomatoes",
            "1 (12 ounce) jar mushroom gravy, or to taste",
            "1 (12 ounce) package frozen mixed vegetables",
            "2 tablespoons vegan margarine (such as Earth Balance\u00ae)",
            "1\u2009\u00be cups water",
            "\u00bc teaspoon garlic powder",
            "\u00bd cup rice milk",
            "2\u2009\u00bc cups instant mashed potatoes (such as Betty Crocker\u00ae Potato Buds\u00ae)",
            "1 tablespoon egg substitute"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat; grease with olive oil spray. Add mushrooms and onion; saute until soft, about 5 minutes. Add diced tomatoes, gravy, and frozen vegetables. Bring to a boil. Reduce heat to low and cook until flavors blend, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt vegan margarine in a saucepan over medium heat. Add water and garlic powder; bring to a boil. Remove from heat. Add rice milk and stir in mashed potatoes and egg substitute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon mushroom mixture into an ungreased 8-inch glass baking dish. Spread potato mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is golden brown, about 20 minutes.\n"
            }
        ],
