    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277212/vegetarian-stuffed-portobello-mushrooms/",
        "name": "Vegetarian Stuffed Portobello Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegetarian Stuffed Portobello Mushrooms"
        },
        "datePublished": "2019-12-04T07:54:55.000Z",
        "description": "I love stuffed mushrooms and these vegetarian stuffed portobellos are packed with flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 stuffed mushroom caps",
        "recipeIngredient": [
            "\u00bd cup water",
            "\u215b cup dry <a href=\"https://www.allrecipes.com/gallery/red-lentil-recipes/\" aria-label=\"red lentils\">red lentils</a>",
            "1 tablespoon olive oil, divided",
            "8 baby portobello mushrooms, stemmed",
            "\u00bd cup finely diced onion",
            "2 cloves garlic, finely diced",
            "\u00be cup shredded Cheddar cheese, divided",
            "\u00bc cup crumbled feta cheese",
            "\u215b cup shredded Parmesan cheese",
            "\u00bc teaspoon dried parsley",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and lentils to a boil in a large saucepan. Cover, reduce heat to low, and simmer until tender, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon oil in a skillet over medium-high heat. Saute mushrooms in the hot oil until softened and liquid starts to cook out, 5 to 10 minutes. Remove from heat and place into a baking dish, cap-side down.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in a small pan over medium-high heat. Add onion and garlic and cook until onion is transparent and garlic fragrant, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine lentils,  onion-garlic mixture, 1/4 cup Cheddar cheese, feta cheese, Parmesan cheese, parsley, salt, and pepper in a bowl. Stir until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill mushroom caps evenly with mixture and top with remaining Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted, 15 to 20 minutes. Serve.\n"
            }
        ],
