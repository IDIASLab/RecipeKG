    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277413/creamy-black-bean-soup/",
        "name": "Creamy Black Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7957005.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-11-23T07:17:07.000Z",
        "description": "This creamy black bean soup is smooth and good served with cheese and chips!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "3 stalks celery, sliced, or more to taste",
            "2 carrots, diced",
            "1 large onion, diced",
            "3 cloves garlic, minced, or more to taste",
            "1 teaspoon diced fresh ginger",
            "4 (15 ounce) cans black beans, rinsed and drained",
            "4 cups water, or more as needed",
            "\u00bd cup salsa",
            "1 tablespoon ground cumin",
            "\u00bd teaspoon chili powder",
            "1 pinch dried oregano"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a stockpot over medium-high heat. Saute celery, carrots, and onion in the hot oil until onion is transparent, 5 to 7 minutes. Toss in garlic and ginger. Pour in beans and water. Add salsa, cumin, chili powder, and oregano. Bring to a boil; continue to boil for 25 to 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and let cool slightly, about 10 minutes. Transfer in batches to an electric blender or food processor and blend to desired consistency.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer soup back into the pot and heat through over medium heat, 3 to 5 minutes.\n"
            }
        ],
