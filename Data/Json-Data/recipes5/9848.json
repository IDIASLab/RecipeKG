        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13732/easy-sausage-balls/",
        "name": "Easy Sausage Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F934187.jpg",
            "width": 250,
            "height": 250
