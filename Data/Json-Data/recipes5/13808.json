    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282467/whole30-friendly-beef-and-vegetable-soup/",
        "name": "Whole30&reg;-Friendly Beef and Vegetable Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7656478.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-01-26T08:48:26.000Z",
        "description": "A hearty and delicious Whole30&reg;-friendly soup that's great for a meal and tastes even better the next day!",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 teaspoon ghee (clarified butter), or as needed",
            "2 pounds 95%-lean ground beef",
            "salt and ground black pepper to taste",
            "1 medium yellow onion, chopped",
            "6 cloves garlic, minced",
            "6 medium russet potatoes, peeled and cubed",
            "1 cup chopped carrots",
            "2 (32 ounce) cartons bone broth",
            "2 tablespoons tomato paste",
            "1 teaspoon dried basil, or to taste",
            "1 teaspoon dried oregano, or to taste",
            "\u00bd medium head cabbage, chopped",
            "smoked sea salt to taste (optional)",
            "2 small zucchini, sliced",
            "1 cup chopped fresh green beans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon ghee in a large stew pot over medium-high heat. Add ground beef and season with salt and pepper. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes. Transfer beef to a plate but leave any grease in the pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add onion and garlic to the pot, adding more ghee if needed. Saute until onion has softened and turned translucent, about 5 minutes. Set onions and garlic with the beef.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook potatoes in the pot with residual oil. Season with salt and pepper, and cook for 3 to 5 minutes. Add carrots; saute for 1 to 2 minutes. Add ground beef, onions, and garlic back into the pot; mix to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add bone broth, tomato paste, basil, and oregano; bring to a boil. Add cabbage and simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add smoked salt, then taste and adjust other seasonings if necessary. Add zucchini and green beans; simmer until vegetables are tender 3 to 5 minutes.\n"
            }
        ],
