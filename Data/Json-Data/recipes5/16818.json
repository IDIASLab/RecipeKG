    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12367/nutty-apple-crisp/",
        "name": "Nutty Apple Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F279970.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-08-26T15:52:23.000Z",
        "description": "Alternative to the apple pie we all love.  Serve just as is, with ice cream, or sour cream.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 quart crisp",
        "recipeIngredient": [
            "5 cups thinly sliced apples",
            "2 tablespoons lemon juice",
            "\u00bc cup water",
            "\u00bd cup all-purpose flour",
            "\u00bd cup rolled oats",
            "1 cup packed brown sugar",
            "\u00be teaspoon ground nutmeg",
            "1 pinch salt",
            "\u00bd cup butter, softened",
            "1 cup coarsely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss apples with lemon juice and water in a shallow 2 quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine flour, oats, sugar, nutmeg, and salt.  Mix to blend thoroughly.  Cut in the butter or margarine until mixture resembles coarse crumbs.  Mix in walnuts. Crumble over apples to cover completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 40 to 45 minutes until top is lightly browned.  Serve warm or at room temperature.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
