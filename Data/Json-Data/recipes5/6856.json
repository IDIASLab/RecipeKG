        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255993/yogurt-marinated-grilled-chicken/",
        "name": "Yogurt-Marinated Grilled Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9276958.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T07:10:25.000Z",
        "description": "This easy, fresh-tasting yogurt marinade helps the chicken stay juicy even if it gets a little overcooked.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT3H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 cup full-fat plain Greek yogurt",
            "1 cup lightly packed chopped cilantro",
            "2 tablespoons lemon juice",
            "1 tablespoon jarred minced garlic",
            "2 teaspoons kosher salt",
            "1 teaspoon ground cumin",
            "1 teaspoon ground cayenne pepper",
            "1 teaspoon ground black pepper",
            "3\u2009\u00bd pounds chicken thighs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix Greek yogurt, cilantro, lemon juice, garlic, salt, cumin, cayenne, and pepper together in a large bowl. Place chicken thighs in a large resealable plastic bag; cover with yogurt mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let chicken marinate in the refrigerator, 3 to 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring chicken to room temperature, about 30 minutes. Pour off marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate. Grill chicken, turning occasionally, until grill marks form, about 5 minutes per side.\n"
            }
        ],
