    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20898/caramel-frosting-vi/",
        "name": "Caramel Frosting VI",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4564753.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2000-06-05T15:43:46.000Z",
        "description": "This is an excellent caramel glaze frosting sprinkled with toasted pecans.  Makes enough frosting for a 9x13 cake.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 frosting for 9x13 inch cake",
        "recipeIngredient": [
            "1 cup brown sugar",
            "2 tablespoons all-purpose flour",
            "3 tablespoons butter",
            "\u00bc cup milk",
            "1 teaspoon vanilla extract",
            "\u2153 cup toasted, chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a shallow 6-inch saucepan, thoroughly mix brown sugar and flour. Stir in butter and milk, and cook over medium-high heat until mixture comes to a rolling boil. Boil exactly 1-1/2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat.  Allow to cool 5 minutes. Beat with a wooden spoon until mixture begins to thicken. Mix in vanilla and continue beating until frosting is the consistency of thick honey, no more than 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Immediately pour caramel frosting onto cooled cake and quickly spread with the back of a warm spoon before frosting begins to set up. Quickly sprinkle top of cake with toasted pecans. Frosting will set up completely in a few hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frostings and Icings"
        ],
