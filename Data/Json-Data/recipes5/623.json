    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270274/chicken-tortellini-casserole/",
        "name": "Chicken Tortellini Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chicken Tortellini Casserole"
        },
        "datePublished": "2019-04-10T23:29:21.000Z",
        "description": "This chicken casserole is easy, crowd-pleasing, warm, comfort food.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H25M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "1 pound skinless, boneless chicken breast halves",
            "2 cloves garlic, peeled and crushed",
            "2 cups chicken broth",
            "2 (12 ounce) packages cheese tortellini",
            "cooking spray",
            "\u00bd cup butter",
            "6 green onions, sliced",
            "2 cups grated Monterey Jack cheese, divided",
            "1 cup sour cream",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "2 (4 ounce) cans button mushrooms, drained and sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken and garlic in a soup pot. Add chicken broth and bring to boil; reduce heat to low and simmer, covered, until chicken is no longer pink in the center and juices run clear, about 30 minutes. Remove from pot; let cool 10 minutes. Dice into bite-sized pieces. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook tortellini in the boiling water, stirring occasionally, until tender yet firm to the bite, 10 to 12 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch oven-proof glass container (such as Pyrex&reg;) with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Add green onions, 1 cup Monterey Jack cheese, sour cream, cream of mushroom soup, and cream of chicken soup; cook until heated through, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine diced chicken, tortellini, cream sauce, and button mushrooms in a large bowl. Pour into the prepared 9x13-inch Pyrex&reg; baking dish. Top with remaining cup of Monterey Jack cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling and top is golden brown, 30 to 45 minutes.\n"
            }
        ],
