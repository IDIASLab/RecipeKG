    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274742/thai-tamarind-chicken-with-glass-rice-noodles/",
        "name": "Thai Tamarind Chicken with Glass Rice Noodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Thai Tamarind Chicken with Glass Rice Noodles"
        },
        "datePublished": "2019-07-29T20:50:38.000Z",
        "description": "The tangy and unique flavor of tamarind is the key to this great and simple Thai chicken and noodles dish.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT4H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup soy sauce",
            "\u00bd cup palm sugar",
            "\u2153 cup fish sauce",
            "\u00bc cup Sriracha sauce",
            "\u00bc cup tamarind concentrate",
            "\u00bc cup sesame oil",
            "4 cloves garlic, minced",
            "2 tablespoons grated ginger",
            "1 pinch cayenne pepper, or to taste",
            "2 pounds skinless, boneless chicken thighs",
            "1 (8 ounce) package dried Thai rice noodles"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine soy sauce, palm sugar, fish sauce, Sriracha sauce, tamarind concentrate, sesame oil, garlic, ginger, and cayenne pepper in a large zip-top bag. Add chicken and marinate in a refrigerator, 3 to 4 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from bag, reserving marinade. Cook on the preheated grill until no longer pink inside and juices run clear, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place noodles in a large bowl and cover with hot water. Set aside until noodles are softened, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour reserved marinade into a saucepan over medium-high heat. Bring to a boil, reduce heat to a simmer, and cook sauce until reduced, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice grilled chicken and place over noodles. Top with sauce as desired.\n"
            }
        ],
