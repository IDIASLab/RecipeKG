    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274086/quick-and-easy-beef-chili/",
        "name": "Quick and Easy Beef Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Quick and Easy Beef Chili"
        },
        "datePublished": "2019-06-14T04:33:33.000Z",
        "description": "This is a quick chili, great for the weeknight when it's cold. I serve this with shredded sharp Cheddar cheese and cornbread muffins.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 (12 ounce) sirloin steak, cut into bite-sized pieces",
            "1 onion, chopped",
            "3 cloves garlic, minced",
            "2 tablespoons all-purpose flour",
            "2 tablespoons chili powder",
            "1 teaspoon ground cumin",
            "1 teaspoon smoked paprika",
            "\u00bd teaspoon dried Mexican oregano",
            "1 (28 ounce) can crushed tomatoes",
            "1 (15 ounce) can tomato sauce",
            "1 cup beer (preferably lager)",
            "2 (15 ounce) cans kidney beans, rinsed and drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Saute ground beef and sirloin steak with onion and garlic in the hot skillet until beef is no longer pink, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle flour, chili powder, cumin, paprika, and oregano over meat mixture and cook for 3 minutes. Add tomatoes, tomato sauce, and beer; bring to a fast simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat to low and cook for 15 to 20 minutes. Add kidney beans and continue to simmer until beans are heated through, 5 to 7 minutes.\n"
            }
        ],
