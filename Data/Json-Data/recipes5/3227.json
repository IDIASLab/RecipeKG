    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24621/steak-salad/",
        "name": "Steak Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F263688.jpg",
            "width": 2288,
            "height": 1712
        },
        "datePublished": "2019-07-10T06:52:37.000Z",
        "description": "This is a quick and easy meal for those hot summer evenings.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "1\u2009\u00be pounds beef sirloin steak",
            "\u2153 cup olive oil",
            "3 tablespoons red wine vinegar",
            "2 tablespoons lemon juice",
            "1 clove garlic, minced",
            "\u00bd teaspoon salt",
            "\u215b teaspoon ground black pepper",
            "1 teaspoon Worcestershire sauce",
            "\u00be cup crumbled blue cheese",
            "8 cups romaine lettuce - rinsed, dried, and torn into bite-size pieces",
            "2 tomatoes, sliced",
            "1 small green bell pepper, sliced",
            "1 carrot, sliced",
            "\u00bd cup sliced red onion",
            "\u00bc cup sliced pimento-stuffed green olives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly oil grate. Place steak on grill and cook for 3 to 5 minutes per side or until desired doneness is reached.  Remove from heat and let sit until cool enough to handle.  Slice steak into bite size pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, whisk together the olive oil, vinegar, lemon juice, garlic, salt, pepper and Worcestershire sauce.   Mix in the cheese.  Cover and place dressing in refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Onto chilled plates arrange the lettuce, tomato, pepper, onion and olives.  Top with steak and drizzle with dressing.  Serve with crusty grilled French bread.  Enjoy!\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Green Salad Recipes",
            "Romaine Lettuce Salad Recipes"
        ],
