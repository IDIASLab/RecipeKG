    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11221/fruitcake-cookies-ii/",
        "name": "Fruitcake Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4224772.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "1998-12-30T15:02:25.000Z",
        "description": "This is one of the recipes that my mom gave that I am putting in our family cookbook. For a slightly more tender cookie use buttermilk in place of the milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 cup shortening",
            "2 cups packed brown sugar",
            "2 eggs",
            "\u2154 cup milk",
            "1 cup chopped pecans",
            "2 cups dates, pitted and chopped",
            "1 cup candied cherries, quartered",
            "1 cup candied mixed citrus peel",
            "\u00bd cup red and green candied cherries, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift the flour, measure and sift it again with the baking soda and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the shortening add the sugar and eggs. Beat until light and fluffy. Add the milk and flour mixture, mixing well. Stir in the nuts, dates, cherries and candied peel. Cover and chill dough for several hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop chilled dough by teaspoons, 2 inches apart, onto lightly greased baking sheets. Top each cookie with a half of a candied cherry. Bake at 350 degrees F (175 degrees C) for 8 to 10 minutes.\n"
            }
        ],
