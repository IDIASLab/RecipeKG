    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273817/bruschetta-pasta/",
        "name": "Bruschetta Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bruschetta Pasta"
        },
        "datePublished": "2019-05-31T04:42:25.000Z",
        "description": "My family and I love bruschetta. This pasta dish is a twist on the traditional bruschetta appetizer. We like to use rigatoni or shell pasta. Serve with grilled chicken.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (16 ounce) box rigatoni pasta",
            "\u00bd small red onion",
            "4 cloves garlic, peeled",
            "2 tablespoons olive oil",
            "2 tablespoons butter",
            "2 medium tomatoes, cored",
            "\u00bd teaspoon dried oregano, or to taste",
            "\u00bd teaspoon dried parsley, or to taste",
            "salt and ground black pepper to taste",
            "\u00bd cup grated Parmesan cheese",
            "\u00bd cup diced fresh mozzarella cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook rigatoni in the boiling water, stirring occasionally until tender yet firm to the bite, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the pasta is cooking, puree onion and garlic in the bowl of a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil and butter in a medium pan over medium-low heat. Add onion and garlic; simmer until fragrant, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree tomatoes in the food processor. Add tomatoes to the onion mixture; simmer for 5 minutes. Mix in oregano, parsley, salt, and pepper. Add cooked rigatoni and Parmesan cheese; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat. Toss in mozzarella cheese and let rest for 5 minutes before serving.\n"
            }
        ],
