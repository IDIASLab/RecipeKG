    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276117/grilled-chicken-drumsticks/",
        "name": "Grilled Chicken Drumsticks",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F896562.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-02-01T07:52:27.000Z",
        "description": "I use this marinade on grilled chicken drumsticks. I served it alongside homemade tortillas and guacamole, as well as Spanish rice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT20H45M",
        "recipeYield": "16 drumsticks",
        "recipeIngredient": [
            "1 yellow onion, chopped",
            "\u00bd white onion, chopped",
            "5 cloves garlic, sliced",
            "16 chicken drumsticks",
            "3 tablespoons chopped fresh cilantro",
            "2 teaspoons dried oregano",
            "2 teaspoons ground cumin",
            "2 teaspoons ground thyme",
            "1 teaspoon garlic powder",
            "salt and ground black pepper to taste",
            "2 large lemons"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix yellow onion, white onion, and garlic together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season chicken drumsticks with cilantro, oregano, cumin, thyme, garlic powder, salt, and pepper. Place chicken into the bowl with onion mixture. Squeeze lemon juice over the chicken, slice the rinds, and place into the same bowl. Cover and let sit in a refrigerator, 20 to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on the preheated grill until an instant-read thermometer inserted into a drumstick reads at least 180 degrees F (82 degrees C), 30 to 45 minutes.\n"
            }
        ],
