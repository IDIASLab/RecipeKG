    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273454/peach-raspberry-tart/",
        "name": "Peach-Raspberry Tart",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Peach-Raspberry Tart"
        },
        "datePublished": "2019-05-11T06:55:04.000Z",
        "description": "This peach-raspberry tart is something refreshing for the summer months. You can use any sliced fresh fruit you like.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT3H35M",
        "recipeYield": "1 9-inch tart",
        "recipeIngredient": [
            "1\u2009\u00bc cups graham cracker crumbs",
            "\u00bd cup melted butter",
            "\u00bc cup ground almonds",
            "1 teaspoon ground cinnamon",
            "\u00bd cup water",
            "1 envelope unflavored gelatin",
            "1 pound cream cheese, softened",
            "1 cup sour cream",
            "\u00bd cup whipping cream",
            "2 teaspoons grated lemon zest",
            "2 large  peaches, peeled and sliced",
            "1 cup fresh raspberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine graham cracker crumbs, butter, almonds, and cinnamon in a bowl. Mix until evenly moistened; press into the bottom and partway up the sides of a 9-inch springform pan. Refrigerate while preparing the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour water into a saucepan. Sprinkle gelatin powder on top. Let stand until softened, about 1 minute. Place over low heat; stir constantly until the granules are completely dissolved, about 3 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese with an electric mixer until light and fluffy, 2 to 3 minutes. Beat in sour cream, whipping cream, and lemon zest. Add gelatin; mix well. Pour into the prepared crust. Refrigerate until set, 3 to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the springform pan and place on a serving plate. Decorate with sliced peaches and raspberries.\n"
            }
        ],
