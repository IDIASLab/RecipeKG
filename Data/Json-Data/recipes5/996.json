    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273026/cheesy-baked-asparagus/",
        "name": "Cheesy Baked Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6571576.jpg",
            "width": 2383,
            "height": 3786
        },
        "datePublished": "2019-04-25T07:59:30.000Z",
        "description": "These asparagus spears are easy and quick to make. You will have no trouble at all getting your kids to eat their veggies with these cheesy, melty asparagus spears. Adults love them too!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound fresh asparagus, trimmed",
            "2 tablespoons olive oil",
            "3 cloves garlic, pressed through a garlic press",
            "\u00bc teaspoon freshly cracked mixed peppercorns",
            "1\u2009\u00bd cups shredded Gouda cheese",
            "freshly cracked Himalayan pink salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread out asparagus on the prepared baking sheet and sprinkle with olive oil, fresh garlic, and cracked peppercorns. Toss to coat evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 12 to 15 minutes. Remove from oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread Gouda cheese down the center of the asparagus spears and broil until cheese is melted, 3 to 5 minutes. Remove from oven and sprinkle with pink Himalayan salt.\n"
            }
        ],
