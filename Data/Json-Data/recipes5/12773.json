    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277774/fudge-topped-shortbread-bars/",
        "name": "Fudge-Topped Shortbread Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fudge-Topped Shortbread Bars"
        },
        "datePublished": "2019-12-21T20:12:25.000Z",
        "description": "Light, fluffy shortbread layer topped with a thick, creamy fudgy layer. Sprinkle crushed nuts or crushed peppermints over warm bars for added flavor.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "1 cup butter, softened",
            "\u00bd cup powdered sugar",
            "\u00bc teaspoon salt",
            "1\u2009\u00bc cups all-purpose flour",
            "1 (14 ounce) can sweetened condensed milk",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, powdered sugar, and salt in a mixing bowl; beat with an electric mixer until creamy. Gradually beat in flour. Spread dough into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until light brown, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sweetened condensed milk and chocolate chips in a microwave-safe bowl. Heat in the microwave for 1 minute. Beat until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour melted chocolate mixture over warm shortbread and spread evenly. Cool until set, about 30 minutes. Cut into small squares.\n"
            }
        ],
