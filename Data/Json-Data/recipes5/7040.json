    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258208/polynesian-grilled-shrimp/",
        "name": "Polynesian Grilled Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4516480.jpg",
            "width": 2568,
            "height": 2246
        },
        "datePublished": "2019-07-10T07:10:53.000Z",
        "description": "Serve these Polynesian-inspired shrimp on skewers for a tropical vibe at your next party.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u2153 cup mango nectar",
            "2\u2009\u00bd tablespoons lemon juice",
            "2 tablespoons honey",
            "2 tablespoons low-sodium soy sauce",
            "2 tablespoons vegetable oil",
            "2 tablespoons minced garlic",
            "1 tablespoon minced fresh ginger",
            "2 teaspoons sweet chili sauce",
            "\u215b teaspoon cayenne pepper",
            "24 (21 to 25 per lb) large shrimp, peeled and deveined, tails left on"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together mango nectar, lemon juice, honey, soy sauce, oil, garlic, ginger, chili sauce, and cayenne in a bowl. Add shrimp, turning to coat, and marinate, covered with plastic wrap, 30 minutes at room temperature or chilled up to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat, 375 to 450 degrees F (200 to 230 degrees C ). Lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lift shrimp from marinade, shaking off excess. Grill shrimp until just cooked through, about 1 minute per side.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Shellfish",
            "Shrimp"
