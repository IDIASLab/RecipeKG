        "mainEntityOfPage": "https://www.allrecipes.com/recipe/163677/tom-collins/",
        "name": "Tom Collins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F673457.jpg",
            "width": 3168,
            "height": 3168
        },
        "datePublished": "2019-07-10T07:00:52.000Z",
        "description": "This is a great warm weather cocktail with or without the splash of bitters.",
        "prepTime": "P0DT0H5M",
        "cookTime": null,
        "totalTime": "P0DT0H5M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "2 fluid ounces gin",
            "2 fluid ounces lemon juice",
            "1 fluid ounce simple syrup",
            "1 dash bitters",
            "1 cup ice cubes",
            "\u00bc cup cold club soda",
            "1 slice lemon, for garnish",
            "1 maraschino cherry"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a cocktail shaker with ice. Pour in the gin, lemon juice, simple syrup and a dash of bitters. Cover and shake until the outside of the container is frosty, about 15 seconds. Strain into a highball glass full of ice. Top off with club soda and garnish with a lemon slice and maraschino cherry.\n"
            }
        ],
