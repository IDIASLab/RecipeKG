    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278344/bacon-cheddar-biscuits/",
        "name": "Bacon-Cheddar Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F287123.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-01-24T05:44:56.000Z",
        "description": "Easy bacon-Cheddar drop biscuits. Although Cheddar tastes the best in this recipe, you can sub any other cheese you have on hand.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "18 biscuits",
        "recipeIngredient": [
            "4 slices bacon, or more to taste",
            "2 cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "5 tablespoons shortening",
            "1 cup buttermilk",
            "1 cup grated Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Let cool enough to handle; crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, baking soda, and salt in a bowl and cut in shortening until it resembles coarse crumbs. Add buttermilk and stir just enough to form a ball. Fold in crumbled bacon and Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop heaping tablespoonfuls on an ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 10 to 12 minutes. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Meat and Seafood",
            "Bacon"
        ],
