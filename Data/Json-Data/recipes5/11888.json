    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8303/banana-rum-cake/",
        "name": "Banana Rum Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7247228.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-07-16T02:56:13.000Z",
        "description": "Use this recipe, dark rum, mashed bananas, and yellow cake mix to easily make a moist and flavorful cake.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9-inch layer cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "\u00bc teaspoon baking soda",
            "\u2154 cup dark rum",
            "\u2154 cup water",
            "2 eggs",
            "1 cup mashed bananas",
            "\u2153 cup chopped pecans",
            "\u2153 cup butter, softened",
            "3 cups confectioners' sugar",
            "2 teaspoons vanilla extract",
            "2 tablespoons dark rum"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease and flour two 9-inch cake pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine cake mix, baking soda, rum, water, eggs, bananas and nuts. Bring together, then beat on medium speed 2 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide into prepared pans. Bake at 350 degrees F (175 degrees C) for 25 to 30 minutes, or until toothpick inserted into center of cake comes out clean. Cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the frosting: Combine 1/3 cup butter or margarine with the confectioners sugar. Blend thoroughly. Stir in vanilla and 2 tablespoons rum. Beat until smooth.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
