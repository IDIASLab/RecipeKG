        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218027/tjs-creamy-salsa/",
        "name": "TJ's Creamy Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F779573.jpg",
            "width": 250,
