    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215676/baked-broccoli/",
        "name": "Baked Broccoli",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1130058.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2020-03-05T05:07:36.000Z",
        "description": "A delicious and rich alternative to bland steamed broccoli! It caramelizes perfectly, and the spices give it a great flavor that kids and parents will love.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "2 tablespoons brown sugar",
            "1\u2009\u00bd tablespoons lemon juice",
            "\u00bd teaspoon cayenne pepper",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon dried thyme",
            "1 pinch salt",
            "1 pinch fresh ground black pepper ",
            "1 pound broccoli florets"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, mix together the olive oil, brown sugar, lemon juice, cayenne pepper, garlic powder, oregano, thyme, salt, and black pepper until thoroughly combined. Add the broccoli florets, and toss until evenly coated with the seasoning. Spread the broccoli florets out onto an baking sheet with a rim.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the broccoli is just browned on the top, 10 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Broccoli"
        ],
