        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13646/finnish-turnips/",
        "name": "Finnish Turnips",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Finnish Turnips"
        },
        "datePublished": "1998-11-07T04:14:03.000Z",
        "description": "A sweet baked turnip side dish",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 small turnips, chopped",
            "2 tablespoons butter",
            "\u00bd teaspoon salt",
            "1 cup heavy whipping cream",
            "2 eggs, beaten",
            "\u2153 cup packed brown sugar",
            "1 cup crushed cornflakes cereal",
            "2 tablespoons crushed cornflakes cereal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a pot of salted water to a boil. Add turnips; cook until tender but still firm. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, add turnips, butter, salt, cream, eggs, brown sugar and 1 cup corn flakes. Blend until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a 2 quart casserole dish, sprinkle with remaining 2 tablespoons crushed corn flakes. Bake for 45 to 50 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables"
        ],
