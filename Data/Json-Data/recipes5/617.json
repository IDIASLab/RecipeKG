    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272768/asperge-du-pauvre/",
        "name": "Asperge du Pauvre",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6536092.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-10T22:05:35.000Z",
        "description": "This French leek dish translates to \"Poor Man's Asparagus\" and is a very tasty side for any meat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd bunch leeks",
            "\u00bc cup water, or as needed",
            "1 tablespoon dry white wine",
            "1 pinch salt",
            "\u00bc cup capers",
            "\u00bc cup Dijon vinaigrette"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Chop off the root and dark green ends off the leek. Keep only the whites and light green center. Slice leek in half lengthwise, then slice each half lengthwise into quarters.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour water into a saucepan. Add white wine and salt, and bring to a simmer over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add leeks to the hot liquid and reduce heat to medium-low. Cover saucepan and simmer until leeks are fork-tender, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, pour capers into a mesh strainer and run under the faucet for about 5 minutes to wash off the brine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain leeks and arrange on plates. Sprinkle with vinaigrette and capers. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Leeks"
        ],
