    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238045/lemon-blueberry-tartlets/",
        "name": "Lemon Blueberry Tartlets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1150041.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2014-06-03T22:14:21.000Z",
        "description": "These pretty little tarts with a creamy lemon filling are topped with whipped cream and fresh blueberries.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 tartlets",
        "recipeIngredient": [
            "1  serving Crisco\u00ae No-Stick Cooking Spray",
            "\u00bd cup Crisco\u00ae All-Vegetable Shortening",
            "4 ounces cream cheese, softened",
            "1\u2009\u00bd cups Pillsbury BEST\u00ae All Purpose Flour",
            "\u00bc teaspoon salt",
            "1 (14 ounce) can Eagle Brand\u00ae Sweetened Condensed Milk",
            "1 large egg, beaten",
            "2 tablespoons fresh lemon juice",
            "2 teaspoons grated lemon peel, divided",
            "\u00be cup fresh blueberries",
            "4 tablespoons Whipped cream",
            "2 teaspoons Honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 425 degrees F. Coat 8 (4 x 3/4-inch) tart pans with no-stick cooking spray. Beat shortening and cream cheese in large bowl with electric mixer on medium speed until fluffy. Beat in flour and salt until dough forms. Press into bottoms and sides of prepared pans. Place tart pans on baking sheet. Bake 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk sweetened condensed milk, egg and lemon juice in medium bowl until smooth. Stir in 1 teaspoon lemon peel. Divide evenly into crusts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven to 375 degrees F. Bake 16 to 18 minutes or until lightly browned. Cool completely. Remove from pans. Top with whipped cream and blueberries. Drizzle with honey. Sprinkle with remaining lemon peel.\n"
            }
        ],
