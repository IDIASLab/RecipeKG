    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275680/pumpkin-cinnamon-cheesecake-truffles/",
        "name": "Pumpkin-Cinnamon Cheesecake Truffles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin-Cinnamon Cheesecake Truffles"
        },
        "datePublished": "2019-09-09T07:18:52.000Z",
        "description": "Pumpkin-cinnamon cheesecake truffles recipe--easy, quick, and so tasty!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 ounces cream cheese",
            "2 tablespoons powdered sugar",
            "4 ounces pumpkin puree",
            "1\u2009\u00be cups vanilla wafer crumbs",
            "\u00bd teaspoon pumpkin pie spice",
            "\u00bd teaspoon ground cinnamon",
            "4 (1 ounce) squares white chocolate",
            "2 teaspoons vanilla wafer crumbs, or to taste",
            "\u00bc teaspoon ground cinnamon, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and powdered sugar in a bowl; beat with an electric mixer until smooth and creamy, about 2 minutes. Add pumpkin puree; beat until combined. Add 1 3/4 cups wafer crumbs, pumpkin pie spice, and cinnamon; beat until everything is combined and the mixture is soft and thick, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll pumpkin mixture into teaspoon-sized balls and place onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place white chocolate in a microwave-safe bowl. Microwave on high power until chocolate is completely melted and smooth, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each truffle into the melted chocolate using 2 forks, and return to the baking sheet. Repeat with remaining truffles. Sprinkle truffles with wafer crumbs and cinnamon and store in the refrigerator until chocolate has completely set, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
