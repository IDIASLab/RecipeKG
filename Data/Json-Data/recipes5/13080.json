    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241257/harmons-brunswick-stew/",
        "name": "Harmon's Brunswick Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6406650.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-01-03T00:50:34.000Z",
        "description": "This is the easiest recipe I have ever seen for Brunswick stew, and it is delicious.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "7 servings",
        "recipeIngredient": [
            "1 tablespoon butter, or as needed",
            "1 large onion, chopped",
            "2 (16.5 ounce) cans cream-style corn",
            "2 (14.5 ounce) cans crushed tomatoes",
            "2 (5 ounce) cans white chicken chunks (such as Swanson\u00ae), drained",
            "1 (10 ounce) can pork in barbeque sauce (such as Vietti\u00ae)",
            "1 (10 ounce) can beef in barbeque sauce (such as Vietti\u00ae)",
            "2 tablespoons Worcestershire sauce",
            "1 teaspoon ground black pepper",
            "1 teaspoon salt",
            "1 teaspoon hot sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over medium-high heat. Saute onion in melted butter until tender, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sauteed onion, cream-style corn, crushed tomatoes, white chicken chunks, pork in barbeque sauce, beef in barbeque sauce, Worcestershire sauce, black pepper, salt, and hot sauce together in a slow cooker crock.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on High until hot, at least 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Brunswick Stew Recipes"
        ],
