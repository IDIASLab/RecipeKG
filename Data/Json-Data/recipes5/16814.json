    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12332/maple-syrup-tarts/",
        "name": "Maple Syrup Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F948231.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-07-04T15:57:43.000Z",
        "description": "For those of us who live in the land of maple syrup, we see plenty in the month of March.  The trees are tapped, maple syrup runs freely, and we all head out to the sugar bush for samples.  This recipe is an old favorite from our ancestors.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 tarts",
        "recipeIngredient": [
            "12 (3 inch) unbaked tart shells",
            "1 egg",
            "1 cup real maple syrup",
            "1 tablespoon butter, melted",
            "\u00bc cup packed brown sugar",
            "1 tablespoon lemon juice",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg gently with a fork, and then mix with maple syrup, melted butter, brown sugar, lemon juice, and nuts.  Fill tarts 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 to 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Tarts"
        ],
