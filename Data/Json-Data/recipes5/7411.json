    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263944/cotton-cheesecake/",
        "name": "Cotton Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cotton Cheesecake"
        },
        "datePublished": "2019-07-10T07:11:54.000Z",
        "description": "This cotton cheesecake is very light and fluffy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "1 9-inch cake",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese",
            "7 tablespoons heavy whipping cream",
            "\u00bc cup unsalted butter",
            "6 eggs, separated",
            "\u00bd cup cake flour",
            "\u00be cup superfine sugar, divided",
            "\u00bc cup cornstarch",
            "1 tablespoon lemon juice, or to taste",
            "1 tablespoon vanilla extract",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Line the bottom of a 9-inch round cake pan with lightly buttered parchment paper. Lightly butter the sides of the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese, heavy cream, and butter in the top of a double boiler over simmering water. Mix well, scraping down the sides with a rubber spatula to avoid scorching, until mixture is very creamy, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cream cheese mixture cool over an ice water bath, about 10 minutes. Mix in egg yolks, flour, 1/2 the sugar, cornstarch, lemon juice, vanilla extract, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a glass, metal, or ceramic bowl using an electric mixer on high speed until soft peaks form. Gradually add the remaining sugar, continuing to beat into stiff peaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 1/3 of the egg whites into the cream cheese mixture until well blended. Gently fold in the remaining egg whites, 1/3 at a time. Do not overmix. Tightly wrap the bottom of the cake pan with aluminum foil. Pour batter into the prepared cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the cake pan into a baking pan filled with boiling water to reach halfway up the cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is golden brown, about 1 hour. Turn oven off and let cake cool inside, with the door closed to prevent cracking, about 1 hour.\n"
            }
        ],
