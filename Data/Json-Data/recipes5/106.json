    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268386/dairy-free-chicken-curry/",
        "name": "Dairy-Free Chicken Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4886811.jpg",
            "width": 1152,
            "height": 2048
        },
        "datePublished": "2019-04-04T04:14:11.000Z",
        "description": "Create a creamy, dairy-free curry with blended cauliflower, two kinds of bouillon, and coconut milk.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 head cauliflower, broken into florets",
            "1 onion, chopped",
            "1 tablespoon coconut oil",
            "2 pounds boneless chicken thighs, cubed",
            "1\u2009\u00bd tablespoons cumin, or to taste",
            "1\u2009\u00bd tablespoons turmeric  ",
            "3 tablespoons curry powder, or to taste, divided",
            "1 (5.6 ounce) can coconut milk",
            "1 teaspoon garlic bouillon base (such as Better Than Bouillon\u00ae)",
            "1 teaspoon chicken bouillon base (such as Better Than Bouillon\u00ae)",
            "\u00bc cup water, or as needed",
            "1 teaspoon minced garlic",
            "1 teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add cauliflower and onions. Reduce heat to medium-low and simmer until tender, 10 to 15 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat coconut oil in a large skillet over medium heat. Add chicken, cumin, turmeric, and 1 tablespoon curry powder; stir and reduce heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, drain cauliflower and onions and transfer to a blender. Add coconut milk, garlic bouillon, and chicken bouillon. Puree, adding small amounts of water if necessary, to desired thickness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour puree into skillet with chicken. Add minced garlic and black pepper and bring to a simmer. Cook and stir until chicken is no longer pink inside, 5 to 10 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season with additional curry and cumin if desired.\n"
            }
        ],
