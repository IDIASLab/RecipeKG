    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268438/gluten-free-pumpkin-date-bars/",
        "name": "Gluten-Free Pumpkin Date Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5855722.jpg",
            "width": 1202,
            "height": 1804
        },
        "datePublished": "2019-04-04T04:14:13.000Z",
        "description": "These delicious pumpkin date bars are gluten free! They are a good source of protein and a great snack for work, school, and more!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "16 bars",
        "recipeIngredient": [
            "3 cups gluten-free rolled oats",
            "1 cup chopped, pitted dates",
            "1 cup chopped hazelnuts",
            "\u00bd cup chopped almonds",
            "\u00bd cup raisins",
            "\u00bd cup dried cranberries",
            "\u00bd cup coarsely chopped gluten-free pretzels",
            "3 tablespoons chia seeds",
            "\u2154 cup water",
            "\u00bd cup ground flax seeds",
            "1 cup pumpkin puree",
            "2 ripe bananas, mashed",
            "2 tablespoons honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a large cookie sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently mix oats, dates, hazelnuts, almonds, raisins, cranberries, pretzels, and chia seeds in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water and flax seeds together in a small bowl. Let stand until thickened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pumpkin puree, bananas, and honey together in a medium bowl. Gently mix into the oat mixture until soft and evenly combined. Spread onto the cookie sheet in an even 1/2-inch layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned but still slightly gooey, 25 to 33 minutes. Let cool before cutting into bars.\n"
            }
        ],
