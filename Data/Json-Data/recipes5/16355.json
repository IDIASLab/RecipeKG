    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7020/pumpernickel-bread-ii/",
        "name": "Pumpernickel Bread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F798353.jpg",
            "width": 3888,
            "height": 2592
        },
        "datePublished": "1998-05-16T13:43:01.000Z",
        "description": "This may not be New York Pumpernickel, but it is the one we use at home.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "9 -1/2x5 inch loaf (2 pounds)",
        "recipeIngredient": [
            "2 cups warm milk",
            "2 tablespoons vegetable oil",
            "4 tablespoons molasses",
            "3\u2009\u00bc cups bread flour",
            "1\u2009\u2153 cups rye flour",
            "\u00bd cup cornmeal",
            "1\u2009\u2153 teaspoons salt",
            "2\u2009\u2154 teaspoons active dry yeast",
            "4 tablespoons unsweetened cocoa powder",
            "2\u2009\u2154 tablespoons brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix well bread flour, rye flour, cornmeal, salt, yeast, cocoa, and brown sugar. Add milk, oil, and molasses. Mix thoroughly. When mixed well enough that the dough holds together, knead by hand 15-20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover, let rise in bowl 30 minutes. Punch down, form, and place into 9 1/2x5 inch pan. Cover with damp cloth and let rise about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated 375 degree F (190 degrees C) oven 25 to 30 minutes, covering top with aluminum foil last 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes",
            "Whole Grain Bread Recipes",
            "Rye Bread"
        ],
