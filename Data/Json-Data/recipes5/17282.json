    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21019/grandpa-hubbards-oatmeal/",
        "name": "Grandpa Hubbard's Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F58202.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2000-07-29T17:06:13.000Z",
        "description": "My grandfather used to make this oatmeal for me on Saturday mornings accompanied by buttered wheat toast. I miss those Saturdays.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3\u2009\u00be cups water",
            "2 cups rolled oats",
            "1 pinch salt",
            "4 teaspoons butter",
            "\u00bc cup brown sugar",
            "1 cup non-dairy creamer",
            "4 tablespoons milk",
            "\u00bc cup brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, heat water to boiling. Reduce heat to low; stir in oats and salt. Cook until oats have thickened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 teaspoon of butter and 1 tablespoon of brown sugar in the bottom of each four serving bowls. Spoon oatmeal into each bowl and stir until butter and sugar are melted. Pour 1/4 cup of creamer and 1 tablespoon of milk over each bowl. Top each serving with another tablespoon of brown sugar. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Cereals",
            "Oatmeal Recipes"
        ],
