    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235575/curried-apple-soup/",
        "name": "Curried Apple Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Curried Apple Soup"
        },
        "datePublished": "2019-07-10T07:05:03.000Z",
        "description": "Something different and delicious, whether served hot or cold.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "1 onion, chopped",
            "1\u2009\u00bd teaspoons ground coriander",
            "1\u2009\u00bd teaspoons garam masala",
            "1 teaspoon ground cumin",
            "1 teaspoon ground turmeric",
            "5 tablespoons all-purpose flour",
            "3\u2009\u00be cups water",
            "2 cubes  chicken bouillon, crumbled",
            "1\u2009\u00bd pounds cooking apples, peeled and roughly chopped",
            "salt and freshly ground black pepper to taste",
            "2 tablespoons mango chutney",
            "\u00bd lemon, juiced",
            "\u00bd cup plain yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large pot over medium-high heat. Cook onion in melted butter until softened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix coriander, garam masala, cumin, and turmeric with the onion; cook until fragrant, about 1 minute. Stir flour into the onion mixture; cook until the flour darkens, about 1 minute more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour water into the pot. Dissolve bouillon in the water. Add apples and season with salt and freshly ground black pepper. Bring the mixture to a boil, stirring occasionally, reduce heat to medium-low, and simmer until the apples are tender, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chutney and lemon juice into the soup; remove pot from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup into a blender no more than half full. Cover and hold lid in place with a towel. Pulse blender a few times before leaving on to blend. Puree in batches until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse the pot clean, pour pureed soup into the pot, and heat over medium heat just until it begins to boil. Turn heat off completely. Season soup with salt and pepper. Ladle soup into bowls; top each serving with a dollop of yogurt to serve.\n"
            }
        ],
