    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233492/cold-cucumber-cream-soup/",
        "name": "Cold Cucumber Cream Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1304912.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2019-04-15T04:40:01.000Z",
        "description": "A cool and refreshing soup for a summer evening. Serve it as an appetizer to a great meal or by itself for lunch.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "1 large sweet onion, peeled and chopped",
            "6 large  cucumbers - peeled, seeded, and cut into pieces",
            "4 cups chicken stock",
            "2 tablespoons dill weed",
            "salt and ground black pepper to taste",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a heavy saucepan over medium heat. Add onion; cook and stir until soft, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cucumbers, chicken broth, dill weed, salt, and pepper into onions. Cover saucepan and simmer for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cucumber soup mixture into a blender no more than half full. Cover and hold lid down; pulse a few times before leaving on to blend. Puree in batches until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour pureed soup into a container, and whisk in heavy cream. Season with salt and ground black pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate soup until completely chilled, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Cream Soup Recipes"
