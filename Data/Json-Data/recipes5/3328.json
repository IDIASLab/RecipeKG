    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26045/pennsylvania-dutch-pound-cake/",
        "name": "Pennsylvania Dutch Pound Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pennsylvania Dutch Pound Cake"
        },
        "datePublished": "2019-07-10T06:52:52.000Z",
        "description": "A rich pound cake made with confectioners' sugar. Tastes best without frosting!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 - 10 inch tube pan",
        "recipeIngredient": [
            "1 cup butter",
            "1 (16 ounce) package confectioners' sugar",
            "4 egg yolks",
            "2 teaspoons lemon juice",
            "1 teaspoon vanilla extract",
            "1 cup milk",
            "3 cups cake flour",
            "1 teaspoon baking powder",
            "4 egg whites"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour a 10 inch tube pan. Sift together the flour and baking powder. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and sugar until light and fluffy. Beat in the yolks one at a time, then stir in the lemon juice and vanilla. Beat in the flour mixture alternately with the milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal mixing bowl, beat egg whites until stiff peaks form. Fold 1/3 of the whites into the batter, then quickly fold in remaining whites until no streaks remain. Pour batter into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 75 minutes, or until a toothpick inserted into the center of the cake comes out clean. Let cool in pan for 10 minutes, then turn out onto a wire rack and cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
