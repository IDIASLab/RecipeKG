    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274023/oven-roasted-cajun-shrimp/",
        "name": "Oven-Roasted Cajun Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Oven-Roasted Cajun Shrimp"
        },
        "datePublished": "2019-06-14T03:27:51.000Z",
        "description": "Delicious dish with shrimp, andouille sausage, corn, and potatoes.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package small red potatoes, or to taste",
            "4 ears fresh corn, halved",
            "4 pounds large shrimp, peeled and deveined",
            "1 (12 ounce) package andouille sausage, sliced, or to taste",
            "1 stick butter",
            "\u00bc cup Worcestershire sauce",
            "\u00bc cup soy sauce",
            "\u00bc cup olive oil",
            "2 cloves garlic, minced",
            "2 tablespoons dried oregano",
            "1 teaspoon salt",
            "\u00bd teaspoon hot sauce (such as Tabasco\u00ae), or to taste",
            "1 pinch cayenne pepper, or to taste",
            "4 lemons, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 500 degrees F (260 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium to medium-low and simmer until barely tender, 10 to 12 minutes. Stir in corn during the last 5 minutes. Drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange shrimp, sausage, potatoes, and corn in a single layer in a large 4-inch-deep roasting pan. Cut butter into slices and place on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Worcestershire sauce, soy sauce, olive oil, garlic, oregano, salt, hot sauce, and cayenne in a small bowl. Pour sauce over the shrimp mixture. Top with lemon slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until shrimp turns pink, basting and turning often, about 10 minutes.\n"
            }
        ],
