    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/45470/pumpkin-polenta-cake/",
        "name": "Pumpkin Polenta Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F343326.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T04:57:44.000Z",
        "description": "The grainy texture of polenta and the marriage of flavours of pumpkin and sweet spices create a unique versatile loaf. Can also be made with cooked sweet potato or butternut squash instead of pumpkin. Serve warm or cold with savoury or sweet dishes.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 8-inch baking dish",
        "recipeIngredient": [
            "2 cups canned pumpkin puree",
            "2 tablespoons butter, room temperature",
            "2 eggs, lightly beaten",
            "4 egg whites, lightly beaten",
            "\u00bd cup brown sugar",
            "\u00bd teaspoon baking soda",
            "1 teaspoon salt",
            "2 teaspoons ground cinnamon",
            "1 teaspoon ground nutmeg",
            "1 teaspoon ground cloves",
            "1 cup low-fat plain yogurt",
            "2 cups polenta or yellow cornmeal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease an 8-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a blender or food processor, blend pumpkin, butter, eggs, and egg whites. Mix in the brown sugar, baking soda, salt, cinnamon, nutmeg, and cloves. Blend until smooth, and transfer to a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix yogurt and polenta into the pumpkin mixture. Pour into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until a toothpick inserted in the center comes out clean, about 45 minutes. Cool slightly before slicing.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
