        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12083/easy-cream-cheese-pie/",
        "name": "Easy Cream Cheese Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1090172.jpg",
            "width": 1920,
            "height": 1440
        },
        "datePublished": "1998-03-08T01:33:10.000Z",
        "description": "This recipe was given to me by a friend.  It has never failed me. Strawberry or blueberry pie filling may be used in place of cherry.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT2H20M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese",
            "1 (14 ounce) can sweetened condensed milk",
            "\u2153 cup lemon juice",
            "2 cups cherry pie filling",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together cream cheese and condensed milk until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add lemon juice and blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into graham cracker crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with fruit pie filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill for 2 hours before serving.\n"
            }
        ],
