        "mainEntityOfPage": "https://www.allrecipes.com/recipe/63096/sweet-carrot-salad/",
        "name": "Sweet Carrot Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2457392.jpg",
