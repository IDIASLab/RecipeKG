        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78014/curried-salmon-pasta-salad/",
        "name": "Curried Salmon Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F39034.jpg",
