        "recipeYield": "8 servings",
        "recipeIngredient": [
            "5 cups cubed whole wheat bread",
            "\u00bd cup extra-virgin olive oil",
            "\u00bc cup red wine vinegar",
            "\u00bc cup lemon juice",
            "\u00bc cup chopped fresh parsley",
            "\u00bd teaspoon salt",
            "6 large tomatoes, cut in wedges",
            "1 large red onion, diced",
            "\u00bd cup black olives",
            "\u00bd cup fresh basil",
            "\u00bd cup shaved Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spread bread cubes over a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake bread cubes in the preheated oven until lightly browned, about 15 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, vinegar, lemon juice, parsley, and salt in a large bowl. Fold in toasted bread, tomatoes, onion, olives, basil, and Parmesan cheese until vinegar dressing is evenly distributed.\n"
            }
        ],
