    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277803/crunchy-chicken-tenders/",
        "name": "Crunchy Chicken Tenders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7720445.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-01-30T01:21:42.000Z",
        "description": "This chicken is dipped in crushed cereal, then baked. A delicious alternative to fried chicken with a great crunch!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 tenders",
        "recipeIngredient": [
            "cooking spray",
            "2 tablespoons all-purpose flour",
            "1 pinch dried basil",
            "1 pinch chili powder",
            "salt and ground black pepper to taste",
            "1 egg",
            "1 tablespoon water",
            "3 cups crushed cornflakes",
            "6 skinless, boneless chicken tenders"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Coat a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, basil, chili powder, salt, and pepper together in a bowl. Mix egg and water together in another bowl. Pour crushed cereal into a third bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each tender in the flour mixture first, shaking off excess. Dip in egg and then in crushed cereal. Place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned and no longer pink in the centers, 10 to 12 minutes on each side.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Baked and Roasted"
        ],
