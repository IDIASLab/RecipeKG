    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220218/moms-halibut-enchiladas/",
        "name": "Mom's Halibut Enchiladas",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mom's Halibut Enchiladas"
        },
        "datePublished": "2019-07-10T07:03:03.000Z",
        "description": "My mom from Alaska brings us delicious fresh halibut several times a year, and this is my favorite recipe for using it.  It's rich, creamy, and wonderful!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds halibut",
            "\u00bd teaspoon seafood seasoning (such as Old Bay\u00ae)",
            "1 tablespoon lemon juice",
            "1 teaspoon cold butter, thinly sliced",
            "1 cup mayonnaise",
            "1 cup sour cream",
            "1 cup cream cheese, softened",
            "2 (4 ounce) cans diced green chiles",
            "2 green onions, chopped",
            "1 teaspoon chopped fresh parsley",
            "salt and ground black pepper to taste",
            "8 (8 inch) flour tortillas",
            "1 cup heavy cream, or as needed",
            "1 cup shredded mozzarella cheese",
            "\u00bc cup Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place halibut in a 9x13 inch baking dish. Sprinkle with seafood seasoning and lemon juice. Dot halibut with butter slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fish flakes easily with a fork, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pan from oven and cool halibut slightly. Flake into bite sized pieces in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mayonnaise, sour cream, and cream cheese in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully stir in flaked halibut, diced green chiles, green onions, parsley, salt, and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon filling onto tortillas in a line down the center, and roll them around the filling into a cylinder shape.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a 9x15 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place rolled tortillas seam sides down in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour heavy cream over rolled tortillas, using as much as needed to cover.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with mozzarella and Parmesan cheeses.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is browned and bubbling, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Latin American",
            "Mexican"
        ],
