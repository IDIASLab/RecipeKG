    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75177/ryans-favorite-beans/",
        "name": "Ryan's Favorite Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F839075.jpg",
            "width": 400,
            "height": 265
        },
        "datePublished": "2020-03-05T04:58:49.000Z",
        "description": "My 17 year old has always loved this recipe, so it's been a mainstay in my family for many years. A great summer potluck dish.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "6 slices bacon",
            "1 pound ground beef",
            "1\u2009\u00bd cups chopped onion",
            "1 (31 ounce) can pork and beans, drained",
            "1 (31 ounce) can pork and beans, with liquid",
            "2 (15 ounce) cans kidney beans, drained",
            "1 (15 ounce) can lima beans, drained",
            "1\u2009\u00bd cups barbeque sauce",
            "\u2153 cup packed brown sugar",
            "2 tablespoons cider vinegar",
            "1 teaspoon liquid smoke flavoring",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a Dutch oven. Cook over medium high heat until evenly brown. Drain, crumble, and set aside. Place the ground beef and onion in the Dutch oven. Cook until beef is evenly brown and onion is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return bacon to the Dutch oven. Mix in the drained pork and beans, pork and beans with liquid, kidney beans, and lima beans. Stir in the barbeque sauce, brown sugar, vinegar, and liquid smoke. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover Dutch oven, and bake 1 hour in the preheated oven.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Beans and Peas",
            "Baked Bean Recipes"
        ],
