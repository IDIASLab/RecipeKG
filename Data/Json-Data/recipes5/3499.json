        "mainEntityOfPage": "https://www.allrecipes.com/recipe/45934/fresh-tuna-ceviche/",
        "name": "Fresh Tuna Ceviche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F613607.jpg",
            "width": 499,
            "height": 333
