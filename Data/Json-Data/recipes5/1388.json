    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254665/roasted-poblano-corn-chowder/",
        "name": "Roasted Poblano Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4470058.jpg",
            "width": 2000,
            "height": 2000
        },
        "datePublished": "2019-05-08T06:29:33.000Z",
        "description": "This spicy corn chowder is perfect on a chilly day! Serve garnished with sour cream and shredded cheese.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 potatoes, peeled and cut into small cubes, or more to taste",
            "4 poblano peppers, halved and seeded, or more to taste",
            "2 tablespoons olive oil, divided",
            "3 ears fresh corn, shucked, or more to taste",
            "salt to taste",
            "\u00bd onion, chopped",
            "2 cloves garlic, minced",
            "1 (14.75 ounce) can creamed corn",
            "1 (14.5 ounce) can chicken broth",
            "\u00bc cup finely chopped cilantro",
            "1 teaspoon ground cumin",
            "1 cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes in a saucepan; cover with water. Bring to a boil; reduce heat and simmer until tender, 15 to 20 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place poblano peppers cut-side down in a baking dish. Drizzle 1 tablespoon olive oil on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake poblano peppers until skin starts to blacken and blister, about 15 minutes. Allow peppers to cool until easily handled, about 10 minutes. Remove and discard skins; coarsely chop peppers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut corn kernels off the cobs. Spread on a greased baking sheet; sprinkle with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake corn kernels in the preheated oven, stirring every 5 minutes, until tender, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining 1 tablespoon olive oil in a saucepan over medium heat. Add onion and garlic; cook, stirring frequently, until onion is translucent, about 5 minutes. Stir in chopped poblano peppers, creamed corn, chicken broth, cilantro, and cumin. Blend soup with an immersion blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring soup to a gentle boil. Stir in potatoes, baked corn kernels, and milk. Season with salt. Simmer until flavors combine, 5 to 10 minutes.\n"
            }
        ],
