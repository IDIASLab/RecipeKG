    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143361/petes-kicked-up-japanese-wings/",
        "name": "Pete's Kicked-Up Japanese Wings",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4567598.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-03-05T05:04:51.000Z",
        "description": "This is one of my favorite recipes for wings. Serve over white rice with remaining sauce spooned on top. Please try it and tell me what you think!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H25M",
        "totalTime": "P0DT4H45M",
        "recipeYield": "2 pounds chicken wings",
        "recipeIngredient": [
            "1 cup Japanese soy sauce",
            "1 cup tightly packed brown sugar",
            "\u00bd cup pineapple juice",
            "\u00bc cup sake",
            "\u00bc cup rice vinegar",
            "8 cloves garlic, crushed",
            "1 tablespoon grated ginger",
            "1 teaspoon monosodium glutamate (MSG)",
            "\u00bd teaspoon Korean red pepper flakes",
            "\u00bd teaspoon toasted sesame seeds",
            "2 pounds chicken wings"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine soy sauce, brown sugar, pineapple juice, sake, rice vinegar, garlic, ginger, monosodium glutamate, red pepper flakes, and sesame seeds in a small saucepan. Bring to a simmer; cook for 10 minutes. Remove sauce from heat and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut wings at the joint to make drumettes. Transfer to a large resealable plastic bag. Pour in sauce. Marinade in the refrigerator, at least 3 hours and up to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate. Remove chicken wings from sauce; grill until lightly browned, about 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauce into a saucepan. Bring to a simmer; cook for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Arrange chicken wings in a single layer in a large baking dish; pour sauce on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, turning and basting frequently, until wings are browned, about 1 hour.\n"
            }
        ],
