        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20645/delicious-apple-sauce/",
        "name": "Delicious Apple Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F48407.jpg",
