        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7252/fruitcake-without-citron/",
        "name": "Fruitcake Without Citron",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fruitcake Without Citron"
        },
        "datePublished": "1998-03-05T14:39:56.000Z",
        "description": "Some people do not like the citron found in fruitcakes, so they might like this one. You may substitute pecans if you prefer.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 10 x 14 inch tube cake",
        "recipeIngredient": [
            "3 cups pitted dates",
            "3 cups candied pineapple chunks",
            "3 cups red and green candied cherries",
            "8 cups walnut halves",
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "4 eggs",
            "\u00bd cup dark corn syrup",
            "\u00bc cup packed brown sugar",
            "\u00bc cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 10 x 14 inch pan; line with waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix fruits and nuts in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs, corn syrup, sugar and oil in a medium size bowl.  Gradually beat in dry ingredients.  Pour over fruit mixture and mix.  Firmly pack into pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at  275 degrees F (135 degree C) about 2 hours and 15 minutes, or until top appears dry.  Cool in pan.\n"
            }
        ],
