    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276544/shepherds-pie-with-ground-lamb-and-sweet-potatoes/",
        "name": "Shepherd's Pie with Ground Lamb and Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8138794.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-10-17T10:57:04.000Z",
        "description": "Yams mixed with tahini and green onions top this special shepherd's pie.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 large white sweet potatoes",
            "1 pound ground lamb",
            "1 onion, finely chopped",
            "\u00bc cup finely chopped leek",
            "\u00bc cup finely chopped celery",
            "1 (16 ounce) can tomato sauce (such as Pom\u00ec)",
            "1 (15 ounce) can chickpeas, drained",
            "1 teaspoon ground cumin, or more to taste",
            "salt and ground black pepper to taste",
            "\u00bd cup tahini",
            "\u00bd cup finely chopped green onions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Prick sweet potatoes several times with a fork and place onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until easily pierced with a fork, 50 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In the meantime, heat a large skillet over medium-high heat. Cook and stir lamb in the hot skillet until browned and crumbly, 5 to 7 minutes. Transfer to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add onion, leek, and celery to the skillet and saute until tender, 4 to 5 minutes. Stir in tomato sauce, chickpeas, cumin, salt, and pepper. Reduce heat; simmer 5 to 10 minutes. Return lamb to the skillet and simmer for 1 to 2 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 325 degrees F (165 degrees C). Pour lamb mixture into a baking pan. Mash the sweet potato in a bowl, stir in tahini and green onions, and spread on top of the lamb.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling throughout and top is lightly browned, about 40 minutes.\n"
            }
        ],
