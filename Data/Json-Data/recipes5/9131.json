        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 Granny Smith apples, peeled and cored",
            "3 tablespoons minced rosemary",
            "2 tablespoons cinnamon sugar",
            "2 tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut each apple into 16 wedges and place in a microwave-safe dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle apples with rosemary, cinnamon sugar, and water. Stir the apples and cover the dish. Microwave on high until tender, about 4 minutes, stirring halfway through.\n"
            }
