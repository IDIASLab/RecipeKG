        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12649/brown-sugar-pie-i/",
        "name": "Brown Sugar Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4571409.jpg",
            "width": 4032,
            "height": 2268
        },
        "datePublished": "1999-09-12T22:10:06.000Z",
        "description": "Brown sugar pie is a basic pie from Quebec, Canada.  In French, it is Tarte au Sucre Brun.  Delicious!!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 Pie",
        "recipeIngredient": [
            "6 tablespoons all-purpose flour",
            "2 cups packed brown sugar",
            "1\u2009\u00bd cups evaporated milk",
            "4 tablespoons butter",
            "\u00bd teaspoon salt",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine flour and sugar. Stir in milk, butter, salt and vanilla. Cook, stirring constantly, until mixture comes to a boil. Pour into an unbaked pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (200 degrees C) for 5 minutes. Reduce heat to 350 degrees F (175 degrees C) and continue baking for 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Vintage Pie Recipes",
            "Chess Pie Recipes"
        ],
