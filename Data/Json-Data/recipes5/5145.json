        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220880/cheezy-flax-crackers/",
        "name": "Cheezy Flax Crackers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F821709.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "2019-07-10T07:03:10.000Z",
        "description": "This is a raw vegan recipe for crackers. These crackers were inspired by one of my childhood favorites, 'Cheez-Its&reg;'.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT18H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 cups ground flaxseed",
            "1 cup cashews",
            "1 red bell pepper, halved and deseeded",
            "1 carrot",
            "1 clove garlic",
            "1 lemon, juiced",
            "\u00bd teaspoon sea salt",
            "\u00bd teaspoon agave nectar, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree flaxseed, cashews, red bell pepper, carrot, garlic, lemon juice, sea salt, and agave nectar in a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mixture onto nonstick food dehydrator sheets and place into food dehydrator following manufacturer's instructions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dehydrate until dry and crisp, about 18 hours. Flip crackers on sheets after 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Break sheets into pieces for serving.\n"
            }
        ],
