    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8109/shoo-fly-cake/",
        "name": "Shoo Fly Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F38866.jpg",
            "width": 600,
            "height": 482
        },
        "datePublished": "1999-01-07T18:59:59.000Z",
        "description": "A variation on the old Pennsylvania Dutch favorite, Shoo Fly Pie. Not as sweet or gooey as Shoo Fly Pie, this cake is much better the day after you bake it. Works really well as cupcakes.  I made mini cupcakes for Christmas, always a hit.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "9 x 13 inch sheet cake",
        "recipeIngredient": [
            "1 teaspoon baking soda",
            "2\u2009\u00bc cups boiling water",
            "4 cups all-purpose flour",
            "1 pound brown sugar",
            "2 teaspoons salt",
            "\u00be cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease one 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve soda in boiling water. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together flour, brown sugar, salt, and vegetable oil to form a crumb-like mixture.  Reserve 1 cup of the crumbs.  Mix the remaining crumbs with the water and soda mixture.  Pour batter into the prepared pan.  Sprinkle the reserved crumbs on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 minutes, or until done.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Sheet Cake Recipes"
        ],
