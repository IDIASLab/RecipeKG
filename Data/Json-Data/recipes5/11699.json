        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228129/classic-savory-deviled-eggs/",
        "name": "Classic Savory Deviled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3275386.jpg",
