    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274715/rib-tips-and-kraut/",
        "name": "Rib Tips and Kraut",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rib Tips and Kraut"
        },
        "datePublished": "2019-07-30T23:58:11.000Z",
        "description": "Baking the rib tips in sauerkraut really tenderizes the meat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H10M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "3 pounds rib tips",
            "1 tablespoon salt, or to taste",
            "1 tablespoon chili powder, or to taste",
            "1 teaspoon garlic powder, or to taste",
            "1 teaspoon onion powder, or to taste",
            "1 (16 ounce) package sauerkraut, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix salt, chili powder, garlic powder, and onion powder together in a bowl. Rub spices on ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sear rib tips on the preheated grill until spices start turning black, 3 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauerkraut into a large baking pan; mix in grilled rib tips. Cover tightly with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until ribs are tender, about 2 hours. Remove from the oven, stir, and let sit for 5 to 10 minutes.\n"
            }
        ],
