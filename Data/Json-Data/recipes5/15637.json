        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/184290/erins-pico-de-gallo/",
        "name": "Erin's Pico de Gallo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F451579.jpg",
