        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/217205/betty-tarts/",
        "name": "Betty Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4802274.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T05:07:59.000Z",
        "description": "Best butter tarts that everyone just loves. People order dozens from me.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 tarts",
        "recipeIngredient": [
            "12 (2 inch) unbaked tart shells",
            "1 cup golden brown sugar",
            "\u00bc cup butter, softened",
            "1 cup light corn syrup",
            "3 eggs",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place the tart shells onto a baking sheet lined with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, mash the brown sugar and butter until thoroughly combined, then mix in corn syrup, eggs, and salt to make a smooth mixture. Spoon into the tart shells, filling each one about 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown and the filling is set, 25 to 30 minutes.\n"
            }
        ],
