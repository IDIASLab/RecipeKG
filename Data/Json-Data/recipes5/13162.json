    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277540/broccoli-potato-and-tofu-casserole/",
        "name": "Broccoli, Potato, and Tofu Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Broccoli, Potato, and Tofu Casserole"
        },
        "datePublished": "2020-01-04T02:43:37.000Z",
        "description": "Easy blend of broccoli, potatoes, and tofu for a cold winter day. Serve with Sriracha sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (16 ounce) package firm tofu",
            "2 tablespoons olive oil",
            "1\u2009\u00bd pounds baby yellow potatoes (such as Trader Joe's\u00ae Dutch Yellow\u00ae), quartered",
            "4 cloves garlic, chopped",
            "1 head broccoli, cut into florets",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place tofu onto a plate and place another plate on top. Set a 3- to 5-pound weight on top. Press tofu for 20 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, pour olive oil in the bottom of a casserole dish. Add potatoes and garlic; toss to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While potatoes are baking, drain and discard the accumulated liquid from the tofu. Chop tofu into small cubes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove potatoes from the oven and stir in broccoli and tofu cubes. Bake for 10 minutes. Mix in Cheddar cheese and bake until cheese has melted, about 10 minutes more.\n"
            }
        ],
