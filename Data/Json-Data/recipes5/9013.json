        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23107/quick-pepperoni-bread/",
        "name": "Quick Pepperoni Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F316361.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-23T22:01:04.000Z",
        "description": "Pepperoni and Muenster are baked together in this yummy appetizer recipe that may be cut into bite-sized pieces, or used as a side dish. It tastes best when served warm.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1 egg",
            "\u00bd (8 ounce) package pepperoni sausage, diced",
            "1 cup milk",
            "\u00be cup all-purpose flour",
            "1 (6 ounce) package shredded Muenster cheese",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease an 8x8 inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg in a medium bowl. Mix in the pepperoni, milk, flour, Muenster cheese, salt and pepper. Pour into the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 30 minutes, or until lightly browned.\n"
            }
        ],
