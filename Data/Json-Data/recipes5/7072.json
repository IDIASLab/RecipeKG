        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258416/mango-nectarine-salsa/",
        "name": "Mango Nectarine Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mango Nectarine Salsa"
        },
        "datePublished": "2019-07-10T07:10:56.000Z",
        "description": "Feel free to play around with other fruit combinations! With this blending of sweet and spicy, you can't go wrong! Eat hot if desired.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1\u2009\u00bd very ripe tomatoes, finely chopped",
            "1 mango, finely chopped",
            "1 nectarine, finely chopped",
            "\u00be teaspoon finely chopped red onion",
            "3 cloves garlic, minced, or more to taste",
            "1 tablespoon lemon juice",
            "1 tablespoon dried basil",
            "1 teaspoon lime juice",
            "\u00bc teaspoon red pepper flakes",
            "salt and ground black pepper to taste",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, mango, nectarine, red onion, and garlic in a large bowl. Stir in lemon juice, basil, lime juice, red pepper flakes, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a skillet lightly with cooking spray. Pour in tomato mixture; cook, stirring occasionally, until softened, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate salsa for 30 minutes before serving.\n"
            }
        ],
