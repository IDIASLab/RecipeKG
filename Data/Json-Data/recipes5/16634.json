    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10446/molasses-cookies-ii/",
        "name": "Molasses Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4574080.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "1998-02-03T20:20:24.000Z",
        "description": "Soft, easy molasses cookies.  Great for kids to help make!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00be cup shortening",
            "1 cup packed brown sugar",
            "1 egg",
            "\u00bd cup light molasses",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon salt",
            "2 teaspoons baking soda",
            "\u00bd teaspoon ground cloves",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground ginger"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together shortening and brown sugar.  Stir in egg and molasses and mix well. Fold in dry ingredients and stir.  Cover and chill until firm (1 - 2 hours).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into small balls (I use a small melon baller to help with this) and roll in white sugar.  Place on lightly greased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 9 - 10 minutes.  Leave on cookie sheet one minute until set.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Molasses Cookie Recipes"
        ],
