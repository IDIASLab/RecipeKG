    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278481/mexican-hotdish/",
        "name": "Mexican Hotdish",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mexican Hotdish"
        },
        "datePublished": "2020-02-19T06:33:29.000Z",
        "description": "This is a Midwestern version of Mexican lasagna. It's easy to prepare and even easier to tailor to your taste. Alter this by using different types of Doritos&reg;, chiles, and by adding additional seasoning to spice it up. Serve with pico de gallo and sour cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 8-inch casserole",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 small onion, diced",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "\u00bd (10 ounce) can enchilada sauce",
            "1 (4 ounce) can diced green chiles",
            "\u00bd teaspoon ground cumin",
            "1 (12 ounce) bag nacho cheese-flavored corn chips (such as Doritos\u00ae), crushed",
            "2 cups shredded Cheddar cheese",
            "aluminum foil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large nonstick skillet over medium-high heat. Add ground beef and onion to the hot skillet; cook and stir until beef is browned and crumbly and onions are translucent, 5 to 7 minutes. Drain and discard grease. Add condensed soup, enchilada sauce, chiles, and cumin until combined. Remove from the heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place crushed chips in the bottom of an 8-inch square baking pan. Pour beef mixture onto the chips and top with shredded Cheddar cheese. Cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until filling is bubbly and cheese is melted, about 20 minutes.\n"
            }
        ],
