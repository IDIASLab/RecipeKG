    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268508/vegan-stuffed-mushrooms/",
        "name": "Vegan Stuffed Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5827106.jpg",
            "width": 2048,
            "height": 1463
        },
        "datePublished": "2019-04-04T04:14:16.000Z",
        "description": "These vegan stuffed mushrooms are filled with cashew cream, finely chopped mushroom stems, and nutritional yeast.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "12 mushrooms",
        "recipeIngredient": [
            "1 cup raw cashews",
            "1 cup water, or more as needed",
            "12 whole fresh mushrooms",
            "cooking spray",
            "1 tablespoon vegetable oil",
            "1 tablespoon minced garlic",
            "\u00bc cup nutritional yeast",
            "1 teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bc teaspoon onion powder",
            "\u00bc teaspoon ground cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak cashews in water for 1 to 2 hours. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend cashews with 1 cup water until creamy. Add 1 to 2 tablespoons more water if needed to blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Clean mushrooms with a damp paper towel. Carefully break off stems and reserve the caps. Chop stems very finely, discarding tough ends.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat. Add chopped mushroom stems and garlic to the skillet. Fry until any moisture has disappeared, taking care not to burn garlic, about 3 minutes. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cashew cream, nutritional yeast, salt, black pepper, onion powder, and cayenne pepper into the cooled mushroom mixture. Use a little spoon to fill each mushroom cap with a generous amount of stuffing. Arrange the mushroom caps on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until mushrooms are piping hot and liquid starts to form under the caps, about 20 minutes.\n"
            }
        ],
