    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264215/king-salmon-with-mushrooms/",
        "name": "King Salmon with Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7563463.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T07:11:56.000Z",
        "description": "We tried this king salmon dish and our daughter loved it, and she is a picky eater. Hope you enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 pounds salmon",
        "recipeIngredient": [
            "3 tablespoons olive oil",
            "2 tablespoons garlic-pepper seasoning (such as McCormick\u00ae)",
            "2 tablespoons chopped fresh chives",
            "1 tablespoon lemon juice",
            "1 clove garlic, minced",
            "1 pinch sea salt",
            "1 pinch ground black pepper",
            "3 pounds Alaskan king salmon",
            "5 large mushrooms, cut into 1/4-inch pieces",
            "\u00bc cup butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine olive oil, garlic seasoning, chives, lemon juice, garlic, salt, and pepper in a medium bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place salmon on a baking sheet and coat with the garlic mixture; reserve any remaining mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fish flakes easily with a fork, 20 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While salmon bakes, heat a large skillet over medium to medium-high heat. Add mushrooms and the rest of the garlic mixture. Cook and stir until mushrooms shrink slightly, 3 to 5 minutes. Add butter; cook and stir until mushrooms are mostly tender, 2 to 3 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and turn the oven to broil. Add mushrooms to the baking sheet with the cooked salmon and broil until golden brown, 5 to 10 minutes.\n"
            }
        ],
