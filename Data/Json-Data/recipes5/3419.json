        "mainEntityOfPage": "https://www.allrecipes.com/recipe/35607/avocado-dressing-i/",
        "name": "Avocado Dressing I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F183583.jpg",
