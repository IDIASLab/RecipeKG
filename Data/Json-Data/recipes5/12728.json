    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283249/onion-tamarind-chutney/",
        "name": "Onion Tamarind Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9118055.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-12-20T09:50:58.000Z",
        "description": "This is a delicious South Indian chutney traditionally served with idlis or dosa, the famous Indian rice pancake.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "2 teaspoons oil, divided",
            "1 teaspoon cumin seeds",
            "1 teaspoon coriander seeds",
            "5 small fresh red chile peppers",
            "2 medium onion, chopped",
            "8 cloves garlic, minced",
            "2 medium tomatoes, chopped",
            "1 tablespoon tamarind paste",
            "1 pinch ground turmeric",
            "1 tablespoon water, or more as needed",
            "\u00bd teaspoon mustard seed",
            "salt to taste",
            "6 curry leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon oil in a skillet and add cumin seeds and coriander seeds. Add red chile peppers and fry for 1 minute. Add onion and saute until soft and translucent, about 3 to 5 minutes. Add garlic and saute until it turns color, about 1 minute. Add tomato and cook until soft, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in tamarind paste and turmeric powder and mix well. Cook for 1 minute. Remove from heat and cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer skillet contents to a blender and season with salt. Add water as needed and blend everything into a smooth paste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining 1 teaspoon oil to a skillet and add mustard seeds and curry leaves. Cook until mixture splutters and pour tempering over chutney. Mix to combine.\n"
            }
        ],
