        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15932/berry-valentine-pie/",
        "name": "Berry Valentine Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Berry Valentine Pie"
        },
        "datePublished": "2000-01-13T22:54:15.000Z",
        "description": "This pie is filled with healthy berries, whipped cream and graham crackers! Use fat-free whipped cream to lower the calories, if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 (9 inch) prepared graham cracker crust",
            "1 (16 ounce) package frozen strawberries, thawed and drained",
            "3 (7 ounce) cans whipped cream",
            "1 (16.5 ounce) can pitted dark sweet cherries",
            "1 (15 ounce) can blueberries",
            "2 graham crackers, crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "On bottom of pie crust, spread strawberries evenly; cover with whipped cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread cherries on whipped cream and cover cherries with whipped cream layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread blueberries on top of whipped cream and cover with last layer of whipped cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle crumbled crackers on top of pie and place in the refrigerator for 45 minutes; serve.\n"
            }
        ],
