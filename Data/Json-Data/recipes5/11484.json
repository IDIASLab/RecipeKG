        "mainEntityOfPage": "https://www.allrecipes.com/recipe/89712/orange-romaine-salad/",
        "name": "Orange Romaine Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F67566.jpg",
