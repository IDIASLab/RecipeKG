        "name": "Easy Cajun Bloody Mary",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5272961.jpg",
            "width": 3254,
            "height": 3254
