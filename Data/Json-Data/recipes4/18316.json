    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255253/og-zuppa-toscana-soup/",
        "name": "\"OG\" Zuppa Toscana Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4461109.jpg",
            "width": 3015,
            "height": 2077
        },
        "datePublished": "2019-04-04T04:06:59.000Z",
        "description": "This is exactly like the soup at 'OG.'",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H44M",
        "totalTime": "P0DT1H4M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound ground Italian sausage",
            "1\u2009\u00bd teaspoons red pepper flakes",
            "1 large white onion, diced",
            "4 strips bacon, diced",
            "2 teaspoons garlic puree",
            "10 cups water",
            "5 cubes  chicken bouillon",
            "1 pound russet potatoes, diced",
            "1 cup heavy whipping cream",
            "\u00bc bunch kale, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sausage and red pepper flakes in a skillet over medium heat; cook and stir until sausage is brown and crumbled, 7 to 10 minutes. Drain excess fat; place in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook onion, bacon, and garlic in the same skillet over medium-low heat until bacon is crisp and onions are soft, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water and chicken bouillon cubes in a large pot; bring to a boil. Stir in potatoes; cook and stir until soft, about 15 minutes. Pour in cream; simmer until heated through, about 2 minutes. Add sausage, onion mixture, and kale; cook and stir until flavors combine, about 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Pork Soup Recipes"
        ],
