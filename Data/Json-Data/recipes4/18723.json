    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257616/split-pea-quinoa-soup/",
        "name": "Split Pea Quinoa Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Split Pea Quinoa Soup"
        },
        "datePublished": "2019-04-04T04:08:20.000Z",
        "description": "A hearty split pea soup with veggies, quinoa, and tofu. Serve with chunks of bread.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H17M",
        "totalTime": "P0DT1H47M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "2 cups diced extra-firm tofu",
            "2 carrots, diced",
            "1 stalk celery, diced",
            "\u2153 cup chopped broccoli florets",
            "\u00bc head red cabbage, chopped",
            "3 cloves garlic, minced, or more to taste",
            "2 tablespoons dried parsley",
            "2 tablespoons dried savory",
            "1 tablespoon dried rosemary",
            "2 (32 fluid ounce) containers chicken stock",
            "1 pound dried split peas",
            "2 cups fresh spinach",
            "1 cup red quinoa",
            "1 ancho chile pepper (dried poblano pepper)",
            "1 dried cayenne pepper",
            "1 bay leaf",
            "water, or as needed",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large pot over medium heat. Add tofu; cook and stir until browned, about 5 minutes. Stir in carrots, celery, and broccoli; cook until starting to soften, about 5 minutes. Add cabbage, garlic, parsley, savory, and rosemary; cook until fragrant, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chicken stock into the pot. Add split peas, spinach, quinoa, whole ancho chile pepper, whole cayenne pepper, and bay leaf. Bring to a boil; reduce heat and simmer, stirring occasionally, until split peas soften and break down, about 1 hour. Add water if soup appears too thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season soup with salt and pepper. Discard ancho chile pepper, cayenne pepper, and bay leaf before serving.\n"
            }
        ],
