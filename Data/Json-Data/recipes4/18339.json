    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255351/spiced-paleo-cranberry-sauce/",
        "name": "Spiced Paleo Cranberry Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spiced Paleo Cranberry Sauce"
        },
        "datePublished": "2019-04-04T04:07:03.000Z",
        "description": "This delicious spiced paleo cranberry sauce ain't your ordinary cranberry sauce! Packed with aromatic spices and extra antioxidants, your health-conscious friends and family who have always kinda liked cranberry sauce will rave about this pretty holiday favorite--you may even get some converts! You can freeze for later use.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "4 quarts",
        "recipeIngredient": [
            "4 cups water",
            "8 cups whole fresh cranberries",
            "1 tablespoon minced fresh ginger root",
            "1 teaspoon ground cloves",
            "\u00bc teaspoon orange zest",
            "1 orange, juiced",
            "3 cups raw honey",
            "1 cup fresh blueberries",
            "\u00bc cup tart cherry juice concentrate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a large pot. Add cranberries. Simmer over medium heat, stirring regularly, until cranberries burst, about 10 minutes. Stir in ginger, cloves, and orange zest. Pour in orange juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave honey in a microwave-safe bowl until runny, about 30 seconds. Scrape into the pot with a spatula. Add blueberries. Simmer until flavors combine, about 20 minutes. Remove sauce from heat and stir in cherry juice concentrate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cranberry sauce to small bowls and chill until set, about 1 hour.\n"
            }
        ],
