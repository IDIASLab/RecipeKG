    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13069/cheapskate-stew/",
        "name": "Cheapskate Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1966605.jpg",
            "width": 4608,
            "height": 3072
        },
        "datePublished": "1999-01-09T00:57:44.000Z",
        "description": "A hearty beef vegetable stew from leftovers you have on hand. I love this recipe on a cold Texas winter nights.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 + servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds lean ground beef",
            "\u00bd onion, chopped",
            "3 stalks celery, chopped",
            "2 cups frozen mixed vegetables",
            "2 cups water",
            "3 carrots, chopped",
            "2 cups cooked elbow macaroni",
            "1 (15 ounce) can tomato sauce",
            "1\u2009\u00bd teaspoons Italian seasoning",
            "salt to taste",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Brown the hamburger meat with onion and celery. Drain (if it is fatty hamburger meat).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add water and carrots, then cook 'til carrots are beginning to get tender. The carrots will be crunchy unless you cook them a fairly long time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add leftover vegetables, macaroni, tomato sauce, Italian spices, and salt and pepper to taste, and simmer until the flavors blend and all ingredients are warm through.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Beef"
        ],
