        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222127/healthier-chocolate-covered-strawberries/",
        "name": "Healthier Chocolate Covered Strawberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F967583.jpg",
