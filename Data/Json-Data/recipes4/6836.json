    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234157/mock-a-roni/",
        "name": "Mock-a-Roni",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1053061.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "2019-04-02T07:01:37.000Z",
        "description": "This recipe was created when my baked macaroni and cheese loving husband asked for something more individualized to minimize leftovers. It has been a weekly staple for several years now and our 6 year old now prefers this to traditional baked macaroni.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "\u00bd cup orzo",
            "1\u2009\u00bd teaspoons butter",
            "1 cup shredded Cheddar cheese",
            "\u00bc cup milk",
            "1 egg, slightly beaten",
            "1\u2009\u00bd teaspoons Italian seasoning",
            "ground black pepper to taste",
            "\u00bc cup Italian-seasoned bread crumbs, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook orzo in the boiling water, stirring occasionally until cooked through but firm to the bite, about 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over low heat; stir Cheddar cheese into melted butter until cheese starts to melt, 2 to 3 minutes. Add milk, egg, Italian seasoning, and black pepper to cheese mixture; cook, stirring constantly, until cheese sauce is smooth and thickened, about 5 minutes. Stir orzo into cheese sauce until evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon orzo mixture into ramekins and top each serving with bread crumbs. Place ramekins in a shallow baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping begins to brown, about 30 minutes. Allow 'mock-a-roni' to cool enough to handle ramekins without potholders, about 5 minutes.\n"
            }
        ],
