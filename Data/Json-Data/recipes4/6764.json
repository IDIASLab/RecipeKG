    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233545/paleo-friendly-applesauce-cookie/",
        "name": "Paleo-Friendly Applesauce Cookie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1034478.jpg",
            "width": 1500,
            "height": 1000
        },
        "datePublished": "2019-04-02T07:01:24.000Z",
        "description": "Very little sugar, no dairy, and no grains or added fats. These are truly good and good for you!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "coconut oil",
            "2 cups unsweetened applesauce",
            "1\u2009\u00bd teaspoons baking soda",
            "3 cups almond flour",
            "\u00bd cup crystallized sugar cane juice (such as Sucanat\u00ae)",
            "\u00bd cup chopped pecans",
            "\u00bd cup dried cranberries",
            "2 eggs, beaten",
            "\u00bc cup coconut flour",
            "2 teaspoons ground cinnamon",
            "2 teaspoons cake spice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Line 2 baking sheets with parchment paper and lightly grease with coconut oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir applesauce and baking soda together in a large bowl until baking soda dissolves. Mix in almond flour, crystallized sugar cane juice, pecans, cranberries, eggs, coconut flour, ground cinnamon, and cake spice. Drop spoonfuls of the dough 2 inches apart onto prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
