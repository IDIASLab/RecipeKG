        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234855/cheaters-chicken-chili/",
        "name": "Cheater's Chicken Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060957.jpg",
            "width": 1564,
