            "\u00be cup milk (80 degrees F/27 degrees C)",
            "1\u2009\u00bd tablespoons vegetable oil",
            "6 tablespoons honey",
            "1\u2009\u00bd teaspoons salt",
            "\u00be cup oats",
            "1 cup oat flour",
            "1\u2009\u00bc cups wheat flour",
            "1\u2009\u00bc cups bread flour",
            "1\u2009\u00bd teaspoons active dry yeast"
