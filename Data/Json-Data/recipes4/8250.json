        "name": "Raw Super Food Oat Smoothie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3349106.jpg",
            "width": 2103,
            "height": 2106
