    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246309/camp-bread/",
        "name": "Camp Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Camp Bread"
        },
        "datePublished": "2016-02-16T19:45:31.000Z",
        "description": "A grilled flat bread with char finishes that serves as a substantial vessel for any dip on race day.  Fire up the grill, pick up a few lemons, limes, jalapenos and chiles then head to your garden for fresh herbs to create a winning combination of flavors.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "5 cups all-purpose flour, plus more for kneading",
            "\u00bc cup olive oil, plus more for brushing",
            "1 tablespoon kosher salt",
            "1 teaspoon cracked black pepper",
            "\u00bd ounce active dry yeast",
            "1\u2009\u2154 cups warm water",
            "1 tablespoon finely chopped fresh herbs such as rosemary or thyme",
            "1 jalapeno or other chile, seeded, chopped",
            "Lemons or limes for juicing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In an electric mixer with a dough hook, add flour, olive oil, kosher salt, and black pepper. Turn on mixer to medium speed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir yeast into warm water. Pour into flour mixture and mix until dough looks smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove dough from mixer and place on floured surface. Gently knead for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add finely chopped fresh herbs or chiles, if desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut dough into four equal parts. Wrap tightly with plastic wrap; refrigerate for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unwrap dough and place on well-floured surface. Dust dough with flour and flatten dough with hands.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let dough rest for about 10 minutes then stretch (as if it were pizza dough) pulling the dough out.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into rectangles then brush with olive oil; season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a medium hot grill, cook bread for 3 to 4 minutes per side. For citrus flavor, squeeze freshly squeezed lemon or lime juice over dough while it's grilling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve immediately.\n"
            }
        ],
