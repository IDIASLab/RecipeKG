    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246684/plum-lamb-casserole/",
        "name": "Plum Lamb Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Plum Lamb Casserole"
        },
        "datePublished": "2019-04-02T07:06:43.000Z",
        "description": "A family favorite. I don't even like chops and I like this recipe! I ran out of plum jam for this recipe once and used apricot instead. It still tasted good. I normally leave out the celery.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon butter",
            "1 onion, sliced",
            "2 tablespoons all-purpose flour",
            "salt and ground black pepper to taste",
            "2 pounds lamb shoulder chops, trimmed",
            "1 carrot, sliced",
            "\u00bd cup sliced celery",
            "\u00bd cup low-sodium vegetable stock",
            "\u00bc cup plum jam",
            "2 tablespoons tomato sauce",
            "2 teaspoons Worcestershire sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a large pot over medium heat until melted. Add onion; cook and stir until softened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season flour with salt and pepper in a shallow dish. Dredge lamb chops in flour mixture on both sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer lamb chops with onion, carrot, and celery in a casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix stock, plum jam, tomato sauce, and Worcestershire sauce together in a small bowl. Pour over lamb in the casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lamb is tender, about 1 hour 30 minutes.\n"
            }
        ],
