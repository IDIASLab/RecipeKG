        "mainEntityOfPage": "https://www.allrecipes.com/recipe/126770/easy-chicken-parmesan-sandwich/",
        "name": "Easy Chicken Parmesan Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F975104.jpg",
            "width": 822,
