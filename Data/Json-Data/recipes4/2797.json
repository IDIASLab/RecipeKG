    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/132705/orange-meltaway-cookies/",
        "name": "Orange Meltaway Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F216205.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:48:45.000Z",
        "description": "These cookies are unique in both flavor and texture. Because they are made with powdered sugar instead of granulated sugar, they have a texture that just melts in your mouth. One of our family favorites!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "140 small cookies",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon cream of tartar",
            "1 cup butter",
            "1\u2009\u00bd cups sifted confectioners' sugar",
            "1 teaspoon orange extract",
            "1 tablespoon grated orange zest",
            "1 beaten egg",
            "\u00bd cup confectioners' sugar, for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Line baking sheets with parchment paper, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the flour, baking soda, and cream of tartar in a bowl. In another bowl, beat the butter and 1 1/2 cups confectioners' sugar together with an electric mixer until light and fluffy. Mix in orange extract, grated orange zest, and egg. Stir in the flour mixture, and blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoon-sized portions of dough about 2 inches apart onto the parchment lined baking sheets. The cookies will spread out to about 1 1/2 inches when baked.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 10 to 14 minutes, until the cookies are light brown. Remove from the oven, and sift remaining confectioners' sugar onto the hot cookies. Allow to cool, and store in an airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
