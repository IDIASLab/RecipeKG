        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23186/ultimate-french-toast/",
        "name": "Ultimate French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3314610.jpg",
