    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222251/no-bean-veggie-burgers/",
        "name": "No Bean Veggie Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2251257.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2019-04-04T03:52:32.000Z",
        "description": "This versatile and delicious veggie burger mix can be made into patties or crumbled up and used in place of meat in other recipes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 medium potato, peeled and diced",
            "1 small onion, diced",
            "\u00bd cup chopped walnuts",
            "1 cup oats",
            "2 cups water",
            "2 tablespoons vegetable oil",
            "1 cup bread crumbs",
            "2 teaspoons salt",
            "2 tablespoons soy sauce",
            "1 teaspoon dried sage",
            "1 tablespoon vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pulse potato, onion, and walnuts in a food processor until finely chopped.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place oats and water in a large microwave safe bowl. Cook in the microwave on High until oats have absorbed the water, 2 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir potato mixture, 2 tablespoons vegetable oil, bread crumbs, salt, soy sauce, and sage into oatmeal; mix thoroughly. Form into patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon vegetable oil in a skillet over medium heat; fry patties until cooked through and browned, 5 to 7 minutes per side.\n"
            }
        ],
