    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238307/the-yummiest-tastiest-raspberry-jell-o-salad/",
        "name": "The Yummiest, Tastiest Raspberry Jell-O&reg; Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5111739.jpg",
            "width": 2449,
            "height": 2334
        },
        "datePublished": "2019-04-04T03:58:04.000Z",
        "description": "This is the best Jell-O&reg; salad ever. Sprinkle chopped walnuts or fresh raspberries over the finished product, if you like.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT4H20M",
        "recipeYield": "1 9x12-inch dish",
        "recipeIngredient": [
            "1 (6 ounce) package raspberry-flavored gelatin mix (such as Jell-O\u00ae)",
            "1 cup water",
            "1 (8 ounce) can crushed pineapple",
            "1 cup cold water",
            "1 (21 ounce) can raspberry pie filling",
            "1 (8 ounce) package cream cheese, softened",
            "1 cup sour cream",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put gelatin mix into a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1 cup water and crushed pineapple together in a saucepan; bring to a boil and immediately pour over the gelatin mix. Stir the mixture to dissolve the gelatin mix; add cold water and raspberry pie filling. Stir the mixture and pour into a 9x12-inch dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover dish with plastic wrap and refrigerate until the gelatin layer is set, about 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip cream cheese until smooth; add sour cream and confectioners' sugar and mix until creamy. Spread the cream cheese mixture over the set gelatin.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes",
            "Jell-O&reg; Salad"
        ],
