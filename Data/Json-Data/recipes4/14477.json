        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/217010/bunko-punch/",
        "name": "Bunko Punch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F711136.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T03:50:23.000Z",
        "description": "This punch was made famous by one of the ladies in my Bunko group, but has now become a favorite for all occasions! Add frozen, chopped fruit chunks to the mix after 'slushing' for a healthy (ha ha) boost!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "22 servings",
        "recipeIngredient": [
            "1 cup brewed tea",
            "7 cups water",
            "4 cups vodka",
            "1 (6 ounce) can frozen orange juice concentrate, thawed",
            "1 (6 ounce) can frozen pink lemonade concentrate, thawed",
            "1 (2 liter) bottle grapefruit flavored soda, chilled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together the tea, water, vodka, orange juice concentrate, and pink lemonade concentrate in a large freezer-proof bowl, such as glass or stainless steel, and freeze overnight or several days ahead. With a fork, scrape the frozen mixture into a slush; pour the grapefruit soda into the slushy mixture, and stir. Keep cold in an ice chest.\n"
            }
        ],
