        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238987/paleo-cauliflower-rice/",
        "name": "Paleo Cauliflower Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1450889.jpg",
            "width": 2500,
