        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24669/roasted-tomato-salsa-ii/",
        "name": "Roasted Tomato Salsa II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F128435.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T03:28:06.000Z",
        "description": "Roasted tomatoes, garlic and onion give this salsa a wonderful smoky flavor.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "3 tomatoes, cut into wedges",
            "5 cloves garlic, diced",
            "1 onion, chopped",
            "1 cup water",
            "\u00bc teaspoon ground black pepper",
            "1 dash hot pepper sauce",
            "1 teaspoon salt",
            "1 teaspoon hot chile sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a medium baking sheet, arrange tomatoes, garlic and onion. Place under the broiler. Checking frequently, heat 15 minutes, or until vegetables are charred. Remove from heat and allow to cool approximately 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place charred vegetables in a blender or food processor with the water and blend until smooth. Stir in ground black pepper, hot pepper sauce, salt and hot chile sauce. Refrigerate the mixture until serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Salsa Recipes",
            "Tomato Salsa Recipes"
        ],
