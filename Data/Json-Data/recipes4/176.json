    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19356/spaghetti-italian/",
        "name": "Spaghetti Italian",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1327337.jpg",
            "width": 1440,
            "height": 1439
        },
        "datePublished": "2000-04-01T17:30:42.000Z",
        "description": "Delicious and not so sweet. Good to the last bowl!!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "\u00bd pound Italian sausage",
            "4 (6.5 ounce) cans tomato sauce",
            "1 (14.5 ounce) can diced tomatoes",
            "2 bay leaves",
            "1 teaspoon Italian seasoning",
            "\u00bd teaspoon garlic powder",
            "1 teaspoon dried basil",
            "1 teaspoon dried oregano",
            "salt and pepper to taste",
            "1 (8 ounce) package spaghetti"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet, brown sausage over medium heat; drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan over medium heat, combine tomato sauce, diced tomatoes, bay leaves, Italian seasoning, garlic powder, basil, oregano, salt, pepper and Italian sausage; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Simmer over medium-low heat for at least one hour; it is best if simmered all day.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sauce with hot pasta; serve.\n"
            }
        ],
