    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254857/south-african-sugar-bean-soup/",
        "name": "South African Sugar Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "South African Sugar Bean Soup"
        },
        "datePublished": "2019-04-04T04:06:47.000Z",
        "description": "A rich, delicious, hearty stew-like soup that's perfect for the cold nights. Easy meal served with bread. My family have been making this for generations.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H50M",
        "totalTime": "P0DT11H10M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups speckled sugar beans",
            "water to cover",
            "2 pounds bone-in beef chuck, cut into chunks",
            "4 large onions, thickly sliced",
            "1 cube beef bouillon",
            "2\u2009\u00bd tablespoons hot water",
            "\u00be cup red wine",
            "\u00bc cup tomato paste",
            "1 packet dry oxtail soup mix",
            "3 tablespoons Worcestershire sauce",
            "2 tablespoons soy sauce",
            "1 pinch cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place sugar beans into a large container and cover with several inches of cool water; let soak, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine beef chuck and onions in a large pot over medium heat. Cook until beef is tender and falling off the bone, about 1 1/2 hour. Transfer beef to a cutting board; discard bones and cut beef into bite-size pieces. Place onions in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sugar beans and their soaking water into the pot. Bring to a boil; reduce heat and simmer, adding more water if needed, until beans are tender, about 1 hour. Stir in beef pieces and onions; bring soup to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve beef bouillon cube in 2 1/2 tablespoons hot water in a bowl. Stir in wine, tomato paste, dry soup mix, Worcestershire sauce, soy sauce, and cayenne pepper. Gradually mix into the boiling soup. Reduce heat and simmer until flavors combine, about 10 minutes.\n"
            }
        ],
