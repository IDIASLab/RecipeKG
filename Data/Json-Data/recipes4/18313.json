    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255221/roasted-bell-pepper-dressing/",
        "name": "Roasted Bell Pepper Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3924016.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-04T04:06:59.000Z",
        "description": "This roasted bell pepper salad dressing is a Weight Watchers&reg;-friendly salad dressing made with fresh ingredients. Easy to make and the perfect addition to any salad. Dressing will stay fresh in the fridge for up to 2 weeks.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "2 red bell peppers",
            "1 cup water, or as needed",
            "3 tablespoons apple cider vinegar",
            "3 tablespoons canola oil",
            "2 tablespoons honey",
            "2 cloves garlic",
            "1 tablespoon cilantro",
            "1 tablespoon dried basil",
            "1 tablespoon sea salt, or to taste",
            "1 tablespoon cracked black pepper, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Spray a baking sheet with cooking spray; place red bell peppers onto baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, turning with tongs every 10 minutes, until skin is evenly blackened and blistered, about 30 minutes. Remove from oven; cover with a heat-proof bowl. Let stand until skin peels off easily, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove skin, stem, and seeds from red bell peppers with a sharp knife.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine roasted red peppers, water, vinegar, oil, honey, garlic, cilantro, basil, salt, and pepper in a food processor; blend until smooth.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Salad Dressing Recipes"
        ],
