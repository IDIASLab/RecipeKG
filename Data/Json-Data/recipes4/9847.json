    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13788/homemade-dressing/",
        "name": "Homemade Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4589823.jpg",
            "width": 4474,
            "height": 2763
        },
        "datePublished": "1999-10-10T18:43:33.000Z",
        "description": "This dressing is made with sweet bread, cranberries and sunflower seeds.  It is very delicious and different!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 (1 pound) loaves Hawaiian sweet bread",
            "1 teaspoon sage",
            "3 teaspoons poultry seasoning",
            "salt and pepper to taste",
            "2 tablespoons butter",
            "3 stalks celery, diced",
            "\u00bd onion, finely diced",
            "1 cup water",
            "1\u2009\u00bd cups dried cranberries",
            "\u00bd cup sunflower seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut day old sweet bread into small pieces and place in large bowl. Add the sage, poultry seasoning, salt and pepper. Toss and allow bread to sit until dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, melt butter and add celery, onion and water. Stir until heated but not boiling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the cranberries, sunflower seeds and vegetable mixture to bowl of dried bread; stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use as a stuffing for turkey, or bake in a buttered casserole dish at 350 degrees F (175 degrees C) for 30 to 40 minutes.\n"
            }
        ],
