    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234873/cockaleekie-soup/",
        "name": "Cockaleekie Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2104386.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-04T03:56:15.000Z",
        "description": "A filling winter stew served best with crusty bread on a cold day. This recipe will make enough to give to your neighbors and friends.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT3H0M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "20 cups water",
            "1 (2 to 3 pound) whole chicken",
            "\u00bd white onion, chopped",
            "2 large carrots, chopped",
            "2 celery ribs, chopped",
            "6 large leeks, cut into 1-inch chunks, or more to taste",
            "\u2154 cup pearl barley",
            "1 teaspoon celery seed",
            "1 teaspoon ground cloves",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a large stock pot; cook chicken in the boiling water until fully cooked, about 1 hour. Remove chicken, keeping water in stock pot, and cool; discard skin, pull meat from bones, and put bones back into water for stock. Add onion, carrots, and celery to water and boil, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak leeks in a bowl of water, about 10 minutes; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove bones and vegetables from stock using a slotted spoon. Add chicken meat, leeks, barley, celery seed, cloves, salt, and black pepper to stock and simmer until barley is tender and flavors have blended, about 1 hour.\n"
            }
        ],
