        "recipeIngredient": [
            "2 cups almond meal",
            "1\u2009\u00bd tablespoons warm honey",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "3 egg whites, beaten stiff"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir almond meal, honey, cinnamon, and nutmeg together in a bowl. Mix beaten egg whites into the almond meal mixture; scoop by teaspoonful onto prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until browned, 10 to 15 minutes.\n"
            }
        ],
