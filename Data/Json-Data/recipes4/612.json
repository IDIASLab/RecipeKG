        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24324/my-potatoes/",
        "name": "My Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1056145.jpg",
