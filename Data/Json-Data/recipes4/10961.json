        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25897/one-bowl-rice/",
        "name": "One Bowl Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F28863.jpg",
            "width": 2048,
            "height": 1536
