    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14722/smothered-pork-chops/",
        "name": "Smothered Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F259017.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "1999-09-27T18:12:35.000Z",
        "description": "The gravy from this recipe is great with any side dish. This is my 15 year-old-sons favorite!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 onion, chopped",
            "4 cloves crushed garlic",
            "6 (3/4 inch) thick pork chops",
            "\u00bd cup water",
            "\u2153 cup all-purpose flour",
            "1 (14.5 ounce) can fat-free chicken broth",
            "1 tablespoon browning sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Saute onion and garlic in a non-stick skillet coated with vegetable spray until tender. Add pork chops, and brown on both sides. Add 1/2 cup of water, and bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together, flour, chicken broth, and browning sauce until smooth. Add to skillet, stirring well. Cover and cook over low heat 30 to 45 minutes or until tender.\n"
            }
        ],
