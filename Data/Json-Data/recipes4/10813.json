    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24979/magic-fruitcake/",
        "name": "Magic Fruitcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Magic Fruitcake"
        },
        "datePublished": "2019-04-04T03:28:15.000Z",
        "description": "A magical blend of mincemeat, pecans, sweetened condensed milk and candied fruit.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 - 9x5 inch loaf pan",
        "recipeIngredient": [
            "1 (9 ounce) package condensed mincemeat, crumbled",
            "\u00bd cup water",
            "1 cup chopped pecans",
            "1 cup candied mixed fruit peel, chopped",
            "1 (14 ounce) can sweetened condensed milk",
            "1 egg, beaten",
            "\u00be cup all-purpose flour",
            "1 tablespoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5 inch loaf pan. Line greased pan with parchment paper and coat paper with non-stick spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine crumbled mincemeat  and water. Bring to a boil and cook, stirring, for 1 minute. remove from heat and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in pecans, candied fruit, sweetened condensed milk, and egg. Stir in flour and baking soda until just blended. Pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 90 minutes, or until a toothpick inserted into the center of the cake comes out clean. Allow to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Fruitcake Recipes"
