    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236881/slow-cooker-stuffed-peppers/",
        "name": "Slow Cooker Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1132027.jpg",
            "width": 2057,
            "height": 1536
        },
        "datePublished": "2019-04-04T03:57:20.000Z",
        "description": "These are slow-cooked stuffed peppers. Red or yellow bell peppers can be used instead of green. Italian diced tomatoes can be used in place of fire-roasted if desired.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT6H5M",
        "totalTime": "P0DT6H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "1 (14.5 ounce) can fire-roasted diced tomatoes",
            "1 cup cooked rice",
            "2 tablespoons ketchup",
            "1 teaspoon Worcestershire sauce",
            "1 teaspoon ground black pepper",
            "6 green bell peppers, tops and seeds removed (reserve tops)",
            "\u2153 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground beef, tomatoes, rice, ketchup, Worcestershire sauce, and black pepper together in a bowl. Stuff each green bell pepper with ground beef mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange stuffed bell peppers in the crock of a slow cooker; place the reserved tops back onto each bell pepper. Pour water around the base of each stuffed bell pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 6 to 8 hours.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Stuffed",
            "Stuffed Bell Pepper Recipes"
        ],
