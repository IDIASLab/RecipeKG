        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/163641/blue-cheese-ball/",
        "name": "Blue Cheese Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9369645.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-02T06:50:19.000Z",
        "description": "Only five ingredients combine to make an unforgettable classic cheese ball with the distinctive flavor of blue cheese.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "1 cheese ball",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "1 cup crumbled blue cheese",
            "1 cup shredded sharp Cheddar cheese",
            "\u00bc cup minced onion",
            "1 tablespoon Worcestershire sauce",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the cream cheese, blue cheese, Cheddar cheese, onion and Worcestershire sauce. Transfer to a separate bowl that has been lined with plastic wrap. Cover and refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "The next day, gather the cream cheese mixture into a ball. Spread the walnuts out on a dinner plate. Roll the cheese ball in nuts until coated. Refrigerate or serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Cheese Ball Recipes"
        ],
