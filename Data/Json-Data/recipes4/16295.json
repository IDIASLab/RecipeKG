        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235204/simple-carrot-soup/",
        "name": "Simple Carrot Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Simple Carrot Soup"
        },
        "datePublished": "2019-04-04T03:56:25.000Z",
        "description": "This soup is low-calorie, high-flavor comfort food and favorite in my house on a chilly autumn evening.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "4 cups chopped carrots",
            "3 cups fat-free chicken broth",
            "\u00bd onion, sliced",
            "water to cover",
            "4 cloves garlic, smashed",
            "1 teaspoon dried thyme",
            "\u00bd teaspoon dried tarragon",
            "2 tablespoons heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine carrots, chicken broth, and onion in a large pot. Pour enough water into the pot to assure the ingredients are covered; add garlic, thyme, and tarragon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the liquid to a boil, reduce heat to medium-low, place a cover on the pot, and simmer the mixture until the carrots are very tender, 40 to 50 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pot from heat and cool soup for 10 minutes. Puree soup with an immersion blender or mash vegetables with a potato masher and whisk the soup until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pot to medium heat and cook until reheated, about 5 minutes. Stir cream through the soup.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Carrot Soup Recipes"
        ],
