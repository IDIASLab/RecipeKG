        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25364/aimees-quick-chicken/",
        "name": "Aimee's Quick Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F13932.jpg",
            "width": 2560,
            "height": 1920
