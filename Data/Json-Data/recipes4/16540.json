    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236658/heavenly-turkey-soup/",
        "name": "Heavenly Turkey Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4568770.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2019-04-04T03:57:12.000Z",
        "description": "This soup serves more than one purpose. It's a light, refreshing sunny-day lunch, yet warm and comforting on the rainy evening.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "10 cups water",
            "1 cup cubed turkey breast meat, with bone, divided",
            "1 large onion, chopped",
            "4 garlic cloves, crushed",
            "1 tablespoon crushed red pepper flakes",
            "1 tablespoon sea salt",
            "3 cubes  chicken bouillon",
            "2 cups chopped broccoli",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 cup chopped fresh asparagus",
            "1 cup sliced red bell pepper",
            "1 (1.8 ounce) tub instant dry lentil soup mix",
            "\u00bd cup bean sprouts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place water, turkey breast bone, onion, garlic, pepper flakes, sea salt, and bouillon cubes in a large pot; bring to a boil. Reduce heat; simmer broth for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir broccoli, cream of mushroom soup, turkey breast meat, asparagus, bell pepper, and instant soup mix into broth. Return soup to a simmer; cook for 10 minutes. Remove breast bone. Ladle soup into bowls; top with bean sprouts.\n"
            }
        ],
