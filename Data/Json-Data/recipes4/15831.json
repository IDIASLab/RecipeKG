    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231688/salmon-lentil-salad/",
        "name": "Salmon Lentil Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2242680.jpg",
            "width": 2265,
            "height": 2265
        },
        "datePublished": "2019-04-04T03:54:52.000Z",
        "description": "A nice, filling salad that can be eaten warm or cold.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "\u00bd cup lentils, rinsed",
            "1 cup water, or as needed to cover",
            "1 salmon fillet",
            "1 slice Black Forest-style bacon",
            "\u00bc red onion, diced",
            "5 small fresh mozzarella balls (boccaccini), cut into quarters",
            "\u00bc cup diced cucumber, or to taste",
            "2 tablespoons balsamic vinegar",
            "2 tablespoons olive oil",
            "1 clove garlic, minced",
            "1 teaspoon mustard",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put lentils in a small pot with enough water to just cover; cook on medium heat until tender, 10 to 15 minutes. Set lentils aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook salmon under the preheated broiler until the fish flakes easily with a fork, 5 to 7 minutes per side.  Chop salmon and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a small skillet over medium-high heat; cook bacon in skillet until crisp, 7 to 10 minutes. Chop bacon and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cooled lentils, salmon, bacon, red onion, mozzarella, and cucumber together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk balsamic vinegar, olive oil, garlic, mustard, salt, and pepper together in a small bowl; drizzle over the lentil mixture and stir gently to coat.\n"
            }
        ],
