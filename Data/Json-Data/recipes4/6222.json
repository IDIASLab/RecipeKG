    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230428/magic-santa-bars/",
        "name": "Magic Santa Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1845209.jpg",
            "width": 3475,
            "height": 2302
        },
        "datePublished": "2019-04-02T06:59:40.000Z",
        "description": "A twist on the average magic bar, this rich and delicious cookie bar has a surprise tartness of Craisins&reg;. We make every Christmas season and they're snatched up in no time!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 dozen cookie bars",
        "recipeIngredient": [
            "1\u2009\u00bd cups graham cracker crumbs",
            "\u00bd cup butter, melted",
            "1 (14 ounce) can sweetened condensed milk, divided",
            "1 cup semi-sweet chocolate chips",
            "1 cup mini marshmallows",
            "\u00be cup flaked coconut",
            "\u00bd cup chopped pecans",
            "\u00bd cup sweetened dried cranberries (such as Ocean Spray\u00ae Craisins\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a 9x13-inch baking pan with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs and butter together in a bowl; press into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 2/3 the sweetened condensed over the graham cracker crust. Sprinkle chocolate chips, marshmallows, coconut, pecans, and cranberries over the sweetened condensed milk layer; press into the pan. Drizzle the remaining 1/3 sweetened condensed milk evenly over the toppings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping is golden brown, 25 to 30 minutes. Cool in the pan for at least 20 minutes before cutting into squares.\n"
            }
        ],
