    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254810/chicken-potato-chip-casserole/",
        "name": "Chicken Potato Chip Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chicken Potato Chip Casserole"
        },
        "datePublished": "2019-04-04T04:06:46.000Z",
        "description": "I received this recipe from an old co-worker. His mom had been making it since he was a kid. This has now jumped to my husband's favorite.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H3M",
        "totalTime": "P0DT1H43M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds boneless chicken breasts",
            "water to cover",
            "2 (1.5 ounce) packages potato chips, crushed",
            "\u00bd cup butter",
            "1 yellow onion, diced",
            "1 (8 ounce) package fresh mushrooms, sliced",
            "1 green bell pepper, diced",
            "1 red bell pepper, diced",
            "\u00bd cup all-purpose flour",
            "2 cups milk",
            "1 (8 ounce) package American cheese slices"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts in a large pot; add enough water to cover. Bring to a boil; reduce heat and simmer until an instant-read thermometer inserted into the center reads at least 165 degrees F (74 degrees C), 15 to 20 minutes. Remove chicken with a slotted spoon and cool until easily handled, about 10 minutes. Reserve cooking liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dice chicken into bite-size pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer 1 bag crushed potato chips in the bottom of a 9x13-inch baking dish. Top with diced chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Add onion, mushrooms, green bell pepper, and red bell pepper; cook and stir until onion is translucent, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour into onion mixture to make a paste; cook until golden, about 3 minutes. Pour in 2 cups reserved cooking liquid and milk; cook and stir until sauce is smooth and thick, about 5 minutes. Add American cheese, 1 slice at a time, stirring until melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauce over chicken in the baking dish. Top with remaining crushed potato chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly and golden, about 30 minutes.\n"
            }
        ],
