        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220287/honey-dijon-brussels-sprouts/",
        "name": "Honey Dijon Brussels Sprouts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4587365.jpg",
            "width": 2262,
            "height": 2262
        },
        "datePublished": "2019-04-02T06:56:21.000Z",
        "description": "My mother made this for Thanksgiving.  It's a wonderful twist on Brussels sprouts!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "10 Brussels sprouts, halved",
            "1\u2009\u00bd teaspoons butter, melted",
            "1\u2009\u00bd teaspoons honey",
            "\u00bd teaspoon Dijon mustard",
            "1 pinch dried dill weed",
            "1 pinch onion powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place Brussels sprouts into a saucepan filled with lightly salted water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Boil over medium high heat until Brussels sprouts are just tender, 8 to 10 minutes; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix butter, honey, Dijon mustard, dill weed, and onion powder in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss Brussels sprouts in mustard mixture to coat.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Brussels Sprouts"
        ],
