    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245217/sausage-and-apple-stuffing-bites/",
        "name": "Sausage-and-Apple Stuffing Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sausage-and-Apple Stuffing Bites"
        },
        "datePublished": "2019-04-02T07:05:23.000Z",
        "description": "This is a delicious twist on traditional Thanksgiving stuffing that works great as a warm appetizer. Enjoy!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "24 mini muffins",
        "recipeIngredient": [
            "cooking spray",
            "4 ounces white country bread, cut into 1/2-inch cubes",
            "\u00bc cup extra-virgin olive oil, divided",
            "freshly ground black pepper to taste",
            "kosher salt to taste",
            "2 tablespoons unsalted butter",
            "\u2154 cup minced onion",
            "\u2154 cup minced celery",
            "\u00bd pound sweet Italian pork sausage, casings removed",
            "6 cloves garlic, minced",
            "\u00be teaspoon dried sage",
            "4 eggs, beaten",
            "1 small Granny Smith apple - peeled, cored, and finely chopped",
            "2 tablespoons chicken broth"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease two 12-cup mini muffin tins with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss bread cubes with 2 tablespoons olive oil on a baking sheet; season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake bread cubes in the preheated oven until toasted, about 10 minutes. Transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter and 2 tablespoons olive oil in a large skillet over medium-high heat. Add onion and celery; cook and stir until golden, about 5 minutes. Stir sausage, garlic, and sage into onion mixture and cook, breaking up the meat until no trace of pink remains, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sausage mixture, eggs, apple, and chicken broth into bowl of toasted bread cubes. Season with salt and pepper. Let stand until flavors absorb, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop stuffing mixture into muffin cups and bake in the preheated oven until golden, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer muffin tins to a rack to cool, about 5 minutes. Loosen the muffins with a sharp paring knife to lift them out.\n"
            }
        ],
