    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/45436/hearty-mince/",
        "name": "Hearty Mince",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2609.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2019-04-04T03:30:24.000Z",
        "description": "Mum's recipe! Absolutely delicious Australian hearty beef mince with noodles. Serve with garlic bread and a 1999 Shiraz.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons margarine",
            "1 pound ground beef",
            "1 onion, chopped",
            "1\u2009\u00bd cups tomato juice",
            "1\u2009\u00bd cups water",
            "\u00bd cup beef stock",
            "1 teaspoon Worcestershire sauce",
            "1 teaspoon celery salt",
            "\u00bd teaspoon ground black pepper",
            "1\u2009\u00bd cups uncooked spiral pasta",
            "\u00bd cup sour cream",
            "1 teaspoon sherry",
            "\u00be cup chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a large, heavy skillet over medium heat, and stir in the ground beef and onion. Cook until onion is tender and beef is evenly browned. Reduce heat to low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix tomato juice, water, beef stock, Worcestershire sauce, celery salt, and pepper. Pour into the beef mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir pasta into the skillet. Cover, and simmer 25 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the mixture from heat, and stir in sour cream and sherry. Sprinkle with parsley to serve.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta"
        ],
