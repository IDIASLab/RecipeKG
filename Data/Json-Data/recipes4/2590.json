    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/97432/velvety-pumpkin-soup-with-blue-cheese-and-bacon/",
        "name": "Velvety Pumpkin Soup With Blue Cheese and Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F127535.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:48:07.000Z",
        "description": "This quick and tasty holiday soup is brought to a higher level with the addition of blue cheese and bacon. Yum! Yum!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 (15 ounce) cans pumpkin",
            "1 quart chicken stock",
            "1 cup half-and-half",
            "1 shallot, minced",
            "\u00bc cup molasses",
            "2 tablespoons butter",
            "1 teaspoon pumpkin pie spice",
            "1 teaspoon salt",
            "\u00bc teaspoon cayenne pepper",
            "6 slices bacon",
            "1 cup crumbled blue cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together the pumpkin, chicken stock, half-and-half, shallot, molasses, butter, pumpkin pie spice, salt, and cayenne pepper in a large stockpot over low heat; simmer 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place bacon in a large skillet over medium-high heat, and cook until crispy; remove to paper towels to drain, then cool and crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Ladle soup into bowls. Top with bacon and blue cheese.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Squash Soup Recipes",
            "Pumpkin Soup Recipes"
        ],
