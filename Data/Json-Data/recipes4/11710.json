        "mainEntityOfPage": "https://www.allrecipes.com/recipe/57259/the-spiced-cider-project/",
        "name": "The Spiced Cider Project",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F149111.jpg",
            "width": 250,
            "height": 250
