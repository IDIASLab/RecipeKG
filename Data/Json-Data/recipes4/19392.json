    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261975/almond-delight-cookies-with-dark-chocolate-sugar-free-almond-pulp-cookies/",
        "name": "Almond Delight Cookies with Dark Chocolate (Sugar-Free Almond Pulp Cookies)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1271378.jpg",
            "width": 2048,
            "height": 1784
        },
        "datePublished": "2019-04-04T04:10:47.000Z",
        "description": "Almond pulp, left over from making almond milk, can be made into these tasty, gluten-free, refined-sugar-free, and vegan cookies.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H56M",
        "recipeYield": "44 cookies",
        "recipeIngredient": [
            "8 pitted dates",
            "2 cups moist almond pulp from making almond milk",
            "\u2153 cup rolled oats",
            "\u00bc cup almond milk",
            "3 tablespoons blanched almond flour",
            "2 tablespoons coconut flour",
            "2 tablespoons coconut oil, melted",
            "1 tablespoon applesauce",
            "1 tablespoon vanilla extract",
            "1 tablespoon cacao nibs, ground",
            "4 pinches ground cinnamon",
            "4 dashes salt",
            "1 dash ground nutmeg",
            "\u00bc cup water",
            "\u2153 cup chopped vegan dark chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with lightly greased parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak dates in hot water until soft, 6 to 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine dates, almond pulp, oats, almond milk, almond flour, coconut flour, coconut oil, applesauce, vanilla extract, cacao nibs, cinnamon, salt, and nutmeg in a food processor. Process to the consistency of cookie dough, adding water if needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll 1 tablespoon of cookie dough with your hands. Pinch sides with your fingers to form a square. Place on the prepared baking sheet and press down with your fingers or a fork to flatten slightly. Repeat with remaining dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 20 minutes. Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt dark chocolate in a microwave-safe glass or ceramic bowl in 15-second intervals, stirring after each interval, 1 to 2 minutes. Drizzle melted chocolate over cookies.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Almond Dessert Recipes"
        ],
