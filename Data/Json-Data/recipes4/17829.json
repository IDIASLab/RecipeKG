    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246073/high-fiber-bread/",
        "name": "High Fiber Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "High Fiber Bread"
        },
        "datePublished": "2019-04-04T04:00:54.000Z",
        "description": "This dense, chewy bread has a good dose of fiber and is great toasted in the morning with my oatmeal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "2 1-pound loaves",
        "recipeIngredient": [
            "1\u2009\u2154 cups warm water",
            "4 teaspoons molasses",
            "1 tablespoon active dry yeast",
            "2\u2009\u2154 cups whole wheat flour",
            "\u00be cup ground flax seed",
            "\u2154 cup bread flour",
            "\u00bd cup oat bran",
            "\u2153 cup rolled oats",
            "\u2153 cup amaranth seeds",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place water, molasses, yeast, wheat flour, ground flax seed, bread flour, oat bran, rolled oats, amaranth seeds, and salt in the pan of a bread machine in the order recommended by manufacturer. Select dough cycle; press start and allow machine to run complete dough cycle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough onto a lightly floured surface. Form dough into 2 loaves and place on baking stone. Cover loaves with a damp cloth and let rise until doubled in volume, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until top is golden brown, 20 to 25 minutes. Slide loaf onto a work surface and gently tap bottom of loaf. If it sounds hollow, bread is done.\n"
            }
        ],
