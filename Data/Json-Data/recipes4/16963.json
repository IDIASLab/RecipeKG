        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239517/moms-best-ever-meatloaf/",
        "name": "Mom's 'Best Ever' Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2743951.jpg",
            "width": 1928,
            "height": 1928
