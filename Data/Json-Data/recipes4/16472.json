    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236360/lovely-lemon-chicken/",
        "name": "Lovely Lemon Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1095390.jpg",
            "width": 1920,
            "height": 1080
        },
        "datePublished": "2019-04-04T03:57:02.000Z",
        "description": "This lovely lemon chicken can be served with mashed potatoes, corn or green beans, and a salad. The lemony sauce tastes delicious mixed with a salad.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 bone-in, skin-on chicken breast halves",
            "4 lemons, cut in half",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "2 teaspoons garlic powder",
            "2 teaspoons onion powder",
            "2 teaspoons paprika",
            "2\u2009\u00bd cups water, divided",
            "1 cube chicken bouillon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts, skin-side up, in baking dish. Season chicken with 1/2 the salt, 1/2 the pepper, 1/2 the garlic powder, 1/2 the onion powder, and 1/2 the paprika, respectively. Turn the chicken over and season with remaining salt, pepper, garlic powder, onion powder, the paprika, respectively. Squeeze lemons over the chicken; discard lemons and add 1 cup water to the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken in the preheated oven for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir remaining 1 1/2 cups water and chicken bouillon together in a saucepan; bring to a boil. Pour bouillon mixture over chicken. Flip chicken skin-side up. Continue baking chicken in the oven until skin is golden and crispy, and the chicken is no longer pink at the bone and the juices run clear, about 45 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            }
        ],
