        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25369/authentic-hungarian-goulash/",
        "name": "Authentic Hungarian Goulash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2672971.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2019-04-04T03:28:24.000Z",
        "description": "This recipe was given to me by my sister, who got it from a lady visiting from Hungary in 1961.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT3H30M",
        "totalTime": "P0DT3H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 large onions, diced",
            "2 pounds flank steak",
            "\u215b teaspoon caraway seed",
            "\u00bc teaspoon dried marjoram",
            "1 clove garlic, minced",
            "5 tablespoons paprika",
            "2 cups water",
            "4 large potatoes, peeled and cubed",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large soup pot over medium high heat. Saute onions until soft, then add beef and brown. Stir in caraway seed, marjoram, garlic and paprika. Pour water over all, lower heat to low and simmer for 2 1/2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add potatoes and cook until tender, another 45 minutes to 1 hour. Season with salt and pepper to taste and serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Goulash Recipes"
        ],
