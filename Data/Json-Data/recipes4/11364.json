    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/44746/cherry-ham-glaze/",
        "name": "Cherry Ham Glaze",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4482059.jpg",
            "width": 1000,
            "height": 1000
        },
        "datePublished": "2019-04-04T03:30:22.000Z",
        "description": "This cherry ham glaze give any ham a delicious, sweet, and slightly tart taste.  I glazed a ham for Thanksgiving with this, and my company couldn't get enough of it.  I was out of ham by the next day!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H10M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "3 tablespoons water",
            "\u00bc cup white vinegar",
            "2 tablespoons light corn syrup",
            "1 (12 ounce) jar cherry preserves",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground cloves",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together water, vinegar, corn syrup, preserves, cinnamon, cloves, and nutmeg in a saucepan over medium-high heat. Bring to a boil, then reduce heat to low and simmer for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush 1/4 to 1/3 cup of this glaze over your ham about 30 minutes before it is finished baking. Stir any ham drippings into the remaining glaze and serve alongside the sliced ham.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Ham Glaze Recipes"
        ],
