    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24212/breakfast-biscuits/",
        "name": "Breakfast Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F714403.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2019-04-02T06:42:32.000Z",
        "description": "The homemade version of fast food breakfast biscuits. You can exchange toppings to fit your taste buds. My kids love them!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (10 ounce) can refrigerated buttermilk biscuit dough",
            "1 pound bacon",
            "5 eggs",
            "\u00bc cup milk",
            "3 tablespoons butter, softened",
            "10 slices Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place biscuits 2 inches apart on an ungreased cookie sheet. Bake in preheated oven for 8 to 11 minutes or until golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium-high heat until evenly brown. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat together eggs and milk. Heat a lightly oiled skillet over medium heat. Scramble eggs to your liking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut open biscuits, lightly butter, layer with eggs, bacon, and cheese.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Breakfast Sandwich Recipes"
        ],
