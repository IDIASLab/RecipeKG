        "recipeYield": "1 smoothie bowl",
        "recipeIngredient": [
            "1 cup frozen blueberries",
            "\u00bd banana",
            "2 tablespoons water",
            "1 tablespoon cashew butter",
            "1 teaspoon vanilla extract",
            "\u00bd banana, sliced",
            "1 tablespoon sliced almonds",
            "1 tablespoon unsweetened shredded coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend blueberries, 1/2 banana, water, cashew butter, and vanilla extract together in a blender until smooth; pour into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top smoothie with sliced banana, almonds, and coconut.\n"
            }
