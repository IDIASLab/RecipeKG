        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix lemons wedges, 1/4 cup plus 2 tablespoons water, lemon juice, and salt together in a microwave-safe container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in the microwave for 10 minutes, stirring at 2-minute intervals to prevent spills.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the mixture into a sterilized, airtight jar. Let stand for 24 hours before using.\n"
            }
