        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230614/easy-slow-cooker-chicken-soup/",
        "name": "Easy Slow Cooker Chicken Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3537917.jpg",
