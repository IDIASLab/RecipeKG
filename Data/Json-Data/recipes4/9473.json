        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9415/rum-balls-i/",
        "name": "Rum Balls I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F111829.jpg",
