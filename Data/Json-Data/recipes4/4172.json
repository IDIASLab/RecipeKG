        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213267/pork-and-vinegar/",
        "name": "Pork and Vinegar",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3696769.jpg",
            "width": 1704,
            "height": 1705
