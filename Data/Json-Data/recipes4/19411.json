        "name": "Super Leftover Spiral Ham and Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4969714.jpg",
            "width": 3264,
