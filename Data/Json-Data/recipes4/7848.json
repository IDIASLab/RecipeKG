    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244514/broccoli-souffle/",
        "name": "Broccoli Souffle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3029218.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2019-04-02T07:04:47.000Z",
        "description": "Passed down through the generations, this broccoli souffle recipe has become a holiday staple in our family. A savory taste with a crispy cheese topping makes it a sure win.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (14 ounce) bag frozen chopped broccoli, thawed",
            "\u00bc cup margarine",
            "\u00bc cup all-purpose flour",
            "1 cup milk",
            "3 eggs, beaten",
            "\u2154 cup mayonnaise",
            "3 tablespoons chopped onion",
            "salt and ground black pepper to taste",
            "\u00bd cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan and fill with water to just below the bottom of the steamer. Bring water to a boil. Add broccoli, cover, and steam until tender, 2 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a saucepan over medium heat. Add flour and stir until mixed. Slowly add the milk, stirring to prevent lumps. Cook until white sauce is thickened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine broccoli, eggs, mayonnaise, and onion in a baking dish; cover with white sauce and stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 minutes. Sprinkle Cheddar cheese on top and bake until set, about 15 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Broccoli"
        ],
