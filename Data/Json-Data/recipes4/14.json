    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17151/old-italian-meat-sauce/",
        "name": "Old Italian Meat Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3309781.jpg",
            "width": 2072,
            "height": 2073
        },
        "datePublished": "2000-03-10T16:49:23.000Z",
        "description": "A robust hearty Italian meat and mushroom pasta sauce. Serve with your favorite pasta, if desired.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT4H0M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "2 pounds lean ground beef",
            "1 pound ground pork",
            "2 tablespoons olive oil",
            "2 onions, chopped",
            "1 clove garlic, crushed",
            "3 cups red wine",
            "2 pounds fresh mushrooms, sliced",
            "\u00bc teaspoon dried rosemary",
            "4 tablespoons chopped fresh oregano",
            "\u00bc teaspoon chopped fresh thyme",
            "3 (29 ounce) cans tomato sauce",
            "1 (6 ounce) can tomato paste",
            "2 tablespoons grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet, brown beef and pork over medium heat until no longer pink; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, warm olive oil over medium heat and saute onions and garlic until tender; add about 1/2 cup of wine; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add mushrooms, rosemary, oregano and thyme to skillet and add another 1/2 cup wine; saute until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add browned meat, tomato sauce and tomato paste to mixture; simmer for 1 hour and add the remaining 2 cups of wine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Simmer sauce on low for 2 to 3 hours, stirring occasionally; serve.\n"
            }
        ],
