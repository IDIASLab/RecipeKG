    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240060/loaded-cauliflower/",
        "name": "Loaded Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1658609.jpg",
            "width": 6984,
            "height": 6984
        },
        "datePublished": "2019-04-02T07:03:57.000Z",
        "description": "A great way to add some zip to your cauliflower. This recipe is low-carb and resembles a loaded potato! Very unique and down right tasty!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 head cauliflower",
            "\u00bd cup sour cream",
            "\u00bd cup shredded Cheddar cheese",
            "1 teaspoon dry ranch salad dressing mix (such as Hidden Valley Ranch\u00ae)",
            "\u00bd teaspoon onion powder",
            "\u00bd teaspoon garlic powder",
            "1 tablespoon butter, cut into small pieces, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan and fill with water to just below the bottom of the steamer. Bring water to a boil. Add cauliflower, cover, and steam until very tender, 15 to 20 minutes. Transfer cauliflower to a bowl, mash, and strain excess water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cauliflower, sour cream, Cheddar cheese, ranch dressing mix, onion powder, and garlic powder together in a 9-inch baking dish; top with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling, 30 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Cauliflower",
            "Mashed"
        ],
