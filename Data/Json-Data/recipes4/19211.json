    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261020/vegan-chicken-balls/",
        "name": "Vegan \"Chicken\" Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan \"Chicken\" Balls"
        },
        "datePublished": "2019-04-04T04:10:11.000Z",
        "description": "Vegan Chinese-style \"chicken\" balls. They taste better than the real thing. If you enjoy eating chicken, you can use the batter with precooked chicken. Serve with your favorite sweet-and-sour sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H4M",
        "totalTime": "P0DT4H24M",
        "recipeYield": "24 balls",
        "recipeIngredient": [
            "2 cubes  vegan chicken-flavored stock",
            "2 cups boiling water",
            "1 clove garlic, minced",
            "1 teaspoon dried thyme",
            "1 teaspoon dried rosemary",
            "\u00bd teaspoon dried sage",
            "1 pinch salt, or more to taste",
            "1 (12 ounce) package tofu, cut into 1-inch cubes",
            "1\u2009\u00bd cups all-purpose flour, divided",
            "2 teaspoons baking powder",
            "1 teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "5 tablespoons water",
            "2 tablespoons flaxseed meal",
            "\u00be cup soy milk",
            "1\u2009\u00bd tablespoons vegetable oil, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve vegan stock cubes in the boiling water in a bowl. Add garlic, thyme, rosemary, sage, and 1 pinch salt. Cut tofu into 1-inch cubes and add to the marinade. Cover with plastic wrap and refrigerate, 4 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1 1/4 cups flour, baking powder, 1 teaspoon salt, and pepper together in a bowl. Place remaining flour in a shallow bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 5 tablespoons water and flaxseed meal together in a separate bowl. Let \"egg\" stand until thickened, about 5 minutes. Add soy milk; mix well. Stir into the flour mixture until batter is well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take the tofu pieces and dip them in bowl of flour; move into the batter to coat. Fry in the hot oil until browned, 2 to 4 minutes per side.\n"
            }
        ],
