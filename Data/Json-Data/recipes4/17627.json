        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245193/blueberry-chia-seed-pancakes/",
        "name": "Blueberry Chia Seed Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Blueberry Chia Seed Pancakes"
        },
        "datePublished": "2019-04-04T04:00:26.000Z",
        "description": "Blueberry pancakes made with chia seeds and hemp milk.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "10 pancakes",
        "recipeIngredient": [
            "1 cup hot water",
            "2 tablespoons chia seeds",
            "1 cup hemp milk, or more as needed",
            "1 cup spelt flour",
            "1 cup millet flour",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 cups fresh blueberries",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir water and chia seeds together in a bowl; set aside until thickened, about 10 minutes. Add enough hemp milk to thin chia mixture and transfer to a blender; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine spelt flour, millet flour, baking powder, baking soda, and salt in a bowl; stir in chia mixture until batter is well mixed. Fold blueberries into batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in hot skillet. Drop batter by large spoonfuls onto the skillet and cook until bubbles form and the edges are dry, 3 to 4 minutes. Flip and cook until browned on the other side, 2 to 3 minutes. Repeat with remaining batter.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes",
            "Whole Grain Pancake Recipes"
        ],
