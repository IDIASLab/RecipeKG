        "mainEntityOfPage": "https://www.allrecipes.com/recipe/155060/indian-chai-hot-chocolate/",
        "name": "Indian Chai Hot Chocolate",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F765567.jpg",
