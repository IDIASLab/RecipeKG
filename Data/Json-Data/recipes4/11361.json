    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/44572/herbal-shrimp-delight-with-beer-sauce/",
        "name": "Herbal Shrimp Delight with Beer Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F58278.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T03:30:21.000Z",
        "description": "Chopped tiger shrimp on a delicious blend of greens and herbs, drizzled with my own beer and tomato sauce!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cup chopped fresh basil",
            "1 cup chopped fresh oregano",
            "1 cup chopped fresh parsley",
            "1 cup chopped fresh spinach",
            "1 cup chopped romaine lettuce",
            "5 tablespoons olive oil, divided",
            "1 teaspoon white wine",
            "2 tablespoons all-purpose flour",
            "\u00bc cup cold water",
            "1 small onion, chopped",
            "1 green onion, chopped",
            "5 cloves garlic, peeled and minced",
            "1 tomato, diced",
            "1 cup Mexican beer",
            "1 tablespoon fresh lime juice",
            "1 pound jumbo shrimp, peeled and deveined",
            "salt and pepper to taste",
            "2 tablespoons freshly grated Parmesan cheese",
            "freshly ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, toss together basil, oregano, parsley, spinach, romaine lettuce, 3 tablespoons olive oil, and white wine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, blend the flour and water to a paste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a medium saucepan over medium high heat, and saute the onion, green onion, and garlic 5 minutes, until tender. Mix in the tomato. Reduce heat to low, and blend in the flour and water. Cook and stir until thickened. Mix in the beer and lime juice, season with salt and pepper, and simmer while cooking the shrimp.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining olive oil in a separate medium saucepan. Saute the shrimp about 3 minutes on each side, until opaque.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove shrimp from heat, and finely chop while warm. Serve over the herb and greens mixture, and drizzle with beer sauce. Sprinkle with Parmesan and freshly ground black pepper.\n"
            }
        ],
