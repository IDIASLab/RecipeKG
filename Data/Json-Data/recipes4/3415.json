    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/167197/frosted-strawberries/",
        "name": "Frosted Strawberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4504384.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2019-04-02T06:50:39.000Z",
        "description": "This is one of my favorite summer treats.  They are elegant enough for a Mother's Day brunch, but easy enough for a mid-week treat.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H35M",
        "recipeYield": "24 strawberries",
        "recipeIngredient": [
            "2 pints whole fresh strawberries",
            "1 cup  roasted peanuts, finely chopped",
            "1 (16 ounce) container <a href=\"https://www.allrecipes.com/recipe/8379/basic-cream-cheese-frosting/\" aria-label=\"cream cheese frosting\">cream cheese frosting</a>"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Wash and pat dry the strawberries with paper towels, leaving the green caps and stems intact.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment or waxed paper, and set aside. Place the finely chopped peanuts in a bowl, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the frosting in the microwave until warm and melted, 20 to 40 seconds. Hold each strawberry by the green top, and dip it straight down into the warm frosting, coating about 3/4 of the berry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently roll the dipped strawberry in chopped peanuts, and place on the lined baking sheet to set up. Serve as soon as they have set.\n"
            }
        ],
