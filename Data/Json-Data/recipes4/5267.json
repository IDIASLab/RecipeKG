        "totalTime": "P0DT0H25M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "6 tablespoons butter",
            "1 cup freshly squeezed orange juice",
            "\u00bc cup white wine",
            "2 lobster tails with shells"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir orange juice into melted butter and bring mixture to a simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in white wine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lobster tails in orange juice and wine mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and steam until lobster shells turn bright red and the meat is opaque, 10 to 15 minutes.\n"
            }
        ],
