    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256241/brazilian-christmas-rice/",
        "name": "Brazilian Christmas Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4222041.jpg",
            "width": 1920,
            "height": 1080
        },
        "datePublished": "2019-04-04T04:07:31.000Z",
        "description": "This festive rice pilaf is a Christmas dish from Brazil. The rice is cooked with Champagne and studded with dried fruits and sun-dried tomatoes.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup Thompson seedless raisins",
            "3 tablespoons extra-virgin olive oil",
            "1 onion, chopped",
            "2 cloves garlic, minced",
            "1\u2009\u00be cups uncooked white rice",
            "salt to taste",
            "2 cups boiling water",
            "1 cup Champagne",
            "\u00be cup dried apricots",
            "1 carrot, peeled and grated",
            "1 tablespoon butter",
            "6 tablespoons chopped sun-dried tomatoes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak raisins in a bowl of hot water until soft, 5 to 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large saucepan over medium heat. Cook and stir onion and garlic in the hot oil until fragrant, about 5 minutes. Stir in raisins, rice, and salt; cook and stir until rice is coated with oil, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour boiling water and Champagne into the saucepan. Cover and cook until liquid is absorbed and rice is tender, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dried apricots in a small saucepan and cover with water. Bring to a boil; simmer until soft, about 5 minutes. Drain and cool slightly, about 5 minutes. Cut into strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place grated carrot in a small bowl and cover with boiling water. Season with salt. Let soak until softened, about 5 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Stir in apricot strips and sun-dried tomatoes; cook for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold apricot strips and sun-dried tomatoes into the cooked rice. Pile rice onto a serving platter; garnish with grated carrot.\n"
            }
        ],
