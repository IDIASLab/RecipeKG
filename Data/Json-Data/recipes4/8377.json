    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246418/traditional-irish-stew/",
        "name": "Traditional Irish Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4485547.jpg",
            "width": 3069,
            "height": 3069
        },
        "datePublished": "2019-04-02T07:06:31.000Z",
        "description": "Pure comfort food for a chilly day for the slow cooker! A little prep time needed up front. Don't be fooled by how much onion and garlic is used. It's honestly not too much! Cheers!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT8H11M",
        "totalTime": "P0DT8H26M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil, or to taste",
            "2 pounds beef chuck roast, cubed",
            "3 russet potatoes, diced",
            "1 pound baby carrots",
            "1 large onion, chopped",
            "4 cloves garlic, minced",
            "1 (16 ounce) bottle stout beer, divided",
            "1 (6 ounce) can tomato paste",
            "1 cup beef broth",
            "1 tablespoon garlic powder",
            "1 tablespoon onion powder",
            "1 tablespoon smoked paprika",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium heat. Add beef; cook until evenly browned, 3 to 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes, carrots, onion, and garlic in a slow cooker; top with browned beef.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1/4 cup beer into the same skillet and bring to a boil while scraping the browned bits of beef off of the bottom of the skillet with a wooden spoon. Stir in tomato paste; cook until thickened, about 5 minutes. Pour beer mixture into slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour beef broth, remaining beer, garlic powder, onion powder, paprika, salt, and pepper into slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 8 hours.\n"
            }
        ],
