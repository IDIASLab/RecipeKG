    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237950/balsamic-grilled-baby-potatoes/",
        "name": "Balsamic Grilled Baby Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1133627.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2019-04-02T07:03:19.000Z",
        "description": "If you've got the grill going anyway, this is a quick and easy side dish that is a hit in our household.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds baby (1 to 2-inch long) potatoes, halved lengthwise",
            "2 tablespoons balsamic vinegar",
            "1 tablespoon olive oil",
            "2 tablespoons onion powder",
            "salt and ground black pepper to taste",
            "2 tablespoons butter, cut into small chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a 24-inch long piece of aluminum foil on a flat work surface. Spread potatoes onto the middle of the foil. Drizzle vinegar and olive oil over the potatoes; season with onion powder, salt, and pepper. Dot the top of the potatoes with small chunks of butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring opposing sides of the foil up and around the potatoes. Fold the two edges together at the top to create a seam on the top of the packet. Fold the remaining two edges to seal the packet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook potatoes in the packet on the preheated grill with the seam-side facing up for about 10 minutes, turn and cook another 10 minutes. Turn the packet once again and continue cooking until a fork stuck through the foil and into the potatoes meet no resistance from the contents, about 5 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables"
        ],
