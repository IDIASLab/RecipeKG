    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/183365/warm-tuna-buns/",
        "name": "Warm Tuna Buns",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F573941.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:51:25.000Z",
        "description": "Wonderful tuna melt type sandwich that is a big hit with kids, teens and adults. Freezes great.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 sandwiches",
        "recipeIngredient": [
            "3 eggs",
            "2 (3 ounce) cans tuna, drained",
            "7 tablespoons mayonnaise",
            "\u00bc cup shredded processed cheese food (such as Velveeta\u00ae)",
            "2 tablespoons sweet pickle relish",
            "2 tablespoons minced onion",
            "3 tablespoons chopped pimiento-stuffed olives",
            "6 hamburger buns"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a saucepan and cover with water. Bring to a boil, remove from heat, and let eggs stand in hot water for 15 minutes. Remove eggs from hot water, cool under cold running water. Peel and chop eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the chopped eggs together with tuna, mayonnaise, processed cheese, relish, onion, and olives in a mixing bowl. Divide the tuna mixture among the bun bottoms, then replace the bun tops.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap each sandwich individually in aluminum foil and place onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the cheese has melted and the tuna salad has heated through, 10 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Tuna"
        ],
