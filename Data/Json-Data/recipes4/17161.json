    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240794/hi-rise-easy-yorkshire-pudding/",
        "name": "Hi-Rise Easy Yorkshire Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2192657.jpg",
            "width": 6192,
            "height": 6192
        },
        "datePublished": "2019-04-04T03:59:06.000Z",
        "description": "Simple and easy Yorkshire pudding recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "3 eggs",
            "2 egg whites",
            "\u00bd cup milk",
            "1 cup all-purpose flour",
            "2 tablespoons cold water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Drop 1 teaspoon olive oil each into 6 muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs, egg whites, and milk together in a glass bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat egg mixture in microwave oven until warmed, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour into the egg mixture until completely incorporated into a batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat muffin cups in preheated oven until the oil is smoking, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cold water into batter. Pour 1/4 cup batter into each muffin cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until risen and cooked in their centers, about 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Popovers and Yorkshire Pudding Recipes"
        ],
