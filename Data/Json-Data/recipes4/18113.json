        "name": "Low-Calorie Root Beer Mocha Frappe",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6842817.jpg",
            "width": 3264,
