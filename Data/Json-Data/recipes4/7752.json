        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242090/crusty-garlic-bread/",
        "name": "Crusty Garlic Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2229915.jpg",
            "width": 2179,
            "height": 2179
        },
        "datePublished": "2019-04-02T07:04:32.000Z",
        "description": "Wonderful garlic bread. Quick and easy. Be careful when unwrapping bread, it will be hot and steamy. Adjust garlic amount to taste.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "3 tablespoons dried minced garlic",
            "1 teaspoon dried basil",
            "\u00bd teaspoon salt",
            "1 loaf French bread, halved lengthwise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, garlic, basil, and salt together in a bowl until smooth. Spread butter mixture onto each half of bread. Wrap each half of bread in aluminum foil and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and crusty, about 20 minutes.\n"
            }
        ],
