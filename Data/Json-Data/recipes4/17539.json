    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244617/molasses-almond-quinoa-vegan-cookies/",
        "name": "Molasses-Almond Quinoa Vegan Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Molasses-Almond Quinoa Vegan Cookies"
        },
        "datePublished": "2019-04-04T04:00:10.000Z",
        "description": "Quinoa, a super grain, in a cookie! Molasses and almonds add extra nutrition to these tasty treats. Enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H32M",
        "totalTime": "P0DT0H57M",
        "recipeYield": "46 cookies",
        "recipeIngredient": [
            "2 cups water",
            "1 cup quinoa",
            "1 cup warm water",
            "3 tablespoons flax seeds",
            "1 cup spelt flour",
            "1 cup amaranth flour",
            "1 cup chopped raw almonds",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "\u00bd cup agave nectar",
            "\u00bd cup crystallized sugar cane juice (such as Sucanat\u00ae)",
            "\u00bc cup molasses",
            "2 teaspoons almond extract",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring 2 cups water and quinoa to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until quinoa is tender, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend 1 cup warm water and flax seeds in a blender until seeds are broken down; let sit until flax mixture is thickened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 2 cup cooked quinoa, spelt flour, amaranth flour, almonds, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine agave nectar, crystallized sugar cane juice, molasses, almond extract, and vanilla extract with flax mixture and blend on high speed until smooth. Mix flax mixture with flour mixture until dough is thoroughly mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon dough onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned around the edges, 12 to 15 minutes.\n"
            }
        ],
