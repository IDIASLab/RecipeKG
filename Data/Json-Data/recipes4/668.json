    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24666/hot-bruschetta-dip/",
        "name": "Hot Bruschetta Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4923041.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2019-04-02T06:42:45.000Z",
        "description": "A small round of brie is softened in the microwave and topped with a flavorful bruschetta mixture. This is excellent for cocktail parties, or any day.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT0H12M",
        "recipeYield": "1 dip",
        "recipeIngredient": [
            "2 roma (plum) tomatoes, chopped",
            "1 small onion, chopped",
            "1 clove garlic, peeled and minced",
            "1 teaspoon chopped fresh basil",
            "1 teaspoon chopped fresh parsley",
            "\u00bc cup olive oil",
            "1 (8 ounce) wheel Brie cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix roma (plum) tomatoes, onion, garlic, basil, parsley and olive oil. Cover and chill in the refrigerator at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove top layer of rind from brie. Place brie in a small microwave safe serving dish. Microwave on high 1 minute, or until the cheese begins to soften. Spoon roma tomato mixture over the cheese. Microwave on high 1 minute, or until slightly melted. Serve immediately with Paris toasts or other dried breads.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Hot Cheese Dip Recipes"
        ],
