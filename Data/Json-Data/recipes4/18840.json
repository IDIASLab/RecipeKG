    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258248/cheesy-vegetarian-enchiladas/",
        "name": "Cheesy Vegetarian Enchiladas",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheesy Vegetarian Enchiladas"
        },
        "datePublished": "2019-04-04T04:08:42.000Z",
        "description": "Loaded with veggies and topped with cheese. Yummy and quick!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H31M",
        "totalTime": "P0DT2H1M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "4 cups water",
            "2 cups uncooked brown rice",
            "1 tablespoon olive oil, or to taste",
            "2 bell peppers, diced",
            "1 onion, diced",
            "2 cups fresh spinach",
            "1 cup green beans",
            "\u00bd cup diced mushrooms",
            "2 teaspoons chili powder, or to taste",
            "\u00bd teaspoon paprika, or to taste",
            "2 (15 ounce) cans fat-free refried beans",
            "10 (8 inch) flour tortillas",
            "2 (10 ounce) cans enchilada sauce",
            "1 (8 ounce) package shredded Mexican cheese blend"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and brown rice to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and liquid has been absorbed, 45 to 50 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat. Add bell peppers, onion, spinach, green beans, and mushrooms; cook and stir until soft, about 8 minutes. Season with chili powder and paprika.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon refried beans into a microwave-safe container. Cover and microwave until hot, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a large baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide brown rice, bell pepper mixture, and refried beans evenly among tortillas. Roll up tortillas to enclose filling; arrange seam-side down in the baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour enchilada sauce over rolled tortillas. Sprinkle Mexican cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted, about 30 minutes. Let cool for 5 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegetarian"
        ],
