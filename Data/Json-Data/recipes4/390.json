        "recipeYield": "4 servings",
        "recipeIngredient": [
            "5 pork chops",
            "\u00bd cup orange marmalade",
            "\u00bd cup soy sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pork chops in a 9x13 baking dish.  In a small bowl stir together the marmalade and soy sauce.  Pour over the chops.  Cover with foil and bake for 1 hour.\n"
            }
        ],
