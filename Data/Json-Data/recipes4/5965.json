        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229001/brussels-sprouts-pizza/",
        "name": "Brussels Sprouts Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1105539.jpg",
            "width": 1300,
            "height": 975
        },
        "datePublished": "2019-04-02T06:58:43.000Z",
        "description": "A white pizza with Brussels sprouts and pancetta.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 teaspoon extra-virgin olive oil",
            "9 slices pancetta",
            "5 teaspoons extra-virgin olive oil",
            "2 cloves garlic, minced",
            "6 Brussels sprouts, trimmed and thinly sliced",
            "1 (8 ounce) package shredded mozzarella cheese",
            "\u00bd teaspoon fennel seed",
            "1 12-inch pizza crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 475 degrees F (245 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon olive oil in a skillet over medium heat; cook and stir pancetta until fat has been released, 3 to 5 minutes. Transfer pancetta to a paper towel-lined plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour remaining 5 teaspoons olive oil into the same skillet; cook and stir garlic until fragrant, about 20 seconds. Add Brussels sprouts to garlic; cook, stirring constantly, until sprouts begin to brown, 5 to 10 minutes. Transfer sprouts and garlic to a bowl and crumble cooled pancetta over the mixture. Add mozzarella cheese and fennel seed; toss to coat. Place pizza crust onto a baking sheet and spread Brussels sprouts mixture atop pizza crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and bubbling, about 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
        ],
