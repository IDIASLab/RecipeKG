    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259477/garlic-sauce-with-broccoli-and-bacon/",
        "name": "Garlic Sauce with Broccoli and Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Garlic Sauce with Broccoli and Bacon"
        },
        "datePublished": "2019-04-04T04:09:19.000Z",
        "description": "Put this over your favorite pasta and top with Parmesan cheese. Even my kids will eat this!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H22M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 slices bacon",
            "2 tablespoons minced fresh garlic",
            "1 quart whole milk",
            "1 (12 ounce) bag broccoli florets",
            "2 teaspoons garlic powder",
            "salt and ground black pepper to taste",
            "\u00bc cup water",
            "3 tablespoons all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Reserve fat; drain bacon slices on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place garlic in the same skillet; cook over medium heat until golden, about 2 minutes. Add milk, broccoli, garlic powder, salt, and pepper; reduce heat to medium-low. Cook and stir until milk is heated through, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir water and flour into the milk mixture. Cook, stirring constantly, until sauce is thickened, 5 to 7 minutes. Mix in the cooked bacon.\n"
            }
        ],
