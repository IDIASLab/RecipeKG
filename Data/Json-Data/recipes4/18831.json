    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258189/lisas-pizza-pockets/",
        "name": "Lisa's Pizza Pockets",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lisa's Pizza Pockets"
        },
        "datePublished": "2019-04-04T04:08:40.000Z",
        "description": "Get ready for the football game! Pizza pockets are fun and everyone of all ages loves them!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (1 pound) loaf frozen bread dough, thawed",
            "cooking spray",
            "1 (8 ounce) can pizza sauce",
            "1 (6 ounce) can tomato paste with Italian herbs (such as Contadina\u00ae)",
            "1 (7 ounce) package pepperoni (such as Hormel\u00ae)",
            "2 cups mozzarella cheese",
            "1 egg white",
            "2 tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Divide bread dough into 4 balls. Place on a flat work surface; cover with plastic wrap sprayed with cooking spray. Let stand until room temperature and pliable, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dust work surface with flour and roll out dough into 8-inch circles. Transfer to baking sheets lined with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pizza sauce and tomato paste together in a bowl. Spread 2 tablespoons of the sauce mixture over each circle, leaving a 1-inch border.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange 15 pepperoni slices and 1/2 cup mozzarella cheese over one half of each circle. Fold circles in half over pepperoni and cheese; pinch edges and fold to seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg white and water together in a small bowl. Brush over top of dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 30 minutes.\n"
            }
        ],
