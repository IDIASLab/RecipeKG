    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254374/nutty-yogi-banana-muffins/",
        "name": "Nutty Yogi Banana Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4496752.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "2019-04-04T04:06:33.000Z",
        "description": "Healthier than flour and sugar recipes, these muffins are naturally sweet, filling, egg-free, and protein-rich. Kid-tested and approved.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "20 muffins",
        "recipeIngredient": [
            "1 cup almonds",
            "2 very ripe bananas, mashed",
            "1 cup teff flour",
            "1 cup brown rice flour",
            "\u00be cup almond milk",
            "\u00bd cup coconut oil, room temperature",
            "2 tablespoons water",
            "2 tablespoons ground flax seeds",
            "1 tablespoon ground cinnamon",
            "1 teaspoon sea salt",
            "1 teaspoon baking powder",
            "\u00bc teaspoon ground allspice",
            "20 walnut pieces, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line 2 muffin tins with cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place almonds in a coffee grinder or food processor and grind into a meal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine almond meal, mashed bananas, teff flour, brown rice flour, almond milk, coconut oil, water, and flax seeds in a large bowl. Blend with an electric mixer until smooth. Mix cinnamon, salt, baking powder, and allspice into the batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into liners, filling each about 2/3 full. Lightly press a walnut piece on top each one.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 35 minutes.\n"
            }
        ],
