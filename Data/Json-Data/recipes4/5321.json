    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220558/corned-beef-and-bacon-wrapped-chicken-breasts/",
        "name": "Corned Beef and Bacon-Wrapped Chicken Breasts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F889745.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:56:34.000Z",
        "description": "This recipe doesn't take too long to prepare, and is very good served with the sauce over rice. I usually tenderize the chicken breasts a little before wrapping them.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 slices thin-sliced cooked corned beef",
            "6 skinless, boneless chicken breast halves",
            "6 slices bacon",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (8 ounce) container sour cream",
            "1 head broccoli, broken into florets"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap 1 corned beef slice around each chicken breast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap a slice of bacon around outside of corned beef wrapping. Secure with toothpicks if needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange wrapped chicken in a deep baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange broccoli florets around wrapped chicken breasts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cream of mushroom soup and sour cream together in a bowl. Pour the mixture over the chicken and broccoli to completely cover. Cover baking dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until chicken is no longer pink in the center and the juices run clear, about 1 1/2 hours. Remove foil from the dish, return to oven, and bake until bacon browns, about 30 more minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
