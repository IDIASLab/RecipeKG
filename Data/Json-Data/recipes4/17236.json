        "name": "Rice and Lentils from a Rice Cooker",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5069513.jpg",
            "width": 3024,
            "height": 4032
