    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/28035/herbed-sweet-n-sour-chicken/",
        "name": "Herbed Sweet 'n' Sour Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F85451.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T03:29:02.000Z",
        "description": "An interesting variation of the normal sweet and sour chicken.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u2154 cup uncooked long-grain white rice",
            "1\u2009\u2153 cups water",
            "6 tablespoons olive oil",
            "6 tablespoons soy sauce",
            "6 tablespoons honey",
            "3 tablespoons distilled white vinegar",
            "1\u2009\u00bd teaspoons dried thyme",
            "1\u2009\u00bd teaspoons ground paprika",
            "\u00bd teaspoon ground cayenne pepper",
            "\u00bd teaspoon ground allspice",
            "1 teaspoon ground black pepper",
            "4 skinless, boneless chicken breast halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix rice and water in a medium saucepan, and bring to a boil. Cover, reduce heat, and simmer 20 minutes, until rice is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, blend the olive oil, soy sauce, honey, and vinegar. Mix in the thyme, paprika, cayenne pepper, allspice, and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts in a medium baking dish, and coat with the olive oil mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken in the preheated oven, and, basting occasionally with the olive oil mixture, cook 30 minutes, or until no longer pink and juices run clear. Serve over the rice with remaining olive oil mixture.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
