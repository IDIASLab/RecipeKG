        "name": "Vegetarian Alternative to Ground Beef",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5186904.jpg",
            "width": 2477,
            "height": 2477
