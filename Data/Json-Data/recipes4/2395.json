        "mainEntityOfPage": "https://www.allrecipes.com/recipe/86696/campfire-foil-onion/",
        "name": "Campfire Foil Onion",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F454833.jpg",
