        "recipeIngredient": [
            "\u00bc cup Brazil nuts",
            "spring water as needed",
            "1 frozen banana, cut into small chunks",
            "\u00bd cup plain kefir",
            "\u00bc cup puffed amaranth",
            "1 tablespoon cocoa powder",
            "1 teaspoon vanilla extract",
            "1 teaspoon honey",
            "spring water as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour enough spring water over Brazil nuts in a bowl to cover by several inches; soak in refrigerator 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain Brazil nuts and put them into a blender; add banana, kefir, amaranth, cocoa powder, vanilla extract, and honey. Blend mixture until smooth, adding spring water as needed to reach desired consistency.\n"
            }
