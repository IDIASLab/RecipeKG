        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25477/bbq-potato-roast/",
        "name": "BBQ Potato Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F71559.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:43:13.000Z",
        "description": "This is an all time favorite with everyone I have ever served it to. Seasoning and oil may be changed depending on taste preference. Serve with sour cream.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "10 potatoes, peeled and halved",
            "\u00bd cup vegetable oil",
            "2 tablespoons seasoned salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes in a large saucepan with enough lightly salted water to cover. Bring to a boil. Cook 15 minutes, or until tender but firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain potatoes, and pat dry. Coat thoroughly with vegetable oil and seasoned salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes on the preheated grill. Cook approximately 20 minutes, turning periodically.\n"
            }
        ],
