    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230624/meatloaf-with-beer/",
        "name": "Meatloaf with Beer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3376240.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2019-04-02T06:59:47.000Z",
        "description": "My husband is a big fan of this meatloaf. Anything you can put beer in is a winner for us!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "\u2154 pound ground beef",
            "\u2154 pound ground pork",
            "\u2154 pound ground veal",
            "2 eggs",
            "\u2154 cup dry bread crumbs",
            "\u00bd cup pale ale",
            "\u00bd cup chopped onion",
            "3 tablespoons ketchup",
            "1 tablespoon Dijon mustard",
            "2 teaspoons dried basil",
            "1 teaspoon garlic powder",
            "1 teaspoon salt",
            "\u00bd teaspoon black pepper",
            "\u2153 cup ketchup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Coat a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ground beef, ground pork, ground veal, eggs, bread crumbs, pale ale, onion, 3 tablespoons ketchup, mustard, basil, garlic powder, salt, and pepper in a large bowl; mix well. Transfer mixture to prepared baking sheet; shape into 10x5x2-inch loaf. Spread 1/3 cup ketchup on top of the loaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until no longer pink in the center, 60 to 65 minutes. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C). Cool meatloaf 10 minutes before slicing.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Beef Meatloaf Recipes"
        ],
