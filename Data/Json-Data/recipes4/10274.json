    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18140/shrimp-mold/",
        "name": "Shrimp Mold",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2020880.jpg",
            "width": 4596,
            "height": 3440
        },
        "datePublished": "1999-06-25T18:11:30.000Z",
        "description": "A great shrimp appetizer! Serve with crackers or toasted French bread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "5 appetizer servings",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed tomato soup",
            "2 (.25 ounce) envelopes unflavored gelatin",
            "\u00bd cup cold water",
            "6 ounces cream cheese, softened",
            "1 cup mayonnaise",
            "1 pound cooked shrimp",
            "1 cup minced celery",
            "\u00bd cup green onions, minced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat soup in a small saucepan or in a microwave oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir gelatin into cold water. Mix hot tomato soup into the gelatin liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium size mixing bowl, combine cream cheese and mayonnaise together. Mix soup mixture into the mayonnaise and cream cheese mixture. Mix well. Add shrimp, celery, and green onions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the mixture into a 1 quart mold and refrigerate for at least 1 hour, but preferably overnight. Unmold onto a serving plate before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Shrimp Dip Recipes"
        ],
