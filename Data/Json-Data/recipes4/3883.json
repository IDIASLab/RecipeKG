        "mainEntityOfPage": "https://www.allrecipes.com/recipe/199382/hawkeye-pork-roast/",
        "name": "Hawkeye Pork Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F357893.jpg",
