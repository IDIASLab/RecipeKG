    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228803/spinach-beet-soup/",
        "name": "Spinach Beet Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2980345.jpg",
            "width": 1696,
            "height": 1697
        },
        "datePublished": "2019-04-04T03:53:32.000Z",
        "description": "This soup can be a whole meal in itself for finicky eaters, especially kids.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup orzo pasta",
            "4 cups fresh spinach",
            "\u00bd cup canned sliced beets",
            "1 teaspoon butter",
            "2 cups water, or to taste",
            "1 teaspoon thyme",
            "salt and ground black pepper to taste",
            "2 tablespoons sour cream",
            "\u00bc cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook orzo in the boiling water, stirring occasionally until cooked through but firm to the bite, about 11 minutes. Drain, reserving some of the water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine spinach and beets in a blender adding as much of the reserved water as needed to help the mixture blend smoothly; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large pot over medium heat. Stir blended spinach mixture with the melted butter; cook until the color begins to change, 5 to 7 minutes. Stir water into the mixture to thin the liquid. Stir drained orzo pasta into the soup; season with thyme, salt, and pepper. Remove pot from heat and stir sour cream through the soup. Sprinkle with Cheddar cheese to serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Spinach Soup Recipes"
        ],
