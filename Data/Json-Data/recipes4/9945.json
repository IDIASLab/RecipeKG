        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14882/rouladen-rats/",
        "name": "Rouladen Rats",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1130740.jpg",
            "width": 960,
