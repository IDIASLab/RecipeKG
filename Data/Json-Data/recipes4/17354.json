        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241921/moms-pasta-dough/",
        "name": "Mom's Pasta Dough",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4462339.jpg",
            "width": 3072,
