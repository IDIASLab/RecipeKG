    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/49506/horseradish-gelatin-salad/",
        "name": "Horseradish Gelatin Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7515572.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-04T03:30:47.000Z",
        "description": "A tangy gelatin salad with horseradish to add zip. A tasty addition to a cold lunch on a hot day.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT4H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cup boiling water",
            "\u00bd tablespoon unflavored gelatin",
            "1 (3 ounce) package lime flavored Jell-O\u00ae mix",
            "1 cup mayonnaise",
            "1 cup small curd cottage cheese",
            "2 teaspoons prepared horseradish",
            "1 (20 ounce) can crushed pineapple, drained",
            "1 cup chopped pecans",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the boiling water, unflavored gelatin, and lime flavored gelatin mix until completely dissolved, about 1 minute. Set aside to cool until slightly thickened. This should take about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, mix the mayonnaise, cottage cheese, and horseradish with an electric mixer until foamy. Stir in the pineapple, pecans and salt by hand. Fold the thickened gelatin mixture into the pineapple mixture. Pour into molds, and refrigerate until set, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To release gelatin from molds, dip the mold in hot water for 30 seconds to loosen, then invert onto a serving plate.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes",
            "Jell-O&reg; Salad"
        ],
