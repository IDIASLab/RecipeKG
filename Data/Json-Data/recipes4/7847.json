        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244508/gluten-free-chicken-pizza/",
        "name": "Gluten-Free Chicken Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2707018.jpg",
            "width": 2288,
