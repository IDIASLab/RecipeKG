    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229037/easy-tasty-fried-chicken/",
        "name": "Easy Tasty Fried Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6918031.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2019-04-04T03:53:39.000Z",
        "description": "This is a simple recipe, and the chicken turns out delicious.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds boneless chicken pieces",
            "1 tablespoon seasoned salt",
            "2 cups all-purpose baking mix (such as Bisquick\u00ae)",
            "2 cups water",
            "1 quart vegetable oil for frying, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Trim any fat from chicken breasts and cut each in half lengthwise. Place chicken pieces in a bowl and sprinkle with seasoned salt, turning chicken pieces to season all sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place baking mix into a shallow bowl; pour water into a separate bowl. Gently press each seasoned chicken piece into the baking mix, dip quickly in water, and press chicken into baking mix again to coat thoroughly. Dip chicken once more quickly in water and coat a third time in baking mix. Tap off any loose baking mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Deep-fry the chicken pieces in batches in the hot oil until the coating is crisp and brown and the chicken is no longer pink inside, about 6 minutes per batch. An instant-read meat thermometer inserted into the thickest part of a breast should read at least 160 degrees F (70 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Fried Chicken Recipes"
        ],
