    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232093/orzo-and-zucchini-salad/",
        "name": "Orzo and Zucchini Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1020767.jpg",
            "width": 3888,
            "height": 2592
        },
        "datePublished": "2019-04-02T07:00:43.000Z",
        "description": "This fresh-tasting, make-ahead side dish was created with casual outdoor dining in mind. Best at room temperature.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup orzo",
            "4 teaspoons olive oil, divided",
            "2 zucchinis, quartered lengthwise and thinly sliced",
            "coarse salt and ground black pepper to taste",
            "\u00bd cup torn fresh basil",
            "1 tablespoon white wine vinegar, or to taste",
            "1 clove garlic, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook orzo in the boiling water, stirring occasionally until cooked through but firm to the bite, about 11 minutes. Drain and spread onto a rimmed baking sheet to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a large skillet over medium heat; cook and stir zucchini and garlic in the hot oil until slightly tender, 4 to 6 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix orzo, zucchini mixture, basil, vinegar, and remaining 1 teaspoon olive oil in a bowl; season with salt and pepper. Cover bowl with plastic wrap and refrigerate to blend flavors, 1 hour to 1 day. Bring to room temperature before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Zucchini Salad Recipes"
        ],
