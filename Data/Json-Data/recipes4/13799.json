    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/205536/dressed-cucumber/",
        "name": "'Dressed' Cucumber",
        "image": {
            "@type": "ImageObject",
