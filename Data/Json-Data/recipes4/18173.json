    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254401/lemon-pudding-lavender-cake/",
        "name": "Lemon Pudding Lavender Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon Pudding Lavender Cake"
        },
        "datePublished": "2019-04-04T04:06:33.000Z",
        "description": "A lemon cake with flavor brought to life with a touch of lavender. Great for get-togethers or as a weeknight dessert.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "cooking spray",
            "1 (15.25 ounce) package lemon cake mix",
            "1 cup water",
            "4 eggs",
            "\u00bd cup butter, softened",
            "1 (3 ounce) package instant lemon pudding mix",
            "1 tablespoon ground lavender",
            "1 (16 ounce) container lemon frosting",
            "2 tablespoons ground lavender"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a fluted tube pan (such as Bundt&reg;) with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cake mix, water, eggs, butter, instant pudding mix, and 1 tablespoon lavender in a large bowl. Beat with an electric mixer on medium-low speed, occasionally scraping down sides, until smooth, about 2 minutes. Pour into the prepared tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 40 to 50 minutes. Cool cake in the pan, 10 to 15 minutes. Invert onto a wire rack and allow to cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lemon frosting and 2 tablespoons lavender together in a small bowl until combined. Spread over cooled cake.\n"
            }
        ],
