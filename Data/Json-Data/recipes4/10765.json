        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24687/chocolate-orange-flavored-mousse/",
        "name": "Chocolate Orange Flavored Mousse",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F55515.jpg",
