    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242256/creamy-lime-pie/",
        "name": "Creamy Lime Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2610635.jpg",
            "width": 1350,
            "height": 980
        },
        "datePublished": "2019-04-04T03:59:46.000Z",
        "description": "This is my no-bake answer to traditional Key lime pie. It is quite rich with a more delicate lime flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT4H20M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 teaspoon unflavored gelatin",
            "\u00bc cup very hot water",
            "1 cup heavy whipping cream",
            "1 (8 ounce) package Neufchatel cheese, softened",
            "3 drops green food coloring",
            "1 limes, zested and juiced",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve gelatin in hot water in small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream in a chilled glass or metal bowl with an electric mixer until soft peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix Neufchatel cheese and green food coloring in a mixing bowl with an electric mixer on low until smooth and even in color. Add gelatin mixture and continue mixing to incorporate. Reserve a pinch of lime zest for garnishing the fishing pie; beat lime zest and lime juice into the cheese mixture. Fold the whipped cream into the cheese mixture using a spatula; spread into the prepared pie crust. Garnish with the reserved lime zest.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover pie loosely with plastic wrap and refrigerate until set, about 4 hours.\n"
            }
        ],
