    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246419/gerrys-easy-irish-stew/",
        "name": "Gerry's Easy Irish Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4418724.jpg",
            "width": 3310,
            "height": 3310
        },
        "datePublished": "2019-04-04T04:01:09.000Z",
        "description": "This is a super simple and delicious Irish stew recipe which is perfect on a Sunday...and it'll make your house smell delightful too!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H34M",
        "totalTime": "P0DT1H49M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "1 pound beef sirloin steak, cut into 1-inch cubes",
            "1 large onion, chopped",
            "2 carrots, chopped",
            "2 ribs celery, chopped",
            "3 cups hot water",
            "2\u2009\u00bd tablespoons dry brown gravy mix",
            "2 cubes beef bouillon",
            "\u00bc cup Worcestershire sauce",
            "1 teaspoon chili powder",
            "\u00bc teaspoon dried oregano",
            "8 potatoes, cubed",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a stockpot over medium heat. Cook beef until browned on all sides, 2 to 4 minutes per side. Stir in onion, carrots, and celery.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water, gravy mix, and bouillon together in a small bowl; pour into stockpot. Stir in Worcestershire sauce, chili powder, and oregano.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover stockpot, reduce heat to low, and simmer for 1 hour. Stir in potatoes, cover, and simmer until potatoes are easily pierced with a fork, about 30 minutes more. Season with salt and pepper.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Irish Stew Recipes"
        ],
