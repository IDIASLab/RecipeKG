    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235918/christmas-morning-casserole-muffins/",
        "name": "Christmas Morning Casserole Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2617347.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2019-04-02T07:02:28.000Z",
        "description": "These savory muffins freeze well so you can make these in advance.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 dozen muffins",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) package bulk pork sausage",
            "2 cups half-and-half",
            "1 cup baking mix (such as Bisquick\u00ae)",
            "6 eggs",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray 12 muffin cups with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir sausage in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and transfer sausage to prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix half-and-half, baking mix, eggs, salt, and black pepper together in a bowl; pour over sausage. Top each muffin with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cooked through and cheese is melted, about 25 minutes.\n"
            }
        ],
