    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221357/lebkuchen-apple-butter/",
        "name": "Lebkuchen Apple Butter",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lebkuchen Apple Butter"
        },
        "datePublished": "2019-04-04T03:52:14.000Z",
        "description": "Delicious slow cooker apple butter with the flavor of Christmas. It comes from the spices of German gingerbread, called lebkuchen. Not a family recipe, but it will be in 2 more generations. Keeps for 7 to 10 days when opened. Refrigerate after opening.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT10H20M",
        "totalTime": "P1DT10H50M",
        "recipeYield": "6 pints",
        "recipeIngredient": [
            "16 cups coarsely chopped apples",
            "2 cups water, or as needed",
            "1 cup honey",
            "2 tablespoons cocoa powder",
            "1 tablespoon ground cinnamon",
            "1 teaspoon ground cardamom",
            "1 teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 4 cups apples and 1/2 cup water in a high-powered blender; puree until smooth. Pour into a large slow cooker. Repeat with remaining apples and water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir honey, cocoa powder, cinnamon, cardamom, and cloves into the apple puree in the slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on High, stirring occasionally, until thickened and brown, about 10 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Immerse six 1-pint jars in simmering water for 5 minutes. Wash lids and rings in warm soapy water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack apple butter into hot jars, filling to within 1/4 inch of the top. Run a clean knife or thin spatula around the insides of the jars after they have been filled to remove any air bubbles. Wipe rims with a moist paper towel to remove any food residue. Top with lids and screw on rings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a rack in the bottom of a large stockpot and fill halfway with water. Bring to a boil and lower jars 2 inches apart into the boiling water using a holder. Pour in more boiling water to cover jars by at least 1 inch. Bring the water to a rolling boil, cover the pot, and process for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the jars from the stockpot and place onto a cloth-covered or wood surface, several inches apart, for 24 hours. Press the center of each lid with a finger to ensure the seal is tight and the lid does not move up or down. Store in a cool, dark area.\n"
            }
        ],
