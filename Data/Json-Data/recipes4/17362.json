        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241959/crescent-milk/",
        "name": "Crescent Milk",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3280450.jpg",
            "width": 1954,
            "height": 2824
        },
        "datePublished": "2019-04-04T03:59:36.000Z",
        "description": "Delicious part of juice cleanse.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "16 fluid ounces water, divided",
            "4 ounces cashews, or more to taste",
            "1 tablespoon agave nectar",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put 8 fluid ounces water, cashews, agave nectar, and vanilla extract in a blender; blend for 2 minutes. Strain mixture through cheesecloth or a fine-mesh sieve into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend the strained bits of cashew mixture in the blender with remaining 8 fluid ounces water until smooth. Strain mixture through cheesecloth or sieve again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour strained liquid into a bottle and add cinnamon; refrigerate until chilled, at least 30 minutes. Shake well before drinking.\n"
            }
        ],
