    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24478/swiss-cheese-scramble/",
        "name": "Swiss Cheese Scramble",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2688884.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2019-04-02T06:42:40.000Z",
        "description": "A family favorite that can be prepared the day before and baked the next morning.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 pound bacon",
            "1 cup bread cubes",
            "2\u2009\u00be cups milk",
            "12 eggs",
            "salt and pepper to taste",
            "\u00bc cup butter",
            "1 pound Swiss cheese, shredded",
            "4 teaspoons butter, melted",
            "1 cup dry bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium-high heat until evenly brown. Drain, crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, Combine the bread cubes and milk.  Drain after 5 minutes. In a separate bowl, beat together  milk, eggs, salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet or saucepan, melt 1/4 cup butter over medium heat. Add the egg mixture and scramble until soft.  Do not fully cook. Add the soaked bread cubes and turn into a greased 9x13 inch baking pan. Sprinkle shredded cheese on top of casserole. In a small bowl, stir together butter and bread crumbs. Sprinkle this mixture over the cheese and then top with the bacon. Cover and refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "The next morning, preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 minutes. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs"
        ],
