    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239628/baked-macaroni-and-cheese/",
        "name": "Baked Macaroni and Cheese!",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1594486.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2019-04-02T07:03:48.000Z",
        "description": "This is a great recipe for anyone who wants to spend a little more time and work on the details. It's well worth it. This tastes great!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd pound elbow macaroni",
            "3 tablespoons butter",
            "3 tablespoons all-purpose flour",
            "1 tablespoon mustard powder",
            "3 cups milk",
            "\u00bd cup minced yellow onion",
            "\u00bd teaspoon paprika",
            "1 bay leaf",
            "1 large egg",
            "12 ounces shredded sharp Cheddar cheese, divided",
            "1 teaspoon kosher salt",
            "freshly ground black pepper",
            "1 cup panko bread crumbs",
            "3 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 3 tablespoons butter in a pot over medium heat. Whisk flour and mustard powder into melted butter until smooth, about 5 minutes. Mix milk, onion, paprika, and bay leaf into flour mixture; bring to a boil, reduce heat to medium, and simmer until sauce is thickened, about 10 minutes. Remove bay leaf from sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat about 2 tablespoons sauce and egg together in a bowl to temper egg. Pour egg mixture into sauce and stir gently. Mix 3/4 of the Cheddar cheese into the sauce until melted and smooth, 2 to 3 minutes; season with salt and pepper. Fold macaroni into cheese sauce and pour into a 2-quart casserole dish. Sprinkle remaining cheese over macaroni mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir panko bread crumbs and 3 tablespoons melted butter together in a bowl; sprinkle over cheese layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and topping is lightly browned, about 30 minutes.\n"
            }
        ],
