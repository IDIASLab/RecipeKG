        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220927/vegan-coconut-macaroons/",
        "name": "Vegan Coconut Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3578416.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-04T03:52:04.000Z",
        "description": "This recipe is so easy and so simple. I was very surprised at how yummy they were.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "olive oil cooking spray (such as PAM\u00ae)",
            "2\u2009\u00bd cups unsweetened, flaked coconut",
            "1 cup unbleached flour",
            "\u00be cup cashews",
            "\u00be cup water",
            "\u00bd cup turbinado raw sugar",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine coconut and flour in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree cashews, water, sugar, salt, and almond extract together in a blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cashew mixture into coconut mixture until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form coconut mixture into 1-inch balls and flatten; arrange on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies in the preheated oven until edges are golden, 12 to 15 minutes.\n"
            }
        ],
