        "name": "Mushroom Chicken in Sour Cream Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F771712.jpg",
            "width": 4000,
            "height": 3000
