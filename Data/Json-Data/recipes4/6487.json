        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231858/warm-chili-cheese-dip/",
        "name": "Warm Chili Cheese Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7930645.jpg",
