    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263831/philly-cheese-steak-sloppy-joes/",
        "name": "Philly Cheese Steak Sloppy Joes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5355998.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2019-04-04T04:11:52.000Z",
        "description": "A quick and easy Philly cheese steak made in the style of a Sloppy Joe. Enjoy a salad or sweet potato fries on the side. Yum!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 Sloppy Joes",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "\u00bd red bell pepper, thinly sliced",
            "\u00bd green bell pepper, thinly sliced",
            "\u00bd yellow onion, thinly sliced",
            "1 clove garlic, minced",
            "1 cup cold water",
            "1 tablespoon cornstarch",
            "2 teaspoons beef bouillon granules",
            "1 teaspoon Worcestershire sauce",
            "salt and ground black pepper to taste",
            "\u00be cup shredded mozzarella cheese",
            "4 whole wheat hamburger buns, split"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes. Add bell peppers, onion, and garlic; saute until beginning to tenderize, about 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir water, cornstarch, bouillon, and Worcestershire sauce together in a bowl. Add to beef mixture in skillet. Cover with a lid and reduce heat to low. Simmer until sauce thickens and veggies are tender, 7 to 10 minutes. Season with salt and pepper. Add mozzarella cheese; cover until melted, about 2 minutes. Stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve beef mixture on buns, Sloppy Joe-style.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Sandwich Recipes",
            "Sloppy Joe Recipes"
        ],
