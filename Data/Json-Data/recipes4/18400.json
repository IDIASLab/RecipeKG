    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255717/tony-vs-vegan-pumpkin-muffins/",
        "name": "Tony V's Vegan Pumpkin Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tony V's Vegan Pumpkin Muffins"
        },
        "datePublished": "2019-04-04T04:07:14.000Z",
        "description": "Totally delicious vegan pumpkin muffins.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "15 muffins",
        "recipeIngredient": [
            "cooking spray",
            "1\u2009\u2153 cups fructose (fruit sugar)",
            "1 cup all-purpose flour",
            "\u00bd cup oat bran",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon pumpkin pie spice",
            "2 tablespoons water",
            "1\u2009\u00bd teaspoons dry egg replacer (such as Ener-G\u00ae)",
            "\u00bd (14 ounce) can pumpkin puree",
            "\u00bc cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease muffin tins with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix fructose, flour, oat bran, cinnamon, baking soda, baking powder, salt, and pumpkin pie spice together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk water and egg replacer together in another bowl until smooth. Stir in pumpkin puree and vegetable oil. Add fructose mixture; mix well to blend batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the greased muffin tins, filling each cup halfway.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 25 to 30 minutes. Transfer to a wire rack to cool.\n"
            }
        ],
