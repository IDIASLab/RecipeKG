    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26916/quebec-pickled-eggs/",
        "name": "Quebec Pickled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2286077.jpg",
            "width": 2378,
            "height": 2378
        },
        "datePublished": "2019-04-04T03:28:58.000Z",
        "description": "Pickled eggs are great served whole or in slices. They're perfect for salads and appetizer trays. The longer the eggs stand before serving, the stronger the vinegar flavor will be.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P1DT0H40M",
        "recipeYield": "12 pickled eggs",
        "recipeIngredient": [
            "12 eggs",
            "12 whole cloves",
            "2 slices fresh ginger root",
            "1 teaspoon black peppercorns",
            "1 bay leaf",
            "2 cups distilled white vinegar",
            "\u00bd cup water",
            "2 teaspoons salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a medium saucepan and cover with cold water. Bring water to a boil and immediately remove from heat. Cover and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool and peel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a cheesecloth, loosely wrap cloves, ginger root, black peppercorns and bay leaf. Place in a medium saucepan with distilled white vinegar, water and salt. Bring to a boil and continue boiling 10 minutes. Discard wrapped spices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place eggs in sterile containers. Cover with the hot vinegar mixture. Seal containers. Chill in the refrigerator at least 2 days before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Pickled Egg Recipes"
        ],
