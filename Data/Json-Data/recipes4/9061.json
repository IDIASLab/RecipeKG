    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244249/cheesy-kale-quiche/",
        "name": "Cheesy Kale Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3637646.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-03T06:31:42.000Z",
        "description": "Created when I had used all of my spinach for spinach dip, this creamy, simple quiche is great for family breakfasts!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 quiche",
        "recipeIngredient": [
            "1 (9 inch) pie crust pastry",
            "\u00bd (8 ounce) package cream cheese",
            "\u00bd cup milk",
            "4 eggs, beaten",
            "8 ounces sharp Cheddar cheese, shredded",
            "10 ounces chopped kale",
            "4 slices cooked bacon, crumbled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Press pie crust into a pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese in a microwave-safe bowl; heat in microwave until warmed, about 10 seconds. Stir cream cheese and milk together in a bowl; stir into beaten eggs. Stir Cheddar cheese into egg mixture; add kale and bacon. Pour egg mixture into the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top has golden brown spots and a toothpick inserted in the center comes out clean, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Quiche"
