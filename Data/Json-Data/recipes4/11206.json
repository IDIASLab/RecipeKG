        "mainEntityOfPage": "https://www.allrecipes.com/recipe/35392/mushrooms-a-la-gill/",
        "name": "Mushrooms a la Gill",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F71531.jpg",
            "width": 250,
