        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258126/homemade-crackers/",
        "name": "Homemade Crackers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6047993.jpg",
            "width": 2367,
            "height": 2367
        },
        "datePublished": "2019-04-04T04:08:38.000Z",
        "description": "Very easy-to-make crackers that can be customized to taste.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup whole wheat flour",
            "1 cup bread flour",
            "3 tablespoons olive oil",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon salt, or more to taste",
            "1 cup water, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix whole wheat flour, bread flour, olive oil, baking powder, and salt together in a large bowl. Mix in enough water to bring dough together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out on a lightly floured surface. Let rest for 10 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into 6 to 8 pieces. Roll out each piece to paper-thin thickness. Transfer pieces of dough to the lined baking sheets; cut into strips or squares.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned and edges curl up, 15 to 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
