        "mainEntityOfPage": "https://www.allrecipes.com/recipe/31620/eggplant-delight/",
        "name": "Eggplant Delight",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1506849.jpg",
