    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/144950/deep-fried-mushrooms/",
        "name": "Deep Fried Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F97238.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T03:44:31.000Z",
        "description": "Button mushrooms are battered and fried in oil for an irresistible snack. These are great for watching sports.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 teaspoon garlic salt",
            "1 egg",
            "1 cup water",
            "1 pound button mushrooms, quartered",
            "2 cups oil for frying, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the flour and garlic salt. Mix in egg and water until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large deep skillet over medium-high heat. Test to see if oil is hot by frying a drop of batter. If it sizzles and floats to the top, the oil is ready.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip mushrooms into the batter and then place in the hot oil. Fry a few at a time so they are not crowded, until golden brown, 3 to 5 minutes. Remove from oil with a slotted spoon and drain on paper towels.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Mushrooms"
        ],
