    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220837/moroccan-spicy-carrot-salad/",
        "name": "Moroccan Spicy Carrot Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Moroccan Spicy Carrot Salad"
        },
        "datePublished": "2005-11-11T00:30:43.000Z",
        "description": "Carrots are cut into thin, round pieces and cooked with the spices.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound carrots, peeled and sliced into thin rounds",
            "2 cups water",
            "2 cloves garlic, minced",
            "2 tablespoons olive oil",
            "\u00bd teaspoon sweet paprika",
            "1 pinch cayenne pepper, or to taste",
            "salt and ground black pepper to taste",
            "1 tablespoon wine vinegar",
            "\u00bd teaspoon ground cumin",
            "\u00bc cup cilantro leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine carrots, water, garlic, olive oil, paprika, cayenne pepper, salt, and black pepper in a shallow pan; bring to a boil over medium-high heat and cook until carrots are tender and water evaporates, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir vinegar and cumin through the carrot mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the pan from heat and set aside to allow salad to cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with cilantro to serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Carrot Salad Recipes"
        ],
