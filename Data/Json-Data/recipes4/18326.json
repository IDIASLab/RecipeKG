    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255297/cashew-chicken-bake/",
        "name": "Cashew Chicken Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4421226.jpg",
            "width": 1000,
            "height": 1000
        },
        "datePublished": "2019-04-04T04:07:01.000Z",
        "description": "This cashew chicken bake is a simple, one-dish dinner packed with protein and vegetables. A healthy, gluten-free meal the whole family will love! Garnish with green onions.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "\u00bd cup brown rice",
            "1 cup diced celery",
            "1 red bell pepper, diced",
            "1 green bell pepper, diced",
            "3 skinless, boneless chicken breasts, cut into 1-inch pieces",
            "1 cup water",
            "2 tablespoons gluten-free soy sauce (tamari)",
            "1 tablespoon rice vinegar",
            "1 tablespoon minced garlic",
            "1\u2009\u00bd teaspoons minced ginger",
            "\u00bd teaspoon ground cumin",
            "\u00bd teaspoon ground black pepper",
            "\u00bc teaspoon salt",
            "1 (8 ounce) can sliced water chestnuts, drained",
            "1 cup cashews"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a large casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread brown rice at the bottom of the casserole dish. Layer celery, red bell pepper, and green pepper on top. Top with chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water, soy sauce, rice vinegar, garlic, ginger, cumin, pepper, and salt in a small bowl. Pour over chicken. Arrange water chestnuts on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is tender, about 45 minutes. Add cashews. Continue baking until browned, about 10 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
