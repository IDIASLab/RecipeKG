    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260175/butter-chicken-chickpeas/",
        "name": "Butter Chicken Chickpeas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4555862.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-04T04:09:41.000Z",
        "description": "Move over chicken, there's a new vegetarian favourite in town. High in protein, this chickpea dish is made with classic Butter Chicken sauce.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon freshly ground pepper",
            "\u00bd onion, chopped",
            "1 red bell pepper, chopped",
            "1 tablespoon fresh ginger, minced",
            "2 cloves garlic, minced",
            "\u00bd cup water",
            "2 (15 ounce) cans chickpeas, drained and rinsed",
            "1 (400 mL) jar Patak's Butter Chicken Cooking Sauce",
            "2 tablespoons chopped fresh cilantro",
            "1 teaspoon red chili pepper flakes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in large Dutch oven or large saucepan over medium-high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add onion and red pepper and saute until onions are translucent.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chickpeas, garlic, and ginger and saute for 2 minutes. Add water and cook, stirring for 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in Patak's Butter Chicken Cooking Sauce, salt, and pepper, and bring to a simmer. Cook covered, stirring occasionally, for 5 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with cilantro and red chilli peppers (optional).\n"
            }
        ],
