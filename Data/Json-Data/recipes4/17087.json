        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240270/easiest-ice-caps-ever/",
        "name": "Easiest Ice Caps Ever",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2465588.jpg",
            "width": 2447,
            "height": 2447
