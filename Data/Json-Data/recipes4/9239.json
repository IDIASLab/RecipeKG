        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269583/three-cheese-grilled-cheese/",
        "name": "Three-Cheese Grilled Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6110080.jpg",
            "width": 960,
