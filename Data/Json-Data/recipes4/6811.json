        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233908/dijon-roasted-potatoes/",
        "name": "Dijon Roasted Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1039440.jpg",
            "width": 2314,
            "height": 2314
        },
        "datePublished": "2019-04-02T07:01:32.000Z",
        "description": "The Dijon mustard provides a slight tang that really kicks these potatoes to the next level! These are a family favorite at our house.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "1 tablespoon Dijon mustard",
            "1 clove garlic, minced",
            "\u00bc teaspoon thyme",
            "\u00bc teaspoon rosemary",
            "salt and ground black pepper to taste",
            "5 red potatoes, cut into 1-inch pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir olive oil, Dijon mustard, garlic, thyme, rosemary, salt, and pepper together in a bowl; add potatoes and toss to coat. Transfer potatoes to prepared roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in preheated oven, turning occasionally, until tender in the middle, 40 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Roasted Potato Recipes"
        ],
