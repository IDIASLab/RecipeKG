        "mainEntityOfPage": "https://www.allrecipes.com/recipe/163486/smooth-poblano-salsa/",
        "name": "Smooth Poblano Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F408114.jpg",
            "width": 250,
