        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17810/fifteen-bean-soup/",
        "name": "Fifteen Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4322795.jpg",
            "width": 3648,
