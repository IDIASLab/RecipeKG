    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257907/roasted-orange-leg-of-lamb/",
        "name": "Roasted Orange Leg of Lamb",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7233058.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-04T04:08:31.000Z",
        "description": "Another recipe from my mother. Simple yet delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H50M",
        "totalTime": "P0DT3H10M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 tablespoon seasoned salt, or as needed",
            "1 (3 pound) leg of lamb",
            "1 cup all-purpose flour",
            "1 tablespoon dry mustard",
            "1 cup fresh orange juice, or more as needed",
            "1 onion, chopped",
            "\u00bd cup water, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 500 degrees F (260 degrees C). Grease a roasting pan with olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rub seasoned salt over leg of lamb.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour and mustard together in a bowl. Rub over leg of lamb. Transfer to the prepared roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast leg of lamb in the preheated oven until browned, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 325 degrees F (160 degrees C). Add orange juice, onion, and water to the roasting pan. Continue baking lamb, basting with orange juice and water every 30 minutes, until tender, about 2 1/2 hours.\n"
            }
        ],
