    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259559/white-chocolate-truffle-cheesecake/",
        "name": "White Chocolate Truffle Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "White Chocolate Truffle Cheesecake"
        },
        "datePublished": "2019-04-04T04:09:22.000Z",
        "description": "Luxurious melt-your-mouth white chocolate cheesecake. Dust with cocoa powder or drizzle melted dark chocolate on top and serve with a side helping of a fruit coulis.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT8H36M",
        "recipeYield": "1 8-inch cheesecake",
        "recipeIngredient": [
            "2\u2009\u00be cups digestive biscuits, crushed",
            "\u00bd cup melted butter",
            "3 tablespoons water",
            "2\u2009\u00bc teaspoons unflavored gelatin",
            "1\u2009\u00bd cups heavy whipping cream",
            "4 teaspoons heavy whipping cream",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (9.7 ounce) package white chocolate, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix biscuits and butter together in a bowl until well blended. Press crust mixture into the base of a deep, 8-inch springform cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place water in a small bowl. Sprinkle gelatin on top. Let stand until thickened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a saucepan over medium-low heat; fill with water to just below the bottom. Place bowl of gelatin mixture over water; heat until mixture is dissolved, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1 1/2 cups plus 4 teaspoons cream in a chilled glass or metal bowl with an electric mixer until soft peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and white chocolate together in a separate bowl bowl using an electric mixer until even and smooth; fold in whipped cream. Add gelatin mixture gradually; mix until just combined. Pour batter over the crust. Chill until set, 8 hours to overnight.\n"
            }
        ],
