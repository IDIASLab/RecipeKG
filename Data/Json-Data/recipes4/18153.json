        "recipeIngredient": [
            "3 cups cider vinegar",
            "1 cup honey",
            "\u00bd cup ginger syrup",
            "11\u2009\u00bd cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir vinegar, honey, and ginger syrup together in a bowl until honey is dissolved; add water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill switchel until flavors combine, 12 to 25 hours.\n"
            }
