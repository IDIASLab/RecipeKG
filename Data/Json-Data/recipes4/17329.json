    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241731/jeera-rice/",
        "name": "Jeera Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4560046.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-04T03:59:31.000Z",
        "description": "A full-flavored, traditional Indian jeera rice. The whole garam masala adds a full and authentic taste and wonderful aroma to the rice.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 cup basmati rice",
            "2 teaspoons vegetable oil",
            "1 teaspoon cumin seeds",
            "4 whole cloves",
            "4 whole black peppercorns",
            "2 cardamom pods",
            "1 bay leaf",
            "1\u2009\u00bd cups water",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rinse rice 3 to 4 times and transfer to a bowl; cover with water and soak for at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat; cook and stir cumin until starting to pop, 2 to 4 minutes. Add cloves, peppercorns, cardamom pods, and bay leaf; cook and stir until fragrant, about 1 1/2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain rice and mix into spice mixture; add 1 1/2 cups water and salt. Cover skillet and cook on high for 5 minutes. Reduce heat to medium and continue cooking for 10 minutes. Reduce heat to low and cook for 15 minutes. Remove skillet from heat and let rest with cover on, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cover and fluff rice with a fork. Remove bay leaf, cardamom pods, cloves, and peppercorns.\n"
            }
        ],
