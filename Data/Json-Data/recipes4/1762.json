        "mainEntityOfPage": "https://www.allrecipes.com/recipe/66066/grilled-salmon-with-cilantro-sauce/",
        "name": "Grilled Salmon with Cilantro Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F699313.jpg",
