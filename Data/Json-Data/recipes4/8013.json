    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245211/southwestern-ham-and-corn-chowder/",
        "name": "Southwestern Ham and Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4868129.jpg",
            "width": 1638,
            "height": 1920
        },
        "datePublished": "2019-04-02T07:05:22.000Z",
        "description": "I created this chowder to help get rid of Thanksgiving leftovers and it has become a staple dinner for my family. It is hearty enough to be eaten as a main dish or an appetizer.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon coconut oil",
            "1 small onion, diced",
            "2 cloves garlic, minced",
            "1 teaspoon paprika",
            "1 teaspoon garlic salt",
            "\u00bd teaspoon cayenne pepper, or to taste",
            "\u00bd teaspoon ground cumin",
            "2 cups chicken broth",
            "1\u2009\u00bd cups cubed cooked ham",
            "1 (7 ounce) can southwest-style corn",
            "1 (15 ounce) can pumpkin puree",
            "1 (13.5 ounce) can coconut milk",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat coconut oil in a stockpot or large saucepan over medium-high heat; cook and stir onion, garlic, paprika, garlic salt, cayenne pepper, and cumin until onion is translucent, about 4 minutes. Add chicken broth, ham, and corn; bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat to medium-low and stir pumpkin and coconut milk into broth mixture. Simmer soup, stirring occasionally, until heated through, about 10 minutes; season with salt and pepper.\n"
            }
        ],
