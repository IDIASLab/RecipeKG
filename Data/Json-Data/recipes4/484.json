        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23725/pineapple-enchiladas/",
        "name": "Pineapple Enchiladas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6195760.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-02T06:42:15.000Z",
        "description": "If you like pineapple on pizza, you love this!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "1 (20 ounce) can crushed pineapple",
            "\u00bc cup sour cream",
            "2 cups shredded Cheddar cheese, divided",
            "1 (10 ounce) can enchilada sauce, divided",
            "6 (8 inch) flour tortillas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine pineapple, sour cream and 1 cup cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1/4 cup enchilada sauce in the bottom of a 9 x 13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill tortillas with pineapple mixture, roll and place in baking dish.  Pour on remaining enchilada sauce and sprinkle with remaining cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, covered, in preheated oven for 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Latin American",
            "Mexican"
        ],
