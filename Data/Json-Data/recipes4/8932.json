    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233253/cuddle-up-chili/",
        "name": "Cuddle Up Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060558.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "2019-04-03T06:31:15.000Z",
        "description": "I love making chili in the fall and snuggling up under a blanket on the couch with loved ones while we eat chili, corn bread, and watch the beloved Broncos! Serve with yummy sweet corn bread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "\u00bc onion, chopped, or to taste",
            "1 tablespoon chopped garlic",
            "2 tablespoons chili powder",
            "1 tablespoon ground cumin",
            "1 teaspoon onion powder",
            "\u00bd teaspoon cayenne pepper",
            "salt and ground black pepper to taste",
            "1 (28 ounce) can diced tomatoes",
            "1 (16 ounce) can hot chili beans",
            "1 (16 ounce) can chili beans",
            "1 (16 ounce) can tomato sauce",
            "1 (4 ounce) can diced green chile peppers",
            "1 (4 ounce) can diced jalapeno peppers",
            "1 lime, juiced",
            "1 dash hot sauce, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large pot over medium-high heat. Cook and stir beef, onion, and garlic in the hot pot until ground beef is browned and crumbly, 5 to 7 minutes; drain and discard grease. Mix chili powder, cumin, onion powder, cayenne pepper, salt, and black pepper into ground beef.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir tomatoes, hot chili beans, chili beans, tomato sauce, green chile peppers, jalapeno peppers, lime juice, and hot sauce into ground beef mixture; bring to a boil. Reduce heat and simmer until flavors blend, at least 30 minutes.\n"
            }
        ],
