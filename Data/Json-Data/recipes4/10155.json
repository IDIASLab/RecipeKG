    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17018/my-navy-bean-soup/",
        "name": "My Navy Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F511936.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "1999-10-30T18:54:27.000Z",
        "description": "A delicious rich and colorful soup. Good for a cool autumn evening.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT4H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package dried navy beans",
            "water to cover",
            "7 cups chicken stock",
            "\u00bd cup margarine",
            "4 carrots, chopped",
            "1 onion, chopped",
            "\u00bd pound bacon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Clean and sort the beans. Place them in a large bowl with water to cover and soak for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pot over medium heat, combine the beans and the chicken stock and allow to simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt the butter or margarine in a large skillet over medium heat. Add the carrots and onion and saute for 10 minutes, or until onion is caramelized. Add this mixture to the pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In the same skillet over medium high heat, saute the bacon for 10 to 15 minutes, or until it is crisp. Drain the grease and add the bacon to the pot. Continue to simmer the soup over medium heat, stirring frequently, until the beans are tender and the soup has thickened.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas"
        ],
