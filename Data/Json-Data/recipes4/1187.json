        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 tablespoon minced garlic",
            "\u00bc cup grated Parmesan cheese",
            "1 tablespoon garlic salt",
            "1 teaspoon Italian seasoning",
            "\u00bd teaspoon ground black pepper",
            "\u00bc teaspoon ground paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine softened butter, minced garlic and parmesan cheese. Season with garlic salt, Italian seasoning, pepper and paprika. Mix until smooth.\n"
            }
