    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235142/hello-dolly-bars/",
        "name": "Hello Dolly Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060368.jpg",
            "width": 2816,
            "height": 2112
        },
        "datePublished": "2019-04-02T07:02:02.000Z",
        "description": "Making and eating Hello Dollies have been a tradition in our family for a long time. Every Christmas we always have to make these.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "2 dozen bars",
        "recipeIngredient": [
            "1\u2009\u00bd cups graham cracker crumbs",
            "\u00bd cup butter, melted",
            "1 (14 ounce) can sweetened condensed milk",
            "1\u2009\u00bd cups semisweet chocolate chips",
            "1 cup chopped walnuts",
            "1 cup shredded coconut",
            "\u00bd cup white chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs and melted butter together in a bowl until evenly moistened; press into the bottom of a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1/2 the can sweetened condensed milk over the graham cracker crust. Sprinkle semisweet chocolate chips, walnuts, coconut, and white chocolate chips over the graham cracker crust; gently press chocolate, nuts, and coconuts into the crust. Drizzle remaining 1/2 can of condensed milk over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fragrant and coconut is golden, 25 to 30 minutes. Cool completely before cutting into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
