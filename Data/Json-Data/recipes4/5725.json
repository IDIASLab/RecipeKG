        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223427/parmesan-asparagus/",
        "name": "Parmesan Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1069424.jpg",
            "width": 3750,
            "height": 2366
        },
        "datePublished": "2019-04-02T06:58:04.000Z",
        "description": "So quick and easy, this cheesy asparagus dish goes great with pork or poultry. Even those in my family that turn up their noses to asparagus, love this dish!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "10 fresh asparagus spears, trimmed",
            "cooking spray",
            "1 tablespoon grated Parmesan cheese",
            "\u215b teaspoon garlic salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C). Grease a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly coat asparagus with cooking spray; place on prepared baking sheet. Combine Parmesan cheese and garlic salt in a small bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 6 minutes; turn asparagus and continue baking until tender, about 6 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle Parmesan cheese mixture over asparagus.\n"
            }
        ],
