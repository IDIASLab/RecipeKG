    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261483/cant-wait-microwave-lava-cake/",
        "name": "Can't Wait Microwave Lava Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4510959.jpg",
            "width": 1024,
            "height": 1024
        },
        "datePublished": "2019-04-04T04:10:32.000Z",
        "description": "This fudgy, easy can't-wait microwave lava cake is ready in only 7 minutes and uses cake mix and prepared frosting in the can. Mind blowing!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT0H22M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "1 (15.25 ounce) package spiced cake mix (such as Betty Crocker\u00ae)",
            "1 cup water",
            "3 eggs",
            "\u2153 cup vegetable oil",
            "1 cup chopped walnuts",
            "1 cup semisweet chocolate chips",
            "1 (16 ounce) can prepared chocolate frosting",
            "1 tablespoon confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a microwave-safe bowl or ramekins with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cake mix, water, eggs, and oil in a large bowl and beat with an electric blender on low speed until moistened, about 30 seconds. Increase to medium speed and beat for 2 minutes. Stir walnuts and chocolate chips into the batter. Pour half of the batter into the prepared microwave safe bowl or ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chocolate frosting over the batter and spread evenly over the top. Pour the rest of the batter on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave until a toothpick inserted in the middle of the cake comes out clean, 7 minutes for a bowl or 3 to 4 minutes for smaller ramekins. Remove from microwave and let rest, about 5 minutes. Dust with confectioners' sugar.\n"
            }
        ],
