    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230349/sweet-n-sour-meatballs/",
        "name": "Sweet 'N Sour Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1025838.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-02T06:59:37.000Z",
        "description": "Mix together and form into sweet 'n sour meatballs. Try also with ground turkey.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 (12 ounce) bottles chili sauce",
            "1 (18 ounce) jar grape jelly",
            "1 pound ground beef",
            "\u00bd cup plain bread crumbs",
            "1 small onion, chopped",
            "2 eggs",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine chili sauce and grape jelly in a slow cooker on Medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground beef, bread crumbs, onion, eggs, salt, and black pepper together in a bowl; form into golf ball-sized balls. Arrange meatballs on an ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned and cooked through, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cooked meatballs to the slow cooker and simmer for 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Meatball Appetizer Recipes"
        ],
