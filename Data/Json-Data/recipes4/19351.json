    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261715/easy-vegan-pot-pie/",
        "name": "Easy Vegan Pot Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Vegan Pot Pie"
        },
        "datePublished": "2019-04-04T04:10:39.000Z",
        "description": "Totally delicious vegan veggie pot pie that my omnivore husband gobbled most of in one meal!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 cup olive oil",
            "\u00bd cup ice water",
            "1 teaspoon salt",
            "2 tablespoons vegan margarine",
            "\u00be cup all-purpose flour",
            "1 teaspoon garlic powder",
            "\u00bd teaspoon cayenne pepper",
            "salt and ground black pepper to taste",
            "1\u2009\u00bd cups soy milk",
            "1 (16 ounce) package frozen mixed vegetables"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3 cups flour, olive oil, ice water, and salt in a bowl until a dough forms. Place half of dough in the prepared pie pan, pressing down to form the bottom crust. Roll out the remaining dough in a circle wide enough to cover the pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a small saucepan over medium heat. Stir in 3/4 cup flour to form a thick paste. Season with garlic powder, cayenne pepper, salt, and black pepper. Add soy milk and stir until combined with the paste. Bring to a boil; remove from heat and stir in mixed vegetables.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable mixture into the pie pan. Top with reserved pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pie crust is golden brown, 30 to 60 minutes.\n"
            }
        ],
