    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265001/besara-egyptian-fava-bean-soup/",
        "name": "Besara (Egyptian Fava Bean Soup)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Besara (Egyptian Fava Bean Soup)"
        },
        "datePublished": "2019-04-04T04:12:25.000Z",
        "description": "Ful beans, broad beans, or fava beans, the name depends on your region, pureed into a delicious and hearty soup that is vegan to boot.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT9H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 cups skinless dried fava beans",
            "5 cups water",
            "3 tablespoons olive oil",
            "2 leek (white part only), chopped",
            "1\u2009\u00bd onions, chopped",
            "5 cloves garlic, chopped",
            "1 cup chopped fresh spinach, or to taste",
            "1 teaspoon ground cumin, or more to taste",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place beans in a large pot with water to cover. Soak 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain beans and return to pot with 5 cups fresh water. Bring to a boil and simmer until soft, 45 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium heat. Add leeks, onions, and garlic; saute until softened, about 5 minutes. Add to the pot with the beans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add spinach, cumin, salt, and pepper to the soup and cook gently, 15 to 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill blender halfway with soup. Cover and hold lid down with a potholder; pulse a few times before leaving on to blend. Pour into a pot. Repeat with remaining soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat until warm until ready to serve.\n"
            }
        ],
