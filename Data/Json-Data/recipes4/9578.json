        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12201/never-fail-pie-crust-i/",
        "name": "Never Fail Pie Crust I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F358635.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "It never fails.  Good for custard pies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 crusts",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1\u2009\u00bd cups shortening",
            "1 teaspoon salt",
            "1 egg",
            "1 tablespoon distilled white vinegar",
            "5\u2009\u00bd tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the flour and salt.  Blend in shortening until texture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together egg, vinegar, and water.  Pour into flour mixture, and mix together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out between wax paper; no extra flour is needed to roll out dough if wax paper is used.  Use as directed in recipe.\n"
            }
        ],
