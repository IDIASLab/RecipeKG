    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10240/italian-wine-cookies/",
        "name": "Italian Wine Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Italian Wine Cookies"
        },
        "datePublished": "1997-12-29T23:49:12.000Z",
        "description": "This is a recipe my grandmother from Italy passed down.  It is similar to other Italian Wine Cookie recipes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups vegetable oil",
            "2\u2009\u00bd cups red wine",
            "water",
            "12 cups all-purpose flour",
            "1 quart vegetable oil for frying",
            "1 cup honey, warmed slightly"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Warm the wine  and oil over medium heat.  Remove from heat and add  12 cups of flour.  Stir in water, 1/2 cup at a time, until the dough comes together in a soft ball. Knead well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the dough into a snake and cut into two inch  pieces.  Run each piece over a basket or use a fork to get ridges in the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat  oil for frying in a large pot.  Deep fry cookies until medium brown.  Drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm honey in a frying pan until very liquid.  Roll the cookies in the honey, and then place in a dish.  Allow the honey to cool and serve.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
        ],
