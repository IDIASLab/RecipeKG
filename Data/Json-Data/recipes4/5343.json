    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220614/cheesy-potato-pancakes/",
        "name": "Cheesy Potato Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F880580.jpg",
            "width": 4288,
            "height": 2848
        },
        "datePublished": "2019-04-02T06:56:36.000Z",
        "description": "Delicious and cheesy, these potato pancakes are a huge hit with my 3 boys! Great as a side dish or for snacking! These travel well wrapped in foil for the lunch box. Can be served plain or with ranch dressing as a dip.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 russet potatoes, peeled and grated",
            "2 eggs",
            "\u00bd cup milk",
            "1 cup all-purpose flour",
            "\u00bd cup grated Parmesan cheese",
            "\u00bd cup shredded Cheddar cheese",
            "\u00bc cup real bacon bits",
            "1 teaspoon onion salt",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bc cup corn oil",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together potatoes, eggs, and milk in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, Parmesan cheese, Cheddar cheese, bacon bits, onion salt, baking powder, salt, and black pepper into potato mixture using a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat corn oil and butter in a large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon about 2 tablespoons potato mixture into per patty into skillet. Cook patties until golden brown, about 4 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain cooked pancakes on paper towel-lined plate.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Pancakes"
        ],
