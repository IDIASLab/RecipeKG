        "mainEntityOfPage": "https://www.allrecipes.com/recipe/27055/garlic-bread-fantastique/",
        "name": "Garlic Bread Fantastique",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F622264.jpg",
