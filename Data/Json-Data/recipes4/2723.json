        "mainEntityOfPage": "https://www.allrecipes.com/recipe/115038/filipino-pork-adobo/",
        "name": "Filipino Pork Adobo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F155187.jpg",
