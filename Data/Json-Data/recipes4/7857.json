        "recipeYield": "12 cookies",
        "recipeIngredient": [
            "\u2154 cup whole wheat pastry flour",
            "\u00bd cup cocoa nibs",
            "\u00bc cup honey",
            "2 tablespoons pureed prunes",
            "\u00bd teaspoon vanilla extract",
            "\u00bc teaspoon baking powder",
            "1 pinch sea salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, cocoa nibs, honey, prunes, vanilla extract, baking powder, and sea salt together in a bowl until batter is smooth. Scoop batter using a cookie scoop and drop into a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned, about 8 minutes.\n"
            }
        ],
