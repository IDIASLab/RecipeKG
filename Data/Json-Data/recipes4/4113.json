    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213041/bacon-ranch-cheese-ball/",
        "name": "Bacon Ranch Cheese Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2651574.jpg",
            "width": 1736,
            "height": 1736
        },
        "datePublished": "2019-04-02T06:52:40.000Z",
        "description": "This cheese ball was a big hit at our office Christmas party! I had cream cheese left over and decided to start adding ingredients and it was wonderful!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 cheese ball",
        "recipeIngredient": [
            "6 slices bacon",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (1 ounce) package buttermilk Ranch dressing mix",
            "\u00bd cup shredded sharp Cheddar cheese",
            "4 green onions, chopped",
            "3 tablespoons chopped black olives",
            "1\u2009\u00bd cups chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the bacon in a large, deep skillet, and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain the bacon slices on a paper towel-lined plate. Chop the cooled bacon into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the cream cheese and ranch dressing mix in a bowl until smooth. Add the bacon, Cheddar cheese, green onions, and olives; stir until well combined. Shape the mixture into a ball and roll in the chopped pecans. Refrigerate overnight.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Bacon",
            "Appetizers"
        ],
