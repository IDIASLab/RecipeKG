    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267715/pizza-sliders/",
        "name": "Pizza Sliders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2191888.jpg",
            "width": 1536,
            "height": 2048
        },
        "datePublished": "2019-04-03T06:32:53.000Z",
        "description": "Quick and easy-to-make sliders. If you're a fan of pizzas and burgers like I am then you will certainly enjoy this combination of the two.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 pizza sliders",
        "recipeIngredient": [
            "2 (12 ounce) packages bulk Italian sausage",
            "1 teaspoon dried basil",
            "1 teaspoon dried oregano",
            "1 teaspoon dried parsley",
            "1 cup grated Parmesan cheese",
            "1 cup shredded mozzarella cheese",
            "6 slider-sized burger buns",
            "1 (2 ounce) package sliced pepperoni",
            "\u00bd (14 ounce) can pizza sauce",
            "1 green bell pepper, sliced",
            "\u00bd (3 ounce) can sliced black olives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Cover a rimmed baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form Italian sausage into 6 patties. Space patties evenly across the baking sheet. Season with basil, oregano, and parsley.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 10 minutes. Flip patties over and continue baking for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle Parmesan and mozzarella cheeses over patties. Cover with a layer of pepperoni slices. Bake until cheese melts, about 4 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast buns in the oven until nearly as crunchy pizza crust. Spread 1/2 tablespoon pizza sauce onto both halves of each bun. Place each patty on a bun and top with 1 slice of bell pepper and 1/2 tablespoon olives.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes"
        ],
