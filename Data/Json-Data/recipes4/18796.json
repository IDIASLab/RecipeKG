        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258043/chocolate-walnut-oatmeal/",
        "name": "Chocolate Walnut Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4509141.jpg",
            "width": 1638,
