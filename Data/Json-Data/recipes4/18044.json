    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246944/salsa-roja-for-enchiladas-or-wet-burritos/",
        "name": "Salsa Roja for Enchiladas or Wet Burritos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4404360.jpg",
            "width": 2205,
            "height": 2205
        },
        "datePublished": "2019-04-04T04:01:29.000Z",
        "description": "This is my personal enchilada sauce that my kids love. I came up with this recipe by mixing my grandmother's and mother-in-law's recipes together... it is muy rico and worth the work.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "6 large tomatoes",
            "3 serrano chile peppers, stemmed, or to taste",
            "3 cloves garlic, peeled",
            "3 dried California chile peppers, stemmed and seeded",
            "\u2153 cup water",
            "\u00bc onion",
            "3 tablespoons chicken bouillon",
            "\u00bd teaspoon ground cumin",
            "\u00bc teaspoon ground cloves",
            "2 pinches ground black pepper",
            "\u00bc cup all-purpose flour",
            "1 pinch salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place tomatoes, serrano chile peppers, garlic, and California chile peppers in a large saucepan. Pour in enough water to cover. Bring to a boil; simmer until tomatoes are fairly soft but not mushy, about 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer tomato mixture to a blender. Add 1/3 cup water, onion, chicken bouillon, cumin, cloves, and black pepper. Puree until smooth, about 2 minutes. Add flour and blend until combined, about 10 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour blended mixture into the saucepan. Cook over medium heat until warmed through, about 5 minutes. Season with salt.\n"
            }
        ],
