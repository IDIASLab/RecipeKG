    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25925/easy-cheesy-chicken-bake/",
        "name": "Easy Cheesy Chicken Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3294987.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2019-04-04T03:28:42.000Z",
        "description": "Cheesy chicken bake with potatoes and broccoli. Only a few ingredients and very quick to prepare! Cheese soup instead of cheese gives a very cheesy flavor without all the fat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (11 ounce) can condensed cheese soup",
            "\u00bd (10.75 ounce) can  water",
            "1 teaspoon dried parsley",
            "3 skinless, boneless chicken breast halves",
            "6 potatoes, sliced",
            "1 head fresh broccoli, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine soup with water and parsley and mix together. Place chicken and potatoes in a 9x13 inch baking dish and pour cheese mixture over all. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, covered, in preheated oven for 60 minutes. Add broccoli to dish and bake for another 10 to 15 minutes, or until cooked through and tender.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
