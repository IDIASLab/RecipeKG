        "mainEntityOfPage": "https://www.allrecipes.com/recipe/91890/slow-cooker-chicken-alfredo/",
        "name": "Slow Cooker Chicken Alfredo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1460153.jpg",
