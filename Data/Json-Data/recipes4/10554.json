    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22918/pop-cake/",
        "name": "Pop Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1030982.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-09-28T15:04:40.000Z",
        "description": "This variation of the poke cake uses raspberry gelatin and lemon-lime soda to make it extra moist.  Serve it straight from the fridge.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT5H5M",
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (18.25 ounce) package white cake mix",
            "1 (16 ounce) can lemon-lime flavored carbonated beverage",
            "1 cup hot water",
            "1 (6 ounce) package raspberry flavored Jell-O\u00ae mix",
            "1 (3 ounce) package instant vanilla pudding mix",
            "2 cups milk",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare and bake cake mix according to package directions for a 9x13 inch pan. Poke holes in cake while still hot with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make gelatin with boiling water, then stir in lemon-lime soda. Pour hot mixture over cake. Cool cake completely, then refrigerate for at least 4 hours before frosting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat together instant pudding and milk until thick, then pour over cake.  Frost cake with whipped topping.  Keep cake refrigerated until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Poke Cake Recipes"
