    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261723/vegan-stuffed-peppers/",
        "name": "Vegan Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2793682.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2019-04-04T04:10:40.000Z",
        "description": "Vegan stuffed peppers. Top with sriracha sauce, sliced avocado, and cilantro.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H42M",
        "totalTime": "P0DT0H57M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 red bell peppers, halved lengthwise and seeded",
            "1 cup water",
            "\u00bd cup bulgur",
            "1 (24 ounce) jar tomato sauce, or more to taste",
            "2 cups arugula",
            "1 cup corn kernels",
            "\u00bd cup garbanzo beans, drained",
            "\u00bd cup lima beans, drained",
            "\u00bd cup black beans, rinsed and drained",
            "\u00bd cup kidney beans, rinsed and drained",
            "1 teaspoon salt",
            "\u00bd teaspoon paprika",
            "\u00bd teaspoon dried basil",
            "\u00bd teaspoon dried oregano"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a 9x11-inch baking pan with aluminum foil. Arrange bell pepper halves in the baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and bulgur to a boil in a small saucepan. Cover and simmer until bulgur is tender, 12 to 15 minutes. Drain excess water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bulgur, tomato sauce, arugula, corn kernels, garbanzo beans, lima beans, black beans, kidney beans, salt, paprika, basil, and oregano in a large bowl. Fill each bell pepper half generously with bulgur mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly and hot, 25 to 30 minutes.\n"
            }
        ],
