        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228720/7-up-biscuits/",
        "name": "7-Up&reg; Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2665338.jpg",
