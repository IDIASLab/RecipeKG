    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257204/breakfast-for-dinner/",
        "name": "Breakfast For Dinner",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4486338.jpg",
            "width": 3089,
            "height": 3089
        },
        "datePublished": "2019-04-04T04:08:04.000Z",
        "description": "This is a dish my mother used to make when I was a kid. It's great and when I made this for my ex-wife, she fell in love with it... and it's requested all the time.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 cup water",
            "\u00bd cup rice",
            "2 slices bacon",
            "1 teaspoon oil, or as needed",
            "2 eggs, beaten",
            "salt and ground black pepper to taste",
            "1 pinch cinnamon sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and rice to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and liquid has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Crumble bacon into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat; cook and stir eggs until set, about 5 minutes. Season scrambled eggs with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer rice, eggs, and bacon in a bowl; top generously with cinnamon sugar.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Scrambled"
        ],
