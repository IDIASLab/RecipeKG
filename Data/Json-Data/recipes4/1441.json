        "mainEntityOfPage": "https://www.allrecipes.com/recipe/49769/basil-pesto-sunshine-sandwich/",
        "name": "Basil Pesto Sunshine Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F381114.jpg",
