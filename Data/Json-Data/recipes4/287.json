    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20963/oven-roasted-potatoes/",
        "name": "Oven Roasted Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F781931.jpg",
            "width": 1995,
            "height": 1404
        },
        "datePublished": "2000-09-04T01:39:50.000Z",
        "description": "A great roasted potato side dish made with olive oil and herbs.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u215b cup olive oil",
            "1 tablespoon minced garlic",
            "\u00bd teaspoon dried basil",
            "\u00bd teaspoon dried marjoram",
            "\u00bd teaspoon dried dill weed",
            "\u00bd teaspoon dried thyme",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon dried parsley",
            "\u00bd teaspoon crushed red pepper flakes",
            "\u00bd teaspoon salt",
            "4 large potatoes, peeled and cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 475 degrees F (245 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine oil, garlic, basil, marjoram, dill weed, thyme, oregano, parsley, red pepper flakes, and salt. Stir in potatoes until evenly coated. Place potatoes in a single layer on a roasting pan or baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast for 20 to 30 minutes in the preheated oven, turning occasionally to brown on all sides.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Roasted Potato Recipes"
        ],
