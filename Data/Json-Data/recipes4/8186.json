        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245814/sugar-free-banana-oatmeal-cookies-with-almonds/",
        "name": "Sugar-Free Banana Oatmeal Cookies with Almonds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3597291.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-02T07:05:58.000Z",
        "description": "I didn't want to use any sugar or flour. I modified oatmeal cookie recipes and found that this is my favorite natural version.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H13M",
        "totalTime": "P0DT0H24M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "3 bananas",
            "12 dates, pitted and chopped",
            "1 tablespoon coconut oil",
            "1 teaspoon vanilla extract",
            "2 cups quick-cooking oats",
            "\u00be cup almond meal",
            "1 tablespoon ground cinnamon",
            "1 teaspoon baking soda",
            "\u00be cup whole raw almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree bananas and dates together in a food processor. Add coconut oil and vanilla extract; pulse to combine. Add oats, almond meal, ground cinnamon, and baking soda; pulse to combine. Fold in whole almonds. Drop spoonfuls of dough 2 inches apart onto ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, about 13 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
