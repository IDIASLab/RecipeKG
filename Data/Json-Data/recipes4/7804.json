        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244368/roasted-banana-ice-cream-sundae/",
        "name": "Roasted Banana Ice Cream Sundae",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Banana Ice Cream Sundae"
        },
        "datePublished": "2019-04-02T07:04:41.000Z",
        "description": "This is a modern mashup of campfire roasted bananas and an ice cream sundae. This dessert is simple, delicious, and fun to make!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "8 sundaes",
        "recipeIngredient": [
            "4 bananas, halved lengthwise",
            "1 (8 ounce) package refrigerated crescent roll dough",
            "8 scoops Neapolitan ice cream",
            "8 teaspoons chocolate syrup",
            "8 teaspoons caramel sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Freeze banana halves for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap each frozen banana half with crescent roll dough until each is completely covered; arrange on a nonstick baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 10 to 14 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place each wrapped banana in a serving dish and top with 1 scoop ice cream, 1 teaspoon chocolate syrup, and 1 teaspoon caramel sauce.\n"
            }
        ],
