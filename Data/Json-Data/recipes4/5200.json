        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220154/spicy-pumpkin-soup/",
        "name": "Spicy Pumpkin Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F805334.jpg",
