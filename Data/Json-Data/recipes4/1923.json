    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72778/marinated-salmon-smoothly/",
        "name": "Marinated Salmon 'Smoothly'",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F25868.jpg",
            "width": 2332,
            "height": 1916
        },
        "datePublished": "2019-04-02T06:46:25.000Z",
        "description": "Salmon fillets are grilled in foil packets with a deliciously tangy marinade! Serve with a salad and toasted bread or baked potatoes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "6 lemons, juiced",
            "1\u2009\u00bd cups soy sauce",
            "1\u2009\u00bd cups white wine",
            "4 teaspoons prepared mustard",
            "2 teaspoons honey",
            "salt and pepper to taste",
            "4 (4 ounce) fillets salmon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a bowl, mix the lemon juice, soy sauce, white wine, mustard, and honey. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place each salmon fillet on a large piece of aluminum foil. Fold the foil around the salmon on all sides, forming a basket shape. Pour equal amounts of the marinade mixture over each salmon fillet. Tightly seal the foil packets. Allow salmon fillets to marinate at least 1 hour in the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place foil packets on the grill, and cook 10 to 15 minutes, until salmon is easily flaked with a fork.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Salmon"
        ],
