    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23391/garlic-stuffed-roast/",
        "name": "Garlic Stuffed Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970714.jpg",
            "width": 974,
            "height": 974
        },
        "datePublished": "2000-10-26T15:33:23.000Z",
        "description": "Super-easy, delicious and tender roast that makes its own gravy. Put it in when you leave for work, and dinner is done when you get home.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT8H0M",
        "totalTime": "P0DT8H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds beef sirloin roast",
            "10 cloves garlic, halved",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (1 ounce) package dry onion soup mix",
            "2 tablespoons Worcestershire sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C). Trim off any excess fat from roast. Make several deep slits and insert a half clove of garlic in each slit.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place roast in an aluminum oven pouch. Pour soup over roast. Then sprinkle soup mix over the roast followed by the Worcestershire sauce. Seal pouch tightly and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated oven for 8 to 9 hours.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Roast Recipes"
        ],
