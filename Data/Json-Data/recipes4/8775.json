        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/141250/cheesy-jalapeno-corn/",
        "name": "Cheesy Jalapeno Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F312829.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-03T06:30:43.000Z",
        "description": "This casserole with corn, jalapenos, and cream cheese is similar to another one on this site, but much better. This side can go with any Mexican dish and many more. I even made it as part of our Thanksgiving dinner.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 quarts",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 (8 ounce) package cream cheese",
            "3 tablespoons all-purpose flour",
            "1 cup milk",
            "10 slices pickled jalapeno pepper, finely chopped",
            "4 (10 ounce) cans white shoepeg corn, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter and cream cheese in a saucepan over medium heat. Stir in the flour, milk, and jalapenos. Cook until the mixture reaches a pudding-like consistency.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the corn into a 2-quart casserole dish. Pour the cheese mixture over the corn. Cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 30 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Corn"
        ],
