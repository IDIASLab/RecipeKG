        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24329/spanish-rice-ii/",
        "name": "Spanish Rice II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F334552.jpg",
