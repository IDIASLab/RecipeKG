    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231271/ham-it-up-spaghetti/",
        "name": "Ham It Up Spaghetti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1001265.jpg",
            "width": 1197,
            "height": 1200
        },
        "datePublished": "2019-04-02T07:00:14.000Z",
        "description": "A dish that is truly comfort food.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (16 ounce) package angel hair pasta",
            "2 (10.75 ounce) cans  condensed cream of chicken soup",
            "1\u2009\u00be cups milk",
            "1 tablespoon dried minced onion",
            "2 teaspoons dried parsley",
            "1 teaspoon Worcestershire sauce",
            "2 cups cubed fully cooked ham",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease a 2 1/2-quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook angel hair pasta in boiling water, stirring occasionally, until cooked through but firm to the bite, 4 to 5 minutes; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream of chicken soup, milk, dried minced onion, parsley, and Worcestershire sauce in a large bowl; add cooked angel hair pasta and ham. Transfer pasta mixture to the prepared baking dish. Sprinkle Cheddar cheese over the pasta mixture. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 minutes; remove foil and continue baking until top is lightly browned, about 5 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Ham"
        ],
