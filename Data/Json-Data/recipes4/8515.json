    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246961/apple-crumble-baked-oatmeal/",
        "name": "Apple Crumble Baked Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3462756.jpg",
            "width": 3771,
            "height": 2121
        },
        "datePublished": "2019-04-02T07:06:58.000Z",
        "description": "This oatmeal is refined sugar-free, can be prepared vegan if using maple syrup, and gluten-free if using GF oats.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 teaspoon butter",
            "2 ripe apples, cut into bite-size pieces",
            "1\u2009\u2154 cups rolled oats",
            "\u2153 cup walnuts, crumbled into small pieces",
            "1 teaspoon ground cinnamon",
            "1\u2009\u00be cups milk",
            "\u00bd cup honey",
            "3 tablespoons coconut oil, melted",
            "1 teaspoon vanilla extract",
            "1 teaspoon olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease an 8-inch glass baking dish with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread apples evenly on the bottom of the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, walnuts, and cinnamon together in a bowl. Spread over apples in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk, honey, coconut oil, vanilla extract, and olive oil in another bowl. Pour evenly over oat mixture; do not stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is golden, 30 to 45 minutes. Cool in the baking dish before serving, about 30 minutes.\n"
            }
        ],
