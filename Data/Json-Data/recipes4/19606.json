    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263345/chicken-pho/",
        "name": "Chicken Pho",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F954028.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-04T04:11:37.000Z",
        "description": "Mom's chicken pho recipe... straight from Vietnam. Serve with hoisin and sriracha sauce.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "10 quarts water",
            "3 pounds chicken bones",
            "1 whole chicken",
            "1 medium onion",
            "1 (1 inch) piece ginger",
            "1 (32 fluid ounce) container chicken broth",
            "\u00bc cup rock sugar",
            "3 teaspoons fish sauce",
            "2 cubes pho ga soup seasoning",
            "1\u2009\u00bd teaspoons salt",
            "2 (16 ounce) packages rice stick noodles (banh pho)",
            "\u00bd pound bean sprouts",
            "1 bunch green onion, chopped",
            "1 bunch cilantro, chopped",
            "6 sprigs Thai basil, or as  needed",
            "1 lime, cut in wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a stockpot. Meanwhile, rinse chicken bones under hot water to get rid of impurities.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bones in the pot of boiling water. Reduce heat and simmer until starting to soften, skimming any fat off the surface of the broth, about 60 minutes. Discard parboiled bones.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place whole chicken into the pot and simmer until no longer pink in the center, 30 to 40 minutes. Remove chicken from broth and set aside to cool. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine onion and ginger in a skillet over medium-high heat. Cook and stir until nicely browned and fragrant, about 7 minutes. Smash ginger with the backside of a knife onto a cutting board. Place onion and ginger into the broth. Add chicken broth, rock sugar, fish sauce, pho ga seasoning, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add rice noodles and boil until tender yet firm to the bite, 2 to 3 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel skin off of the cooled chicken; discard skin and bones, reserving the meat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve noodles in bowls topped with chicken meat and broth. Garnish with bean sprouts, green onion, cilantro, and Thai basil. Squeeze a wedge of lime into each bowl.\n"
            }
        ],
