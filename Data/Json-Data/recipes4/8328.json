    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246320/jills-grasshopper-pie/",
        "name": "Jill's Grasshopper Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4486819.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2019-04-02T07:06:25.000Z",
        "description": "One of my favorite childhood desserts. Always fun for St. Patrick's Day, too!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT3H28M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "14 chocolate sandwich cookies (such as Oreo\u00ae), crushed",
            "2 tablespoons butter, melted",
            "24 large marshmallows",
            "\u00bd cup milk",
            "\u00bc cup green creme de menthe liqueur",
            "2 tablespoons creme de cacao",
            "2 drops green food coloring, or as needed",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix crushed cookies with melted butter to make the crust. Pat onto the bottom and sides of a 9-inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat marshmallows and milk together in a large saucepan over low heat, stirring constantly, until marshmallows are melted, about 8 minutes. Stir in creme de menthe, creme de cacao, and green food coloring until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Immerse the bottom of the saucepan in cool water for several minutes to cool the marshmallow mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream in a chilled glass or metal bowl with an electric mixer until soft peaks form. Fold whipped cream into marshmallow mixture. Pour into pie crust and smooth the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover pie with plastic wrap and freeze until set, about 3 hours.\n"
            }
        ],
