    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13102/bean-with-bacon/",
        "name": "Bean with Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F507252.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "1999-01-28T13:45:16.000Z",
        "description": "A great winter soup.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H20M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (16 ounce) package dried navy beans",
            "9 cups water",
            "1 pound bacon",
            "2 onions, chopped",
            "2 stalks celery, chopped",
            "4 teaspoons chicken bouillon",
            "1 bay leaf",
            "\u2153 teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u215b teaspoon ground cloves",
            "1 (16 ounce) can diced tomatoes",
            "4 cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Boil the beans in 9 cups of the water and then let sit for one hour. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the bacon to your desired texture (it can be soft or crisp, whatever you prefer) and drain except for 1/4 cup grease. Coarsely chop the bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the onions and celery to the reserved grease and bacon and saute until soft, do not drain. Add the chicken base or cubes,  4 cups water, beans, bay leaf, salt, pepper, and cloves, and simmer for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the tomatoes with their juice. Serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas"
        ],
