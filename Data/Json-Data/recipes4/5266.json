    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220348/slow-cooked-chili/",
        "name": "Slow-Cooked Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1039527.jpg",
            "width": 1550,
            "height": 1033
        },
        "datePublished": "2019-04-02T06:56:24.000Z",
        "description": "Here's a hearty chili made with ground beef and kidney beans, cooked for hours in a slow cooker.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT8H0M",
        "totalTime": "P0DT8H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 pounds ground beef",
            "2 (16 ounce) cans kidney beans, rinsed and drained",
            "2 (14.5 ounce) cans diced tomatoes, undrained",
            "1 onion, chopped",
            "2 green bell peppers, chopped, divided",
            "1 red bell pepper, chopped, divided",
            "2 tablespoons chili powder",
            "2 cloves garlic, minced",
            "2 teaspoons salt",
            "1 teaspoon ground black pepper",
            "10 tablespoons shredded Cheddar cheese for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir ground beef in a skillet over medium heat until no longer pink, about 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer ground beef to a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in kidney beans, diced tomatoes, onion, half the green bell peppers, half the red bell peppers, chili powder, garlic, salt, and ground black pepper until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place remaining green bell peppers and red bell peppers on top of mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the cooker and cook on High setting until vegetables are soft and the flavors have blended, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chili and cook on Low setting for 6 more hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve in bowls; garnish each serving with 1 tablespoon of shredded Cheddar cheese.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chili Recipes",
            "Beef Chili Recipes"
        ],
