        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/41636/california-thai-flank-steak/",
        "name": "California-Thai Flank Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F856374.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:44:31.000Z",
        "description": "This Thai-inspired marinated steak is a family favorite. Serve with basmati rice, a fresh salad and/or soup, and fresh fruit for desert!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT6H50M",
        "recipeYield": "8 Servings",
        "recipeIngredient": [
            "\u2153 cup soy sauce",
            "\u00bc cup rice vinegar",
            "\u00bc cup rice wine",
            "\u00bc cup fresh lime juice",
            "2 tablespoons dark sesame oil",
            "\u00bd small red onion, chopped",
            "\u00bc cup chopped fresh basil",
            "\u00bc cup chopped fresh mint",
            "3 tablespoons sliced lemon grass",
            "3 tablespoons crushed peanuts",
            "3 tablespoons chile paste",
            "1 tablespoon ground coriander",
            "\u00bd teaspoon garlic salt",
            "2 pounds flank steak"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine soy sauce, rice vinegar, rice wine, lime juice and sesame oil. Stir in onion, basil, mint, lemon grass and crushed peanuts. Season with chile paste, coriander and garlic salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer flank steak with marinade. Refrigerate 6 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium high heat, and lightly oil grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a large sheet of aluminum foil with non-stick cooking spray. Drain liquid from marinade. Layer meat on foil with the remaining non-liquid marinade ingredients. Fold edges of foil to seal, then grill for 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Asian",
            "Thai"
        ],
