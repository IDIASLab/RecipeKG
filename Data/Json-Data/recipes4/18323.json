    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255261/best-roast-beef-dinner-ever/",
        "name": "Best Roast Beef Dinner Ever!",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Best Roast Beef Dinner Ever!"
        },
        "datePublished": "2019-04-04T04:07:00.000Z",
        "description": "This dish is easy, yet the best you will ever have! Flavorful, tender, and will please everyone.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT4H0M",
        "totalTime": "P0DT12H40M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4 pounds chuck roast, or more to taste",
            "3 shallots, sliced 1/4-inch thick",
            "6 tablespoons chopped garlic",
            "salt and ground black pepper to taste",
            "8 sprigs fresh thyme",
            "2 cups water",
            "1 (14 ounce) can beef broth",
            "6 carrots, or more to taste, cut into thirds",
            "8 stalks celery, cut into thirds",
            "1\u2009\u00bd pounds baby red potatoes, halved",
            "3 tablespoons rosemary",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut 1/2-inch incisions into the meaty parts of the roast; stuff with shallots and garlic. Season roast with salt and pepper; top with thyme sprigs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Center roast in a large roasting pan. Pour in water and beef broth. Spread carrots and celery around the roast. Cover and marinate in the refrigerator, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes in a large bowl; cover with water. Let soak, 10 to 15 minutes. Drain and pat dry. Add rosemary and olive oil; toss to coat. Spread potatoes around the roast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until potatoes are very tender, about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 250 degrees F (120 degrees C). Continue baking until an instant-read thermometer inserted into the center of the roast reaches 145 degrees F (65 degrees C), about 1 hour more.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Roast Recipes"
        ],
