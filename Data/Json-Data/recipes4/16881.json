    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238677/dilly-round-steak/",
        "name": "Dilly Round Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2929045.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "2019-04-04T03:58:13.000Z",
        "description": "This is a variation of beef stroganoff. The dill gives it a wonderful flavor. The meat melts in your mouth, and it's so easy to make; serve over mashed potatoes or noodles.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT6H10M",
        "totalTime": "P0DT6H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds boneless round steak, cut into 6 pieces",
            "salt and ground black pepper to taste",
            "1 onion, sliced",
            "\u00bd cup water",
            "1 cube beef bouillon",
            "1 teaspoon dill weed",
            "\u00bc cup cold water",
            "\u00bc cup all-purpose flour",
            "3 tablespoons sour cream, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season steaks with salt and pepper and place in a slow cooker; add onion, 1/2 cup water, bouillon, and dill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until steaks are tender, about 6 hours. Remove steaks from broth and place on a serving platter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 1/4 cup cold water and flour together in a bowl until smooth; pour into broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase slow cooker heat to High and cook gravy until thickened, about 10 minutes. Stir sour cream into thickened gravy; serve alongside steaks.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks",
            "Round Steak Recipes"
        ],
