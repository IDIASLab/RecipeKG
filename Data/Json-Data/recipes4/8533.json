        "recipeIngredient": [
            "1 (14 ounce) can chicken broth",
            "4 boneless, skinless chicken breasts",
            "1 (14 ounce) jar spaghetti sauce",
            "12 slices sandwich pepperoni",
            "6 slices jalapeno",
            "\u00bd (16 ounce) package cooked spaghetti",
            "4 slices mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour broth into the bottom of a slow cooker. Add chicken breasts. Pour spaghetti sauce over chicken. Layer pepperoni and jalapeno slices on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until chicken is cooked through, 4 to 5 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide spaghetti among 4 serving plates. Lay mozzarella cheese slices over spaghetti. Divide chicken mixture among plates.\n"
            }
        ],
