        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215094/lemon-basmati-rice/",
        "name": "Lemon Basmati Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3600984.jpg",
