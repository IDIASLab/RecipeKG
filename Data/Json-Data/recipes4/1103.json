    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/33153/pineapple-scones/",
        "name": "Pineapple Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1338058.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2019-04-02T06:44:06.000Z",
        "description": "Coconut milk gives these light, delectable scones an exotic touch, and the pineapple and orange zest make them really memorable for a breakfast or brunch party.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 tablespoon baking powder",
            "\u00bc teaspoon salt",
            "\u00bc cup butter, chilled",
            "\u00be cup coconut milk",
            "\u00bc cup honey",
            "1 egg, beaten",
            "1 (8 ounce) can crushed pineapple, drained",
            "1 orange, zested"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix flour, baking powder, and salt. Cut in chilled butter until the mixture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix coconut milk, honey, and egg. Stir into the flour mixture. Fold in pineapple and orange zest. Divide the mixture into 12 rounded pieces, and arrange in a single layer on a medium ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 15 to 20 minutes in the preheated oven, until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Scone Recipes"
        ],
