    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220644/spinach-mushroom-lasagna/",
        "name": "Spinach Mushroom Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1123026.jpg",
            "width": 4608,
            "height": 3440
        },
        "datePublished": "2019-04-02T06:56:38.000Z",
        "description": "Delicious! This lasagna combines fresh mushrooms, spinach, four cheeses, and plenty of garlic. Can be prepared up to 48 hours ahead.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9x13-inch lasagna",
        "recipeIngredient": [
            "1 (8 ounce) package lasagna noodles",
            "2 eggs, beaten",
            "2 cups cottage cheese",
            "1 (15 ounce) container ricotta cheese",
            "2 teaspoons dried Italian seasoning",
            "2 tablespoons olive oil",
            "6 cups sliced fresh mushrooms",
            "1 cup chopped onion",
            "4 cloves garlic, minced",
            "2 tablespoons all-purpose flour",
            "1 teaspoon ground black pepper",
            "1\u2009\u00bc cups milk",
            "2 (10 ounce) packages frozen chopped spinach, thawed and drained",
            "\u00bd cup shredded Parmesan cheese",
            "1 (8 ounce) package shredded mozzarella cheese",
            "\u00bc cup shredded Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large bowl of salted water to a boil. Stir in lasagna noodles and return to a boil. Cook the pasta uncovered, stirring occasionally, until the noodles have cooked through but are still slightly firm, about 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs, cottage cheese, ricotta cheese, and Italian seasoning in a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat. Cook and stir mushrooms, onion, and garlic in the hot oil until vegetables are tender, 5 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour and black pepper into mushroom mixture until thoroughly combined; add milk. Cook and stir until sauce is thick and bubbly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove skillet from heat and stir spinach and 1/2 cup Parmesan cheese into the mushroom sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange 3 lasagna noodles in a single layer in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1/3 of the cottage cheese mixture over the noodles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1/3 of the mushroom sauce over the cottage cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle 1/3 of the mozzarella cheese on top. Repeat the layers twice more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle remaining 1/4 cup Parmesan cheese over the lasagna.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and sauce is bubbly, about 35 minutes. Allow lasagna to rest for 10 minutes before slicing and serving.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
