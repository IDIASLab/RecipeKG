    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255646/individual-turkey-pot-pies/",
        "name": "Individual Turkey Pot Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4022574.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-10-05T02:16:39.000Z",
        "description": "These personal-sized pies can be made any way you like to top your pot pie, puff pastry, pie crust or mashed potatoes, and make the most of your leftover turkey.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 tablespoons butter",
            "\u00bd cup diced onion",
            "\u00bd cup sliced celery",
            "\u00bc cup flour",
            "1 tablespoon minced fresh rosemary",
            "1 teaspoon chopped fresh thyme",
            "\u00bc teaspoon black pepper",
            "2 cups College Inn\u00ae Chicken Broth",
            "2 cups cubed cooked turkey",
            "1 cup frozen peas and carrots",
            "1\u2009\u00bd cups chopped cooked green beans",
            "1 (17.3 ounce) package frozen puff pastry sheets, thawed according to package directions",
            "1 egg",
            "1 tablespoon water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F. Melt butter in a large saucepan over medium heat. Cook onion, celery, flour, rosemary, thyme and pepper, 3 minutes, stirring constantly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly add broth, stirring until smooth. Cook, stirring constantly, until thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in turkey, peas and carrots and green beans. Spoon into four 10-oz. ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut 4 circles out of puff pastry sheet, using outer edge of ramekin as a guide. Place pastry over turkey mixture in each ramekin. Blend egg and water; brush over pastry. Place ramekins on rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 25 minutes or until golden brown and puffed. Let stand 10 minutes before serving.\n"
            }
        ],
