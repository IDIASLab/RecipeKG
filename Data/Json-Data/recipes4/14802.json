    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220252/broiled-asparagus-parmesan/",
        "name": "Broiled Asparagus Parmesan",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F798315.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2019-04-04T03:51:42.000Z",
        "description": "This is a tasty fresh vegetable dish ready in 15 minutes. The asparagus stays nice and green when steamed in the microwave or steamer if you prefer. The parmesan gives it a little extra flavor.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H13M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound fresh asparagus, tough ends trimmed",
            "2 tablespoons water",
            "2 tablespoons melted butter",
            "1 lemon wedge",
            "\u00bc cup grated Parmesan cheese",
            "ground white pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place asparagus and water in 8x8 inch microwave-safe dish and cover.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat in microwave on high until tender, 6 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven's broiler and set the oven rack about 6 inches from the heat source.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove asparagus from dish and place in a single layer on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze lemon wedge into melted butter and drizzle over asparagus.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle Parmesan cheese and ground white pepper over asparagus.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place under broiler until lightly browned, about 2 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Asparagus",
            "Baked"
        ],
