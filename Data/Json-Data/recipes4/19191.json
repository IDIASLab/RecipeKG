    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260810/chicken-broccoli-and-spinach-alfredo/",
        "name": "Chicken, Broccoli, and Spinach Alfredo",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chicken, Broccoli, and Spinach Alfredo"
        },
        "datePublished": "2019-04-04T04:10:01.000Z",
        "description": "Verrrry creamy and filling perfect for a family meal. Serve with Parmesan cheese and a loaf of crusty bread.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H28M",
        "totalTime": "P0DT0H43M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (12 ounce) package linguine pasta",
            "2\u2009\u00bd cups chopped broccoli florets",
            "10 slices bacon",
            "\u00bc cup boiling water",
            "4 cubes  chicken bouillon",
            "1 cup sliced fresh mushrooms",
            "6 cloves garlic, minced",
            "4 skinless, boneless chicken breast, cut into bite-size pieces",
            "1 (8 ounce) package cream cheese, softened",
            "\u00be cup milk",
            "\u00bd cup grated Parmesan cheese",
            "\u00bd teaspoon dried basil",
            "\u00bd teaspoon dried parsley",
            "2 cups fresh baby spinach"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook linguine at a boil until almost tender, about 9 minutes. Add broccoli; continue cooking until linguine is tender, about 2 minutes more. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Drain grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine boiling water and chicken bouillon cubes in a bowl; mix until dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chicken bouillon mixture into the same skillet used to cook the bacon. Bring to a simmer. Add mushrooms and garlic; cook for 1 minute. Add chicken; cook and stir until no longer pink in the center, about 5 minutes. Stir in cream cheese, milk, Parmesan cheese, basil, and parsley; cook until melted together and heated through, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place spinach in a microwave-safe bowl. Fill bowl halfway with water. Cook in the microwave until wilted, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir wilted spinach into the cooked linguine. Pour chicken and sauce on top. Crumble bacon on top.\n"
            }
        ],
