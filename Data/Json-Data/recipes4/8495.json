    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246877/chickpea-vegetarian-meatballs/",
        "name": "Chickpea Vegetarian Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3645114.jpg",
            "width": 1310,
            "height": 1310
        },
        "datePublished": "2019-04-02T07:06:52.000Z",
        "description": "These are a versatile and easy way to make meatball out of chickpeas. They are very filling and tasty. Serve with marinara or your favorite dipping sauces.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "2 tablespoons olive oil",
            "1 white onion, minced",
            "6 cloves garlic, minced",
            "2 cups canned chickpeas, drained",
            "\u00be cup dry bread crumbs",
            "1 egg",
            "2 tablespoons parsley",
            "1 tablespoon salt-free seasoning blend",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat. Add onion and garlic; cook and stir until tender, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pulse chickpeas in a food processor until finely ground. Add onion and garlic, bread crumbs, egg, parsley, seasoning blend, salt, and black pepper; process until mixture holds together like a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop out 2 tablespoons of the mixture and roll into a ball; set on the prepared baking sheet. Repeat with remaining mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bottom is golden brown, about 15 minutes. Turn each meatball over and continue baking until golden brown on top, about 10 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegetarian"
        ],
