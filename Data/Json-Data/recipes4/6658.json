        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cup self-rising flour",
            "\u00bd cup milk",
            "\u00bc cup mayonnaise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease 6 large muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, milk, and mayonnaise together in a bowl until smooth. Pour flour mixture into prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until golden brown, about 30 minutes .\n"
            }
        ],
