        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245337/tiffanys-almond-crescent-cookies/",
        "name": "Tiffany's Almond Crescent Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tiffany's Almond Crescent Cookies"
        },
        "datePublished": "2019-04-02T07:05:29.000Z",
        "description": "These cookies taste like they came fresh from an Italian bakery. Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "24 crescents",
        "recipeIngredient": [
            "\u00bd cup butter, at room temperature",
            "\u2153 cup confectioners' sugar",
            "1 teaspoon vanilla extract",
            "1 teaspoon almond extract",
            "\u215b teaspoon salt",
            "\u00be cup all-purpose flour",
            "2 tablespoons all-purpose flour",
            "\u00bd cup finely chopped almonds",
            "\u00bd cup confectioners' sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and 1/3 cup confectioners' sugar together in a large bowl with an electric mixer until light and fluffy, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vanilla extract, almond extract, and salt into butter mixture. Add 3/4 cup plus 2 tablespoons flour slowly, beating until just mixed. Fold in almonds; mix briefly until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into tiny crescents and arrange on ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crescents are golden, about 15 minutes. Cool on the pan for 5 minutes before transferring to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll cooled crescents in 1/2 cup confectioners' sugar.\n"
            }
        ],
