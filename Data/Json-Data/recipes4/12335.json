    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/80982/buzzsaws-bbq-sauce/",
        "name": "Buzzsaw's BBQ Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8088309.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-04T03:33:17.000Z",
        "description": "This sauce can be used as a marinade for beef, pork, or chicken.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 teaspoon vegetable oil",
            "\u00bd onion, diced",
            "1 clove garlic, minced",
            "3 tablespoons Worcestershire sauce",
            "3 tablespoons vinegar",
            "2 tablespoons molasses",
            "1 teaspoon prepared mustard",
            "1 cup ketchup",
            "\u00bd cup cold water",
            "1 teaspoon cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a saucepan over medium heat, and saute the onion until tender and golden brown. Stir in garlic. Mix in Worcestershire sauce, vinegar, molasses, and mustard. Cook and stir 5 minutes, then mix in ketchup, cold water, and cornstarch. Reduce heat to low, and continue cooking 10 minutes, until thickened.\n"
            }
        ],
