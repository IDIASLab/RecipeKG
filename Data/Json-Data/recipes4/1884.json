        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71146/garlic-corn-on-the-cob/",
        "name": "Garlic Corn on the Cob",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F225775.jpg",
