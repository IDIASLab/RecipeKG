    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232713/wine-and-cheese-strata/",
        "name": "Wine and Cheese Strata",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3060964.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "2019-04-02T07:01:03.000Z",
        "description": "Fantastic crowd pleasing brunch dish we have made for everything from Christmas morning to Mother's Day brunch with mimosas.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P1DT1H40M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "\u00bd loaf dry French bread, broken into pieces",
            "3 tablespoons butter, melted",
            "8 ounces shredded Swiss cheese",
            "4 ounces shredded Monterey Jack cheese",
            "1 (1/4 inch thick) ham slice, coarsely chopped",
            "1\u2009\u00be cups milk",
            "9 eggs",
            "4 green onions, minced",
            "\u00bc cup sherry",
            "1\u2009\u00bd tablespoons Dijon mustard",
            "\u00bc teaspoon ground black pepper",
            "\u215b teaspoon cayenne pepper",
            "\u00be cup sour cream",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange bread pieces in the prepared baking dish; drizzle with melted butter. Top with Swiss cheese, Monterey Jack cheese, and ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk, eggs, green onions, sherry, mustard, black pepper, and cayenne pepper together in a bowl until foamy; pour over bread mixture. Cover dish tightly with aluminum foil and refrigerate for 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove dish from refrigerator 30 minutes before baking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake strata in the preheated oven for 1 hour. Remove aluminum foil and top with sour cream and Parmesan cheese. Bake, uncovered, until a knife inserted in the strata comes out clean, about 10 more minutes.\n"
            }
        ],
