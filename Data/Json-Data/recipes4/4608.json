    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216076/mexican-goulash/",
        "name": "Mexican Goulash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F830898.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-02T06:54:12.000Z",
        "description": "A quick mid-week dinner.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "\u00bd onion, chopped",
            "1 (12 ounce) package elbow macaroni",
            "\u00bd pound processed cheese (such as Velveeta\u00ae), diced",
            "1\u2009\u00bd cups salsa",
            "1 (15 ounce) can pinto beans, drained and rinsed",
            "1 (14 ounce) can whole kernel corn, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook the ground beef and onion into a large skillet over medium heat until browned and crumbly, about 10 minutes. Break the meat apart as it cooks. Drain the excess grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil over high heat. Once the water is boiling, stir in the macaroni, and return to a boil. Cook the pasta uncovered, stirring occasionally, until the pasta has cooked through, but is still firm to the bite, about 8 minutes. Drain well in a colander set in the sink.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the macaroni to the the beef and onion and place over medium-low heat; stir in the processed cheese until melted, about 3 minutes. Add the salsa, pinto beans, and corn to the mixture; stir and heat until the mixture bubbles slightly, another 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Goulash Recipes"
        ],
