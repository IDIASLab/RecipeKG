    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258075/nabak-kimchi-water-kimchi/",
        "name": "Nabak Kimchi (Water Kimchi)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Nabak Kimchi (Water Kimchi)"
        },
        "datePublished": "2019-04-04T04:08:37.000Z",
        "description": "This is a watery kimchi that is good to eat during the hotter months.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P1DT1H40M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd napa cabbage, cut into 1-inch pieces",
            "1 teaspoon sea salt",
            "\u00bd teaspoon Korean red pepper powder",
            "5 cups water",
            "1 tablespoon sweet rice flour (glutinous rice flour)",
            "\u00bd yellow onion, cut into chunks",
            "\u00bd Asian pear - peeled, cored, and thinly sliced",
            "3 green onions, cut into 1-inch pieces",
            "6 cloves garlic, sliced",
            "3 slices fresh ginger",
            "1 (1 gram) packet granular sucralose sweetener (such as Splenda\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cabbage in a large bowl; sprinkle salt and red pepper powder on top. Toss well and let stand until cabbage looks wilted, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and rice flour to a boil in a large pot. Simmer for 5 minutes. Remove from heat and let cool, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine onion, Asian pear, and green onions in a half-gallon jar. Add wilted cabbage, garlic, ginger, and sweetener. Pour water mixture on top. Seal jar and keep at room temperature to ferment for 1 to 2 days. Place in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Pickled"
        ],
