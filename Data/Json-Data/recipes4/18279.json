        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254966/dill-pickled-cherry-tomatoes/",
        "name": "Dill-Pickled Cherry Tomatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5257444.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2019-04-04T04:06:52.000Z",
        "description": "Pickled green cherry tomatoes. This is perfect for end-of-the-season dill tomatoes or before they are ripe tomatoes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P7DT0H12M",
        "recipeYield": "1 quart",
        "recipeIngredient": [
            "4 cups green cherry tomatoes",
            "1 (1 quart) sterilized canning jar with lid and ring",
            "1 cup distilled white vinegar",
            "1 cup water",
            "3 tablespoons kosher salt",
            "1 tablespoon sriracha sauce",
            "1 tablespoon pickling spice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pack tomatoes into the 1-quart jar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine vinegar, water, salt, sriracha sauce, and pickling spice in a glass measuring cup. Heat in the microwave until boiling and salt dissolves, 1 1/2 to 3 minutes. Pour over tomatoes in the jar; seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until pickled, 1 to 2 weeks.\n"
            }
        ],
