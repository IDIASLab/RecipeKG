    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216809/creamy-parmesan-brussels-sprouts/",
        "name": "Creamy Parmesan Brussels Sprouts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7517229.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-02T06:54:38.000Z",
        "description": "The rich creamy sauce is baked over the sprouts to make a delicious side everyone adores! Have the recipe on hand when serving it to guests, because they WILL ask for it!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon butter",
            "4 cups sliced Brussels sprouts",
            "1\u2009\u00bc cups heavy cream",
            "\u00bc cup freshly shredded Parmesan cheese",
            "1 clove garlic, minced",
            "salt and ground black pepper to taste",
            "1 pinch ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Generously grease a 9x9-inch baking dish with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the sliced Brussels sprouts out into the prepared baking dish in an even layer. In a bowl, whisk together the cream, Parmesan cheese, garlic, salt, and black pepper until thoroughly combined; pour the cream mixture over the Brussels sprouts. Sprinkle the top with nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the dish with foil, and bake in the preheated oven for 1 hour; remove foil, and bake uncovered until the top has browned, 15 more minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Brussels Sprouts"
        ],
