    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259084/slow-cooker-shredded-jalapeno-chicken-sandwiches/",
        "name": "Slow Cooker Shredded Jalapeno Chicken Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4608616.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-03T06:32:33.000Z",
        "description": "This is quite spicy.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT4H30M",
        "totalTime": "P0DT4H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound chicken breasts",
            "1 cup chicken broth",
            "1 (4 ounce) can diced jalapeno peppers",
            "\u00bd teaspoon jalapeno pepper seasoning",
            "\u00bd teaspoon onion powder",
            "\u00bd teaspoon garlic powder",
            "\u00bd cup sour cream",
            "4 ounces cream cheese",
            "1 cup shredded pepper Jack cheese",
            "4 sandwich buns"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine chicken breasts, broth, diced jalapenos, jalapeno seasoning, onion powder, and garlic powder in a 1.5 to 2-quart slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until chicken is tender and no longer pink in the center, 4 to 10 hours. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain liquid from the slow cooker and reserve. Shred chicken in the slow cooker using a fork. Add some of the reserved liquid as desired. Stir in sour cream, cream cheese, and pepper Jack cheese. Cook on Low until flavors fully combine, about 30 minutes. Stir well and serve in sandwich buns.\n"
            }
        ],
