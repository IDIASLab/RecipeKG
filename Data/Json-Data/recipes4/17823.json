        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246064/funfetti-rice/",
        "name": "Funfetti&reg; Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3327032.jpg",
            "width": 2787,
