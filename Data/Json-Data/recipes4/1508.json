        "mainEntityOfPage": "https://www.allrecipes.com/recipe/52531/grilled-cabbage-ii/",
        "name": "Grilled Cabbage II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F807303.jpg",
