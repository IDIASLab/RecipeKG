    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221990/new-orleans-steamer/",
        "name": "New Orleans Steamer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3542689.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2019-04-02T06:57:11.000Z",
        "description": "Can be easily adapted to any taste - it is a little bit spicy as written, but adapt it, add or subtract ingredients...even adaptable for kids. It is a fun meal for a crowd",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "parchment paper",
            "cooking spray",
            "2 cups cooked white rice",
            "1\u2009\u00bd teaspoons gumbo file powder",
            "1\u2009\u00bd teaspoons Cajun seasoning",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon ground cumin",
            "2 tomatoes, chopped",
            "1 red bell pepper, sliced",
            "1 green bell pepper, sliced",
            "1 green onion, sliced",
            "4 fresh okra, chopped",
            "1\u2009\u00bd teaspoons Cajun seasoning",
            "\u00bd pound crawfish, thawed if frozen",
            "\u00bd pound large shrimp, peeled and deveined",
            "\u00bd pound andouille sausage, sliced",
            "1\u2009\u00bd teaspoons Cajun seasoning",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon ground cumin",
            "\u00bc cup chicken broth",
            "16 littleneck clams"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray four 10x20-inch sheets of parchment paper with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cooked rice, file powder, 1 1/2 teaspoons Cajun seasoning, 1/2 teaspoon garlic powder, and 1/2 teaspoon cumin in a bowl. Divide rice mixture evenly over prepared parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss tomatoes, red bell pepper, green bell pepper, green onion, and okra in a large bowl with 1 1/2 teaspoon Cajun seasoning until well coated. Divide vegetable mixture evenly over rice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine crawfish, shrimp, andouille sausage, remaining 1 1/2 teaspoons Cajun seasoning, 1/2 teaspoon garlic powder, and 1/2 teaspoon cumin in another bowl. Divide seafood mixture evenly over vegetables. Pour about 1 tablespoon chicken broth over seafood mixture and top each portion with 4 clams.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold parchment paper around seafood mixture, sealing edges to form a pouch. Place the pouches on a microwave-safe platter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat pouches in microwave on high until shrimp are bright pink on the outside and the meat is no longer transparent in the center, about 4 minutes.\n"
            }
        ],
