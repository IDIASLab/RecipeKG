    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240145/apple-sausage-stuffing/",
        "name": "Apple Sausage Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Sausage Stuffing"
        },
        "datePublished": "2019-04-02T07:03:59.000Z",
        "description": "My Thanksgiving go-to recipe! You can use any kind of sausage with this recipe. Feel free to explore!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "8 ounces chicken apple sausage links",
            "\u00bd cup margarine",
            "1 large onion, chopped",
            "1 cup chopped celery",
            "3 apples - peeled, cored, and finely chopped",
            "6 ounces corn bread stuffing mix",
            "6 ounces seasoned stuffing mix",
            "1\u2009\u00bd teaspoons poultry seasoning",
            "1 pinch salt and ground black pepper to taste",
            "1\u2009\u00bd cups chicken broth",
            "2 eggs, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook sausages in a skillet over medium-high heat until browned and heated through, 7 to 10 minutes. Remove from heat and cut sausages into bite-size pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a large skillet over medium heat. Cook and stir onion and celery in margarine until softened, 7 to 10 minutes. Transfer skillet contents to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sausage and apples into onion mixture. Add cornbread stuffing mix, seasoned stuffing mix, poultry seasoning, salt, and black pepper; mix well. Stir chicken broth and eggs into stuffing mixture; pour mixture into a 3-quart baking dish and cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Remove aluminum foil from dish and continue baking until browned and crisp on top, 10 to 15 minutes.\n"
            }
        ],
