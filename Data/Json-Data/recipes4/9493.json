        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 cup honey",
            "1 egg",
            "\u00be cup shortening",
            "\u00bc cup water",
            "1 teaspoon vanilla extract",
            "3 cups rolled oats",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat together shortening, honey, egg, water, and vanilla until creamy.  Add combined remaining ingredients; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded teaspoonfuls onto greased cookie sheet.  Bake at 350 degrees F (175 degrees C) for 12 to 15 minutes.\n"
            }
        ],
