    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233686/tex-mex-chicken-casserole/",
        "name": "Tex-Mex Chicken Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1978691.jpg",
            "width": 4288,
            "height": 3216
        },
        "datePublished": "2019-04-02T07:01:29.000Z",
        "description": "This is a version of the famous King Ranch Chicken casserole. Careful, it packs a bit of heat!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (10 ounce) bag nacho cheese-flavored corn chips (such as Doritos\u00ae), crushed",
            "1 tablespoon butter",
            "1 small onion, finely chopped",
            "1 (14.5 ounce) can diced tomatoes",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  cream of mushroom soup",
            "1 (4.5 ounce) can chopped green chiles",
            "\u2153 cup milk",
            "2 tablespoons sour cream",
            "1 teaspoon chili powder",
            "1 teaspoon ground cumin",
            "2\u2009\u00bd cups chopped cooked chicken ",
            "1 (8 ounce) package shredded sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly grease a 9x13-inch baking dish. Spread corn chip crumbs over the bottom of the baking dish and press crumbs into place.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large skillet over medium-high heat; cook and stir onion in melted butter until tender, 6 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir diced tomatoes, cream of chicken soup. cream of mushroom soup, green chiles, milk, sour cream, chili powder, and cumin in a large bowl. Add chicken and cooked onion; stir to combine. Pour mixture over corn chips and top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly and cheese is melted, 55 to 60 minutes. Let stand for 10 minutes before serving.\n"
            }
        ],
