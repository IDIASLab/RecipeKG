    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220676/golden-beet-bread/",
        "name": "Golden Beet Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F818579.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "2019-04-04T03:51:52.000Z",
        "description": "The sweet, earthy flavor of golden beets is ideal for this holiday bread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H35M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "1 large loaf",
        "recipeIngredient": [
            "\u00bd pound golden beets, greenery removed",
            "cooking spray",
            "1 cup whole wheat flour",
            "1 cup unbleached flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup water",
            "1 cup turbinado sugar",
            "\u00bd cup margarine, melted",
            "2 eggs",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stand beets upright in a saucepan. Fill saucepan with about 1-inch of water and bring to a boil. Lower heat to a simmer, cover, and steam beets until tender, about 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully remove beets from saucepan, peel, and coarsely chop into chunks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place beets in a blender and puree until smooth. Measure out 1 cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray 1 large loaf pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix whole wheat flour, unbleached flour, baking soda, and salt together in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 1 cup beet puree with 1/2 cup water together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and margarine together in a large bowl with an electric hand mixer on low speed until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and beet mixture into sugar mixture until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually stir flour mixture into wet ingredients, mixing after each small addition, until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold walnuts into batter until evenly incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until a toothpick inserted in the center comes out clean, 55 to 65 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
        "recipeCuisine": [],
        "author": [
