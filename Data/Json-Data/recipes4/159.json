        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19016/peanut-butter-candy-bars/",
        "name": "Peanut Butter Candy Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F23197.jpg",
            "width": 1225,
            "height": 1182
        },
        "datePublished": "2000-06-01T21:43:42.000Z",
        "description": "These delicious bars taste like peanut butter cups!  We enjoy these any time of the year!",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 -10x15 pan",
        "recipeIngredient": [
            "1\u2009\u00bd cups margarine, melted",
            "2 cups peanut butter",
            "4\u2009\u00bd cups confectioners' sugar",
            "2 cups graham cracker crumbs",
            "\u00bd cup margarine",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 10x15 inch pan.  In a large bowl, combine 1 1/2 cups melted margarine, peanut butter, confectioners' sugar and graham cracker crumbs.   Spread in prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup margarine and chocolate chips in a medium saucepan over medium-low heat.  Stir occasionally until melted and smooth.  Spread over peanut butter mixture.  Let cool completely before cutting into bars.\n"
            }
        ],
