    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223018/not-so-stuffed-peppers/",
        "name": "Not So Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1554278.jpg",
            "width": 966,
            "height": 966
        },
        "datePublished": "2019-04-04T03:52:50.000Z",
        "description": "A great tasting stuffed pepper recipe that takes half the time to make and no baking involved.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large green bell pepper, top and seeds removed",
            "\u00bd cup white rice",
            "1 cup water",
            "1 pound ground beef",
            "\u2153 onion, chopped",
            "1 (14.5 ounce) can diced tomatoes with garlic",
            "1 teaspoon Worcestershire sauce",
            "2 (10.75 ounce) cans  creamy tomato soup",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a pot of lightly salted water to a boil; cook green bell pepper until tender, about 5 minutes. Remove with a slotted spoon and cool. Chop into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring rice and water to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until the rice is tender, and the liquid has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir beef and onion in a skillet over medium heat until beef is browned and crumbly, and onion is softened, 10 to 15 minutes. Add chopped green bell pepper, cooked rice, tomatoes, Worcestershire sauce, and tomato soup; stir to combine. Cover and simmer until heated through, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon beef and rice mixture into individual bowls; top each serving with Cheddar cheese.\n"
            }
        ],
