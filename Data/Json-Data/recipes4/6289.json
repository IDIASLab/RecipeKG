    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230759/chocolate-covered-bacon-strips/",
        "name": "Chocolate Covered Bacon Strips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4647006.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2019-04-02T06:59:52.000Z",
        "description": "This is the perfect marriage. The sweetness of chocolate and the saltiness of bacon. What could be better?",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 skewers",
        "recipeIngredient": [
            "12 slices thick-cut bacon",
            "12 wooden skewers",
            "1 cup semisweet chocolate chips",
            "1 tablespoon vegetable shortening"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Place a baking rack on top of a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Thread each slice of bacon onto a skewer; place skewered bacon on the baking rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake bacon in the preheated oven until crisp; 20 to 25 minutes. Remove from oven and cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chocolate chips and shortening in a microwave-safe bowl. Heat in the microwave in 30 second intervals until chocolate is melted and smooth, stirring after each interval.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat each side of the skewered bacon with the chocolate mixture using a pastry brush. Place coated bacon on a sheet of waxed paper. Refrigerate until firm, at least 30 minutes. Store in refrigerator until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Bacon",
            "Appetizers"
