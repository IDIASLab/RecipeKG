    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21186/vegetable-fried-rice/",
        "name": "Vegetable Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1114524.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2000-08-26T08:09:21.000Z",
        "description": "This dish combines the nutty flavor of brown rice with the fresh taste of bell peppers, baby peas, and other vegetables.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 cups water",
            "1\u2009\u00bd cups quick-cooking brown rice",
            "2 tablespoons peanut oil",
            "1 small yellow onion, chopped",
            "1 small green bell pepper, chopped",
            "1 teaspoon minced garlic",
            "\u00bc teaspoon red pepper flakes",
            "3 green onions, thinly sliced",
            "3 tablespoons soy sauce",
            "1 cup frozen petite peas",
            "2 teaspoons sesame oil",
            "\u00bc cup roasted peanuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan bring water to a boil. Stir in rice. Reduce heat, cover and simmer for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat peanut oil in a large skillet or wok over medium heat. Add onions, bell pepper, garlic and pepper flakes, to taste. Cook 3 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase heat to medium-high and stir in cooked rice, green onions and soy sauce. Stir-fry for 1 minute. Add peas and cook 1 minute more. Remove from heat. Add sesame oil and mix well. Garnish with peanuts, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Rice",
            "Fried Rice Recipes"
        ],
