    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235227/bang-bang-shrimp-rival/",
        "name": "Bang Bang Shrimp&reg; Rival",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3738632.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2019-04-02T07:02:06.000Z",
        "description": "Serve this as a starter or entree. It will rival the Bone Fish Grill&reg;! Serve over a bed of lettuce in soup type bowl.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cups vegetable oil, or as needed",
            "2 pounds shrimp, peeled and deveined",
            "1\u2009\u00bd teaspoons salt",
            "1 cup mayonnaise",
            "3 tablespoons Sriracha hot chili sauce, or more to taste",
            "1 tablespoon fish sauce (such as Thai Kitchen\u00ae)",
            "\u00bd teaspoon hot chili sesame oil",
            "\u00be cup milk",
            "2 eggs, well beaten",
            "8 ounces seafood breader (such as House-Autry\u00ae Original)",
            "\u00bc cup chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place shrimp in a bowl and sprinkle evenly with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk mayonnaise, hot chili sauce, fish sauce, and sesame oil together in a bowl until evenly mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk and eggs together in a bowl. Pour seafood breader into a shallow bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip shrimp in egg mixture. Transfer shrimp to seafood breader and coat well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry coated shrimp, working in batches, in the hot oil until coating is browned and shrimp are pink and fully cooked, about 5 minutes. Try to keep shrimp separate from each other while in the fryer. Remove cooked shrimp with a slotted spoon and drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mayonnaise mixture and cilantro into a large resealable plastic bag; add cooked shrimp. Seal bag and evenly coat shrimp with mayonnaise mixture.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
        ],
