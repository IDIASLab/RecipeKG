        "recipeIngredient": [
            "3 cups water",
            "2 (10 ounce) cans sliced mushrooms, drained",
            "2 (10 ounce) cans peas, drained",
            "2 cups uncooked rice",
            "1\u2009\u00bd cups sliced carrots",
            "\u00bd cup soy sauce",
            "\u00bc cup canola oil",
            "1 (1.5 ounce) package dry onion soup mix"
