        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water, mushrooms, peas, rice, carrots, soy sauce, oil, and soup mix together in a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until rice and vegetables are tender, 45 to 60 minutes.\n"
            }
