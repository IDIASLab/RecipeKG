        "name": "White Cheddar and Horseradish Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1011461.jpg",
            "width": 2456,
            "height": 2448
