        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Lightly spray a 5-quart or larger slow cooker crock with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water, oats, mashed bananas, cocoa powder, and sweetener in prepared slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 6 hours.\n"
            }
