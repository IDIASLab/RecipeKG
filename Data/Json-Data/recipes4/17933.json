        "recipeIngredient": [
            "1 banana",
            "1 cup frozen mango chunks",
            "2 leaves kale, torn into several pieces",
            "3 tablespoons white beans, drained",
            "2 tablespoons unsweetened shredded coconut",
            "\u00bd teaspoon matcha green tea powder",
            "1 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine banana, mango, kale, white beans, coconut, and matcha powder in a blender; add water. Blend mixture until very smooth.\n"
