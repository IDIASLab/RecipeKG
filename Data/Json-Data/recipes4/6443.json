    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231467/hazelnut-palmiers/",
        "name": "Hazelnut Palmiers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F973461.jpg",
            "width": 820,
            "height": 820
        },
        "datePublished": "2019-04-02T07:00:26.000Z",
        "description": "Hazelnut in puff pastry; it doesn't get any better than that. Very simple.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 dozen palmiers",
        "recipeIngredient": [
            "1 sheet frozen puff pastry, thawed",
            "1 cup chocolate-hazelnut spread (such as Nutella\u00ae)",
            "\u2153 cup finely chopped, roasted hazelnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Lay puff pastry flat onto a work surface. Spread chocolate-hazelnut spread over puff pastry and sprinkle with hazelnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Starting with a long edge of the pastry, roll pastry tightly around filling, stopping in the middle. Repeat with the opposite long edge, meeting the first roll. Refrigerate until chilled, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut pastry into 3/4-inch slices. Arrange slices on prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until golden, 6 to 7 minutes. Flip cookies and continue baking until crisp, 4 to 5 minutes more.\n"
            }
        ],
