    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/93202/slow-cooker-pumpkin-soup/",
        "name": "Slow Cooker Pumpkin Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F135324.jpg",
            "width": 2048,
            "height": 1531
        },
        "datePublished": "2019-04-02T06:48:01.000Z",
        "description": "This easy soup will really warm you up on a winter evening!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT4H0M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "2 quarts",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 medium sugar pumpkin, seeded and cubed",
            "1 medium onion, chopped",
            "3 cups chicken stock, or as needed",
            "1 sprig fresh rosemary",
            "1 sprig fresh thyme",
            "1 sprig fresh sage",
            "2 small cinnamon sticks",
            "2 bay leaves",
            "\u00bd cup heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the olive oil in a large skillet over medium-high heat. Add pumpkin and onion; cook and stir until lightly browned. Transfer to a slow cooker. Pour in enough chicken broth to cover the pumpkin. Tie the rosemary, thyme, sage, cinnamon, and bay leaves into a piece of cheesecloth, and place in the slow cooker. Cover and cook on Low for 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After 4 hours, remove the herb sachet. Stir in the cream, and puree the soup with a hand blender until smooth. Serve.\n"
            }
        ],
