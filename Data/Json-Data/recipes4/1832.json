    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/70051/italian-sausage-delight/",
        "name": "Italian Sausage Delight!",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8047012.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-04-02T06:46:10.000Z",
        "description": "My husband who loves Italian dishes was getting bored with the same ones, so I made this one day for dinner and the whole family loved it!  I hope you will too!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 (3.5 ounce) links hot Italian sausage",
            "1 large onion, chopped",
            "1 red bell peppers, seeded and diced",
            "1 green bell pepper, seeded and diced",
            "1 (4 ounce) can mushrooms, drained",
            "1 (16 ounce) package penne pasta",
            "\u00bd cup Italian salad dressing",
            "\u00bc cup grated Parmesan cheese for topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add penne pasta, and cook until tender, about 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the Italian sausage links in a small skillet over medium heat. Cook, turning occasionally until firm and cooked through.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate larger skillet, combine the red and green bell peppers, mushrooms, and Italian dressing. Cook and stir until the vegetables are tender, about 5 minutes. Slice the Italian sausages into rounds, and stir into the vegetables. Continue to cook and stir for another 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, place servings of pasta onto plates. Top with the sausage and vegetable mixture. Sprinkle liberally with Parmesan cheese.\n"
            }
        ],
