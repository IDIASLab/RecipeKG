        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/52585/hot-chicken-sandwiches-ii/",
        "name": "Hot Chicken Sandwiches II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F785213.jpg",
