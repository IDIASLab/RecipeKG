    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24681/oven-fried-pork-chops/",
        "name": "Oven-Fried Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F422232.jpg",
            "width": 2019,
            "height": 1513
        },
        "datePublished": "2019-04-02T06:42:45.000Z",
        "description": "This is a quick, simple, and above all, a cheap way to make pork chops.  I love it!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 pork chops, trimmed",
            "2 tablespoons butter, melted",
            "1 egg, beaten",
            "2 tablespoons milk",
            "\u00bc teaspoon black pepper",
            "1 cup herb-seasoned dry bread stuffing mix"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C). Pour butter into a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together egg, milk and pepper. Dip pork chops in egg mixture, coat with stuffing mix and place in pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 10 minutes. Turn chops and bake for another 10 minutes, or until no pink remains in the meat and juices run clear.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes",
            "Baked"
        ],
