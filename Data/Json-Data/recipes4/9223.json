    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268523/baked-sausage-dip/",
        "name": "Baked Sausage Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1128132.jpg",
            "width": 4320,
            "height": 2432
        },
        "datePublished": "2019-04-03T06:32:56.000Z",
        "description": "A sausage dip I made up for my culinary classes when we did a lab with chips (made in the microwave) and dips.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 teaspoons olive oil, divided",
            "\u00bd pound ground Italian sausage",
            "1 cup diced onion",
            "\u00bd cup diced pepper",
            "2 cloves garlic, minced, or more to taste",
            "1 (8 ounce) package cream cheese",
            "\u00bd cup marinara sauce",
            "2 teaspoons Italian seasoning",
            "2 ounces shredded mozzarella cheese",
            "2 ounces grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 teaspoons oil in a saute pan over medium heat. Cook and stir sausage until browned and crumbly, 5 to 7 minutes. Drain on a paper towel-lined plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add remaining 2 teaspoons oil to the same pan. Saute onion, pepper, and garlic until onion is translucent, about 5 minutes. Stir in cooked sausage. Add cream cheese, marinara sauce, and Italian seasoning. Mix all together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into a baking dish. Top with mozzarella cheese and Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheeses melt and start to brown, 25 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes"
        ],
