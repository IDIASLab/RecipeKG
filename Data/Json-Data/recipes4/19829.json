    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265398/one-pot-tortellini-and-sausage/",
        "name": "One-Pot Tortellini and Sausage",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "One-Pot Tortellini and Sausage"
        },
        "datePublished": "2019-04-04T04:12:37.000Z",
        "description": "It's technically a 'one-skillet' dish, but the idea is the same - everything goes into the skillet and dishes are kept at a minimum.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "1 pound spicy Italian sausage, casings removed, crumbled",
            "3 cups spaghetti sauce",
            "\u00bd cup water",
            "1 teaspoon Italian seasoning",
            "1 (20 ounce) package fresh cheese tortellini",
            "1 cup mozzarella cheese",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in an oven-safe skillet over medium-high heat. Add crumbled sausage. Cook and stir until sausage is browned and no longer pink, 5 to 10 minutes. Stir in spaghetti sauce, water, and Italian seasoning. Add tortellini and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring liquid in the skillet to a boil. Reduce heat to low, cover, and simmer until tortellini are tender, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler to low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove skillet from heat. Sprinkle mozzarella cheese and Parmesan cheese over tortellini.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place skillet under broiler and cook until cheese is melted, 2 to 3 minutes.\n"
            }
        ],
