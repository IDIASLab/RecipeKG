    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232353/nancys-spaghetti-salad/",
        "name": "Nancy's Spaghetti Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1008115.jpg",
            "width": 1197,
            "height": 1200
        },
        "datePublished": "2019-04-02T07:00:53.000Z",
        "description": "A salad that can be served as a meal or a side dish.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "10 serving",
        "recipeIngredient": [
            "2 cups chopped tomatoes",
            "1 (8 ounce) package shredded Cheddar cheese",
            "2 cucumbers, chopped",
            "1 cup chopped red bell pepper",
            "1 cup chopped green bell pepper",
            "1 cup chopped red onion",
            "1 (6 ounce) can sliced black olives",
            "1 tablespoon Italian seasoning",
            "1 (16 ounce) package spaghetti",
            "2 cups shredded cooked chicken",
            "1 (16 ounce) bottle Italian-style salad dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix tomatoes, Cheddar cheese, cucumbers, red bell pepper, green bell pepper, red onion, black olives, and Italian seasoning together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally until cooked through but firm to the bite, about 12 minutes. Drain and run under cold water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss spaghetti with tomato mixture; fold in chicken. Pour salad dressing over salad; toss to coat. Refrigerate until flavors have blended, about 2 hours.\n"
            }
        ],
