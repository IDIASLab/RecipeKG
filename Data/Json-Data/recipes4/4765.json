    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216818/nippy-pork-salad/",
        "name": "Nippy Pork Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F861301.jpg",
            "width": 2252,
            "height": 2252
        },
        "datePublished": "2019-04-02T06:54:39.000Z",
        "description": "Delicious pork that can be served on sandwiches, crackers, or just on the side with another salad. Best if left in fridge at least 4 hours before serving. Great next day, too.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT4H15M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "\u00bc stalk celery, chopped",
            "3 green onions, sliced",
            "4 teaspoons Dijon mustard",
            "4 teaspoons fat-free creamy salad dressing (such as Miracle Whip Free\u00ae)",
            "4 teaspoons prepared horseradish",
            "2 drops hot pepper sauce (such as Tabasco\u00ae), or to taste",
            "\u00bc teaspoon salt",
            "2 cups cubed cooked pork roast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix the pork, celery, and green onions in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the mustard, creamy salad dressing, horseradish, hot pepper sauce, and salt in blender. Blend to make a saucy dressing, about 1 minute. Lightly toss the dressing with the pork mixture. Cover and refrigerate until serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Beef and Pork Salad Recipes",
            "Ham Salad Recipes"
        ],
