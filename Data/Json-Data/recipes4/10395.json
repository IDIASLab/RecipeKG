    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20385/gordos-good-venison/",
        "name": "Gordo's Good Venison",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6785918.jpg",
            "width": 1440,
            "height": 2560
        },
        "datePublished": "2000-08-09T23:32:41.000Z",
        "description": "Most people think you have to take the 'wild' taste out of deer. But really you can keep that good 'game' flavor by using a variety of herbs and spices.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (2 pound) venison roast",
            "1\u2009\u00bd cups water",
            "1 cup chicken broth",
            "\u00bd cup olive oil",
            "3 tablespoons butter",
            "3 cloves garlic, minced",
            "1 teaspoon Louisiana-style hot sauce",
            "1 tablespoon distilled white vinegar",
            "\u00bc cup chopped fresh parsley",
            "3 leaves fresh basil, chopped",
            "2 tablespoons fresh rosemary",
            "1 teaspoon ground cumin",
            "\u00bd teaspoon paprika",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice venison roast into thick 1 inch steaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine water, chicken broth, olive oil, butter, garlic, and hot sauce; bring to a boil. Reduce heat, and stir in vinegar. Season with parsley, basil, rosemary, cumin, paprika, salt, and pepper. Simmer for 20 minutes, stirring regularly. Allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cooled sauce over venison steaks, and marinate in the refrigerator for 3 to 4 hours, turning regularly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil grate. Take steaks out of marinade, reserve marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill steaks for 7 to 8 minutes per side, or to desired doneness. Turn them regularly and make sure you don't over-cook. Mop steaks with reserved marinade while cooking.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Game Meats",
            "Venison"
        ],
