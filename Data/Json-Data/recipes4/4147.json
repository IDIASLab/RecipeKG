    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213199/potato-crunchy-tenders/",
        "name": "Potato Crunchy Tenders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1102450.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-02T06:52:45.000Z",
        "description": "I was trying to come up with something different than pizza on football practice nights, so i came up with this quick meal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd cup vegetable oil for frying",
            "1\u2009\u00bd cups milk",
            "1 egg",
            "1 (7.6 ounce) package garlic flavored instant mashed potatoes",
            "2 teaspoons salt",
            "2 teaspoons ground black pepper",
            "1\u2009\u00bd pounds chicken tenders"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the oil is heating, beat the milk and egg together in a bowl. In another bowl, stir together the instant mashed potatoes, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the chicken tenders with the milk mixture to coat thoroughly, then shake off excess milk and dip each tender into the potato flakes. Place the breaded tenders into the hot oil, and fry until golden brown, 7 to 10 minutes. Remove from oil and drain on paper towels.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Fried Chicken Recipes"
        ],
