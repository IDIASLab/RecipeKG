    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233296/cheese-and-tomato-macaroni/",
        "name": "Cheese and Tomato Macaroni",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheese and Tomato Macaroni"
        },
        "datePublished": "2019-04-04T03:55:31.000Z",
        "description": "A family favorite. Very easy to make and ideal if you have a tight budget.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package elbow macaroni",
            "1 tablespoon vegetable oil",
            "1 tablespoon all-purpose flour",
            "1 teaspoon garlic paste, or to taste",
            "1 (14.5 ounce) can plum tomatoes, chopped, juice reserved",
            "4 ounces shredded Cheddar cheese, divided",
            "\u00bc cup water, or as needed",
            "1 tomato, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 minutes. Drain and transfer pasta to a large baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil and flour in a large skillet over medium heat; cook and stir until mixture is frothy and smooth, about 5 minutes. Add reserved tomato juice and garlic paste. Cook and stir until mixture is thickened and stir in chopped tomatoes and about 3/4 the Cheddar cheese. Simmer until cheese is melted, about 5 minutes more. Stir water into tomato sauce if mixture becomes too thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour tomato sauce over macaroni and top with remaining Cheddar cheese and sliced tomato.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until cheese is melted and browned, about 5 minutes.\n"
            }
        ],
