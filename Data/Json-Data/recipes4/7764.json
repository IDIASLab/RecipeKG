    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242269/slow-cooker-buffalo-chicken-lettuce-wraps/",
        "name": "Slow Cooker Buffalo Chicken Lettuce Wraps",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4548370.jpg",
            "width": 2976,
            "height": 3968
        },
        "datePublished": "2019-04-02T07:04:34.000Z",
        "description": "Quick, easy, and healthier than traditional Buffalo wings! Perfect for family dinners, BBQ parties, football Sundays, or just about any event!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT6H0M",
        "totalTime": "P0DT6H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 pounds skinless, boneless chicken breasts",
            "1 (12 ounce) bottle cayenne pepper sauce (such as Frank's\u00ae RedHot\u00ae)",
            "1 (1 ounce) package ranch dressing mix",
            "1 head Boston lettuce leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put chicken into the crock of a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cayenne pepper sauce and ranch dressing mix together in a bowl until smooth; pour over chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 6 to 7 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer chicken to a large bowl and use 2 forks to shred chicken into strands.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain liquid from slow cooker crock into a bowl to serve on the side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chicken into lettuce leaves and roll lettuce around chicken.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Wraps and Rolls"
        ],
        "recipeCuisine": [],
