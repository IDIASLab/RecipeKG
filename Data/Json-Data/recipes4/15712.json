        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231156/barbequed-country-ribs/",
        "name": "Barbequed Country Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F996794.jpg",
            "width": 1536,
            "height": 1536
        },
        "datePublished": "2019-04-04T03:54:36.000Z",
        "description": "Aunt Iola MacArthur's from Nita Nolan. Just the best. Ribs can be simmered the day before then finished on the grill. Bring ribs to near room temp before grilling.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H45M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00be cup cold water",
            "\u00be cup ketchup",
            "2 tablespoons vinegar",
            "2 tablespoons Worcestershire sauce",
            "1 tablespoon salt, or to taste",
            "1 tablespoon paprika",
            "1 teaspoon chili powder",
            "\u00bd teaspoon cracked black pepper",
            "1 (4 pound) package country-style pork ribs",
            "water, to cover"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir cold water, ketchup, vinegar, Worcestershire sauce, salt, paprika, chili powder, and black pepper together in a large pot. Add ribs to the pot. Pour enough water into the pot to cover the ribs completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the water to a boil, reduce heat to medium-low, and cook at a simmer until ribs are tender, about 90 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook ribs on preheated grill, basting frequently with sauce, until browned, 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes",
            "Country-Style Ribs"
        ],
