        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244795/lavender-tom-collins/",
        "name": "Lavender Tom Collins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2552345.jpg",
            "width": 1280,
            "height": 1280
        },
        "datePublished": "2019-04-04T04:00:15.000Z",
        "description": "Lavender brings some yummy sophistication to the classic Tom Collins. You can make your own Collins mix with soda and lavender simple syrup, or try this quick work-around. The flavor will slowly melt, giving the perfect hint of lavender!",
        "prepTime": "P0DT0H5M",
        "cookTime": null,
        "totalTime": "P0DT3H20M",
        "recipeYield": "1 cocktail",
        "recipeIngredient": [
            "12 lavender flowers",
            "3 cups boiling water",
            "1 cup ice cubes",
            "6 fluid ounces Collins mix",
            "2 fluid ounces gin",
            "2 maraschino cherries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place lavender flowers in an empty ice cube tray; pour boiling water over flowers. Place ice cube tray in the refrigerator to cool, about 15 minutes. Transfer to freezer and freeze until solid, about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a glass with ice. Pour Collins mix and gin over ice; stir. Top drink with a lavender ice cube and garnish with maraschino cherries.\n"
            }
        ],
