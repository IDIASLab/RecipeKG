        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244365/funfetti-cookies/",
        "name": "Funfetti&reg; Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3532194.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2019-04-02T07:04:41.000Z",
        "description": "Easy, yummy cookies made from a Funfetti&reg; cake mix! Top with your favorite icing and sprinkles.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "1 (15.25 ounce) package cake mix with candy bits (such as Pillsbury\u00ae Funfetti\u00ae), or more to taste",
            "\u00bd cup butter, softened",
            "1 egg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cake mix, butter, and egg together in a bowl until well combined; roll into balls and place on a baking sheet, about 1 1/2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies start to brown and crack, about 10 minutes. Allow cookies to sit on baking sheet for a few minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
