    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236394/green-borscht/",
        "name": "Green Borscht",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4536582.jpg",
            "width": 2248,
            "height": 2224
        },
        "datePublished": "2019-04-02T07:02:41.000Z",
        "description": "A delicious and easy Russian soup with potatoes and spinach. Leftovers keep well in the refrigerator for a day or two. Serve with a dollop of sour cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "3 eggs, or more to taste",
            "3 potatoes, cut into cubes, or more to taste",
            "\u00bd cup rice",
            "1 bunch green onions, finely chopped",
            "salt to taste",
            "1 bunch fresh spinach, stems removed and leaves chopped",
            "1 bunch fresh parsley, chopped",
            "\u00bc cup sour cream, or more to taste",
            "1 teaspoon chopped fresh dill, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a saucepan and cover with salted cold water. Bring to a boil, remove from heat, cover saucepan, and let eggs stand in hot water, 10 to 12 minutes. Drain water from saucepan and cover eggs with ice water until chilled, about 10 minutes. Peel eggs and chop or shred.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large pot halfway with water; bring to a boil. Add potatoes, rice, green onions; reduce heat and simmer until potatoes are mostly tender, 15 to 20 minutes. Add spinach and parsley; continue simmering until wilted, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sour cream into soup until well mixed; add dill. Bring soup back to a boil, remove pot from heat, and stir eggs into soup. Season with salt.\n"
            }
        ],
