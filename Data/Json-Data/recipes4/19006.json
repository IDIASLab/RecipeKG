    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259741/cheesy-quinoa-patties-with-kale-and-spinach/",
        "name": "Cheesy Quinoa Patties with Kale and Spinach",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4716857.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2019-04-04T04:09:27.000Z",
        "description": "Very healthy recipe with tons of superfoods.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H54M",
        "totalTime": "P0DT1H24M",
        "recipeYield": "12 patties",
        "recipeIngredient": [
            "1 cup uncooked quinoa",
            "1\u2009\u00bd cups water",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 clove garlic",
            "1 tablespoon vegetable oil",
            "\u00bc onion, chopped",
            "\u00be cup finely chopped kale",
            "\u00be cup finely chopped spinach",
            "1 cup grated mozzarella cheese",
            "\u00bd cup bread crumbs",
            "1 tablespoon flaxseed meal",
            "1 egg, beaten",
            "\u00bc cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and quinoa to a boil in a saucepan. Add salt, black pepper, and garlic. Reduce heat to medium-low, cover, and simmer until quinoa is tender, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon oil in a skillet over medium heat; add onion. Cook and stir until translucent, about 2 minutes. Transfer to a bowl. Add kale and spinach to hot skillet; cook and stir until tender, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cooked quinoa, onions, kale, spinach, mozzarella cheese, bread crumbs, and flaxseed meal in a bowl; stir to combine. Add beaten egg; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Oil a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form golf-ball-size portions of the quinoa mixture into patties. Place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown and crusty, about 15 minutes per side.\n"
            }
        ],
