        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228805/dark-brown-bbq-ribs/",
        "name": "Dark Brown BBQ Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3301285.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2019-04-04T03:53:32.000Z",
        "description": "The best sauce recipe I've found so far.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 pounds pork spareribs, cut into serving-size pieces",
            "1 pinch salt and ground black pepper to taste",
            "\u00bd cup molasses",
            "\u2153 cup vinegar",
            "\u00bc cup prepared mustard",
            "\u00bc cup water",
            "2 tablespoons Worcestershire sauce",
            "1 tablespoon instant coffee granules",
            "\u00bd teaspoon hot pepper sauce (such as Tabasco\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange ribs in a shallow roasting pan; season with salt and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake ribs in preheated oven for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix molasses, vinegar, mustard, water, Worcestershire sauce, instant coffee granules, and hot pepper sauce together in a bowl. Brush generously over the ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue baking ribs, basting with sauce every 15 minutes, until the meat pulls away easily from the bone, about 1 additional hour.\n"
            }
        ],
