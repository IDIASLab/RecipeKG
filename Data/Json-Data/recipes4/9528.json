    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11649/easy-chocolate-crackled-cookies/",
        "name": "Easy Chocolate Crackled Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F889143.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "1999-11-06T04:04:53.000Z",
        "description": "This is a really easy recipe because you just use a cake mix as the base.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package devil's food cake mix",
            "\u00bd cup butter flavored shortening",
            "1 tablespoon water",
            "2 eggs",
            "1 cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, beat together the shortening, water, and eggs. Add the cake mix, and mix until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the dough into walnut sized balls, and roll the balls in the confectioners' sugar. Place cookies 2 inches apart on the prepared cookie sheets. Bake for 10 minutes in the preheated oven. These are best served the same day, as the confectioners' sugar becomes absorbed by the cookie giving it a pasty look. This can be fixed by dusting the cookies with sugar again.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
