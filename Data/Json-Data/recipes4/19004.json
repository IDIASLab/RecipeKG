    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259752/vegan-apple-nut-muffins-gluten-free/",
        "name": "Vegan Apple-Nut Muffins (Gluten Free)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4557156.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-04-04T04:09:27.000Z",
        "description": "These healthier gluten-free, vegan, no-sugar-added muffins are great for breakfast or an afternoon snack. No guilt eating one of these.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 dozen muffins",
        "recipeIngredient": [
            "canola oil cooking spray",
            "14 Medjool dates, pitted and chopped",
            "1 cup soy milk",
            "\u2153 cup water",
            "2 tablespoons ground flax seeds",
            "1 cup oat flour",
            "\u00be cup millet flour",
            "\u00bd cup tapioca flour",
            "2 teaspoons baking powder",
            "2 teaspoons ground cinnamon",
            "1 teaspoon baking soda",
            "\u00bd cup applesauce",
            "1 teaspoon vanilla extract",
            "3 small apples, peeled and diced",
            "1 cup roughly chopped walnuts",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a 12-cup muffin tin with parchment paper liners; spray liners with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dates in a bowl; cover with soy milk. Let stand until softened, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water and flax seeds together in a separate bowl. Let stand until thickened, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oat flour, millet flour, tapioca flour, baking powder, cinnamon, and baking soda in a bowl. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the soy milk-date mixture, applesauce, and vanilla extract in a blender or food processor. Blend until smooth. Transfer to the bowl with the flour mixture; add the soaked flax seeds. Mix with a spoon until no dry spots remain. Fold in apples, walnuts, and raisins gently. Spoon batter into the muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean and tops begin to brown and form cracks, 30 to 35 minutes. Cool for 15 to 20 minutes before serving.\n"
            }
        ],
