    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8090/red-velvet-cake-iii/",
        "name": "Red Velvet Cake III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F555738.jpg",
            "width": 1332,
            "height": 1665
        },
        "datePublished": "1998-12-15T15:48:54.000Z",
        "description": "Moist and delicious easy cake!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9 x 13-inch cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "4 eggs",
            "1 ounce red food coloring",
            "\u00bd cup vegetable oil",
            "1 cup water",
            "1 tablespoon distilled white vinegar",
            "1 tablespoon unsweetened cocoa powder",
            "1 cup cold milk",
            "1 (8 ounce) package cream cheese",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease and flour one 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together cake mix, 1 box pudding mix, eggs, oil, food coloring, water, vinegar, and cocoa. Pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 35 to 40 minutes.  Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat together milk and cream cheese.  Mix in 1 box pudding mix.  Fold in whipped topping.  Spread on cooled cake.  Refrigerate.\n"
            }
        ],
