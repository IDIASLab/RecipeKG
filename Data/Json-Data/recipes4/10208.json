    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17636/my-mommas-easy-homemade-veggie-soup/",
        "name": "My Momma's Easy Homemade Veggie Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3372164.jpg",
            "width": 2205,
            "height": 2205
        },
        "datePublished": "2000-02-17T00:38:56.000Z",
        "description": "This is my mom's vegetable soup recipe. It's really good, and it's very easy! It combines ground beef with lots of potatoes and mixed vegetables in a tomato base. Enjoy!  Enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H35M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 pounds ground beef",
            "6 potatoes, peeled and cubed",
            "water to cover",
            "4 (15 ounce) cans mixed vegetables, drained",
            "1 onion, chopped",
            "2 cups chopped cabbage",
            "1 (15 ounce) can tomato sauce",
            "2 tablespoons ground black pepper",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the ground beef in a large skillet over medium high heat. Saute for 10 to 15 minutes, or until browned and crumbly; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pot over high heat, combine the potatoes with water to cover and cook for 20 minutes, or until potatoes are almost tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the mixed vegetables, onion, cabbage, tomato sauce, reserved ground beef and ground black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring to a boil, reduce heat to low and simmer for 1 1/2 to 2 hours. Season with salt to taste.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes"
        ],
