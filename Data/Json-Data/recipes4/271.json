        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20763/garlic-pepper-steak/",
        "name": "Garlic Pepper Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F699286.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-08-16T04:29:33.000Z",
        "description": "An easy garlic and pepper rub lightly adorns juicy slabs of steak.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "2 cloves garlic, peeled and crushed",
            "1 tablespoon coarsely ground black pepper",
            "2 pounds round steak, 1 1/2 inches thick"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix together olive oil, garlic and pepper. Score steak and rub with the olive oil mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place steak on the prepared grill. Cook 20 minutes, or to desired doneness, turning once.\n"
            }
        ],
