        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21294/apricot-pork-chops/",
        "name": "Apricot Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F238064.jpg",
