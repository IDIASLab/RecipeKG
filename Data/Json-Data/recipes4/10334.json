        "recipeIngredient": [
            "1 (15 ounce) can solid pack pumpkin puree",
            "1 (12 fluid ounce) can frozen apple juice concentrate",
            "\u215b teaspoon ground nutmeg",
            "1 teaspoon ground cinnamon",
            "2\u2009\u00bd cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove pumpkin from can and freeze for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a blender combine partially frozen pumpkin, frozen apple juice concentrate, nutmeg and cinnamon. Blend until smooth. Continue to blend while adding water to fill the blender.\n"
            }
