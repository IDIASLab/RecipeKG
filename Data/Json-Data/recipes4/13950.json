    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/212912/beef-nilaga/",
        "name": "Beef Nilaga",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F809737.jpg",
            "width": 1023,
            "height": 685
        },
        "datePublished": "2019-04-04T03:48:23.000Z",
        "description": "This is another one-meal-in-a-pot dish. It contains soup, meat and vegetables all cooked together in one delicious broth. Terrific during cooler weather. Serve hot alone or with rice.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "4\u2009\u00bd pounds beef short ribs",
            "1 quart water",
            "1 tablespoon black peppercorns, crushed",
            "2 onions, chopped",
            "2 beef bouillon cubes",
            "2 carrots, cut in chunks",
            "2 stalks celery, quartered",
            "1 chayote squash, peeled and quartered",
            "2 potatoes, quartered",
            "salt to taste",
            "\u00bc head cabbage, cut into wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the beef ribs in a large pot with enough water to cover. Bring to a boil over high heat, and boil for 5 minutes. Drain; discard the cooking water (the ribs will not be fully cooked).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the beef to the pot. Pour in the 1 quart water, peppercorns, onions, and beef bouillon. Bring to a boil, cover, reduce the heat, and simmer for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Skim any fat floating on the surface. Stir in the carrots, celery, chayote, and potatoes; cover, and simmer until potatoes are tender, about 20 minutes. Season with salt. Add cabbage and cook an additional 5 minutes.\n"
            }
        ],
