    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24076/speedy-spaghetti/",
        "name": "Speedy Spaghetti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4563007.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2019-04-04T03:27:54.000Z",
        "description": "If your tired of the traditional spaghetti noodles with sauce poured over, give this one a try. This is the only way my family will eat their spaghetti.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd pound ground beef",
            "1 small onion, chopped",
            "2 (8 ounce) cans tomato sauce",
            "1\u2009\u00bd cups water",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon dried parsley",
            "\u00bd pinch dried basil",
            "\u00bd teaspoon black pepper",
            "4 ounces uncooked spaghetti"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium heat, brown the ground beef with the onion until all pink is gone; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in tomato sauce, water, salt, parsley, basil and pepper; mix well.  Heat until sauce boils. Break spaghetti in half and drop into sauce a little at a time. Cover and turn to low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook until spaghetti is tender, approximately 20 to 25 minutes.  Stir occasionally to keep from sticking to pan and from noodles sticking together. Add water, 1/2 to 1 cup, if it starts to dry out and noodles are not cooked.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
            "Spaghetti"
        ],
