        "mainEntityOfPage": "https://www.allrecipes.com/recipe/162444/slow-cooker-tomato-chicken/",
        "name": "Slow Cooker Tomato Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F138300.jpg",
