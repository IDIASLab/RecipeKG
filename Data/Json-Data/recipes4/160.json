        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19042/easy-chocolate-ice-cream/",
        "name": "Easy Chocolate Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F46000.jpg",
