        "name": "Bohemian-Style Absinthe Cocktail",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1939208.jpg",
            "width": 1017,
            "height": 1185
