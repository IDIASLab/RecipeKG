    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/73034/bbq-pie/",
        "name": "BBQ Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F16750.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2019-04-02T06:46:26.000Z",
        "description": "Baked barbeque beans and ground beef with a crust. This pie is perfect for any occasion.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ground beef",
            "\u00bc cup diced onion",
            "\u00bc teaspoon ground black pepper",
            "2 (15 ounce) cans baked beans with pork",
            "1 teaspoon Worcestershire sauce",
            "1 cup barbeque sauce",
            "1 cup biscuit baking mix",
            "\u00bd cup milk",
            "1 egg",
            "\u00bc cup shredded Cheddar cheese",
            "1 tablespoon barbecue sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crumble the ground beef into a large skillet over medium heat. When it is starting to brown, stir in the onion and season with pepper. Continue to cook and stir until beef is browned and the onion is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain off the grease from the beef, and stir in the baked beans, Worcestershire sauce and 1 cup of barbeque sauce. Transfer to a large casserole dish. In a separate bowl, mix together the baking mix, milk and egg. Pour this mixture evenly over the beef and beans in the dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 40 to 45 minutes in the preheated oven, until the biscuit toping is golden brown. Spread a small amount of barbeque sauce over the top and sprinkle with Cheddar cheese while hot from the oven.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Beef Pie Recipes"
        ],
