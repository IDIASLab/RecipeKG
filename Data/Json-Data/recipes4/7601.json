        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240438/easy-roasted-broccoli/",
        "name": "Easy Roasted Broccoli",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1765553.jpg",
