        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13900/peanut-sauce-i/",
        "name": "Peanut Sauce I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8312513.jpg",
