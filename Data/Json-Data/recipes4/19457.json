    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262303/mexican-lentil-soup-with-bacon/",
        "name": "Mexican Lentil Soup with Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mexican Lentil Soup with Bacon"
        },
        "datePublished": "2019-04-04T04:11:02.000Z",
        "description": "Simple lentil soup with bacon, onion, tomato and cilantro. In Mexico, this soup is traditionally served with banana slices.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound lentils",
            "2 cups water, or as needed",
            "salt to taste",
            "\u00bd (12 ounce) package bacon, chopped",
            "1 small onion, chopped",
            "3 tomatoes, chopped",
            "\u00bd cup chopped fresh cilantro leaves",
            "3 firm bananas, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place lentils in a soup pot and cover with 2 inches of water. Bring to a boil. Reduce heat to medium-low and simmer until lentils are soft, 25 to 30 minutes. Season with salt during the last few minutes of cooking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Add onion; saute until transparent, about 5 minutes. Add tomatoes and cilantro; cook until tomatoes change color, about 5 minutes. Pour mixture over cooked lentils. Bring to a boil and cook until flavors blend, 3 to 5 minutes more. Serve with banana slices.\n"
            }
        ],
