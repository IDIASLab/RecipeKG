    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220268/slow-cooker-cream-of-broccoli-soup/",
        "name": "Slow Cooker Cream of Broccoli Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1080126.jpg",
            "width": 1600,
            "height": 1071
        },
        "datePublished": "2019-04-02T06:56:20.000Z",
        "description": "This is a most excellent soup and very easy to make. The kids love it.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT3H5M",
        "totalTime": "P0DT3H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "1 small onion, chopped",
            "2 (10 ounce) packages frozen chopped broccoli, thawed",
            "2 (10.75 ounce) cans  cream of celery soup",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 cup shredded American cheese",
            "2 (10.75 ounce) cans  milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a large pot over medium heat; cook the onion in the hot oil until soft, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain as much liquid from the onion as possible. Transfer the drained onion to a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the broccoli, cream of celery soup, cream of mushroom soup, American cheese, and milk into the slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until the broccoli is tender and the flavors meld, 3 to 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Cream Soup Recipes",
            "Cream of Broccoli Soup"
        ],
