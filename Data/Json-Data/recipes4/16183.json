        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234451/amandas-quinoa-salad/",
        "name": "Amanda's Quinoa Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1133775.jpg",
            "width": 1702,
            "height": 2048
        },
        "datePublished": "2019-04-04T03:56:03.000Z",
        "description": "This is my stepdaughter Amanda's concoction, I had it once and was hooked! Keep it in the fridge all the time!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "2 cups water",
            "1 cup quinoa",
            "5 ounces mango salsa",
            "1 bunch green onions, chopped (green parts only)",
            "\u2153 cup dried cranberries",
            "1\u2009\u00bd ounces sunflower kernels",
            "2 tablespoons champagne vinegar",
            "1 tablespoon French vinaigrette (such as Briannas Home Style\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and quinoa to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until quinoa is tender and water has been absorbed, 15 to 20 minutes. Transfer quinoa to a bowl and fluff with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir mango salsa, green onions, cranberries, sunflower kernels, champagne vinegar, and French vinaigrette into the quinoa. Cover bowl with plastic wrap.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate salad at least 4 hours before serving.\n"
            }
        ],
