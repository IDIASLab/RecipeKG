    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235326/moms-au-gratin-potatoes/",
        "name": "Mom's Au Gratin Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1615670.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2019-04-02T07:02:10.000Z",
        "description": "Creamy and rich side dish perfect for holidays with ham or roast.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "6 potatoes",
            "\u00bd cup butter",
            "3 green onions, chopped, or more to taste",
            "1 pint sour cream",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 cup shredded sharp Cheddar cheese",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes. Drain and cool. Peel and slice potatoes into thin rounds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Saute green onions in hot butter until tender, 3 to 4 minutes. Stir sour cream, condensed chicken soup, Cheddar cheese, and salt into onion mixture until completely combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange potato slices in the bottom of an 8x8-inch baking pan. Pour cheese sauce over potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until brown around the edges, about 45 minutes.\n"
            }
        ],
