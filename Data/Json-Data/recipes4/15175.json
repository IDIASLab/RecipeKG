        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Steep tea bags in boiling water for 2 minutes. Remove and discard tea bags. Set tea concentrate aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a tall glass with ice cubes. Pour club soda, juice cocktail, and 3 ounces of the tea concentrate over the ice cubes; stir.\n"
            }
