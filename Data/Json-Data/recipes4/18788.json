    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257947/gluten-free-flatbread/",
        "name": "Gluten-Free Flatbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4526387.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "2019-04-04T04:08:33.000Z",
        "description": "I first made these to go with gyros. I folded mine over, and it was pretty close to the taste of a whole-wheat pita!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H53M",
        "recipeYield": "8 flatbreads",
        "recipeIngredient": [
            "1 cup sorghum flour",
            "\u00bd cup quinoa flour",
            "\u2153 cup cornstarch",
            "\u2153 cup tapioca flour",
            "\u2153 cup potato starch flour",
            "\u00be teaspoon baking soda",
            "\u00be teaspoon xanthan gum",
            "\u00be teaspoon salt",
            "\u00bd teaspoon active dry yeast",
            "1 cup warm water",
            "\u00bc cup olive oil",
            "2 tablespoons apple cider vinegar",
            "2 tablespoons honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sorghum flour, quinoa flour, cornstarch, tapioca flour, potato starch flour, baking soda, xanthan gum, salt, and yeast together in a bowl. Stir water, olive oil, cider vinegar, and honey together in a separate bowl until honey is dissolved; pour over flour mixture and mix until dough comes together in a ball.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1/4 cup dough between 2 sheets waxed paper or plastic wrap and roll until very thin using a rolling pin or tortilla press. Place on the prepared baking sheet and repeat with remaining dough; let rise for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 8 minutes.\n"
            }
        ],
