        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232278/zucchini-chips/",
        "name": "Zucchini Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1014795.jpg",
