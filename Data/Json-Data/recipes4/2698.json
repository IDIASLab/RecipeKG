        "mainEntityOfPage": "https://www.allrecipes.com/recipe/109731/fierro-chicken/",
        "name": "Fierro Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F247681.jpg",
