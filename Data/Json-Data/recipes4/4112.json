        "name": "Avocado and Black Eyed Pea Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1012720.jpg",
            "width": 2200,
            "height": 2200
