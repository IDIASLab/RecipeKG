        "recipeIngredient": [
            "1 (.25 ounce) package active dry yeast",
            "1 cup quick cooking oats",
            "3 cups bread flour",
            "1 teaspoon salt",
            "\u00bd cup molasses",
            "1 tablespoon vegetable oil",
            "1\u2009\u2153 cups water",
            "\u00bc cup honey"
        ],
