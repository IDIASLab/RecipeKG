        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261240/hot-bacon-cheese-jalepeno-dip-pumpkin-optional/",
        "name": "Hot Bacon Cheese Jalepeno Dip (Pumpkin Optional)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1641653.jpg",
            "width": 1200,
            "height": 1600
        },
        "datePublished": "2019-04-03T06:32:39.000Z",
        "description": "This dip is perfect for holiday parties, game day gatherings, or Halloween parties! Serve with fresh vegetables, tortilla chips, crackers, or pretzels.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "2 cups shredded Monterey Jack cheese",
            "1 pound bacon strips, cooked and crumbled",
            "\u00be cup salsa verde (green salsa)",
            "2 tablespoons chopped green onion",
            "2 tablespoons chopped canned jalapeno peppers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese and Monterey Jack cheese in top of a double boiler over simmering water. Stir frequently, scraping down the sides with a rubber spatula to avoid scorching, until cheese is melted, about 5 minutes. Transfer to a baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bacon, green salsa, green onions, and jalapeno peppers in with the cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake dip in the preheated oven until warm and bubbly, 10 to 12 minutes.\n"
            }
        ],
