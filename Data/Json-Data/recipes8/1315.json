    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277271/filipino-chicken-adobo/",
        "name": "Filipino Chicken Adobo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6582757.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:43:08.000Z",
        "description": "This Pinoy recipe is a popular Filipino main dish usually served with rice and veggie sides.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 tablespoons low-sodium soy sauce",
            "2 jalapeno peppers, seeded and sliced",
            "3 cloves fresh garlic, minced",
            "1 pound chicken wings",
            "2 tablespoons vegetable oil",
            "1\u2009\u00bd cups water",
            "3 bay leaves",
            "1 teaspoon whole peppercorns",
            "2 tablespoons white vinegar",
            "1 teaspoon white sugar, or to taste",
            "\u00bc teaspoon fine salt, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk soy sauce, jalapeno peppers, and garlic together in a large glass or ceramic bowl. Add chicken wings and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken wings from the marinade and shake off excess. Reserve marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saucepan over medium heat. Place chicken wings in the hot oil and pan-fry for 2 minutes per side. Slowly pour in reserved marinade (including garlic) and water. Bring to a boil. Add bay leaves and peppercorns and simmer until chicken is tender, about 20 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in vinegar and cook for 5 minutes. Stir again and season with sugar and salt to taste. Serve hot.\n"
            }
        ],
