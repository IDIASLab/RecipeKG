        "recipeYield": "4 12-ounce cocktails",
        "recipeIngredient": [
            "2 cups frozen pitted cherries",
            "2 cups chopped peaches",
            "\u00be cup vodka",
            "3 tablespoons brown sugar",
            "3 tablespoons water",
            "3 tablespoons lime juice",
            "2 tablespoons amaretto liqueur",
            "2 cups lemon-lime flavored soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree cherries, peaches, vodka, brown sugar, water, lime juice, and amaretto in a blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide among 4 (1-quart) resealable bags. Label and freeze at least 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, massage bags to soften and add 1/2 cup lemon-lime soda to each bag.\n"
            }
