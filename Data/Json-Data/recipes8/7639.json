    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281706/grilled-chicken-caesar-salad-with-gouda/",
        "name": "Grilled Chicken Caesar Salad with Gouda",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grilled Chicken Caesar Salad with Gouda"
        },
        "datePublished": "2011-06-08T19:41:55.000Z",
        "description": "This grilled chicken Caesar salad is a nice summertime meal!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u2153 cup Caesar salad dressing",
            "3 tablespoons orange juice",
            "2 (5 ounce) skinless, boneless chicken breasts, cut into strips",
            "1 tablespoon canola oil",
            "2 tablespoons chopped fresh chives",
            "2 tablespoons chopped fresh parsley",
            "1 tablespoon chopped fresh tarragon",
            "salt and freshly ground black pepper to taste",
            "1 head romaine lettuce, shredded",
            "\u00bd cup croutons",
            "4 slices bacon, cooked and crumbled",
            "4 ounces grated Gouda cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix salad dressing and orange juice in a large serving bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat chicken strips with canola oil, followed by chives, parsley, and tarragon. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook chicken strips on the preheated grill with the lid closed until no longer pink in the centers and juices run clear, 7 to 10 minutes, depending on thickness of strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While chicken cooks, add romaine, croutons, bacon, and Gouda cheese to the bowl with dressing and toss well. Top with hot chicken and serve immediately.\n"
            }
        ],
