    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284869/roasted-garlic-and-horseradish-cream-sauce-for-prime-rib/",
        "name": "Roasted Garlic and Horseradish Cream Sauce for Prime Rib",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Garlic and Horseradish Cream Sauce for Prime Rib"
        },
        "datePublished": "2020-12-25T20:28:53.000Z",
        "description": "A rich, creamy, earthy sauce that pairs perfectly with prime rib, beef tenderloin, smoked brisket, or your favorite steak. Serve warm over beef. The sauce can also be refrigerated and served cool as sandwich spread.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 head garlic",
            "\u00bd teaspoon olive oil",
            "freshly ground black pepper to taste",
            "2 cups heavy cream",
            "\u00bd cup prepared horseradish",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut about 1/2 inch off the top of the garlic head, making sure that the tops of the cloves are open and exposed. Place on a square of aluminum foil, drizzle the top with olive oil, and lightly season with pepper. Wrap the bulb up in the foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the cloves are soft and the tops are brown and caramelized, 30 to 40 minutes. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the cream in a small saucepan and heat over medium heat, stirring often, until just starting to bubble. Reduce heat to low and simmer, stirring occasionally, until reduced by half, 25 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the garlic cloves from the head using a fork and place them in a small bowl. Mash them with the fork until smooth. Add horseradish and salt and stir until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir horseradish-garlic mixture into the cream and continue to simmer until the sauce is thick and creamy and the horseradish flavor has mellowed, 5 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
