    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278540/french-molten-chocolate-cake/",
        "name": "French Molten Chocolate Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "French Molten Chocolate Cake"
        },
        "datePublished": "2009-03-01T03:43:13.000Z",
        "description": "Moelleux au chocolat&hellip; A French classic that literally melts in your mouth! It is also surprisingly easy to make! Serve with vanilla ice cream, if desired.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 5-ounce ramekins",
        "recipeIngredient": [
            "5 tablespoons butter",
            "1 (3.5 ounce) package dark chocolate, chopped",
            "2 each eggs",
            "1 egg yolk",
            "3 tablespoons white sugar",
            "3 tablespoons all-purpose flour",
            "2 teaspoons unsweetened cocoa powder",
            "1 pinch salt",
            "1 tablespoon powdered sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place butter and chocolate in the top of a double boiler over simmering water. Stir frequently, scraping down the sides with a rubber spatula to avoid scorching, until chocolate is melted, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine eggs and egg yolk in a bowl. Add sugar and whisk until mixture is light yellow in color and sugar is dissolved, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour some of the warm chocolate mixture into the egg mixture, whisking quickly to avoid scrambling the eggs. Add remaining chocolate and whisk until combined and smooth. Sift in flour, cocoa powder, and salt; fold in with a spatula until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter four 5-ounce ramekins and dust with cocoa powder. Spoon batter into the prepared ramekins, tapping gently to settle any air bubbles. Place in the refrigerator for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ramekins in a baking dish and bake until edges start to pull away from the sides, 13 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and dust with powdered sugar. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "French"
