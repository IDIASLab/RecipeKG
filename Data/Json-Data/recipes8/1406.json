    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277473/spiced-cranberry-sauce/",
        "name": "Spiced Cranberry Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8868602.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:43:18.000Z",
        "description": "Super easy and a huge hit! I make this spiced cranberry sauce every Thanksgiving and Christmas. Tart and spicy! The spices just add to the overall holiday feel of the dish.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00be cup orange juice",
            "\u00bc cup water",
            "1 cup white sugar",
            "1 cinnamon stick",
            "1 dash ground cloves",
            "1 dash ground nutmeg",
            "1 dash ground allspice",
            "1 (12 ounce) bag fresh cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat orange juice and water in a saucepan over medium heat until warm, about 3 minutes. Add sugar and stir until dissolved. Mix in cinnamon stick, cloves, nutmeg, and allspice. Add cranberries and cook, stirring occasionally, until they pop, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and pour sauce into a bowl. Cover and refrigerate, 8 hours to overnight.\n"
            }
        ],
