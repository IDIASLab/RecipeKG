    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279161/gluten-free-texas-sheet-cake/",
        "name": "Gluten-Free Texas Sheet Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7797712.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:44:54.000Z",
        "description": "A mouth-watering gluten-free version of the classic Texas sheet cake that will have your family and friends wanting more!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "24 pieces",
        "recipeIngredient": [
            "cooking spray",
            "1 cup unsalted butter",
            "1 cup water",
            "\u2153 cup cocoa powder",
            "2 cups white sugar",
            "1 teaspoon vanilla extract",
            "\u00bd cup buttermilk",
            "2 eggs",
            "2 cups gluten-free all-purpose baking flour (such as Cup4Cup\u2122)",
            "2 teaspoons baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup salted butter",
            "\u2153 cup cocoa powder",
            "\u2153 cup milk",
            "3 cups confectioners' sugar, sifted",
            "1 teaspoon vanilla extract",
            "1 cup pecans, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease an 18x13x1-inch baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, water, and cocoa powder in a saucepan over medium heat. Stir together and bring to a boil. Remove from heat and mix in sugar and vanilla extract. Set aside and allow to cool, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together buttermilk and eggs in a small bowl. Combine gluten-free flour, baking soda, and salt in a large bowl, mixing well. Add buttermilk-egg mixture, followed by cocoa mixture. Stir batter well until no lumps remain. Pour into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted comes out clean and the cake is starting to pull from the sides of the pan, 25 to 35 minutes. Allow to cool for 15 minutes before frosting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, cocoa powder, and milk in a saucepan over medium heat to make the frosting. Bring to a boil. Remove from heat once milk is boiling and whisk in confectioners' sugar and vanilla. Pour frosting over the warm cake and spread evenly over the tops and the sides with a spatula. Sprinkle with chopped pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow cake and frosting to cool completely before serving, about 30 minutes.\n"
            }
        ],
