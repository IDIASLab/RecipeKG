    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281687/baked-hasselback-potatoes/",
        "name": "Baked Hasselback Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8729151.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-04-09T15:22:38.000Z",
        "description": "These baked hasselback potatoes don't need a lot of toppings to make them wonderful. Simple, yet fancy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 potatoes",
        "recipeIngredient": [
            "4 medium potatoes",
            "3 tablespoons olive oil",
            "4 cloves garlic, thinly sliced",
            "1 pinch smoked paprika",
            "salt to taste",
            "2 tablespoons grated Parmesan cheese, or more to taste",
            "2 teaspoons chopped fresh chives, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a potato between 2 wooden spoons. Slice potato crosswise at 1/8-inch intervals, cutting until you hit the spoon handles, being careful not to cut all the way through. Repeat with remaining potatoes and transfer to a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine olive oil, garlic, paprika, and salt in a small bowl. Brush mixture over the top and in between the slits of potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until potatoes have fanned out a bit, and are cooked through on the insides but a bit crispy on the outsides, about 45 minutes. Sprinkle 2 tablespoons Parmesan cheese over top and bake until melted, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with additional Parmesan and chives.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Baked Potato Recipes"
        ],
