    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220595/turkey-portobello-pizza/",
        "name": "Turkey Portobello Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1113354.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2018-06-08T14:00:39.000Z",
        "description": "This delicious mushroom pizza is easy and fun. You can substitute the turkey meat to make it vegetarian, too!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 pizzas",
        "recipeIngredient": [
            "4 large portobello mushroom caps, stems removed",
            "\u00bc cup extra-virgin olive oil",
            "3 tablespoons extra-virgin olive oil",
            "1 small red onion, diced",
            "2 cloves garlic, minced",
            "1 pound ground turkey",
            "salt to taste",
            "2 roma (plum) tomatoes, diced",
            "1 (8 ounce) jar basil pesto",
            "6 ounces fresh mozzarella cheese, sliced",
            "2 ounces grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat portobello mushrooms with about 1/4 cup olive oil on both sides; arrange mushrooms gill-side up on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 3 tablespoons olive oil in a large skillet over high heat. Cook and stir onion and garlic in hot oil until onion begins to turn translucent, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir turkey into onion mixture and reduce heat to medium-high; season with salt. Continue cooking and stirring until turkey is no longer pink, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain turkey, reserving about 1 tablespoon grease in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir tomatoes and pesto sauce into the turkey; simmer, stirring occasionally, until the sauce is heated through, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a slice of mozzarella cheese in the center of each mushroom cap and sprinkle evenly with Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide turkey pesto sauce evenly over the mushroom caps.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese has melted, 10 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
