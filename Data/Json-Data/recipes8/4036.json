    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237331/gluten-free-carrot-cake-with-a-kick/",
        "name": "Gluten-Free Carrot Cake with a Kick",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Carrot Cake with a Kick"
        },
        "datePublished": "2020-06-19T03:58:55.000Z",
        "description": "Carrot cake with spice and an after-burner. Tastes great topped with whipped cream or powdered sugar. You can also bake the cake in two 8-inch round pans.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 cups gluten-free all-purpose baking flour",
            "1 tablespoon baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon salt",
            "1 pinch ground cloves",
            "\u00be cup vegetable oil",
            "\u00be cup applesauce",
            "\u00be cup brown sugar",
            "\u00be cup white sugar",
            "\u2153 cup orange juice",
            "4 eggs",
            "1\u2009\u00bd cups grated carrots",
            "\u00bd cup chopped walnuts",
            "2 tablespoons seeded and minced jalapeno pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder, cinnamon, ginger, nutmeg, salt, and cloves together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat oil, applesauce, brown sugar, white sugar, and orange juice together in a separate bowl using an electric mixer until smooth. Beat eggs, 1 at a time, into applesauce mixture, beating until each egg is fully incorporated before adding next egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour mixture into applesauce mixture, alternating with carrots, walnuts, and jalapeno pepper, until batter is just blended; pour into prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the cake comes out clean, about 50 minutes.\n"
            }
        ],
