    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269599/fried-olives/",
        "name": "Fried Olives",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6251887.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-04-04T04:15:03.000Z",
        "description": "Toasted walnuts and bread crumbs give flavor and texture to these fried olives.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "72 olives",
        "recipeIngredient": [
            "1\u2009\u00bd cups California walnuts",
            "1\u2009\u00bd cups fresh, unseasoned bread crumbs",
            "3 large eggs",
            "2 tablespoons water",
            "1 pound pimento-stuffed green olives, drained",
            "1 cup all-purpose flour",
            "1\u2009\u00bd quarts canola oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spread walnuts onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast in the preheated oven until nuts start to turn golden brown and become fragrant, 10 to 15 minutes. Cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine walnuts and bread crumbs in a food processor; process until finely ground but mixture is still light and airy. Spread into a shallow pan and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and water together in a shallow bowl. Spread flour on a shallow plate. Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip olives, a few at a time, into the beaten eggs. Roll in flour, then dip again into the beaten eggs. Let excess egg drip off the olives, then roll in the walnut mixture. Transfer to the lined baking sheet. Repeat until all the olives are coated with the walnut mixture. Refrigerate for at least 2 hours before frying.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 360 degrees F (180 degrees C). Lower 12 olives into the oil and fry until coating is golden, about 45 seconds. Drain briefly on paper towels. Repeat with remaining olives. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Olives"
        ],
