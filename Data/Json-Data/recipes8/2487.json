    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257440/watermelon-chutney/",
        "name": "Watermelon Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4557636.jpg",
            "width": 3629,
            "height": 2722
        },
        "datePublished": "2020-06-20T01:51:00.000Z",
        "description": "Great way of using up fresh watermelon. It is very tasty on sandwiches or with meat.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 jars",
        "recipeIngredient": [
            "\u00bc cup oil",
            "1 red onion, minced",
            "1 red bell pepper, minced",
            "1 clove garlic, thinly sliced",
            "1 teaspoon minced fresh ginger, or to taste",
            "2 pounds watermelon, coarsely chopped",
            "1 pound tomatoes, coarsely chopped",
            "1 tablespoon lime juice, or to taste",
            "1 tablespoon curry powder",
            "1 pinch salt, or to taste",
            "1 pinch white sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a stockpot over medium heat; cook and stir red onion, red bell pepper, garlic, and ginger until tender, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix watermelon, tomatoes, lime juice, curry powder, salt, and sugar into onion mixture; bring to a boil. Reduce heat to medium-low and simmer, uncovered, until watermelon and tomatoes are slightly broken down, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill 4 clean jars with chutney, seal, and turn upside down to mix for 5 minutes. Turn right-side up and refrigerate.\n"
            }
        ],
