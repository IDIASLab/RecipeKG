    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273975/bourbon-brownies/",
        "name": "Bourbon Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6747771.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T04:37:51.000Z",
        "description": "Thick, moist brownies with a whiff of Kentucky bourbon. We call them bourbrownies!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 8-inch pan",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 stick butter, melted",
            "6 tablespoons unsweetened cocoa powder",
            "2 tablespoons vegetable oil",
            "1 teaspoon vanilla extract",
            "2 eggs, beaten",
            "\u00bc cup bourbon",
            "6 tablespoons all-purpose flour",
            "\u00bd teaspoon baking powder",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease an 8-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar, melted butter, cocoa powder, oil, and vanilla extract together until smooth. Mix in eggs, then add bourbon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, and salt in a separate bowl. Add to the cocoa mixture. Smooth batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is dry and edges have started to pull away from the sides of the pan, about 25 minutes. Let cool completely before slicing.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes"
        ],
