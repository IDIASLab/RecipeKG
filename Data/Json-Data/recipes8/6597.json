    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274862/grilled-halloumi-kebabs/",
        "name": "Grilled Halloumi Kebabs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6858563.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2019-07-23T20:08:12.000Z",
        "description": "Think Caprese but warm and with a grillable cheese like halloumi instead of mozzarella.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "6 kebabs",
        "recipeIngredient": [
            "\u00bd cup olive oil",
            "6 fresh basil leaves",
            "8 ounces halloumi cheese, cut into cubes",
            "\u00bd cup balsamic vinegar",
            "2 tablespoons honey",
            "20 cherry tomatoes",
            "metal skewers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine oil and basil leaves in a food processor; pulse to combine. Place halloumi cheese in a flat bowl. Pour basil-infused oil over the cheese. Toss to coat and let marinate 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vinegar and honey in a small saucepan over medium heat. Simmer until sauce begins to thicken, about 7 minutes. Turn heat off and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill to medium-high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cherry tomatoes to the cheese and toss until coated. Thread cheese and tomatoes on skewers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on the preheated grill. Grill for 3 minutes, flip and grill for 3 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer skewers to a serving platter. Drizzle with vinegar reduction and serve immediately. Garnish with additional basil if desired.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegetarian"
        ],
