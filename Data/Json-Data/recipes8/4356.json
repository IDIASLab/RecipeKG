        "mainEntityOfPage": "https://www.allrecipes.com/recipe/206833/easy-corn-and-green-onion-salad/",
        "name": "Easy Corn and Green Onion Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F349379.jpg",
