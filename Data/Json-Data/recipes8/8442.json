    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283520/roasted-tomato-salsa/",
        "name": "Roasted Tomato Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Tomato Salsa"
        },
        "datePublished": "2020-11-18T08:40:57.000Z",
        "description": "Tomatoes, onions, and jalapenos get added flavor when they're roasted first before being combined in this easy homemade salsa recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 large Roma tomatoes, halved lengthwise",
            "2 medium jalapeno peppers, halved lengthwise",
            "\u00bd medium white onion, coarsely chopped",
            "1 tablespoon olive oil, or to taste",
            "salt and ground black pepper to taste",
            "1 medium lime, juiced",
            "1 tablespoon chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 10 inches from the heat source and preheat the oven's broiler for 5 minutes. Line a baking sheet with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread tomatoes and jalapenos, skin-side up, on the prepared baking sheet. Add onion. Drizzle with a little olive oil and season lightly with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook under the broiler for 20 minutes. Remove from the oven and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove stems from tomatoes and jalapenos. Transfer to a blender with onions and all roasting juices. Add lime juice and more salt, to taste. Pulse to blend.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to a serving bowl and mix in cilantro.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Salsa Recipes",
            "Tomato Salsa Recipes"
        ],
