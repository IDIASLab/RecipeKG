    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277341/christmas-biscotti/",
        "name": "Christmas Biscotti",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Christmas Biscotti"
        },
        "datePublished": "2020-06-19T04:43:13.000Z",
        "description": "Fragrant orange oil gives a special touch to this biscotti recipe. Great with a cup of coffee on a chilly Christmas morning!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "2 dozen biscotti",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u2154 cup white sugar",
            "\u00bd cup butter",
            "2 large eggs",
            "\u00bd teaspoon orange oil",
            "\u00bd teaspoon vanilla extract",
            "\u00bd cup white chocolate chips",
            "\u00bd cup dried cranberries",
            "\u00bd cup coarsely chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease 2 cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, baking soda, and salt in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend sugar and butter together using an electric mixer in another bowl. Beat in eggs, orange oil, and vanilla extract until smooth. Sift in flour mixture and beat until completely incorporated. Fold in white chocolate chips, cranberries, and pecans. Divide dough in half and form into rectangles on the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Remove from the oven and let cool enough to handle, about 5 minutes; slice into biscotti as desired. Return to the oven and bake until toasted, about 15 minutes per side.\n"
            }
        ],
