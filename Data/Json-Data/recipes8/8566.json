    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282333/potato-soup-for-busy-people/",
        "name": "Potato Soup for Busy People",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Potato Soup for Busy People"
        },
        "datePublished": "2004-03-20T18:40:22.000Z",
        "description": "I made this potato soup with items I had on hand at home one day after work. It cooks in just 35 minutes.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 slices bacon",
            "1 cup chopped carrot",
            "\u00bd cup chopped celery",
            "\u00bd medium onion, chopped",
            "3\u2009\u00bd cups chicken broth",
            "2 teaspoons garlic powder",
            "1 teaspoon salt",
            "1 bay leaf",
            "freshly ground black pepper to taste",
            "1 (16 ounce) package frozen cubed hash brown potatoes, thawed",
            "1 cup evaporated milk",
            "2 tablespoons grated Asiago cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Transfer bacon to a paper towel-lined plate and crumble when cool enough to handle. Discard all but 2 teaspoons bacon drippings from the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add carrot, celery, and onion to the bacon drippings. Saute over medium heat for 2 minutes. Add 1 cup chicken broth, garlic powder, salt, bay leaf, and pepper. Reduce heat to low and cook for 5 minutes. Add potatoes and remaining chicken broth; cook for 10 minutes. Add evaporated milk, increase heat to a simmer, and cook for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve with crumbled bacon and grated Asiago cheese.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Potato Soup Recipes"
        ],
