    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275353/gluten-free-blueberry-crisp/",
        "name": "Gluten-Free Blueberry Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6929036.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T04:40:18.000Z",
        "description": "This is a fast-to-fix dessert and is suitable for those who are following a gluten-free diet. Feel free to swap around the fruit flavor and cake mix flavors. For instance, cherry and yellow cake mix with pecans.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) can blueberry pie filling",
            "8\u2009\u00bc ounces gluten-free yellow cake mix",
            "\u00bd cup gluten-free oats (such as Bob's Red Mill\u00ae)",
            "\u00bd cup sliced almonds",
            "\u00bc cup melted butter",
            "1 tablespoon white sugar",
            "\u00bc teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour blueberry filling in a 9x9-inch baking dish and smooth to even out in the dish. Sprinkle dry cake mix over the filling, smoothing evenly to cover the top. Sprinkle with oats and almonds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour melted butter over the top, covering evenly. Mix sugar and cinnamon in a small bowl; sprinkle evenly on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until filling is bubbly and top is brown, 40 to 50 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cool for 30 minutes. Serve warm or cooled.\n"
            }
        ],
