        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14361/marilyns-special-salad/",
        "name": "Marilyn's Special Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F993678.jpg",
