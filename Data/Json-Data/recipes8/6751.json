    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281489/grilled-chicken-tenders/",
        "name": "Grilled Chicken Tenders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8579401.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-07-09T17:21:28.000Z",
        "description": "I love this marinade, it permeates the grilled chicken tenders and has a great flavor. I usually grill extra for leftovers... perfect for salads and sandwiches.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT3H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup water",
            "\u00bd cup soy sauce",
            "2 tablespoons olive oil",
            "2 tablespoons lemon juice",
            "1 tablespoon brown sugar",
            "3 cloves garlic, crushed",
            "1 (16 ounce) package boneless, skinless chicken tenders"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk water, soy sauce, olive oil, lemon juice, brown sugar, and garlic together in a large glass or ceramic bowl. Add chicken tenders and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator for at least 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken on the grill and reduce heat to low. Cook for 1 minute, then turn and grill until no longer pink in the center and the juices run clear, 4 to 6 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken"
        ],
