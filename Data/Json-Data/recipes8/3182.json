    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264311/vegan-yuca-tacos/",
        "name": "Vegan Yuca Tacos",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Yuca Tacos"
        },
        "datePublished": "2020-06-23T21:52:36.000Z",
        "description": "A vegan way to enjoy tacos in the summertime. A twist on soft tacos without losing the classic flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "10 soft tacos",
        "recipeIngredient": [
            "1\u2009\u00bd pounds frozen yuca",
            "1 cup frozen corn",
            "1 medium green bell pepper, diced",
            "1 medium onion, diced",
            "salt to taste",
            "1 (16 ounce) can black beans, rinsed and drained",
            "3 tablespoons olive oil, divided, or as needed",
            "10 (6 inch) corn tortillas",
            "10 toothpicks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place yuca into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, 20 to 25 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine corn, bell pepper, and onion in an aluminum pan. Add 1 tablespoon olive oil, or enough to coat the vegetables and the bottom of the pan. Cook on the grill, stirring every 5 minutes, until pepper is tender and onion is translucent, 10 to 20 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash yuca and black beans together in a large bowl until smooth and well blended, adding about 1 tablespoon olive oil as needed. Add the grilled vegetables; mix well, kneading by hand if necessary.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm tortillas in the microwave, 30 seconds to 1 minute. Add equal amounts of the yuca mixture to the tortillas. Fold each taco in half over the filling and secure with a toothpick. Paint the top of each tortilla with remaining olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the tacos on a grill topper. Grill for about 5 minutes. Remove from heat and remove the toothpicks.\n"
            }
        ],
