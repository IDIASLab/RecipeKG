    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281285/grilled-lamb-chops/",
        "name": "Grilled Lamb Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F813727.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-03-31T01:29:33.000Z",
        "description": "After marinating these lamb chops, grill to medium-rare or medium and enjoy the best meal of your life!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT8H25M",
        "recipeYield": "10 lamb chops",
        "recipeIngredient": [
            "\u00be cup soy sauce",
            "\u00bc cup olive oil",
            "8 cloves garlic, minced",
            "4 teaspoons chopped fresh thyme",
            "2 teaspoons minced fresh ginger root",
            "2 teaspoons ground black pepper",
            "4 bay leaves, crumbled",
            "10 lamb chops"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix soy sauce, oil, garlic, thyme, ginger, pepper, and bay leaves together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lamb chops in a large zip-top bag or resealable container. Cover with the marinade. Refrigerate for 8 to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate. Wipe off marinade from the lamb chops.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook lamb chops, covered, on the preheated grill until browned, 3 to 4 minutes. Flip using grilling tongs; do not use a fork as it will let out all of the juices. Close the lid and continue to grill to medium doneness, about 5 minutes more. An instant-read thermometer inserted into the center should read at least 130 degrees F (54 degrees C). Be careful not to overcook.\n"
            }
        ],
