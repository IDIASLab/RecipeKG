    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16008/cracker-pie-ii/",
        "name": "Cracker Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8293952.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-10-15T19:42:49.000Z",
        "description": "This is a light meringue pie made with buttery crackers and walnuts. Delicious with whipped cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "3 egg whites",
            "1 cup white sugar",
            "2 teaspoons baking powder",
            "16 buttery round crackers, crushed",
            "1 teaspoon vanilla extract",
            "1 cup chopped black walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 320 degrees F (160 degrees C). Lightly grease a 9-inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal bowl, beat egg whites until soft mounds form. Gradually add sugar and baking powder while continuing to beat. Whip whites until stiff peaks form. Fold in crushed crackers, vanilla, and walnuts. Pour mixture into greased pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 20 to 25 minutes. Cool before serving. Garnish with whipped cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies"
        ],
