    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285147/summer-grilled-corn-salad/",
        "name": "Summer Grilled Corn Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9331800.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2021-08-02T22:14:38.000Z",
        "description": "This easy to make, summer grilled corn salad is the ultimate summer salad and is totally addictive. It is made with fresh and healthy ingredients to get the best results. Enjoy as a side with any main dish. Try it with Mexican pulled chicken, or oven-grilled chicken.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 ears fresh corn",
            "1 cup quartered cherry tomatoes",
            "1 small red onion, diced",
            "1 medium jalapeno pepper, diced",
            "1 medium lime, juiced",
            "1 tablespoon extra-virgin olive oil",
            "1 teaspoon ground cumin",
            "1 teaspoon paprika",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the corn on the preheated grill, turning often to ensure even cooking, until browned, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a sharp knife, shuck the corn and toss the cobs. Place the corn in a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer tomatoes, onion, jalapeno, lime juice, oil, cumin, paprika, and salt to the bowl; mix to combine.\n"
            }
        ],
