    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278923/grammys-blueberry-cupcakes/",
        "name": "Grammy's Blueberry Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grammy's Blueberry Cupcakes"
        },
        "datePublished": "2020-06-19T04:44:41.000Z",
        "description": "This is my grammy's recipe for blueberry cupcakes. Serve with a warm vanilla sauce.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon salt",
            "6 tablespoons butter, softened",
            "1\u2009\u00bc cups white sugar",
            "2 large eggs",
            "\u00bd cup milk",
            "1 pint fresh blueberries",
            "2 teaspoons white sugar",
            "1 pinch ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar and butter in a large bowl with an electric mixer. Add eggs, one at a time, beating well after each addition. Add flour mixture in 2 batches, alternating with milk, beating batter briefly after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crush 1/2 of the blueberries with a fork in a small bowl; mix into the batter. Gently fold remaining blueberries into the batter and fill the prepared muffin cups 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar and cinnamon for topping together in a small bowl; sprinkle over cupcakes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, about 30 minutes. Cool in the tin for 30 minutes. Transfer to a wire rack to cool completely.\n"
            }
        ],
