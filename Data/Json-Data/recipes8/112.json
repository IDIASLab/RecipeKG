        "name": "Grandma's Lace Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fcf-images.us-east-1.prod.boltdns.net%2Fv1%2Fstatic%2F1033249144001%2F0bc62e73-ba6f-4f6e-a25d-2a4f3117393b%2F46368ca9-d73d-4b03-a151-e21f2688397d%2F1280x720%2Fmatch%2Fimage.jpg",
            "width": null,
