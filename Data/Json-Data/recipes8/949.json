    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276180/delicious-chocolate-chip-cookie-bites/",
        "name": "Delicious Chocolate Chip Cookie Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9426218.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:41:18.000Z",
        "description": "The perfect bite-sized chocolate chip cookie recipe--crispy on the outside and chewy on the inside. Delicious fresh from the oven.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "40 cookie bites",
        "recipeIngredient": [
            "1 cup brown sugar",
            "\u00bd cup white sugar",
            "\u00bd cup butter",
            "2 egg whites",
            "\u00bd teaspoon vanilla extract",
            "1 teaspoon white vinegar",
            "1\u2009\u00be cups all-purpose flour",
            "\u00bd teaspoon fine sea salt",
            "\u00bd teaspoon baking soda",
            "\u00be cup mini chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease cookie sheets or line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, white sugar, butter, egg whites, and vanilla extract together in a large bowl using an electric mixer. Mix in vinegar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, sea salt, and baking soda in a separate bowl. Mix into butter mixture until just combined. Fold in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1-inch balls. Place onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until just barely browned on the edges, 8 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Chip Cookie Recipes"
        ],
