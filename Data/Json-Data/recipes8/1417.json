    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277507/eggnog-cutout-cookies/",
        "name": "Eggnog Cutout Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7358472.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:43:19.000Z",
        "description": "Speckled with nutmeg, these thick, soft, and tasty cookies are filled with the flavor of eggnog--perfect for holiday cookie plates!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon ground nutmeg",
            "\u00bc teaspoon ground cinnamon",
            "2 cups white sugar",
            "\u00be cup butter, softened",
            "2 eggs",
            "\u00bc cup eggnog",
            "1 teaspoon vanilla extract",
            "1 teaspoon rum extract",
            "\u00bc cup eggnog",
            "2 tablespoons light corn syrup",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon rum extract",
            "\u00bd teaspoon ground nutmeg",
            "3 cups powdered sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together flour, baking powder, nutmeg, and cinnamon in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together sugar and butter with an electric mixer in a second bowl. Mix in eggs, eggnog, vanilla extract, and rum extract. Add flour mixture and mix by hand until dough comes together. Cover the bowl with plastic wrap and chill for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out dough to 1/2\" thickness on a lightly floured surface. Cut into shapes with cookie cutters and place 1-inch apart on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in batches in the preheated oven until cookies are lightly browned on the bottom, 8 to 10 minutes. Carefully transfer to a wire rack and allow to cool, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, whisk together eggnog, corn syrup, vanilla extract, rum extract, and nutmeg for the icing in a bowl. Mix in as much powdered sugar 1 cup at a time, until icing is smooth and desired consistency is reached.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip cookie tops in icing and place on waxed paper to dry.\n"
            }
        ],
