    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274470/chocolate-freezer-cookies/",
        "name": "Chocolate Freezer Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Freezer Cookies"
        },
        "datePublished": "2020-06-19T04:39:10.000Z",
        "description": "Thin, chewy, chocolate cookies, delicious straight out of the freezer. Great by themselves and very good for ice cream sandwiches.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT4H35M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups sifted all-purpose flour",
            "2\u2009\u00bd teaspoons baking powder",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups white sugar",
            "\u2154 cup butter-flavored shortening",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "1 cup unsweetened cocoa powder",
            "1 (12 ounce) package miniature semisweet chocolate chips",
            "4 tablespoons milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and shortening together using an electric mixer in a mixing bowl. Add eggs 1 at a time while continuing to beat until combined. Beat in vanilla extract followed by cocoa; beat in chocolate chips. Beat in flour mixture and milk, alternating, until combined. Chill dough in the refrigerator for at least 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll cookie dough as thin as chocolate chips will allow. Cut out cookies using a biscuit cutter and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set, 3 to 5 minutes. Remove from the oven and let cool completely, 15 to 20 minutes. Store in an airtight container in the freezer.\n"
            }
        ],
