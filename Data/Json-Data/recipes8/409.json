    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274700/holiday-chocolate-chip-pumpkin-bread/",
        "name": "Holiday Chocolate Chip Pumpkin Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2871008.jpg",
            "width": 2016,
            "height": 1512
        },
        "datePublished": "2020-06-19T04:39:20.000Z",
        "description": "This chocolate chip pumpkin bread is more of a cake than a bread but a sure hit with the holiday gatherings!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "1 teaspoon butter, or as needed",
            "3\u2009\u00bd cups unbleached all-purpose flour",
            "2 cups white sugar",
            "2 teaspoons baking soda",
            "2 teaspoons ground cinnamon",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon ground nutmeg",
            "\u00bc teaspoon ground ginger",
            "1 (29 ounce) can pumpkin puree",
            "1 cup unsalted butter, melted",
            "4 eggs",
            "1\u2009\u00bd tablespoons vanilla extract",
            "1\u2009\u00bd cups dark chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Butter two 7x3-inch loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, baking soda, cinnamon, salt, nutmeg, and ginger together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pumpkin puree, melted butter, eggs, and vanilla extract together in a separate bowl. Stir into flour mixture; mix well to combine. Fold in chocolate chips. Divide batter between the prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the centers of the loaves comes out clean, about 1 hour and 20 minutes.\n"
            }
        ],
