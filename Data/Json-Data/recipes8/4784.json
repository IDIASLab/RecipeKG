    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16337/portabella-mushroom-dressing/",
        "name": "Portabella Mushroom Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1755167.jpg",
            "width": 3356,
            "height": 2269
        },
        "datePublished": "1998-12-04T09:34:20.000Z",
        "description": "I love the added flavor the Portabella and Shiitake Mushrooms give this dressing. It's a favorite!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 stalks celery",
            "1 onion, chopped",
            "2 large portobello mushrooms, sliced",
            "3 fresh shiitake mushrooms, stemmed and sliced",
            "8 crimini mushrooms, sliced",
            "2 cloves garlic, minced",
            "1 (12 ounce) package dry bread stuffing mix with seasoning packet reserved",
            "3 (14.5 ounce) cans chicken broth"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a skillet, cook and stir celery, onion, mushrooms, and garlic for 3 minutes. Stir in one can of chicken broth and cook until vegetables are tender. Combine mushroom mixture with bread crumb stuffing.  Pour packet of seasoning over dressing mixture and toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 2 quart saucepan, heat remaining chicken broth until hot.  Pour broth over mixture.  Stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour stuffing into a 2 quart casserole dish and bake for 30 to 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Stuffing and Dressing Recipes",
            "Mushroom Stuffing and Dressing"
        ],
