    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260123/gluten-free-and-lactose-free-pancakes/",
        "name": "Gluten-Free and Lactose-Free Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4556781.jpg",
            "width": 1500,
            "height": 1000
        },
        "datePublished": "2020-06-19T04:24:27.000Z",
        "description": "These gluten-free and lactose-free pancakes are light, fluffy, and delicious. Perfect for breakfast or dessert.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H4M",
        "totalTime": "P0DT0H34M",
        "recipeYield": "6 pancakes",
        "recipeIngredient": [
            "1 cup self-rising gluten-free flour",
            "2 tablespoons white sugar",
            "1 pinch ground cinnamon",
            "3 large eggs",
            "1 teaspoon vanilla extract",
            "1 cup lactose-free milk",
            "1 tablespoon coconut oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour into a large bowl; mix in sugar and ground cinnamon. Stir in eggs and vanilla extract until well combined. Stir in milk gradually until batter is well combined and has the right consistency, not too runny and not too thick. Check consistency by filling a spoon with batter and tipping it to the side. Count the seconds it takes to fall back into the bowl; batter is ready when it takes about 4 seconds and leaves a small coating on the spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rest mixture for 15 to 20 minutes to even out and thicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 1 1/2 teaspoons coconut oil in a skillet over medium heat. Remove from heat and pour 1/6 of the batter into the skillet, moving the skillet in circular motion so batter spreads out in an even circle. Return skillet to the heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook pancake until bubbles start to form in the batter and the edges look dry, 2 to 3 minutes. Flip pancake using a spatula and continue to cook on the other side until pancake is lightly browned, about 2 minutes. Repeat with remaining batter.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
