    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281884/valencia-orange-ricotta-pancakes/",
        "name": "Valencia Orange Ricotta Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Valencia Orange Ricotta Pancakes"
        },
        "datePublished": "2008-07-15T03:29:12.000Z",
        "description": "Delicious pancakes that are light and fluffy with a refreshing essence of Valencia orange.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 large pancakes",
        "recipeIngredient": [
            "1 medium Valencia orange",
            "1 (15 ounce) container whole-milk ricotta cheese",
            "4 large eggs",
            "\u00bd cup all-purpose flour",
            "2 teaspoons white sugar",
            "\u00bd teaspoon baking powder",
            "cooking spray",
            "\u00bc cup maple syrup, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grate 1 teaspoon zest from orange. Remove and discard the remaining peel, and cut orange into wheels. Set zest and orange wheels aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk ricotta cheese, eggs, and orange zest together in a medium bowl. Add flour, sugar, and baking powder; whisk to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a nonstick skillet with cooking spray and heat over medium heat for 1 minute. Drop about 1/4 of the batter into the hot skillet and cook until bubbly, 4 to 5 minutes. Flip and cook until golden brown, another 4 to 5 minutes. Repeat with remaining batter to make 3 more pancakes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve with orange wheels and maple syrup.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
