    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283233/homemade-chicken-stew/",
        "name": "Homemade Chicken Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8985732.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2021-01-29T02:03:14.000Z",
        "description": "A hearty, creamy chicken stew, chock-full of veggies and tender bites of chicken!",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 medium yellow onion, diced",
            "4 medium carrots, peeled and diced",
            "3 ribs celery, diced",
            "2 tablespoons minced garlic",
            "salt and ground black pepper to taste",
            "2 tablespoons salted butter",
            "\u00bc cup all-purpose flour",
            "2 cups chicken broth",
            "2 cups milk",
            "1 teaspoon ground thyme",
            "1 teaspoon salt, or more to taste",
            "\u00bd teaspoon ground black pepper, or more to taste",
            "\u00bc teaspoon ground turmeric",
            "1 cup frozen peas",
            "\u00be cup frozen corn",
            "2 cups diced cooked chicken"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large soup pot over medium to medium-high heat. Saute onion, carrots, and celery until they begin to become tender and caramelize, about 10 minutes. Add garlic and saute for 1 minute. Season vegetables with a generous sprinkling of salt and pepper while cooking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in butter to melt; stir flour into the veggies and butter until evenly distributed and veggies are coated. Pour in broth and milk, stirring until well combined. Turn the heat up and bring the mixture to a simmer. Add thyme, salt, pepper, and turmeric. Stir in peas and corn. Stir in chicken to combine and allow to simmer, 10 to 15 minutes. Taste and adjust seasonings before serving.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Chicken"
        ],
