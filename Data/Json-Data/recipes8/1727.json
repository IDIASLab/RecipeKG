    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278359/grandmas-pumpkin-oatmeal-cookies/",
        "name": "Grandma's Pumpkin Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1052001.jpg",
            "width": 4416,
            "height": 3312
        },
        "datePublished": "2020-06-19T04:44:14.000Z",
        "description": "This is a recipe I adapted from my grandmother. It is my favorite pumpkin cookie recipe. Plus it makes enough for a crowd!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "3 cups white sugar",
            "2 sticks unsalted butter, at room temperature ",
            "2 cups pumpkin puree",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "4 cups all-purpose flour",
            "2 cups quick-cooking oats",
            "2 teaspoons baking powder",
            "2 teaspoons baking soda",
            "2 teaspoons ground cinnamon",
            "1 teaspoon salt",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar and butter in a large mixing bowl. Beat with an electric mixer until creamy. Add pumpkin, eggs, and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, oats, baking powder, baking soda, cinnamon, and salt in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add dry ingredients to the pumpkin mixture; mix until evenly incorporated. Fold in chocolate chips. Drop scoopfuls of cookie dough onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are firm and bottoms are golden brown, about 12 minutes. Repeat with remaining cooking dough.\n"
            }
        ],
