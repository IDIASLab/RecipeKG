    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281456/honey-glazed-pork-chops/",
        "name": "Honey-Glazed Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Honey-Glazed Pork Chops"
        },
        "datePublished": "2013-02-20T03:38:53.000Z",
        "description": "I tried this honey-glazed pork chop recipe and my family loved it. It was a nice change from fried and regular baked chops. It's a new family favorite.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 pork chops",
        "recipeIngredient": [
            "1  serving nonstick cooking spray",
            "4 bone-in pork loin chops",
            "1 pinch salt and ground black pepper to taste",
            "2 tablespoons packed brown sugar",
            "2 tablespoons honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a baking dish with cooking spray. Season pork chops with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat a skillet with cooking spray and heat over medium heat. Add chops to the hot skillet and cook until browned, about 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the chops are browning, combine brown sugar and honey in a microwave-safe bowl. Microwave on high until brown sugar has softened or melted, about 20 seconds. Stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer browned chops to the prepared baking dish and spoon 1/2 of the honey glaze evenly over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven for 15 minutes. Remove from the oven, turn over, and spread the remaining honey glaze over top. Bake until no longer pink in the center, 15 to 20 minutes more. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C).\n"
            }
        ],
