    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278831/italian-cookies-with-icing/",
        "name": "Italian Cookies with Icing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2775568.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2020-06-19T04:44:34.000Z",
        "description": "These Italian sugar cookies feature almond extract in both the cookies and the icing, but you can switch it up and use either vanilla or anise extract, if preferred.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "60 cookies",
        "recipeIngredient": [
            "6 cups all-purpose flour",
            "2 tablespoons baking powder",
            "1 cup unsalted butter, softened",
            "1 cup white sugar",
            "6 large eggs",
            "4 teaspoons almond extract",
            "1\u2009\u00bd cups confectioners' sugar",
            "1 teaspoon almond extract, or to taste",
            "\u00bc cup milk",
            "2 tablespoons nonpareils, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour and baking powder together into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar together in a large bowl with an electric mixer until creamy. Add eggs, 1 at a time, beating well after each addition. Mix in almond extract. Add flour mixture, 1 cup at a time; mix on low speed until incorporated. Roll batter into teaspoon-sized balls and place on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in batches in the preheated oven until edges are golden, 8 to 10 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cookies are baking, beat confectioners' sugar, milk, and almond extract for icing in a bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set cooled cookies close together on a cookie sheet. Frost with icing and sprinkle with nonpareils.\n"
            }
        ],
