    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278841/apple-flautas/",
        "name": "Apple Flautas",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Flautas"
        },
        "datePublished": "2020-06-19T04:44:35.000Z",
        "description": "Cinnamon-apple filling deep-fried in a flour tortilla and drizzled with confectioners' glaze. This easy apple flauta treat will become a favorite!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 flautas",
        "recipeIngredient": [
            "2 Granny Smith apples - peeled, cored, and chopped",
            "\u00bd cup white sugar",
            "1 tablespoon ground cinnamon",
            "6 (8 inch) flour tortillas",
            "canola oil for frying",
            "\u00bd cup confectioners' sugar",
            "1 tablespoon milk, or more as needed",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir apples, sugar, and cinnamon together in a small saucepan over medium heat until apples are tender, 5 to 7 minutes. Spoon 2 or 3 heaping tablespoons of mixture into each tortilla; roll tortillas into cylinders that are 1 inch in diameter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm 1/2 inch of oil in a frying pan over medium heat. Place 2 flautas carefully into the hot oil with metal tongs and fry until light golden brown on all sides, 3 to 4 minutes. Drain on paper towels. Repeat with remaining flautas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix confectioners' sugar, milk, and vanilla extract together in a small bowl until smooth and consistency is slightly thick, but pourable. Drizzle over flautas.\n"
            }
        ],
