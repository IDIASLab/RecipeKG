        "name": "Slow Cooker Pork Tenderloin and Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F05%2F04%2F276778_SlowCookerPorkTenderloinandPotatoes1_DanielleK-2000.jpg",
            "width": 2000,
