    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275835/the-best-sweet-potato-casserole/",
        "name": "The Best Sweet Potato Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6029375.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-07-08T09:50:06.000Z",
        "description": "This is a sweet potato casserole, Southern-style, and is sweet enough to be considered dessert. It's delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 casserole",
        "recipeIngredient": [
            "4 medium sweet potatoes, peeled and cubed",
            "1 cup white sugar",
            "\u00bd cup sweetened condensed milk",
            "\u2153 cup evaporated milk",
            "\u00be stick butter, melted",
            "2 eggs",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bd teaspoon salt",
            "1 cup chopped pecans",
            "\u2154 cup brown sugar",
            "1 stick butter, melted",
            "\u2153 cup all-purpose flour",
            "\u00bc cup sweetened condensed milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sweet potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes. Drain and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix white sugar, sweetened condensed milk, evaporated milk, melted butter, eggs, vanilla extract, and salt together ina bowl using an electric mixer until smooth. Spread into the bottom of a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pecans, brown sugar, melted butter, flour, and sweetened condensed milk in a bowl and crumble topping over sweet potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 25 minutes.\n"
            }
        ],
