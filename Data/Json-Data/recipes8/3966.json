    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284792/vegan-and-gluten-free-chocolate-chip-cookies/",
        "name": "Vegan and Gluten-Free Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8312050.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-07-08T23:49:42.000Z",
        "description": "Traditional chocolate chip cookies that are free of gluten, dairy, nuts, and seeds. We know you will love them as much as we do.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "2 cups gluten-free oat flour",
            "2 tablespoons coconut flour",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon baking soda",
            "\u00bd cup palm shortening",
            "2 tablespoons palm shortening",
            "\u00bd cup coconut sugar",
            "2 large eggs",
            "1 tablespoon vanilla extract",
            "1 cup vegan chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix oat flour, coconut flour, salt, and baking soda together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream 1/2 cup plus 2 tablespoons shortening and coconut sugar in a bowl with an electric mixer until light and fluffy. Add eggs, one at a time, beating well and scraping down the sides of the bowl after each addition. Mix in vanilla. Slowly mix in dry ingredients. Stir in chocolate chips. Place dough in the refrigerator for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll tablespoonfuls of dough into balls. Place on an ungreased cookie sheet and flatten with wet fingers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 14 minutes; cookies will be tender when they come out of the oven. Let rest on the cookie sheet for 10 minutes, then transfer to a wire rack to finish cooling completely.\n"
            }
        ],
