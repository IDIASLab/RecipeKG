    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285395/apple-butter-snickerdoodles/",
        "name": "Apple Butter Snickerdoodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9379398.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2021-08-31T08:13:14.000Z",
        "description": "These snickerdoodle cookies have a hint of apple butter for a different take on the classic favorite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "46 cookies",
        "recipeIngredient": [
            "2\u2009\u00be cups all-purpose flour",
            "\u00bd teaspoon cream of tartar",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "1\u2009\u00be cups white sugar, divided",
            "\u00bd cup unsalted butter, softened",
            "1 large egg, at room temperature",
            "\u00bd teaspoon vanilla extract",
            "\u00be cup apple butter",
            "2 tablespoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together flour, cream of tartar, baking soda, salt, 1/4 teaspoon cinnamon, and nutmeg in a medium bowl until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together butter and 1 1/2 cups sugar in a large bowl until thoroughly combined. Stir in egg and vanilla until incorporated. Add apple butter and mix until combined. Pour in 1/2 of the dry ingredients and mix until just combined. Add remaining dry ingredients and mix just until no dry clumps of flour remain. Cover bowl and refrigerate for at least 2 hours, or up to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together remaining 1/4 cup sugar and 2 tablespoons cinnamon in a shallow bowl or pie pan. Drop tablespoon-sized pieces of dough into the cinnamon-sugar mixture and roll into balls, coating well. Place 1 inch apart on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are just set, about 10 minutes. Allow cookies to cool on baking sheets for 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Snickerdoodle Recipes"
