    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277247/lemon-cream-pie-bars/",
        "name": "Lemon Cream Pie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F370924.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:43:07.000Z",
        "description": "Smooth and luscious, these bars are more like little bites of a lemon cream pie than anything else!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "24 bars",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00be cup canola oil",
            "\u2154 cup confectioners' sugar",
            "1 (14 ounce) can sweetened condensed milk",
            "3 eggs",
            "\u00bd cup lemon juice",
            "\u2153 cup white sugar",
            "1 lemon, zested",
            "\u00bc teaspoon baking powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, oil, and sugar in a bowl. Pat into a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden around the edges, about 20 minutes. Cool on a wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, beat sweetened condensed milk, eggs, lemon juice, sugar, lemon zest, and baking powder together until just blended. Pour over the cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake until the filling is set, 13 to 17 minutes. Cool on a wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for at least 2 hours before cutting into 24 bars.\n"
            }
        ],
