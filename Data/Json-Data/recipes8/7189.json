    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281785/meatless-baked-ziti/",
        "name": "Meatless Baked Ziti",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Meatless Baked Ziti"
        },
        "datePublished": "2009-09-17T20:49:39.000Z",
        "description": "This meatless baked ziti can feed a lot of people, and is cheap and easy to make ... wonderful comfort food for the fall and winter.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (16 ounce) package ziti pasta",
            "1 pound shredded part-skim mozzarella cheese",
            "1 (15 ounce) container whole-milk ricotta cheese",
            "1 cup grated Parmesan cheese, divided",
            "\u00bd cup grated Romano cheese",
            "\u00bd medium yellow onion, chopped",
            "\u00bc cup chopped fresh parsley",
            "1 large egg",
            "2 teaspoons minced garlic",
            "1 teaspoon dried basil",
            "1 teaspoon dried oregano",
            "\u00bc teaspoon ground thyme",
            "salt and freshly ground black pepper to taste",
            "4 cups marinara sauce, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add ziti and cook, stirring occasionally, until tender yet firm to the bite, about 8 minutes. Rinse and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mozzarella cheese, ricotta cheese, 1/2 cup Parmesan cheese, Romano cheese, onion, parsley, egg, garlic, basil, oregano, thyme, salt, and pepper together in a large bowl. Stir in drained pasta and 2 cups of marinara sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1 cup marinara into the bottom of a 9x13-inch baking dish and spread to coat. Pour pasta and cheese mixture into the pan, top with remaining marinara, and sprinkle with remaining Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, covered, in the preheated oven until heated through, about 20 minutes. Remove cover and continue to bake until bubbly and cheese is browned, about 10 minutes more. Remove from the oven and let sit for 10 minutes before serving.\n"
            }
        ],
