    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274965/grilled-spiced-corn/",
        "name": "Grilled Spiced Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7855347.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:39:37.000Z",
        "description": "Take your corn side dish from ordinary to extraordinary! Sweet corn is grilled and then rubbed with lemon wedges that have been dipped in a mouthwatering combination of spices.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 ears of corn",
        "recipeIngredient": [
            "2 teaspoons ground cumin",
            "2 teaspoons ground coriander",
            "1 teaspoon salt",
            "1 teaspoon white sugar",
            "1 teaspoon chili powder",
            "\u00bd teaspoon ground ginger",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon pepper",
            "\u215b teaspoon ground cloves",
            "1 lemon",
            "8 ears corn, husked",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together cumin, coriander, salt, sugar, chili powder, ginger, cinnamon, pepper, and cloves in a small bowl. Slice lemon into 8 wedges. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush corn with olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill corn on the hot grill, turning every few minutes, until lightly browned, about 15 minutes. Remove from grill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip lemon wedges into spice mixture. Rub 1 lemon wedge over each ear of corn.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Corn"
        ],
