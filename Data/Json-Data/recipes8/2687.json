    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12757/fish-taquitos/",
        "name": "Fish Taquitos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6863774.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "1999-07-03T03:36:38.000Z",
        "description": "A seafood twist to a Mexican favorite; rolled taquitos made with shark. Serve with salsa, guacamole, shredded lettuce, chopped tomatoes, and sour cream. Enjoy!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1 pound shark fillets",
            "12 (6 inch) corn tortillas",
            "\u00bc cup canola oil",
            "\u00bc cup lemon juice",
            "1 clove garlic, minced",
            "1 teaspoon dried oregano",
            "1 teaspoon Cajun seasoning",
            "1 cup shredded Cheddar cheese",
            "2 quarts vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut shark into strips 3/4 inch thick and 3 inches long.  Mix canola oil, lemon juice, garlic, oregano, and Cajun style spice mix in a plastic container.  Place shark strips in container, and marinate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm tortillas in a microwave to make them pliable.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain marinade, and pat shark dry with paper towels.  Place a strip of shark at one end of a tortilla, roll tightly, and secure with a toothpick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep fryer.  Submerge taquitos in hot oil.  Fry until golden brown, no more than 3 or 4 minutes each. Drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place taquitos in a baking dish large enough to accommodate all twelve.  Sprinkle with shredded cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 5 minutes, or until cheese melts.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Latin American",
            "Mexican"
        ],
