    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276987/triple-lemon-pound-cake/",
        "name": "Triple Lemon Pound Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7796859.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:51.000Z",
        "description": "This is my adaptation of my favorite classic pound cake recipe. This is not difficult to make, even though it's made from scratch, and it always gets rave reviews.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "1 tube cake",
        "recipeIngredient": [
            "2 sticks butter, softened",
            "\u00bd cup vegetable oil",
            "3 cups white sugar",
            "5 eggs, at room temperature",
            "3\u2009\u2153 cups all-purpose flour, sifted twice",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 cup milk",
            "1 lemon, zested and juiced",
            "2 teaspoons vanilla extract",
            "1 teaspoon lemon extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Grease and flour a 10-inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter in a bowl with an electric mixer until creamy. Add oil; beat until creamy. Pour in sugar and beat until thoroughly incorporated, pausing to scrape down the sides of the bowl as needed. Add eggs one at a time, beating well after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, baking powder, and salt together in a bowl. Add to the sugar mixture alternately with milk, beginning and ending with the flour mixture. Mix in lemon zest and juice. Add extracts. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of the cake comes out clean, about 90 minutes. Cool in the pan on a rack. Remove from the pan when cool.\n"
            }
        ],
