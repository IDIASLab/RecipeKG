    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278877/thin-and-crispy-chocolate-chip-cookies/",
        "name": "Thin and Crispy Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2360128.jpg",
            "width": 2592,
            "height": 1456
        },
        "datePublished": "2015-06-28T20:25:40.000Z",
        "description": "Crispy, buttery chocolate chip cookies with a caramel flavor that can't be beat!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd sticks butter, at room temperature",
            "1 cup dark brown sugar",
            "\u00bd cup white sugar",
            "2  large eggs eggs",
            "2 teaspoons vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, brown sugar, and white sugar in a large bowl; beat with an electric mixer until creamy. Add eggs and vanilla extract. Beat until well incorporated. Stir in flour and baking soda until smoothly blended. Fold in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoonfuls of cookie dough onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until deeply browned and crisp, 12 to 15 minutes. Transfer to wire racks to cool. Repeat with remaining cookie dough.\n"
            }
        ],
