        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285391/instant-pot-pumpkin-quinoa-porridge/",
        "name": "Instant Pot&reg; Pumpkin Quinoa Porridge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9367492.jpg",
            "width": 0,
