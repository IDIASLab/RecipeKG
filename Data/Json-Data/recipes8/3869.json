        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280133/any-fruit-frozen-margaritas/",
        "name": "Any-Fruit Frozen Margaritas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9108200.jpg",
            "width": 0,
