        "name": "The Perfect Deviled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F03%2F23%2F9085093-the-perfect-deviled-eggs-photo-by-allrecipes-2000.jpg",
            "width": 2000,
            "height": 1333
