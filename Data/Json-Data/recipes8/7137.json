    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281835/baked-lemon-garlic-tilapia-and-potatoes-in-foil/",
        "name": "Baked Lemon-Garlic Tilapia and Potatoes in Foil",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8605633.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-10-05T04:39:51.000Z",
        "description": "Packet dinners, whether cooked in foil or en papillote, allow for easy cleanup. This simple meal is on the table in 30 minutes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 pieces heavy-duty aluminum foil",
            "cooking spray",
            "1 medium potato, peeled and cut into 1/2-inch cubes",
            "2 tablespoons diced red bell pepper",
            "1 (1 ounce) package seafood seasoning",
            "3 teaspoons butter, divided",
            "2 (7 ounce) tilapia fillets",
            "1 medium lemon, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Spray foil with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place equal amounts of diced potato and red bell pepper in the middle of each piece of foil and sprinkle with 1/2 of the seafood seasoning. Dot 1 1/2 teaspoons of butter on top of the potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 1 tilapia fillet to each packet of potatoes. Sprinkle fillet with remaining seafood seasoning. Dot remaining 1 1/2 teaspoons butter on top of the fillets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut 1/2 of the lemon into thin slices and place 1 to 2 lemon slices on top of each fillet. Seal foil packets and crimp edges tightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tilapia flakes easily with a fork, about 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Open packets carefully and vent steam. Squeeze remaining lemon juice over the tilapia and serve immediately.\n"
            }
        ],
