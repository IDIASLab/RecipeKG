    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276740/best-spritz-cookies/",
        "name": "Best Spritz Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Best Spritz Cookies"
        },
        "datePublished": "2020-06-19T04:42:36.000Z",
        "description": "This is the best tasting spritz cookie ever! The addition of brown sugar to the dough gives them great flavor. Buttery, crispy, and the aroma of the spices while they're baking is just incredible. I hope you enjoy them as much as we have!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "72 cookies",
        "recipeIngredient": [
            "1 cup butter, at room temperature",
            "1 cup packed brown sugar",
            "\u2153 cup white sugar",
            "2 teaspoons vanilla extract",
            "2 egg yolks",
            "3 ounces cream cheese, cut into pieces",
            "\u00be teaspoon freshly grated nutmeg",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "2\u2009\u00bc cups all-purpose flour",
            "2 tablespoons colored decorating sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, brown sugar, white sugar, and vanilla extract in a large bowl; beat with an electric mixer until light and fluffy, 3 to 5 minutes. Add egg yolks, 1 at a time, mixing for about 15 seconds after each yolk. Add cream cheese and mix until well combined. Mix in nutmeg, cinnamon, and salt; beat until completely mixed. Add flour and stir just until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a cookie press with dough and press out cookies, using your favorite shape, onto ungreased cookie sheets. Sprinkle with colored sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 8 to 10 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely. Repeat with remaining dough.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Spice Cookie Recipes"
        ],
