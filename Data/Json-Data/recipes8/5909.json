    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281492/roasted-lemon-garlic-potato-wedges/",
        "name": "Roasted Lemon-Garlic Potato Wedges",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8485310.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-08-27T06:00:13.000Z",
        "description": "These roasted potato wedges are flavored with fresh lemon juice and garlic, and make for an easy, savory side dish. They are delicious served with chicken or pork.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1  serving cooking spray",
            "4 large Yukon Gold potatoes, cut into wedges",
            "2 tablespoons olive oil",
            "2 tablespoons salted butter, melted",
            "1 tablespoon fresh lemon juice",
            "2 cloves garlic, minced",
            "\u00bd teaspoon dried thyme",
            "\u00bc teaspoon garlic salt",
            "\u00bc teaspoon onion powder",
            "1 pinch freshly ground black pepper to taste",
            "1 teaspoon dried parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Spray a 9x13-inch baking dish with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss potato wedges with olive oil, melted butter, lemon juice, minced garlic, thyme, garlic salt, and onion powder in a large bowl. Season with freshly ground cracked pepper to taste. Spread the potatoes in an even layer in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven for 20 minutes. Remove the pan from the oven and flip the potato wedges. Continue roasting until the potatoes are golden and cooked through, about 20 minutes more. Sprinkle with parsley and serve.\n"
            }
        ],
