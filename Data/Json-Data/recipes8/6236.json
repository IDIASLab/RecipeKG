    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/145371/jalapeno-yummies/",
        "name": "Jalapeno Yummies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F683905.jpg",
            "width": 2640,
            "height": 1980
        },
        "datePublished": "2013-09-19T06:00:45.000Z",
        "description": "Stuffed jalapenos wrapped in bacon make a lively appetizer. Any extra cream cheese filling can be mixed with sour cream and turned into a great dip.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "12 half peppers",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "1 (1.25 ounce) envelope dry onion soup mix",
            "6 fresh jalapeno peppers, halved lengthwise and seeded",
            "6 slices bacon, cut in half",
            "12 toothpicks",
            "\u00bd cup sour cream, for dip"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix the cream cheese with the onion soup mix in a bowl until smooth. Fill the jalapeno pepper halves with the cheese mixture. Wrap each pepper with a piece of bacon to enclose the filling, and secure with a toothpick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place peppers in a skillet over medium heat, and cook until the bacon is crisp on all sides, 10 to 15 minutes. Cool slightly and serve.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make the dip, place the sour cream in a bowl, and mix with any leftover cream cheese mixture. Cover and refrigerate until needed.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Jalapeno Popper Recipes"
        ],
