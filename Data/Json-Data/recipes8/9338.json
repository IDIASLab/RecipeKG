    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285343/easy-seafood-party-dip/",
        "name": "Easy Seafood Party Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Seafood Party Dip"
        },
        "datePublished": "2021-01-31T23:47:46.000Z",
        "description": "A seafood dip for a special occasion. Serve with breadsticks or crackers.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups fresh spinach leaves ",
            "1 (8 ounce) package cream cheese",
            "1 (10 ounce) jar artichoke hearts, drained and chopped",
            "\u00bd cup diced cooked shrimp",
            "\u00bd cup diced cooked crabmeat",
            "\u2153 cup sour cream",
            "\u2153 cup mayonnaise",
            "\u2153 cup freshly grated Parmesan cheese",
            "2 cloves garlic, minced",
            "1 pinch cayenne pepper",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a pot of lightly salted water to a boil. Add spinach, cover, and cook until tender, 1 to 2 minutes. Drain in a colander, press to remove remaining moisture, and chop finely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese in a microwave-safe bowl and microwave on high until warm and soft, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the microwave and add chopped spinach, artichoke hearts, shrimp, crab, sour cream, mayonnaise, Parmesan cheese, garlic, cayenne pepper, salt, and pepper. Stir until well combined. Serve warm.\n"
            }
        ],
