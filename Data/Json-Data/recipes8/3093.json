    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258431/slow-cooked-korean-tacos/",
        "name": "Slow-Cooked Korean Tacos",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Slow-Cooked Korean Tacos"
        },
        "datePublished": "2020-06-23T21:52:21.000Z",
        "description": "Korea meets Mexico! And they are friends when nestled in a tortilla!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT8H10M",
        "totalTime": "P1DT9H0M",
        "recipeYield": "8 tacos",
        "recipeIngredient": [
            "3 pounds whole pork belly",
            "2 cups water",
            "1 cup kosher salt",
            "\u00bd cup white sugar",
            "\u00bc cup soy sauce",
            "3 cloves clove garlic, chopped",
            "1 (1 inch) piece fresh ginger, sliced ",
            "14 fluid ounces Hawaiian barbeque sauce",
            "2 (12 ounce) packages 6-inch corn tortillas, or as needed",
            "2 red bell peppers, sliced, or more to taste",
            "1 cup sliced radishes, or to taste",
            "\u00bd (12 ounce) jar pickled onions, or to taste",
            "1 bunch cilantro, chopped, or to taste",
            "2 tablespoons sriracha sauce, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut diagonal lines across the pork belly skin in 2 opposing directions, forming a diamond pattern.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir water, salt, sugar, soy sauce, garlic, and ginger in a pot over medium heat until salt is dissolved, about 5 minutes. Let marinade cool, 20 to 30 minutes. Pour into a resealable plastic bag. Add the pork belly to the marinade and refrigerate for 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain pork belly, discarding marinade; rinse pork belly, pat dry, and place into a slow cooker. Coat with barbeque sauce and cook on Low until meat begins to fall apart, about 8 hours. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir the pork belly in a skillet over medium-high heat until crispy, about 5 minutes. Transfer to a cutting board and shred with forks. Place shredded pork belly on tortillas and top with red bell peppers, radishes, pickled onions, cilantro, and sriracha sauce.\n"
            }
        ],
