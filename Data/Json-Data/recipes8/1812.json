    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278699/soft-chocolate-cream-cheese-cookies/",
        "name": "Soft Chocolate Cream Cheese Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Soft Chocolate Cream Cheese Cookies"
        },
        "datePublished": "2020-06-19T04:44:29.000Z",
        "description": "This is a great recipe that uses both cream cheese and pudding mix. The pudding mix helps make the chocolate cookies light and fluffy.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "60 cookies",
        "recipeIngredient": [
            "4 cups white sugar",
            "1\u2009\u00bd cups unsalted butter, softened",
            "1 cup cream cheese, softened",
            "1 (3.4 ounce) package instant vanilla pudding mix",
            "1 (3.3 ounce) package instant white chocolate pudding mix",
            "4 eggs",
            "1 tablespoon vanilla extract",
            "4 cups all-purpose flour",
            "2 cups unsweetened cocoa powder",
            "2 teaspoons baking soda",
            "1 teaspoon salt",
            "1 (1.75 ounce) package multicolored candy sprinkles"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar, butter, cream cheese, and pudding mixes together in a bowl. Beat in eggs, one at a time, and beat well after each addition. Add vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, cocoa, baking soda, and salt together in a separate bowl. Add to sugar mixture and beat until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by large spoonfuls onto ungreased cookie sheets. Sprinkle the top of each cookies with sprinkles before baking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set, 12 to 15 minutes. Be careful not to overbake; cookies will get too hard and chewy.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
