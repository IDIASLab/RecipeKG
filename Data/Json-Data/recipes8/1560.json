    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277922/classic-german-side-salad/",
        "name": "Classic German Side Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Classic German Side Salad"
        },
        "datePublished": "2020-06-19T04:43:55.000Z",
        "description": "This salad is served in most restaurants in Germany as a side salad (\"kleiner gruener Salat\"). It's a mixture of leafy greens with a simple cream dressing.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT16H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 ounces heavy cream",
            "1 tablespoon lemon juice",
            "1 teaspoon white sugar",
            "1 teaspoon salt",
            "freshly ground white pepper to taste",
            "1 pinch dried marjoram, or to taste",
            "1 medium head Belgian endive, leaves separated",
            "\u00bd orange, juiced",
            "\u00bd head green leaf lettuce, torn into bite-sized pieces",
            "\u00bd head butter lettuce, torn into bite-sized pieces",
            "\u00bd head red leaf lettuce, torn into bite-sized pieces",
            "\u00bd cup garden cress, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare the dressing the day before serving by combining cream, lemon juice, sugar, salt, white pepper, and marjoram in a small bowl. Mix well, making sure salt and sugar are dissolved. Cover and store in the refrigerator for 16 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange 2 endive stalks from the center to the edge of 4 small salad plates and sprinkle stems with orange juice. Combine red leaf lettuce, butter lettuce, and green leaf lettuce in a bowl and set a handful of the mixture on top of the endive stalks on each plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix dressing and drizzle about 3 tablespoons over each salad. Garnish with garden cress.\n"
            }
        ],
