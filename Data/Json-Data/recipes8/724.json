    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275561/plum-chutney/",
        "name": "Plum Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Plum Chutney"
        },
        "datePublished": "2020-06-19T04:40:32.000Z",
        "description": "Sugary sweet and full of spices, this plum chutney is easy to make on the stovetop to use as a condiment for a variety of dishes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "10 medium plums, pitted and chopped, or more to taste",
            "1 cup white sugar",
            "\u00bd teaspoon fine sea salt",
            "\u00bd teaspoon ground ginger",
            "\u00bd teaspoon chili powder, or to taste",
            "\u00bd teaspoon ground cloves, or more to taste",
            "\u215b teaspoon ground cinnamon",
            "2 tablespoons white vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix plums and sugar together in a heavy-bottomed stockpot. Cook over medium heat, stirring occasionally, until mixture turns to liquid, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir salt, ginger, chili powder, cloves, and cinnamon into the mixture. Cook over low heat until desired consistency is reached, stirring often to prevent chutney from sticking, about 15 minutes. Taste and adjust spices if needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vinegar into the pot and cook over low heat, stirring frequently to prevent burning, for 5 minutes more. Let cool completely, about 30 minutes. Refrigerate and use within 3 to 4 weeks.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Chutney Recipes"
        ],
