        "name": "Sweet and Spicy Grilled Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1176395.jpg",
            "width": 2874,
