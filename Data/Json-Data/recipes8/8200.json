        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282267/roasted-red-pepper-panini/",
        "name": "Roasted Red Pepper Panini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8743465.jpg",
