    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261207/baked-polenta-with-balsamic-mushrooms/",
        "name": "Baked Polenta with Balsamic Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4575402.jpg",
            "width": 2948,
            "height": 3008
        },
        "datePublished": "2020-01-15T01:50:14.000Z",
        "description": "Mushrooms roasted with balsamic vinegar are nestled in creamy, cheesy polenta in this bake that doubles as a vegetarian main dish or side.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9x13-inch baking dish",
        "recipeIngredient": [
            "14 baby bella mushrooms, stemmed",
            "2 tablespoons olive oil",
            "2 tablespoons balsamic vinegar",
            "1 teaspoon dried thyme",
            "2\u2009\u00bd cups water",
            "2 cups half-and-half",
            "\u00be cup polenta",
            "\u00be cup diced Swiss cheese",
            "1 teaspoon sea salt",
            "\u00bd teaspoon freshly ground black pepper",
            "\u00bd cup finely grated Parmesan cheese",
            "2 cups half-and-half",
            "1 teaspoon black truffle salt, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mushrooms in a large baking dish, gill-side up. Drizzle olive oil and balsamic vinegar over mushrooms. Crush thyme between your fingers; sprinkle over mushrooms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, about 20 minutes. Transfer to a plate using tongs. Leave any accumulated juices in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and half-and-half to a boil in a large pot. Add polenta in a steady stream, whisking constantly. Reduce heat and cook, stirring constantly, until thickened, about 10 minutes. Remove from heat. Add Swiss cheese, sea salt, and pepper; stir until cheese is melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour hot polenta into the same baking dish used to bake the mushrooms. Smooth the top with a spatula. Arrange mushrooms, gill-side up, in the polenta. Sprinkle Parmesan cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until mushrooms are soft, about 20 minutes. Remove from oven and garnish with truffle salt.\n"
            }
        ],
