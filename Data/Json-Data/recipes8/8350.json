    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282279/impossible-shepherds-pie/",
        "name": "Impossible&trade; Shepherd's Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2020%2F11%2F16%2FLizzie-Impossible-Comforting-Sheperds-Pie-pic2.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2020-11-10T04:25:26.000Z",
        "description": "Impossible&trade; ground meatless burger is easily whipped up into a comforting shepherd's pie that is sure to please the whole family!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 tablespoons salted butter, divided",
            "1 (12 ounce) package Impossible Burger",
            "2 medium shallots, chopped",
            "2 cloves garlic, chopped",
            "1 cup frozen peas and carrots, thawed",
            "8 ounces brown gravy",
            "1 tablespoon Worcestershire sauce",
            "salt and ground black pepper to taste",
            "2 cups mashed potatoes",
            "1 cup shredded sharp Cheddar cheese",
            "1 tablespoon chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon butter in a 10-inch skillet over medium to medium-high heat. Cook and stir the meatless ground beef with shallots and garlic until browned and crumbly and shallots are soft, about 10 minutes. Add peas and carrots, gravy, Worcestershire sauce, salt, and pepper. Saute for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the mixture onto the bottom of a 10-inch round baking dish. Spread mashed potatoes evenly over the top. Sprinkle with Cheddar cheese and dot with remaining butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the center of the preheated oven until the cheese and butter have fully melted, about 20 minutes. Top with parsley and serve.\n"
            }
        ],
