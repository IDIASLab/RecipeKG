    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272667/keto-mushroom-stuffed-chicken-breasts/",
        "name": "Keto Mushroom-Stuffed Chicken Breasts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6518293.jpg",
            "width": 1442,
            "height": 960
        },
        "datePublished": "2019-10-18T01:51:19.000Z",
        "description": "This rich and hearty keto-friendly meal is perfect for those on low-carb diets. Serve it with your favorite vegetables or veggie 'rice.'",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 skinless, boneless chicken breast halves",
            "2 tablespoons butter",
            "1 (8 ounce) package baby portabella mushrooms, sliced",
            "2 cloves garlic, minced",
            "\u00bd teaspoon thyme",
            "1 (8 ounce) package cream cheese",
            "\u00bc teaspoon salt",
            "toothpicks",
            "2 tablespoons Dijon mustard",
            "2 tablespoons grated Parmesan cheese, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Set an oven rack in the center of the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts between 2 sheets of heavy plastic on a solid, level surface. Firmly pound with the smooth side of a meat mallet to 1/2-inch thickness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Add mushrooms and cook until softened, about 4 minutes. Add garlic and thyme and cook until mushrooms are tender and most of the liquid has evaporated, 4 to 5 minutes. Reduce heat to medium-low. Stir in cream cheese and season with salt. Cook and stir until cream cheese has completely melted. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon mushroom-cream mixture onto the chicken breasts. Wrap chicken around the mixture and secure with toothpicks. Set chicken bundles, seam-side up, in a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Remove from oven and brush with Dijon mustard. Sprinkle with Parmesan cheese and return to the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue baking until no longer pink in the center and the juices run clear, 15 to 20 minutes more. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Discard toothpicks before serving.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
