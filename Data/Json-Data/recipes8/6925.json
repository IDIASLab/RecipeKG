        "recipeIngredient": [
            "1 cup white vinegar",
            "\u00be cup salad oil",
            "\u00bc cup white sugar",
            "2 tablespoons prepared mustard",
            "1 tablespoon celery seed",
            "1 large head cabbage, shredded",
            "1 medium onion, thinly sliced",
            "\u00bc cup white sugar",
            "1 tablespoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine vinegar, oil, sugar, mustard, and celery seed for dressing in a saucepan over medium-high heat; bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine cabbage, onion, sugar, and salt for coleslaw in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour hot dressing over cabbage mixture and mix until incorporated. Place in the refrigerator until chilled, about 2 hours.\n"
            }
