        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273147/easy-chicken-marbella/",
        "name": "Easy Chicken Marbella",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1030342.jpg",
