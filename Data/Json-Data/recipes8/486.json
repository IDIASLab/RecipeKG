    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274809/easy-cappuccino-brownies/",
        "name": "Easy Cappuccino Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7044853.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T04:39:32.000Z",
        "description": "Kids and adults alike love these cappuccino brownies. They're easy to make, and something different.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "24 brownies",
        "recipeIngredient": [
            "19\u2009\u00bd ounces baking chocolate, grated",
            "6 eggs",
            "2\u2009\u2153 cups white sugar",
            "1\u2009\u00bd cups unsalted butter",
            "1\u2009\u215b cups all-purpose flour",
            "\u00bc cup cocoa powder",
            "\u00bc cup instant coffee granules",
            "2 tablespoons water",
            "\u215e cup heavy cream",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in a microwave-safe glass or ceramic bowl in 15-second intervals, stirring after each interval, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs and sugar together in a bowl. Stir in melted chocolate and butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour and cocoa powder together in another bowl; add to the chocolate mixture. Dissolve instant coffee in water and pour into the batter. Add heavy cream and walnuts; mix well. Pour into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 40 to 50 minutes.\n"
            }
        ],
