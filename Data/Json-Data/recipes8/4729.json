    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281059/crispy-teriyaki-tofu/",
        "name": "Crispy Teriyaki Tofu",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8308035.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-07-07T09:30:14.000Z",
        "description": "Crispy tofu coated in a bold teriyaki sauce that is full of flavor. Serve over rice for a complete meal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 tablespoons vegetarian teriyaki sauce",
            "2 green onions, chopped",
            "1 tablespoon sherry",
            "1 teaspoon minced garlic",
            "1 teaspoon Sriracha sauce",
            "\u00bd teaspoon ginger paste",
            "5 tablespoons cornstarch",
            "1 (14 ounce) package extra-firm tofu, pressed and cut into slices",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk teriyaki sauce, green onions, sherry, garlic, Sriracha sauce, and ginger paste together until smooth. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cornstarch in a gallon-sized plastic zip-top bag. Drop tofu slices into the bag, seal, and gently shake until all the tofu is coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium-high heat. Cook tofu for 5 minutes. Flip with tongs and cook until browned on the second side, about 5 minutes more. Transfer the tofu to a plate and wipe the skillet clean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour reserved sauce into the skillet and cook until slightly thickened, about 2 minutes. Remove from heat. Return the tofu to the skillet and toss to combine. Transfer the tofu to serving plates and spoon sauce on top just before serving.\n"
            }
        ],
