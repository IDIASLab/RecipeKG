    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277338/christmas-cobbler/",
        "name": "Christmas Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Christmas Cobbler"
        },
        "datePublished": "2020-06-19T04:43:13.000Z",
        "description": "This Christmas cobbler is a festive combo of apples and cranberries in a spice sauce covered with an oatmeal cookie-flavored crunchy topping! Yummy stuff! Serve with vanilla ice cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9x13-inch cobbler",
        "recipeIngredient": [
            "2 cups baking mix (such as Bisquick\u00ae)",
            "1\u2009\u00bd cups milk",
            "1 cup white sugar",
            "1 (21 ounce) can apple pie filling",
            "2 cups sweetened dried cranberries (such as Craisins\u00ae)",
            "\u00bc cup apple juice",
            "3 cups large-flake oats",
            "1 cup packed brown sugar",
            "\u00bd cup all-purpose flour",
            "2 tablespoons ground cinnamon",
            "2 teaspoons salt",
            "1\u2009\u00bd cups butter, cubed, at room temperature"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix baking mix, milk, and white sugar together in a bowl until it reaches a cake batter-like consistency. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apple pie filling, sweetened dried cranberries, and apple juice together in another bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats, brown sugar, flour, cinnamon, and salt in a bowl. Mix in butter to a crumbly texture with some unmixed butter chunks. Set topping aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour baking mix mixture into the prepared pan until half full. Add fruit mixture and spread out from the center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until starting to brown, about 45 minutes. Remove from the oven and sprinkle topping over cobbler while hot. Return to the oven and bake until browned, about 15 minutes more. Serve hot or cold.\n"
            }
        ],
