    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282104/marzipan-cookies/",
        "name": "Marzipan Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8705052.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-10-26T23:53:02.000Z",
        "description": "If you love marzipan, you will love these cookies. They are super easy to make, but you need to make the dough ahead of time, as they need to dry overnight.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H50M",
        "recipeYield": "40 cookies",
        "recipeIngredient": [
            "18 ounces marzipan",
            "1 cup confectioners' sugar",
            "1 large egg white",
            "2\u2009\u00bd tablespoons all-purpose flour",
            "2 tablespoons rose water, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine marzipan, confectioners' sugar, egg white, and flour in a bowl. Knead into a smooth dough. Roll out to 1/3-inch thickness and cut into whatever shapes you like.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with baking parchment and place cookies onto it. Allow to dry, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies in the preheated oven until lightly browned, 12 to 15 minutes. Transfer to a wire rack and let cool. Brush the cooled cookies with rose water.\n"
            }
        ],
