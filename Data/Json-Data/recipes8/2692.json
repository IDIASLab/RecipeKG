    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13231/azteca-soup/",
        "name": "Azteca Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4595669.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "1999-06-18T07:18:41.000Z",
        "description": "This is a very popular Mexican soup. It can be served with any other Mexican dish. Just before serving, add tortilla strips and avocado cubes, so the tortillas stay crunchy.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "10 (6 inch) corn tortillas, cut into 1/2 inch strips",
            "1\u2009\u00bd cups vegetable oil for frying",
            "salt to taste",
            "4 cups chicken broth",
            "1 cup fresh cilantro leaves",
            "2 large tomatoes",
            "1 large onion",
            "1 tablespoon chopped chipotle peppers in adobo sauce",
            "1 teaspoon salt, or to taste",
            "2 avocados - peeled, pitted and diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in large skillet and carefully drop in half the tortilla strips. Fry in batches until golden brown and crunchy. Place fried tortilla strips on a paper towel-lined plate and pat dry. Sprinkle salt on tortillas, tossing to coat both sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a blender or food processor, blend tomatoes, onion and cilantro until completely pureed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, heat chicken broth, chipotle chili, and pureed vegetables. Bring to a boil; remove from heat. Ladle soup into serving bowls. Prior to serving, add tortilla strips and avocado. Buen provecho!\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes"
        ],
