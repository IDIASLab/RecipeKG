        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10776/easy-lemon-cookies/",
        "name": "Easy Lemon Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F887122.jpg",
            "width": 4213,
            "height": 3401
        },
        "datePublished": "1998-04-06T23:51:20.000Z",
        "description": "The basic cake mix cookie with a twist.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen cookies",
        "recipeIngredient": [
            "1 (18.25 ounce) package lemon cake mix",
            "2 eggs",
            "\u2153 cup vegetable oil",
            "1 teaspoon lemon extract",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cake mix into a large bowl. Stir in eggs, oil, and lemon extract until well blended. Drop teaspoonfuls of dough into a bowl of confectioners' sugar. Roll them around until they're lightly covered. Once sugared, put them on an ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 6 to 9 minutes in the preheated oven. The  bottoms will be light brown, and the insides chewy.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
