    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277045/cannoli-cups/",
        "name": "Cannoli Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cannoli Cups"
        },
        "datePublished": "2020-06-19T04:42:58.000Z",
        "description": "Easy cannoli cups, just enough for kids and dinner parties to satisfy that cannoli craving. You can sprinkle with pistachios before eating.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "48 cannoli cups",
        "recipeIngredient": [
            "1 (15 ounce) package prepared pie pastry, at room temperature",
            "3 tablespoons white sugar",
            "1 teaspoon ground cinnamon",
            "1 (15 ounce) container ricotta cheese, drained",
            "\u00bd cup confectioners' sugar",
            "2 teaspoons vanilla extract",
            "\u2153 cup mini dark chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll pie dough out to desired thickness. Mix sugar and cinnamon together in a small bowl. Sprinkle mixture over one side and firmly press into the dough. Place a nonstick mini cupcake pan on top of dough for an outline. Cut out dough cups using a knife or biscuit butter. Press dough pieces into the mini cups; do not grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until dough has slightly puffed and is golden brown, 6 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ricotta cheese, confectioners' sugar, and vanilla extract in a bowl until smooth. Fold in chocolate chips. Place into a plastic bag and refrigerate until ready to serve.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to serve, cut a small hole in the corner tip of the plastic bag and pipe into pastry shells.\n"
            }
        ],
