        "name": "Jalapeno Popper Dip with Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1352179.jpg",
            "width": 2272,
