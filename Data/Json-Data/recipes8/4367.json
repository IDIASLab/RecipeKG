        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10528/raspberry-dream-bars/",
        "name": "Raspberry Dream Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F975163.jpg",
            "width": 1197,
