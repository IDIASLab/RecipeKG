    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281448/grilled-chicken-fajitas/",
        "name": "Grilled Chicken Fajitas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8591397.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-08-15T22:06:26.000Z",
        "description": "Mouthwatering grilled chicken fajitas with a little zing. Serve on a bed of mixed lettuces with guacamole, salsa, and sour cream, if desired.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "10 fajitas",
        "recipeIngredient": [
            "2 lime (2\" dia)s limes, juiced",
            "2 tablespoons olive oil",
            "2 tablespoons fajita seasoning",
            "1 pound skinless, boneless chicken breast halves",
            "2 large yellow bell peppers",
            "2 large red bell peppers",
            "10 (6 inch) flour tortillas, warmed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk lime juice, olive oil, and fajita seasoning together in a bowl and pour into a resealable plastic bag. Add chicken breasts, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut bell peppers in half and discard the inner membranes and seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken and peppers (cut-sides down) on the preheated grill. Cook, turning occasionally, until chicken is no longer pink in the center and the juices run clear, and peppers have nice grill marks on them, about 15 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut peppers and chicken into strips and serve on tortillas.\n"
            }
        ],
