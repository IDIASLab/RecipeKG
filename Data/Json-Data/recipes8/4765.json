    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280907/caramelized-banana-splits/",
        "name": "Caramelized Banana Splits",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Caramelized Banana Splits"
        },
        "datePublished": "2003-07-31T22:14:48.000Z",
        "description": "Bananas are pan-fried with a caramel sauce while chocolate and marshmallows are melted in a saucepan with other yummy ingredients. Serve on top of your favorite ice cream!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd cup brown sugar",
            "3 tablespoons unsalted butter",
            "\u00bd teaspoon salt",
            "1 pinch ground cinnamon",
            "4 bananas, sliced into rounds",
            "1 (10.5 ounce) package miniature marshmallows",
            "1\u2009\u00be cups chocolate chips",
            "\u00be cup chopped pecans",
            "\u00bd cup sweetened flaked coconut",
            "1 tablespoon orange juice",
            "1 cup crushed pineapple",
            "1 cup mashed strawberries",
            "\u00bd gallon vanilla ice cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt brown sugar and butter in a pan over medium-low heat for about 2 minutes. Add salt and cinnamon. Place sliced bananas on top of caramel mixture in an even layer. Turn bananas after about 30 seconds and cook for about 30 seconds more. Remove from heat and place in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt marshmallows and chocolate chips in a pot over low heat. Stir in pecans and coconut along with orange juice. Remove from heat and place in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pineapple and strawberries in separate bowls. Scoop ice cream into serving dishes and top with the bananas and caramel, melted chocolate mixture, and fruit.\n"
            }
        ],
