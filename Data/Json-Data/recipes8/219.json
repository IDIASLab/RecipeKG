    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274286/lemon-custard-freezer-pie/",
        "name": "Lemon Custard Freezer Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon Custard Freezer Pie"
        },
        "datePublished": "2020-06-19T04:38:21.000Z",
        "description": "Not too sweet, but not too sour either, this lemon custard freezer pie makes a very light and refreshing dessert. Serve with fresh whipped cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT9H0M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "\u00be cup sugar",
            "3 tablespoons all-purpose flour",
            "1 teaspoon salt",
            "\u00bd cup milk",
            "\u00bd cup half-and-half",
            "\u2153 cup fresh lemon juice",
            "2 tablespoons fresh lemon zest",
            "2 tablespoons butter",
            "1 prepared shortbread pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar, flour, and salt together in a 3-quart pot. Slowly add milk and half-and-half and bring to a boil. Cook until slightly thickened, 2 to 3 minutes. Remove from heat. Whisk in lemon juice, lemon zest, and butter. Return to medium heat and cook, stirring constantly, for 2 minutes. Remove from heat and set custard aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake empty crust in the preheated oven for 3 minutes. Remove from oven and cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill cooled crust with lemon custard filling and allow pie to cool completely. Place in the freezer and freeze for 8 hours to overnight.\n"
            }
        ],
