    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274621/tot-sliders/",
        "name": "Tot Sliders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7941905.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-07-10T04:27:14.000Z",
        "description": "If you didn't think sliders could get any smaller--or cuter--then you haven't tried swapping coin-shaped tots for buns! This dish is one of the times where it tastes as good as it looks.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "16 mini sliders",
        "recipeIngredient": [
            "32 frozen potato rounds (such as Ore-Ida\u00ae Crispy Crowns\u00ae)",
            "2 tablespoons vegetable oil",
            "8 ounces ground beef (85% lean)",
            "\u00bc pinch kosher salt",
            "\u00bc teaspoon black pepper",
            "4 slices American cheese",
            "1 tablespoon ketchup, or to taste",
            "1 tablespoon mustard, or to taste",
            "16 pickled gherkins, drained",
            "16 toothpicks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss tots with oil on a 10x15-inch baking pan, then spread into a single layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, turning once halfway through, until browned and crisp, 15 to 18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, put ground beef on a sheet of waxed paper. Top with another sheet and press beef into a 4x6-inch rectangle. Remove top sheet. Sprinkle beef with salt and pepper, then cut into sixteen 1x1 1/2-inch rectangles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Break each cheese slice into quarters; fold quarters over to make 16 pieces of double-layered cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook beef in a large skillet over medium heat until browned on one side, about 3 minutes. Turn burgers over, top with cheese; cook, covered, until beef is no longer pink in centers and cheese has melted, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top 1 tot with a cheeseburger, ketchup, mustard, pickle, and another tot; spear stack with a toothpick. Repeat with remaining ingredients.\n"
            }
        ],
