    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278993/honey-glazed-baby-carrots/",
        "name": "Honey-Glazed Baby Carrots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7815314.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:44:46.000Z",
        "description": "Not even kids can resist vegetables like carrots when they are smothered in butter and honey!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (16 ounce) package baby carrots",
            "3 tablespoons butter",
            "2 tablespoons honey",
            "\u00bd teaspoon cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan and fill with water to just below the bottom of the steamer. Bring water to a boil. Add carrots, cover, and steam until tender, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While carrots are steaming, melt butter in a saucepan over low heat. Combine with honey and cornstarch.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour glaze over cooked carrots and toss to coat. Serve immediately.\n"
            }
        ],
