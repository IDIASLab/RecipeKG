    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284032/pasta-with-crispy-sage-and-miso-brown-butter/",
        "name": "Pasta with Crispy Sage and Miso Brown Butter",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pasta with Crispy Sage and Miso Brown Butter"
        },
        "datePublished": "2020-08-28T06:31:36.000Z",
        "description": "The ultimate mashup! A magical pasta sauce with miso paste and brown butter. Works with any shape of pasta.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package dry pasta",
            "1 tablespoon extra-virgin olive oil, or as needed",
            "2 medium shallots, thinly sliced",
            "7 tablespoons butter",
            "20 large sage leaves",
            "2 tablespoons miso paste",
            "\u00bd cup freshly grated Parmesan cheese, plus more for serving",
            "\u00bd lemon, juiced",
            "salt and freshly ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook, stirring occasionally, until tender yet firm to the bite, about 11 minutes. Drain, saving 1 cup of pasta cooking water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat olive oil in a skillet over medium heat and cook shallots until softened, about 8 minutes. Remove all from heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in the same skillet until it foams. Add sage leaves, reduce heat slightly, and cook until butter is browned and sage is crispy, about 3 minutes. Remove from heat and take out sage from the butter. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk miso paste into the browned butter immediately. Add a splash of preserved pasta water, shallots, Parmesan cheese, and lemon juice. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve with a drizzle of olive oil and top with crispy sage and Parmesan cheese.\n"
            }
        ],
