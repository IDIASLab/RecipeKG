        "recipeIngredient": [
            "1 1/2 parts Makers Mark&reg; Bourbon",
            "1 teaspoon Sugar",
            "2 dashes Bitters",
            "1 slice Orange",
            "1 Cherry",
            "1 splash Water",
            "Ice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Muddle sugar, bitters, orange, cherry, and water in an old fashioned glass.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the orange rind and fill glass 3/4 full of ice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add Maker's Mark&reg; Bourbon and stir.\n"
