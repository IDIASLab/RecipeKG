    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284828/easy-banoffee-pie/",
        "name": "Easy Banoffee Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Banoffee Pie"
        },
        "datePublished": "2021-06-21T08:14:28.000Z",
        "description": "Banana, caramel, and whipped cream in a graham cracker crust. What's not to love?",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (14 ounce) can sweetened condensed milk",
            "1 (9 inch) prepared graham cracker crust",
            "\u00bd pint heavy whipping cream",
            "2 tablespoons white sugar",
            "1 teaspoon vanilla extract",
            "3 medium bananas, thinly sliced",
            "\u00bd teaspoon cocoa powder for dusting, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour condensed milk into a pie pan and cover tightly with foil for the caramel. Place in a larger pan. Add hot water to the pan but keep it below the top of the pie pan; you don't want to get any water into the milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the caramel is thick and brown, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and transfer caramel to a bowl. Beat with an electric mixer until smooth. Pour into the graham cracker crust, cover, and cool in the refrigerator for about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream in a chilled glass or metal bowl with an electric mixer until frothy. Add sugar gradually, continuing to beat until stiff peaks form. Mix in vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pie crust and caramel from the refrigerator. Cover caramel completely with banana slices. Spread whipped cream over top and sprinkle lightly with cocoa powder. Cover and chill until ready to serve.\n"
            }
        ],
