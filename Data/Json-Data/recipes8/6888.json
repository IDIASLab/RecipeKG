    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284239/microwave-chi-chi-dango/",
        "name": "Microwave Chi Chi Dango",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8568455.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-09-23T01:54:25.000Z",
        "description": "Super easy chi chi dango recipe. Mix it all together and put it in the microwave! This is a great recipe to have the kids do and an easy treat!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "16 dango",
        "recipeIngredient": [
            "cooking spray",
            "1 pound sweet rice flour (mochiko)",
            "2\u2009\u00bd cups white sugar",
            "2 cups water",
            "1 (14 ounce) can coconut milk",
            "1 teaspoon vanilla extract",
            "potato starch or kinako for rolling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Generously grease a 9x13-inch microwave-safe pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mochiko and sugar in a bowl. Add water, coconut milk, and vanilla; stir everything together until smooth. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on high for 18 minutes until cake is set. If the top center of the cake is still sticky, microwave in increments of 2 minutes until no longer sticky.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the pan from the microwave and set on a cooling rack. Cool for about 90 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut cake into 16 pieces and roll in potato starch or kinako.\n"
            }
        ],
