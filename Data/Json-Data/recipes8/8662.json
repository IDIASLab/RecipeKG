        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239224/coconut-chicken-curry-stew/",
        "name": "Coconut Chicken Curry Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1905381.jpg",
            "width": 1989,
            "height": 1989
        },
        "datePublished": "2019-04-02T07:03:41.000Z",
        "description": "This easy, slow-cooker chicken dish is a a hearty stew that is sure to warm you from the inside out!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT6H5M",
        "totalTime": "P0DT6H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "2 pounds skinless, boneless chicken thighs, cut into 1-inch pieces",
            "6 cups chopped carrots",
            "4 cups chopped onion",
            "12 cloves garlic, minced",
            "2 tablespoons freshly grated ginger",
            "2 (14.25 ounce) cans low-sodium chicken broth",
            "2 cups light coconut milk",
            "2 tablespoons curry powder",
            "1 teaspoon salt",
            "\u00bd cup chopped fresh cilantro",
            "2 tablespoons lime juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray a nonstick skillet with cooking spray and place over medium-high heat. Cook and stir chicken pieces in hot skillet until lightly browned, about 3 minutes. Drain excess fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread chicken, carrots, onion, garlic, and ginger in the bottom of a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk chicken broth, coconut milk, curry powder, and salt together in a bowl; pour over chicken mixture into slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 6 to 8 hours (or cook on High for 3 to 4 hours). Stir in cilantro and lime juice.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Chicken"
        ],
