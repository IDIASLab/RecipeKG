    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270858/roasted-butternut-and-black-bean-enchiladas/",
        "name": "Roasted Butternut and Black Bean Enchiladas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F03%2F12%2F270858-roastedbutternutsquashandblackbeanenchiladas-passano-alr0321-9473-2000.jpg",
            "width": 2000,
            "height": 2000
        },
        "datePublished": "2020-06-23T21:53:01.000Z",
        "description": "A hearty vegetarian enchilada filled with roasted squash, goat cheese, and black beans. Chipotle chile powder adds a great smoky heat to this dish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 enchiladas",
        "recipeIngredient": [
            "4 cups peeled and diced butternut squash",
            "\u00bd onion, diced",
            "2 tablespoons olive oil",
            "5 cloves garlic, minced",
            "2 teaspoons chipotle powder",
            "\u00bd teaspoon kosher salt",
            "cooking spray",
            "1 (15 ounce) can black beans, rinsed and drained",
            "3 ounces crumbled goat cheese",
            "1 (14 ounce) can enchilada sauce",
            "8 (6 inch) corn tortillas",
            "2 ounces crumbled cotija cheese",
            "\u00bc cup chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butternut squash, onion, olive oil, and garlic on a baking sheet; toss well. Sprinkle chipotle powder and salt over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven, stirring once, until butternut squash is tender, about 25 minutes. Remove from heat and let cool. Reduce oven temperature to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a 10x6-inch casserole with cooking spray. Pour 1/4 cup enchilada sauce over the bottom.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add black beans and goat cheese to the butternut squash mixture; mix gently to keep the goat cheese in good-sized pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour some enchilada sauce onto a small plate. Quickly dip each tortilla into the sauce and fill with some of the squash-bean mixture. Roll up and place seam-side down in the prepared casserole dish. Tuck any remaining filling into the exposed ends of the enchiladas. Spoon remaining sauce over the enchiladas. Top with cotija cheese and cilantro.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through, about 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash",
            "Winter Squash",
            "Butternut Squash"
        ],
