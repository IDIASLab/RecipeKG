    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8293232/gluten-free-mashed-potato-stuffed-meatloaf/",
        "name": "Gluten-Free Mashed Potato-Stuffed Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Mashed Potato-Stuffed Meatloaf"
        },
        "datePublished": "2021-10-19T12:49:39.000Z",
        "description": "This is a fantastic way to make a meatloaf without gluten or a sugary glaze. It also incorporates a surprise element of carrots in the meatloaf to help retain moisture and add great flavor.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "1\u2009\u00bd pounds lean ground beef",
            "1\u2009\u00bd cups crispy rice cereal squares (such as Rice Chex\u00ae), finely crushed",
            "1 (12 ounce) can tomato sauce, divided",
            "1 cup finely shredded carrots",
            "\u00bc cup chopped fresh parsley",
            "2 large eggs",
            "1 clove garlic, minced",
            "\u00be teaspoon salt",
            "\u00be teaspoon ground white pepper",
            "3 cups prepared mashed potatoes",
            "1 cup shredded Italian cheese blend"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a 2 1/2-quart baking dish (either a 9-inch square or a 7x11-inch) with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together ground beef, crushed cereal, 2/3 of the tomato sauce, carrots, parsley, eggs, garlic, salt, and pepper until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press 1/2 of the beef mixture into the prepared pan. Spread mashed potatoes on top, then spread remaining beef mixture on top of that. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 45 minutes. Remove foil and top with remaining tomato sauce. Sprinkle Italian cheese blend over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake until cheese is completely melted or begins to gently brown, 10 to 15 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool for 15 minutes before serving.\n"
            }
        ],
