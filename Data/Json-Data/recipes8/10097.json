        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd cup dark brown sugar",
            "\u00bc cup unsalted butter, melted",
            "2 tablespoons soy sauce",
            "2 tablespoons sherry",
            "6 (5 ounce) salmon fillets"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, melted butter, soy sauce and sherry together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place salmon fillets in a baking dish and pour brown sugar sauce over top. Allow to sit at room temperature for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until salmon flakes easily with a fork, about 10 minutes.\n"
            }
