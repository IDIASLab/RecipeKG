        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20220/watermelon-vodka-slush/",
        "name": "Watermelon Vodka Slush",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F851146.jpg",
            "width": 250,
