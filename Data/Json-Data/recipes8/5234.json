    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270307/moroccan-chicken-thighs/",
        "name": "Moroccan Chicken Thighs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5303267.jpg",
            "width": 2016,
            "height": 1512
        },
        "datePublished": "2019-05-21T21:50:41.000Z",
        "description": "Quick and easy thighs with complex Moroccan flavors. Serve with rice, garnished with lemon wedges.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 bone-in chicken thighs",
            "kosher salt and ground black pepper to taste",
            "1\u2009\u00bd cups chicken broth",
            "3 tablespoons paprika",
            "3 tablespoons ground cumin",
            "3 teaspoons minced fresh ginger root",
            "3 teaspoons ground turmeric",
            "2 teaspoons ground cinnamon",
            "1 lemon, zested and juiced",
            "olive oil, or to taste",
            "1 tablespoon canola oil, or to taste",
            "\u00bd white onion, chopped",
            "1 cup pimento-stuffed green olives",
            "2 tablespoons chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season chicken thighs with salt and pepper. Let come to room temperature, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chicken broth, paprika, cumin, ginger, turmeric, cinnamon, and 2 teaspoons lemon zest in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oils in a cast iron skillet until they begin to smoke. Add chicken thighs, skin-side down, and cook for 4 minutes. Flip thighs and continue cooking until skin is crispy, about 4 minutes more. Transfer to a plate. Saute onion in the skillet until softened, 3 to 5 minutes. Pour chicken broth mixture carefully over onion; scrape up any browned bits off the bottom of the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return chicken thighs to the skillet. Spoon some of the liquid over the thighs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer skillet to the preheated oven; bake until chicken thighs are no longer pink in the center, 25 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add olives to the skillet. Drizzle lemon juice over the thighs and garnish the whole dish with parsley.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Thigh Recipes"
        ],
