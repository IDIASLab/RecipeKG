    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10253/basic-coconut-cookies/",
        "name": "Basic Coconut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F462067.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This recipe uses a pre-made mix that can be stored for several weeks at room temperature.   You can look at the recipe for Basic Cookie Mix here at Allrecipes.com! Use 2 cups for this recipe.  Add the following ingredients to the mix for coconut cookies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "2 cups basic cookie mix",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00bd cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter over low heat and stir into the Basic Cookie Mix.  Beat egg lightly and add to mixture.  Stir in vanilla and coconut and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "With lightly floured hands shape into 1 inch balls and arrange about 2 inches apart on a greased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake  for 12 to 15 minutes until golden.  Cool a couple of minutes before removing from cookie sheet to racks to complete cooling.\n"
            }
        ],
