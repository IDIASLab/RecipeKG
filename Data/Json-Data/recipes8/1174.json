    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276772/gingerbread-cheesecake-bars/",
        "name": "Gingerbread Cheesecake Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7194823.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:39.000Z",
        "description": "These cheesecake bars are packed full of gingerbread flavor that gingerbread lovers won't be able to get enough of.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT4H50M",
        "recipeYield": "24 bars",
        "recipeIngredient": [
            "2\u2009\u00bc cups all-purpose flour",
            "2 teaspoons ground ginger",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground cloves",
            "\u00bc teaspoon salt",
            "\u00be cup margarine, softened",
            "1 cup white sugar",
            "1 egg",
            "\u00bc cup molasses",
            "1 tablespoon water",
            "2 (8 ounce) packages cream cheese",
            "\u00bd cup powdered sugar",
            "\u00bd cup brown sugar",
            "6 tablespoons molasses",
            "2 teaspoons ground ginger",
            "2 teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "3 cups whipped topping",
            "2 tablespoons whipped topping, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together flour, ginger, baking soda, cinnamon, cloves, and salt for the cookie base in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream margarine and white sugar in a large bowl with an electric mixer until light and fluffy. Add egg and beat until well combined. Stir in molasses and water. Gradually stir flour mixture into the molasses mixture. Transfer dough to a 9x13-inch pan, patting it down so it covers the entire bottom of the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 25 to 30 minutes. Allow to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, powdered sugar, and brown sugar for the filling in a large bowl with an electric mixer until smooth. Add molasses, ginger, cinnamon, nutmeg, and cloves and stir, scraping down the sides of the bowl, until mixture is smooth. Fold in 3 cups whipped topping until well combined. Spread filling into the cooled cookie base. Cover with plastic wrap and refrigerate until set, at least 4 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with remaining whipped topping before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Holiday Cake Recipes"
        ],
