    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277144/pecan-pie-with-chocolate-chips/",
        "name": "Pecan Pie with Chocolate Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pecan Pie with Chocolate Chips"
        },
        "datePublished": "2020-06-19T04:43:02.000Z",
        "description": "A nice twist on pecan pie for chocolate lovers. You can freeze a pie if not using both right away.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "2 9-inch pies",
        "recipeIngredient": [
            "1 (15 ounce) package pie crusts",
            "5 large eggs",
            "1 cup light corn syrup",
            "\u00bd cup brown sugar",
            "\u00bd cup white sugar",
            "\u00bd cup shortening (such as Crisco\u00ae)",
            "1 tablespoon all-purpose flour",
            "3 teaspoons instant espresso powder",
            "\u00bd teaspoon salt",
            "1 (12 ounce) bag dark chocolate chips (such as Ghirardelli\u00ae)",
            "1\u2009\u00bd cups pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line two 9-inch pie plates with pie crusts; fold excess dough to form a rim and flute as desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs, corn syrup, brown sugar, white sugar, shortening, flour, espresso powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer 1/2 of the chocolate chips on the bottom of each pie crust. Divide batter evenly between the 2 pies, covering the chocolate chips completely. Lay whole pecans on top in concentric circles or chop pecans and press under batter. Cover with aluminum foil so as not to overcook pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Uncover and continue to bake until set, about 30 minutes more. Remove from the oven and let cool completely, about 30 minutes. Chill for at least 1 hour before serving.\n"
            }
        ],
