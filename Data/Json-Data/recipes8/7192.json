    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281848/bacon-wrapped-jamaican-jerk-pork-tenderloin/",
        "name": "Bacon-Wrapped Jamaican Jerk Pork Tenderloin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F542678.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-12-21T20:12:29.000Z",
        "description": "A zesty, flavorful take on an old favorite. Very easy to make in about an hour's time! You could also top with diced jalapeno peppers, if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons brown sugar",
            "2 tablespoons Jamaican jerk seasoning",
            "1 teaspoon ground paprika",
            "1 teaspoon ground thyme",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon seafood seasoning (such as <a href=\"https://www.allrecipes.com/article/what-is-old-bay-seasoning/\" aria-label=\"Old Bay\">Old Bay</a>\u00ae)",
            "1 pinch crushed red pepper, or to taste",
            "1 (1 1/2 pound) pork tenderloin",
            "4 slices bacon, or more to taste",
            "1 tablespoon olive oil, or more as needed",
            "\u00bd medium sweet onion, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, jerk seasoning, paprika, thyme, oregano, seafood seasoning, and crushed red pepper in a bowl; rub generously all over the tenderloin. Wrap with bacon and secure with toothpicks. Coat lightly with oil and place in a loaf pan or casserole dish. Sprinkle any remaining spice rub on top, cover with some diced onion, and surround with remaining onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven until pork is slightly pink in the center, 45 to 60 minutes. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C).\n"
            }
        ],
