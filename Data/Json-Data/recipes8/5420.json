        "recipeIngredient": [
            "\u00bd cup coconut oil, melted",
            "\u00bd cup cocoa powder ",
            "\u00bd cup almond butter, room temperature",
            "\u00bc cup raw honey",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line 12 muffin cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend coconut oil, cocoa powder, almond butter, honey, vanilla extract together in a blender or food processor until smooth. Spoon mixture into prepared muffin cups. Refrigerate until set, about 30 minutes.\n"
            }
