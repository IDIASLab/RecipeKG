        "name": "Chef John's All-Purpose Steak Rub and Barbecue Dry Rub",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F11%2F04%2Fimage-441.jpg",
            "width": 1280,
