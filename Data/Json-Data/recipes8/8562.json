    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282321/slow-cooker-sweet-and-sour-chicken-with-pineapple/",
        "name": "Slow Cooker Sweet and Sour Chicken with Pineapple",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9094611.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-03-08T21:59:51.000Z",
        "description": "A flavorful sweet and sour chicken dish with yummy pineapple!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT4H5M",
        "totalTime": "P0DT4H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "6 (5 ounce) bone-in chicken thighs",
            "1 cup dark brown sugar",
            "4 tablespoons reduced-sodium soy sauce",
            "3 tablespoons honey",
            "3 cloves garlic, minced",
            "\u00bd tablespoon ground ginger",
            "2 cups pineapple chunks",
            "1 medium white onion, cut into large chunks",
            "2 tablespoons cornstarch",
            "2 tablespoons water",
            "1 (8 ounce) package frozen sugar snap peas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat. Brown chicken in hot oil, about 2 1/2 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While chicken is cooking, mix brown sugar, soy sauce, honey, garlic, and ginger together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer chicken to the bottom of a slow cooker. Place pineapple and onion on top of the chicken. Pour sauce over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook on High for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 500 degrees F (260 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from the slow cooker and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken in the preheated oven until no longer pink at the bone and the juices run clear, about 15 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, mix cornstarch and water together in a small bowl. Add it to the slow cooker, stir, and cook until sauce has thickened, about 30 minutes. Add snap peas and chicken and cook until heated through, 15 to 20 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Slow Cooker",
            "Main Dishes",
            "Chicken"
        ],
