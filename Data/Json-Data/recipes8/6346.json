        "name": "Ultra Easy Pineapple Chicken Kabobs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2020%2F07%2F23%2FUltra-Easy-Pineapple-Chicken-Kabobs-by-Gayle.jpg",
