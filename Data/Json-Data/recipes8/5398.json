    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240047/college-kid-oatmeal-cookies/",
        "name": "College Kid Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "College Kid Oatmeal Cookies"
        },
        "datePublished": "2020-06-19T04:01:22.000Z",
        "description": "A really fast and easy way to make oatmeal if you're a college student and don't have access to a fully-stocked pantry. It's quick, simple, and wonderful for birthdays and dessert nights!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00be cup coconut oil",
            "\u00be cup white sugar",
            "\u00be cup firmly packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00be teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "2\u2009\u00be cups instant oatmeal",
            "1 cup blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat coconut oil, white sugar, and brown sugar together in a bowl until smooth. Add eggs and vanilla; beat until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking soda, cinnamon, and salt together in another bowl. Gradually beat flour mixture into sugar mixture; stir in oatmeal and blueberries until dough is combined. Drop dough by the teaspoonful 2-inches apart onto baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies in the preheated oven until golden brown, 8 to 10 minutes. Cool slightly on the baking sheet before transferring to a wire rack to cool completely.\n"
            }
        ],
