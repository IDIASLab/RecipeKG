    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281973/oat-spelt-bread/",
        "name": "Oat-Spelt Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Oat-Spelt Bread"
        },
        "datePublished": "2008-05-20T17:01:07.000Z",
        "description": "This yummy spelt bread won't have a chance to go stale on your countertop!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "4 loaves",
        "recipeIngredient": [
            "5 cups warm water, divided",
            "12\u2009\u00be cups All-Purpose Spelt White Flour  PUR",
            "3 (.25 ounce) packages active dry yeast",
            "1 tablespoon white sugar",
            "2 cups quick oats",
            "\u2154 cup brown sugar",
            "\u2154 cup vegetable oil",
            "1 tablespoon molasses",
            "1 tablespoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup warm water, 1/4 cup spelt flour, yeast, and white sugar in a very large mixing bowl. Let the yeast proof and grow for about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in remaining water, oats, brown sugar, vegetable oil, and molasses. Add salt, then add remaining spelt flour, 1 cup at a time, and stir until dough starts to pull away from the sides of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the dough to a generously oiled bowl, turning dough several times to oil all sides so it won't stick or dry out. Let rise in a warm place until doubled in size, about 1 hour. Divide dough into 4 equal pieces and shape into loaves. Transfer loaves to greased loaf pans and let rise until dough is about 1/2 inch higher than the top of the pans, about 1 hour longer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake loaves in the preheated oven until tops are brown, about 30 minutes. Let cool in the pans for 15 minutes, then remove from the pans and finish cooling on wire racks.\n"
            }
        ],
