    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275295/earl-grey-tea-cookies/",
        "name": "Earl Grey Tea Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1945533.jpg",
            "width": 2549,
            "height": 2549
        },
        "datePublished": "2020-06-19T04:40:03.000Z",
        "description": "Earl Grey tea is one of the best teas to use in this recipe, but you can use any tea you prefer. Enjoy these cookies with any type of tea or milk.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "cooking spray",
            "2 cups all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd cup confectioners\u2019 sugar",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon salt",
            "6 Earl Grey tea bags",
            "\u00bd cup unsalted butter, melted halfway",
            "2 eggs",
            "1 teaspoon water",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Lightly spray a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, white sugar, confectioners' sugar, baking soda, and salt in a large bowl. Cut open tea bags and mix tea leaves evenly into the flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add butter, eggs, water, and vanilla extract to the flour mixture. Mix with an electric mixer or rubber spatula until dough comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into balls and shape into 2-inch cookies on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are light brown, about 12 minutes. Cool cookies for 5 minutes before serving.\n"
            }
        ],
