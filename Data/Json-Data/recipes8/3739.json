    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280746/high-protein-power-bars/",
        "name": "High-Protein Power Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "High-Protein Power Bars"
        },
        "datePublished": "2020-07-02T06:39:29.000Z",
        "description": "Our teen son no longer needed his huge container of chocolate protein powder. At my husband's request, I made this power bar recipe. It is a good, quick breakfast or cookie alternative.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 bars",
        "recipeIngredient": [
            "cooking spray",
            "1 cup rolled oats",
            "\u00bd cup whole wheat flour",
            "1 scoop chocolate-flavored protein powder",
            "\u00bc teaspoon ground cinnamon",
            "4 large egg whites",
            "\u00bc cup applesauce",
            "2 tablespoons olive oil",
            "2 tablespoons peanut butter",
            "\u00bd cup chocolate chips",
            "\u00bc cup sunflower seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Spray a 9-inch square baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir oats, flour, protein powder, and cinnamon together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix egg whites, applesauce, olive oil, and peanut butter together in another bowl. Add to the oat mixture and mix well. Fold in chocolate chips and sunflower seeds; batter will be quite thick. Spread into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, about 30 minutes. Remove from the oven and let cool, 20 to 30 minutes. Cut into bars.\n"
            }
        ],
