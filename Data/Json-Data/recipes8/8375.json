    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282305/lemon-crumb-muffins/",
        "name": "Lemon Crumb Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8710566.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-11-09T03:54:24.000Z",
        "description": "Lemony muffins complete with an oat streusel topping are perfect for breakfast or brunch.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "\u2153 cup white sugar",
            "3 tablespoons all-purpose flour",
            "1 tablespoon oats",
            "2 tablespoons cold salted butter, cut into small pieces",
            "\u00bc cup applesauce",
            "\u2153 cup white sugar",
            "1 large egg",
            "1 tablespoon lemon zest",
            "\u00bd cup lemon juice",
            "\u00bc cup sour cream",
            "1\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a muffin tin with parchment paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar, flour, and oats together in a small bowl until combined. Work butter into the dry ingredients until it resembles damp sand. Set the topping aside while you prepare the muffin batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together applesauce, sugar, egg, and lemon zest in a large bowl. Add lemon juice and sour cream and stir until combined. Add flour, baking powder, and salt. Stir everything together until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling each halfway. Sprinkle the topping on each muffin until completely covered.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick comes out clean, 20 to 25 minutes. Cool in the tin for about 10 minutes before serving.\n"
            }
        ],
