    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275873/creamy-vegan-pasta-salad/",
        "name": "Creamy Vegan Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6978574.jpg",
            "width": 1080,
            "height": 1256
        },
        "datePublished": "2020-06-19T04:40:55.000Z",
        "description": "I bring this macaroni salad to BBQs and potlucks and everyone loves it--people never even realize it's vegan!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cups whole-wheat elbow pasta",
            "\u00bd bell pepper, diced",
            "\u2153 cup corn",
            "1 stalk celery, diced",
            "2 tablespoons diced red onion",
            "\u00bd cup vegan mayonnaise",
            "3 teaspoons white vinegar",
            "1\u2009\u00bd teaspoons Dijon mustard",
            "\u00bd teaspoon white sugar",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 8 minutes. Strain pasta and rinse under cold water. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bell pepper, corn, celery, and red onion in a bowl. Mix together vegan mayo, vinegar, Dijon mustard, sugar, salt, and pepper in a small bowl; pour over vegetables. Mix in cold pasta and toss to combine. Taste and adjust seasonings as needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover pasta salad and let cool in fridge for at least 90 minutes before serving, stirring occasionally to mix sauce into pasta evenly.\n"
            }
        ],
