    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276356/white-chocolate-coconut-cookies/",
        "name": "White Chocolate-Coconut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7731507.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:41:48.000Z",
        "description": "Lime and coconut combine with white chocolate for a unique cookie experience.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "cooking spray",
            "\u00bd cup butter, softened",
            "6 tablespoons brown sugar",
            "6 tablespoons white sugar",
            "1 large egg, beaten",
            "1 fresh lime",
            "1 cup all-purpose flour",
            "1 cup white chocolate chips",
            "\u00bd teaspoon baking soda",
            "1 pinch salt",
            "2 cups finely shredded coconut, divided, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Lightly spray 2 cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, sugars, and egg in a bowl using a wooden spoon, not an electric mixer. Grate lime zest into the mixture. Stir in 1/2 to 3/4 teaspoon lime juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, white chocolate chips, baking soda, and salt in a separate bowl. Mix in butter mixture until a stiff dough forms. Fold in 1 cup coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place remaining coconut on a flat dish. Roll dough into ping pong ball-sized balls; roll in coconut. Arrange dough on the prepared cookie sheets; do not press down.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned around the edges, about 10 minutes.\n"
            }
        ],
