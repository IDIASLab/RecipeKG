        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280462/make-ahead-broccoli-salad/",
        "name": "Make-Ahead Broccoli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9265868.jpg",
