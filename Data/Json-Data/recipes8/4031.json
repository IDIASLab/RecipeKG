    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236409/gluten-free-coconut-lime-cookies/",
        "name": "Gluten-Free Coconut Lime Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7011287.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:58:08.000Z",
        "description": "Gluten-free cookies with the taste of coconut and lime. Grate the whole lime, use about half of it for the juice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bc cup white sugar",
            "2 tablespoons vegetable oil",
            "1 large egg",
            "\u00bd cup gluten-free all purpose baking flour",
            "\u00bc cup shredded unsweetened coconut",
            "1 teaspoon grated lime zest",
            "\u00bc teaspoon baking powder",
            "\u00bd cup confectioners' sugar",
            "1 tablespoon lime juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat white sugar, oil, and egg together in a bowl using an electric mixer until creamy and smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, coconut, lime zest, and baking powder together in a separate bowl; stir into creamed mixture until dough is just combined. Drop dough by spoonfuls onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are golden around the edges, about 8 minutes. Transfer cookies to a wire rack to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk confectioners' sugar and lime juice together in a bowl until smooth; spread over each cookie.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Fruit Cookie Recipes",
            "Coconut"
        ],
