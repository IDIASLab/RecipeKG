    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282296/southwestern-sausage-stuffing/",
        "name": "Southwestern Sausage Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8735145.jpg",
            "width": 1441,
            "height": 960
        },
        "datePublished": "2020-11-10T05:36:22.000Z",
        "description": "A mixture of cornbread and bolillo rolls make up the base of this tasty Jimmy Dean&reg; sausage stuffing. The addition of creamed corn, green chiles, and Mexican spices make this dish an addictively tasty twist on traditional stuffing!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 2-quart casserole",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) package Jimmy Dean\u00ae Premium Pork Regular Sausage",
            "1 cup diced onion",
            "2 teaspoons poultry seasoning",
            "1 teaspoon salt",
            "1 teaspoon ground cumin",
            "1 teaspoon chili powder",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon cayenne pepper",
            "\u00bd teaspoon dried oregano",
            "\u00bd teaspoon dried sage",
            "\u00bd teaspoon garlic powder",
            "3 cups stale or toasted cornbread, crumbled",
            "3 (2.6 ounce) bolillo rolls (Mexican bread rolls), diced and toasted",
            "1 (14.75 ounce) can creamed corn",
            "1 cup chicken broth",
            "1 (4 ounce) can diced green chiles",
            "2 large eggs, lightly beaten",
            "\u00bc cup chopped cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 2-quart baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sausage to a large frying pan over medium-high heat. Cook and stir with a spatula, breaking apart the sausage until cooked, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix poultry seasoning, salt, cumin, chili powder, black pepper, cayenne, oregano, sage, and garlic powder into the sausage mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cornbread crumbles and bolillo pieces in a large bowl. Mix in sausage mixture. Add creamed corn, chicken broth, green chiles, eggs, and cilantro; stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer stuffing mixture to the prepared baking dish. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 minutes. Uncover and continue to bake until golden, about 15 minutes more.\n"
            }
        ],
