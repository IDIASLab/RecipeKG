    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284715/lemon-strawberry-trifle/",
        "name": "Lemon Strawberry Trifle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9251674.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2021-06-15T04:27:22.000Z",
        "description": "With strawberries, whipped cream, lemon pudding and angel food cake, this trifle is the perfect balance of rich and fruity.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 .25 cups",
        "recipeIngredient": [
            "2 (3.4 ounce) packages instant lemon pudding mix",
            "2\u2009\u00bd cups Almond Breeze Unsweetened Vanilla Almondmilk",
            "1 (16 ounce) container frozen whipped topping, thawed",
            "14 ounces angel food cake",
            "2 pounds sliced fresh strawberries",
            "\u2154 cup lemon curd"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk lemon pudding mixes and almondmilk together for 2 minutes. Fold in 8 oz of whipped topping. Refrigerate for at least 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut angel food cake into small, bite-sized pieces. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, combine remaining 8 oz of whipped topping with lemon curd, folding together again thoroughly combined. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer half of the angel food cake in the bottom of a large bowl or trifle dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with half of the pudding mixture, half of the sliced strawberries, and half of the lemon curd mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Repeat layers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with fresh strawberries and lemon slices if desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Keep refrigerated until ready to serve.\n"
            }
        ],
