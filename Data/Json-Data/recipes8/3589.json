    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9449/homemade-buttercrunch-candy/",
        "name": "Homemade Buttercrunch Candy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2786643.jpg",
            "width": 4272,
            "height": 2848
        },
        "datePublished": "1999-09-13T17:35:54.000Z",
        "description": "Mom used to make this every Christmas that I can remember.  We kids used to look forward to it.  Now I make it for my family.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pound",
        "recipeIngredient": [
            "1 cup margarine",
            "1 cup white sugar",
            "3 tablespoons water",
            "1 tablespoon light corn syrup",
            "1 cup toasted chopped almonds, divided",
            "\u00be cup milk chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter a baking sheet, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in 2 quart saucepan. Add sugar, and stir over medium heat until sugar dissolves. Stir in water and corn syrup. Cook over medium heat, stirring often, to 270 degrees F (132 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, and quickly stir in 1/2 of the chopped nuts. Spread onto the prepared baking sheet. Sprinkle chocolate over hot candy; when it begins to melt, spread evenly over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Finely chop remaining almonds and sprinkle over candy. Refrigerate until firm. Break into small pieces to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Nut Candy Recipes"
        ],
