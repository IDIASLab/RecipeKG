    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244886/scotcheroos-on-a-stick/",
        "name": "Scotcheroos on a Stick",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2562718.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2015-09-03T00:29:53.000Z",
        "description": "Perfect for your next bake sale or an easy to eat backyard BBQ treat.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "36 treats",
        "recipeIngredient": [
            "1 cup Karo\u00ae Light or Dark Corn Syrup",
            "1 cup sugar",
            "1 cup creamy peanut butter",
            "6 cups crispy rice cereal",
            "36 lollipop sticks (5-inch length)",
            "1 cup semi-sweet chocolate chips",
            "chopped peanuts, colored sprinkles, finely chopped candy bars"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine corn syrup and sugar together in a large pan over medium heat, stirring to dissolve sugar. Bring mixture to a boil. Remove from heat. Stir in peanut butter; mix well. Add cereal; stir until evenly coated. Let cool in pan for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat hands with butter or spray with cooking spray. Form Scotcheroo mixture into 1-inch balls and place on waxed paper. Carefully insert lollipop sticks into each ball.  Cool for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chocolate chips in a microwave-safe bowl. Microwave on HIGH 1 to 2 minutes, stirring frequently until chocolate is melted. Dip half of each ball into melted chocolate. Or drizzle tops with chocolate. Immediately sprinkle with toppings, if desired. Place on waxed paper. Cool at least 1-1/2 hours until chocolate is completely set.\n"
            }
        ],
