    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235978/autumn-squash-soup/",
        "name": "Autumn Squash Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1095052.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "2020-06-19T03:57:55.000Z",
        "description": "Great butternut squash soup.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 cups butternut squash cubes",
            "1 teaspoon salt",
            "2 dashes ground cinnamon, or to taste",
            "1 tablespoon coconut oil",
            "\u00bd teaspoon curry powder",
            "salt to taste",
            "1 large Honeycrisp apple - peeled, cored, and diced",
            "\u00bd large yellow onion, diced",
            "3\u2009\u00bd cups vegetable broth",
            "\u00bd cup almond milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread butternut squash cubes onto a baking sheet; season with 1 teaspoon salt and cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast squash in preheated oven until fork-tender, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt coconut oil in a large pot over medium-high heat. Stir curry powder and a few dashes salt into the oil. Saute apple and onion in the seasoned coconut oil until softened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable broth and almond milk into the pot; add butternut squash and bring the liquid to a boil. Reduce heat to medium-low and simmer about 20 minutes; season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup into a blender no more than half full. Cover and hold lid in place; pulse a few times before leaving on to blend. Puree in batches until smooth.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
