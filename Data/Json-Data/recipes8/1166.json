        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276741/cream-cheese-spritz-cookies/",
        "name": "Cream Cheese Spritz Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F533195.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:36.000Z",
        "description": "These cookies are a family tradition every Christmas in my family, especially in tree or wreath form! The cream cheese gives regular spritz cookies a nice little boost.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 1/2 dozen cookies",
        "recipeIngredient": [
            "1 cup butter, at room temperature",
            "1 (3 ounce) package cream cheese, at room temperature",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "8 drops green food coloring, or as desired",
            "2 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter and cream cheese together in a bowl using an electric mixer; add sugar and beat well. Mix in vanilla extract and food coloring. Slowly add flour and mix well to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a cookie press with dough and form cookies onto ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set, 8 to 10 minutes, making sure to remove at the first hint of browning.\n"
            }
        ],
