    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282108/apple-spice-muffins/",
        "name": "Apple-Spice Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8762872.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-10-27T01:49:00.000Z",
        "description": "Choose your favorite extract, spice, and fruit to make nutritious muffins with the ingredients you have on hand.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "2 medium ripe bananas, mashed",
            "\u00be cup packed brown sugar",
            "\u2153 cup unsweetened applesauce",
            "1 large egg, lightly beaten",
            "1 teaspoon vanilla extract",
            "1 cup whole wheat flour",
            "2 teaspoons baking powder",
            "1\u2009\u00bc teaspoons salt",
            "1 teaspoon apple pie spice",
            "\u00bd teaspoon baking soda",
            "1 cup quick-cooking oats",
            "1 cup diced apple"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease twelve 2 1/2-inch muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together bananas, brown sugar, applesauce, egg, and vanilla extract in a large bowl. Sift together flour, baking powder, salt, apple pie spice, and baking soda in another bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add flour mixture and oats to banana mixture; gently stir just until combined. Do not overmix. Fold in apple. Spoon batter evenly into prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the centers comes out clean, 15 to 20 minutes. Remove muffins from pan, transfer to a wire rack, and let cool completely.\n"
            }
        ],
