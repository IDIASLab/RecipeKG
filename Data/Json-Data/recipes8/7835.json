        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281906/easy-vegetarian-stuffing/",
        "name": "Easy Vegetarian Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8865395.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-02-18T15:47:38.000Z",
        "description": "Now my vegetarian doesn't have to miss out on stuffing. And it is a good way to use up slightly stale bread--just stick the ends in the freezer!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cube vegetable bouillon",
            "1 cup boiling water",
            "3 tablespoons salted butter",
            "\u00bd cup chopped onion",
            "1 stalk celery, chopped",
            "2 cloves garlic, minced",
            "2 teaspoons dried parsley",
            "\u00bd teaspoon dried sage",
            "\u00bc teaspoon dried marjoram",
            "\u215b teaspoon ground black pepper",
            "4\u2009\u00bd cups cubed stale bread"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine boiling water and bouillon cube; stir until dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a 2-quart saucepan over medium-low to low heat. Add onion, celery, and garlic; cook until softened, about 5 minutes. Stir in parsley, sage, marjoram, and pepper, then pour in dissolved bouillon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring mixture to a boil, then remove pan from the heat. Gently stir in bread cubes. Cover and let sit for 5 minutes.\n"
            }
        ],
