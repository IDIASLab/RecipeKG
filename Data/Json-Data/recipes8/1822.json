        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278737/carrot-cake-oatmeal-cookies/",
        "name": "Carrot Cake Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7792597.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:44:32.000Z",
        "description": "Cake-like carrot cake oatmeal cookies with a sweet and fresh taste. Drizzle with melted white or cream cheese store-bought or homemade icing if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "48 cookies",
        "recipeIngredient": [
            "\u00be cup melted butter",
            "\u00be cup light brown sugar",
            "\u00be cup white sugar",
            "1 egg",
            "1 cup shredded carrots",
            "\u00bc cup water",
            "2 tablespoons grated orange zest",
            "2 cups rolled oats",
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place butter, brown sugar, white sugar, and egg in a large bowl and mix with an electric mixer for 1 minute. Add carrots, water, and orange zest; mix until fully blended. Add oats, flour, baking soda, salt, nutmeg, and cinnamon; mix for 1 minute more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto the prepared baking sheets, 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set and golden, 8 to 10 minutes. Cool on wire racks.\n"
            }
        ],
