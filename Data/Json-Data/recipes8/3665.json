    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280604/molasses-oatmeal-white-chocolate-chip-cookies/",
        "name": "Molasses Oatmeal White Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8245315.jpg",
            "width": 960,
            "height": 1208
        },
        "datePublished": "2020-07-01T23:48:24.000Z",
        "description": "Soft molasses spice cookies with a touch of oats and white chocolate chips to put a twist on a classic cookie.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT4H25M",
        "recipeYield": "42 cookies",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup McCann's\u00ae Quick Cooking Rolled Irish Oats",
            "2 teaspoons Spice Islands\u00ae Ground Ginger",
            "1 teaspoon Spice Islands \u00ae Ground Saigon Cinnamon",
            "1 teaspoon Clabber Girl\u00ae Baking Soda",
            "\u00bc teaspoon Spice Islands\u00ae Ground Nutmeg",
            "\u00bc teaspoon salt",
            "\u00be cup unsalted butter, at room temperature",
            "\u00be cup brown sugar, firmly packed",
            "\u00bd cup Grandma's\u00ae Molasses",
            "1 egg",
            "1 teaspoon Spice Islands\u00ae Pure Vanilla Extract",
            "1 cup white chocolate chips",
            "\u00bc cup granulated sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, oats, ginger, cinnamon, baking soda, nutmeg, and salt in large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a stand mixer with the paddle attachment (or a large bowl with electric mixer), cream together the butter and brown sugar in large bowl with electric mixer on medium speed until light and fluffy, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add molasses, egg, and vanilla and beat on medium speed until fully incorporated. Using a spatula, scrape down the sides to release some of the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually add flour mixture, one cup at a time, on low speed until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in the white chocolate chips and evenly distribute as best you can.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press dough into a thick flat disk. Wrap in plastic wrap. Refrigerate 4 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Shape dough into 1-inch balls. Roll in granulated sugar. Place 2 inches apart on baking sheets lined with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 8 to 10 minutes or until edges of cookies just begin to brown. Remove to wire racks; cool completely.\n"
            }
        ],
