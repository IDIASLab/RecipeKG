    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276705/peach-glazed-raspberry-and-cream-cheese-pie/",
        "name": "Peach-Glazed Raspberry and Cream Cheese Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7187565.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:29.000Z",
        "description": "Raspberry pie with a peach glaze and a layer of cream cheese on the bottom. Serve topped with sweetened whipped cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT4H40M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) refrigerated unbaked pie crust ",
            "1 cup crushed canned peaches",
            "\u00be cup water, divided",
            "\u00be cup white sugar, divided",
            "2 tablespoons cornstarch",
            "1 tablespoon lemon juice",
            "1 (8 ounce) package cream cheese, at room temperature",
            "3 cups fresh raspberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a 9-inch pie plate with pie crust. Press crust firmly against sides and bottom and prick with a fork. Fold excess crust under and press together to form thick edge.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until light brown, 10 to 12 minutes. Remove and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine crushed peaches, 1/2 cup water, and 1/2 cup sugar in a saucepan over medium heat. Mix cornstarch into remaining 1/4 cup water in a small bowl; add to the peach mixture and stir until boiling. Boil until thick, about 5 minutes longer. Remove from the heat, stir in lemon juice, and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and remaining 1/4 cup sugar in a medium bowl; beat with an electric mixer until smooth and creamy. Spread into the bottom of the cooled pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place raspberries in a large bowl. Pour cooled peach glaze over top and toss gently to coat. Pour mixture onto cream cheese layer and spread gently to fill pie. Chill in the refrigerator until pie is set, at least 4 hours.\n"
            }
        ],
