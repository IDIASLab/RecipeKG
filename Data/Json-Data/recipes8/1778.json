    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278639/double-chocolate-layer-cake/",
        "name": "Double Chocolate Layer Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Double Chocolate Layer Cake"
        },
        "datePublished": "2020-06-19T04:44:23.000Z",
        "description": "A rich, thick, completely double chocolate layer cake that is constructed from scratch. This is a cake that never gets old in any gathering.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 9-inch layer cake",
        "recipeIngredient": [
            "\u00be cup unsalted butter",
            "2 (1 ounce) squares unsweetened chocolate",
            "2\u2009\u00bc cups all-purpose flour",
            "2 cups white sugar",
            "2 teaspoons baking soda",
            "1 teaspoon salt",
            "\u00bc teaspoon unsweetened cocoa powder",
            "1\u2009\u00be cups buttermilk, at room temperature",
            "2 eggs",
            "\u00be cup heavy whipping cream",
            "1\u2009\u00bd cups semisweet chocolate chips "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease and flour two 9-inch round cake pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter and unsweetened chocolate together in a saucepan over low heat, stirring constantly, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, baking soda, salt, and cocoa powder in a large bowl of an electric mixer. Add melted chocolate mixture, buttermilk, and eggs. Beat on low speed for 1 minute; increase speed to high and beat until light and fluffy, about 2 minutes. Divide batter between the prepared cake pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cakes spring back when gently pressed, 25 to 30 minutes. Cool in the pans for 5 minutes; invert onto a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, bring cream just to a boil in a saucepan. Remove from heat and stir in chocolate chips; mix until frosting is smooth and starts to thicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 cooled cake layer on a cake plate. Spread with 1/3 cup frosting. Top with second layer. Frost the top and sides with remaining frosting.\n"
            }
        ],
