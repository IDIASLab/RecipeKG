    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9136/green-bean-casserole-from-scratch/",
        "name": "Green Bean Casserole from Scratch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F524009.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "A classic tradition for Thanksgiving day. Originally submitted to ThanksgivingRecipe.com.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 to 10 servings",
        "recipeIngredient": [
            "1 cup chopped onion",
            "2 tablespoons butter",
            "2 tablespoons all-purpose flour",
            "1 cup milk",
            "\u00be cup shredded sharp Cheddar cheese",
            "\u00bd cup sour cream",
            "1 tablespoon white sugar",
            "\u00bd teaspoon salt",
            "1 pound frozen cut green beans, thawed and drained",
            "2 cups prepared stuffing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place green beans in a 9x13 inch glass pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Saute onions with butter in a large frying pan over medium heat until translucent. Add flour and stir to coat flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually add milk and bring to a boil. Add cheese, sour cream, sugar, and salt. Cook till thickened and cheese is melted. Pour mixture over green beans and top with a layer of stuffing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 to 30 minutes.\n"
            }
        ],
