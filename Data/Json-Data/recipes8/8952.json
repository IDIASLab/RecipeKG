    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285520/creamy-mushroom-sauce-for-pasta/",
        "name": "Creamy Mushroom Sauce for Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Creamy Mushroom Sauce for Pasta"
        },
        "datePublished": "2020-12-02T03:29:45.000Z",
        "description": "Creamy mushroom sauce for pasta, steak, or chicken.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 ounces dried porcini mushrooms",
            "2 cups hot water, or as needed",
            "1 stick butter",
            "\u00bc cup finely chopped fresh Italian parsley",
            "2 cloves garlic, finely chopped",
            "\u2154 cup heavy cream",
            "\u2154 cup dry white wine",
            "salt and freshly ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place porcini mushrooms in a bowl and cover with hot water. Let soak until plumped up, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mushrooms and soaking liquid into a coffee filter or fine mesh sieve set over a bowl. Save soaking liquid for sauce. Rinse rehydrated mushrooms well to remove any grit, then coarsely chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add butter to a large, nonstick skillet over medium-low heat. When butter has melted, add parsley and garlic; stir to combine. Stir in chopped mushrooms. Moisten the mixture with a few tablespoons reserved soaking water. Cook, stirring a few times, until the moisture has largely evaporated, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cream and wine and stir to combine. Cook at barely a simmer until liquid has reduced slightly, about 5 minutes. Season with salt and pepper.\n"
            }
        ],
