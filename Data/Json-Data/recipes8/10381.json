        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285743/cheesecake-fruit-salad/",
        "name": "Cheesecake Fruit Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9413426.jpg",
            "width": 0,
