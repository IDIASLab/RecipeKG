    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282886/chocolate-peppermint-bark-cookies/",
        "name": "Chocolate Peppermint Bark Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8871275.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-12-22T22:59:19.000Z",
        "description": "An easy-to-make chocolate cookie base features the flavorful addition of peppermint bark and pecans. You can use homemade or store-bought bark, with any chocolate you prefer.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "32 cookies",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd cup dark cocoa powder (such as Hershey's\u00ae Special Dark)",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00be teaspoon salt",
            "1 cup unsalted butter, softened",
            "1 cup firmly packed dark brown sugar",
            "\u00bd cup white sugar",
            "2 large eggs, at room temperature",
            "2 teaspoons vanilla extract",
            "\u00bd teaspoon peppermint extract",
            "1 cup chopped chocolate peppermint bark",
            "\u00be cup chopped pecans",
            "\u00bc cup crushed candy canes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, cocoa powder, baking powder, baking soda, and salt together in a bowl until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, brown sugar, and white sugar in a large bowl and beat with an electric mixer until light and fluffy. Add eggs, vanilla, and peppermint extract and mix until thoroughly combined. Mix in half of the dry ingredients until just combined. Mix in remaining dry ingredients until just combined. Fold in peppermint bark pieces, pecans, and crushed candy cane pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 tablespoon-sized balls of dough 1 1/2 inches apart on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops of cookies just begin to set, 10 to 15 minutes. Allow cookies to cool for at least 5 minutes on the baking sheets before removing to a wire rack to cool completely.\n"
            }
        ],
