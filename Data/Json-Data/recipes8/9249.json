    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283891/shishito-pepper-ice-cream/",
        "name": "Shishito Pepper Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Shishito Pepper Ice Cream"
        },
        "datePublished": "2021-01-10T00:29:33.000Z",
        "description": "This cool, creamy, homemade ice cream laced with a bit of heat from skillet-blistered shishito peppers is a great dessert to make for spice enthusiasts.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT12H45M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 ounces shishito peppers",
            "3 tablespoons sesame oil",
            "1 pinch flaky sea salt",
            "\u00be cup white sugar",
            "2 cups heavy cream",
            "1\u2009\u00bd cups milk",
            "\u00bd cup dry milk powder",
            "4 large egg yolks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set a cast iron skillet over high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip peppers in oil to coat, then set in the hot skillet to blister, 30 seconds per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the heat and sprinkle with salt. Remove and discard stems. Blend peppers and sugar in a food processor until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pepper-sugar mixture, cream, milk, and milk powder in a saucepan over low heat. Cook and stir until sugar and milk powder have dissolved, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg yolks in a small bowl. Add to the saucepan and cook, stirring occasionally until mixture reaches a temperature of 175 degrees F (80 degrees C), about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour ice cream base into a bowl, cover, and refrigerate 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove ice cream base from the refrigerator and strain out pepper fragments with a mesh sieve.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour base into an ice cream maker and freeze according to manufacturer's instructions, about 20 minutes. Transfer to an airtight container and freeze until firm, about 4 hours.\n"
            }
        ],
