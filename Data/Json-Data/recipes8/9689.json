    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284073/chinese-popcorn-snack-mix/",
        "name": "Chinese Popcorn Snack Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9166225.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2021-04-21T21:38:08.000Z",
        "description": "Jazz up your popcorn by adding some Chinese-inspired seasonings to give it a unique taste.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "10 cups plain popped popcorn, unsalted",
            "1\u2009\u00bd cups chow mein noodles",
            "1 cup honey roasted peanuts",
            "3 tablespoons unsalted butter, melted",
            "2 tablespoons low-sodium soy sauce",
            "1 tablespoon freshly squeezed lime juice",
            "1\u2009\u00bd teaspoons sesame oil",
            "1 teaspoon Sriracha sauce, or to taste",
            "1 teaspoon ground ginger"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C). Line a large baking sheet with aluminum foil or parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine popcorn, chow mein noodles, and peanuts in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk butter, soy sauce, lime juice, sesame oil, Sriracha, and ginger together in a small bowl. Drizzle the butter mixture over the popcorn, tossing to evenly coat. Distribute evenly on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes, stirring halfway through. Remove baking sheet from oven.  Set aside to cool before storing in an airtight container.\n"
            }
        ],
