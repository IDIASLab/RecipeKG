    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282862/sweet-potato-waffles-with-cinnamon-maple-syrup/",
        "name": "Sweet Potato Waffles with Cinnamon Maple Syrup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sweet Potato Waffles with Cinnamon Maple Syrup"
        },
        "datePublished": "2020-11-03T09:56:59.000Z",
        "description": "Fluffy sweet waffles that taste like fall on a plate!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (15 ounce) cans sweet potatoes, drained",
            "1 cup milk",
            "6 tablespoons unsalted butter, melted",
            "\u00bc cup packed dark brown sugar",
            "\u00bd teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon salt",
            "6 large egg whites",
            "3 cups maple syrup",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sweet potatoes, milk, butter, brown sugar, and vanilla in a food processor; pulse until well combined and smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking powder, and salt together in a large bowl. Stir in sweet potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat and grease a waffle maker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a stand mixer on high speed until stiff peaks form. Fold into the waffle batter, 2 cups at a time, until well incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop 1 cup batter onto the preheated waffle maker. Close the lid and cook until slightly golden and crispy, about 5 minutes. Remove to a plate and repeat to make remaining waffles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the waffles are cooking, heat maple syrup in a pot over low heat until warm. Stir in cinnamon. Serve with waffles.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Waffle Recipes"
