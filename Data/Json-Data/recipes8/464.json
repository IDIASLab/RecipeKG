    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274794/cherry-almond-pie/",
        "name": "Cherry-Almond Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cherry-Almond Pie"
        },
        "datePublished": "2020-06-19T04:39:28.000Z",
        "description": "Fresh cherries and almonds are a simple combination that works remarkably well! I like my pie a little less sweet, so adjust accordingly.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 8-inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd cup butter, cubed",
            "\u00bd cup sliced almonds",
            "ice water",
            "2 cups pitted fresh tart cherries, halved, or more to taste",
            "\u00bd cup white sugar, or more to taste",
            "\u00bc teaspoon salt",
            "1 tablespoon almond extract, or more to taste",
            "1 tablespoon amaretto liqueur, or more to taste",
            "1 tablespoon cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour and sugar for crust in a bowl. Cut in butter using hands to mix and mush until thoroughly combined and mixture resembles tiny pebbles or cornmeal. Stir in almonds. Sprinkle with ice water until dough holds together in a ball. Cover in plastic wrap and refrigerate at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss cherries, sugar, and salt for filling together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine almond extract, amaretto, and cornstarch in a separate bowl until smooth; add to cherry mixture and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out refrigerated crust dough using a lightly floured rolling pin on a lightly floured work surface to desired thickness. Press into the bottom of an 8-inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until set, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from the oven and fill with cherry mixture. Increase the oven temperature to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake pie in the hot oven until edges of dough are golden and filling is bubbly, 35 to 45 minutes.\n"
            }
        ],
