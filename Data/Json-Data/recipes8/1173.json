    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276769/scandinavian-pear-tart/",
        "name": "Scandinavian Pear Tart",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7282942.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:39.000Z",
        "description": "This luscious dessert combines the flavors of almond, cinnamon, cardamom, cream cheese, and pear to create a tart that will not soon be forgotten!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT3H50M",
        "recipeYield": "1 9-inch tart",
        "recipeIngredient": [
            "cooking spray",
            "\u00bd cup white sugar",
            "3 tablespoons butter, softened",
            "\u00be cup all-purpose flour",
            "\u2153 cup finely chopped almonds",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup white sugar",
            "1 egg",
            "\u00be teaspoon almond extract",
            "1 (15 ounce) can pears, drained and thinly sliced",
            "1 tablespoon white sugar",
            "\u00be teaspoon ground cardamom",
            "\u00bc teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Coat a 9-inch fluted tart pan with a removable bottom with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter together in a small bowl until crumbly. Beat in flour and almonds. Press over the bottom and up the sides of the prepared tart pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese in another small bowl until smooth. Beat in sugar, egg, and vanilla extract. Spread over the crust. Arrange pear slices over the cream cheese layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, cardamom, and cinnamon in a small bowl. Sprinkle over the pears.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 10 minutes. Reduce oven temperature to 350 degrees F (175 degrees C). Continue baking until filling is set and an instant-read thermometer inserted into the center reads 160 degrees F (71 degrees C), 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool tart on a wire rack for 1 hour. Refrigerate for at least 2 hours before slicing.\n"
            }
        ],
