    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282361/quick-chili-and-rice-casserole/",
        "name": "Quick Chili and Rice Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Quick Chili and Rice Casserole"
        },
        "datePublished": "2005-01-05T13:06:38.000Z",
        "description": "A delicious, quick chili and rice casserole that everyone loves! Your family will really enjoy this one! For an extra bang serve with tortilla chips and salsa!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ground beef",
            "2 (15 ounce) cans tomato sauce (such as Hunt's\u00ae)",
            "2 (10 ounce) boxes frozen whole-kernel corn",
            "1 cup water",
            "\u00bd cup chopped onion",
            "2 tablespoons chili powder",
            "1 teaspoon dry mustard",
            "2 cups instant white rice (such as Minute\u00ae)",
            "\u00bc cup shredded extra-sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir ground beef in a nonstick skillet over medium-high heat until browned and crumbly, 5 to 7 minutes. Drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cooked beef, tomato sauce, corn, water, onion, chili powder, and dry mustard in a large saucepan over medium-high heat. Cover and bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in rice and reduce heat to low. Cover and simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While rice is cooking, set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer beef and rice mixture into a casserole dish and sprinkle with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until cheese is melted and bubbly, 2 to 3 minutes.\n"
            }
        ],
