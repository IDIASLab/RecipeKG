    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261752/gf-vegan-sugar-cookies/",
        "name": "GF Vegan Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "GF Vegan Sugar Cookies"
        },
        "datePublished": "2020-06-19T04:26:02.000Z",
        "description": "I made these vegan, gluten-free sugar cookies in a cooking class. Tastes great!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "2 cups gluten-free all purpose baking flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon sea salt",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "1 cup white sugar",
            "\u00bd cup vegan butter, softened",
            "\u00bc cup dairy-free eggnog",
            "2 teaspoons vanilla extract (such as Frontier\u00ae Organic)",
            "1\u2009\u00bd teaspoons egg substitute"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking soda, salt, cinnamon, and nutmeg in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and vegan butter together in a bowl using an electric mixer until creamy. Add eggnog, vanilla extract, and egg substitute; mix well. Add the flour mixture; mix dough well. Form dough into balls using an ice cream scoop. Place 2 inches apart on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 12 minutes.\n"
            }
        ],
