    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276189/christmas-cape-cod/",
        "name": "Christmas Cape Cod",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6203928.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:41:21.000Z",
        "description": "A holiday spin on this classic cocktail, taking the refreshing taste of cranberry and marrying it with the Christmas flavors of ginger and cinnamon. Garnish with a maraschino cherry or cinnamon stick. Cinnamon syrup can be kept lidded and refrigerated for up to 2 weeks.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 cocktail",
        "recipeIngredient": [
            "1 cup water",
            "1 cup white sugar",
            "2 cinnamon sticks",
            "4 fluid ounces ginger ale",
            "3 fluid ounces cranberry juice",
            "1 fluid ounce vodka",
            "ice as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a saucepan over high heat. Stir in sugar until completely dissolved, 1 to 2 minutes. Add cinnamon sticks, reduce heat to low, and let simmer for 8 minutes. Remove from heat and let steep for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cinnamon sticks and pour syrup into a squeeze bottle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ginger ale, cranberry juice, vodka, and 1 teaspoon cinnamon syrup mixture together in a 12-ounce highball glass. Stir or shake thoroughly. Fill with ice to the top of the glass. Serve.\n"
            }
        ],
