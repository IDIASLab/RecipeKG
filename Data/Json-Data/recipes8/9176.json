    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282749/mexican-christmas-beet-salad/",
        "name": "Mexican Christmas Beet Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mexican Christmas Beet Salad"
        },
        "datePublished": "2020-12-22T05:24:35.000Z",
        "description": "This Christmas salad is a mix of Mexican colors, flavors, and textures.\u00a0It is a sweet salad that combines beets with five different fruits, peanuts, and candies. A fundamental dish of Mexican Christmas traditions.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 large beets",
            "1 large jicama, peeled and diced",
            "10 fresh tejocotes (Mexican hawthorns), seeded and quartered",
            "4 medium Guavas, common, raw",
            "2 large  Oranges, raw",
            "3 medium Persian sweet limes, raw",
            "1 tablespoon white sugar, or to taste",
            "1 cup orange juice",
            "1 cup dry-roasted unsalted peanuts",
            "\u00bd pound colaciones (Mexican anise candies)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place beets in a pot, cover with lightly salted water, and bring to a boil over medium-high heat. Reduce to a simmer and cook until beets are soft, 30 to 40 minutes. Drain and cool until easily handled. Peel and cube.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place beets in a large bowl and add jicama, tejocotes, guava, oranges, and sweet limes; mix well and sweeten with sugar. Pour in orange juice, mix again, and sprinkle with peanuts. Allow salad to sit for about 10 minutes so flavors can blend.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with colaciones just before serving so they don't soften.\n"
            }
        ],
