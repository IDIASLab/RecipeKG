        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283686/sarahs-wake-up-coffee-drink/",
        "name": "Sarah's Wake Up Coffee Drink",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9109042.jpg",
            "width": 0,
