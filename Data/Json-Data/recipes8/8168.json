        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine nutritional yeast, salt, cumin, garlic powder, onion powder, paprika, chili powder, and cayenne pepper in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place popcorn in a bowl; add nutritional yeast mixture and toss to coat.\n"
            }
