        "name": "Carlota de Limon (Mexican Lemon Icebox Cake)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9089212.jpg",
            "width": 0,
