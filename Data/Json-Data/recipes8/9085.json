    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282504/ground-turkey-stroganoff/",
        "name": "Ground Turkey Stroganoff",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8895513.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-01-24T00:53:24.000Z",
        "description": "Easy to make, fast ground turkey stroganoff recipe for cold nights.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (12 ounce) package egg noodles",
            "\u00bc cup salted butter",
            "\u00bd (8 ounce) package fresh mushrooms, thinly sliced",
            "\u00bd cup minced onion",
            "3 cloves garlic, minced",
            "1 pound ground turkey",
            "2 tablespoons all-purpose flour",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "2 tablespoons Worcestershire sauce",
            "1 (10.75 ounce) can  condensed cream of mushroom soup with roasted garlic",
            "\u2153 cup milk",
            "\u2153 cup water",
            "\u00bd cup sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rapid boil. Cook egg noodles at a boil until tender yet firm to the bite, 7 to 9 minutes. Drain and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat butter in a large saucepan or deep skillet over medium heat. Saute mushrooms, onion, and garlic until soft, 5 to 7 minutes. Stir in ground turkey and cook until browned and no longer pink, 5 to 7 minutes more. Stir in flour, salt, and pepper; cook for 5 minutes. Mix in Worcestershire sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in soup, milk, and water. Bring to a simmer and cook, uncovered, for 10 minutes. Add sour cream and stir until heated through. Serve over egg noodles.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
