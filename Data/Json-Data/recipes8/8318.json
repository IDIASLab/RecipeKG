    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234539/davids-favorite-football-dip/",
        "name": "David's Favorite Football Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2435149.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-02-26T05:51:27.000Z",
        "description": "I created this recipe and it was instantly my husband's favorite football season dip!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ground turkey",
            "1 onion, chopped",
            "1 green bell pepper, chopped",
            "1 (28 ounce) can vegetarian baked beans",
            "1 (16 ounce) can vegetarian refried beans",
            "1 tablespoon coconut oil",
            "1 cup salsa",
            "\u00bd cup taco seasoning mix",
            "2 tablespoons ranch dressing",
            "2 teaspoons soy sauce",
            "2 teaspoons liquid smoke flavoring",
            "\u00bd cup shredded Cheddar cheese",
            "\u00bd cup shredded mozzarella cheese",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir ground turkey in the hot skillet until browned and crumbly, 5 to 7 minutes; add onion and bell pepper. Cover skillet and simmer until onion is softened, about 5 more minutes. Transfer turkey mixture to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir baked beans, refried beans, and coconut oil together in the same skillet used for turkey mixture; cook and stir over medium heat until beans are heated through, 5 to 7 minutes. Mix salsa, taco seasoning, ranch dressing, soy sauce, and liquid smoke into bean mixture and sprinkle with Cheddar cheese and mozzarella cheese. Cover skillet and cook until cheeses melts, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bean mixture into ground turkey mixture and stir well; season with salt and pepper.\n"
            }
        ],
