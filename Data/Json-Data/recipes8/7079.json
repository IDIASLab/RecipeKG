    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281735/gluten-free-fried-oatmeal-bites/",
        "name": "Gluten-Free Fried Oatmeal Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8192697.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-01T21:13:58.000Z",
        "description": "Delicious treat! Healthy dessert alternative!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups water",
            "\u00bd cup almond milk",
            "1 cup quick-cooking oats",
            "\u00bc cup cane sugar",
            "1 pinch Himalayan pink salt",
            "1 large egg",
            "\u00bd cup gluten-free all purpose baking flour",
            "\u00bc cup olive oil, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a pot; reduce heat to low. Add almond milk and stir in oats. Add sugar, stirring vigorously. Stir in salt. Continue to simmer until oats have reached your desired consistency, 3 to 5 minutes. Remove from heat and let cool, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer oats to a lidded container, cover, and chill in a refrigerator, 30 minutes to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg in a small bowl. Place gluten-free flour in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop chilled oatmeal into medium-size balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place oatmeal balls into egg wash until completely covered. Place in flour; cover entirely. Use a slotted spoon to remove floured oatmeal balls; shake off excess flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place oatmeal balls into the hot oil and cook until golden brown, 3 to 4 minutes on each side.\n"
            }
        ],
