    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277612/pumpkin-layer-cake/",
        "name": "Pumpkin Layer Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Layer Cake"
        },
        "datePublished": "2020-06-19T04:43:25.000Z",
        "description": "Yummy pumpkin spice layer cake.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 6-inch layer cake",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup vegetable oil",
            "4 large eggs",
            "2 cups all-purpose flour",
            "2 teaspoons baking soda",
            "1 teaspoon baking powder",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground nutmeg",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cloves",
            "\u00bc teaspoon ground ginger",
            "1 (15 ounce) can pumpkin puree",
            "4 cups powdered sugar",
            "1 (8 ounce) package cream cheese, softened",
            "\u00be cup butter",
            "1 teaspoon pure vanilla extract",
            "1 cup toasted slivered almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease two 6-inch round springform pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, oil, and eggs in a large mixing bowl and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, baking powder, cinnamon, nutmeg, salt, cloves, and ginger into a separate bowl; stir into sugar mixture. Stir in pumpkin until well blended. Pour batter evenly into the prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of each cake layer comes out clean, 45 to 55 minutes. Turn out onto wire racks to cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut rounded tops off of cooled cakes with a large, serrated bread knife so they are flat and even.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine powdered sugar, cream cheese, butter, and vanilla extract in a large mixing bowl. Whip until there are absolutely no lumps; this may take up to 5 minutes. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 tier of cake onto a serving plate. Frost the top liberally; place second tier on top. Generously frost the entire cake. Stick almonds gently by the handful onto the sides of the cake.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
