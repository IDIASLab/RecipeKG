        "name": "Simple Instant Pot\u00ae Mashed Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fcf-images.us-east-1.prod.boltdns.net%2Fv1%2Fstatic%2F1033249144001%2F849b6511-3079-4b67-a92b-750c8afe840c%2F8c797645-994d-4421-8410-63edb144d73a%2F1280x720%2Fmatch%2Fimage.jpg",
            "width": null,
