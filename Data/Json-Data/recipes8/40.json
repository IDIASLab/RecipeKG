    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273826/gingersnap-cream-cookie-cups/",
        "name": "Gingersnap-Cream Cookie Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gingersnap-Cream Cookie Cups"
        },
        "datePublished": "2020-06-19T04:37:45.000Z",
        "description": "These cute cookie cups are so pretty and tasty that no one will even suspect that they contain whole wheat flour! Topped off with a smooth cream cheese filling, these cookies are sure to impress!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "36 cookie cups",
        "recipeIngredient": [
            "1 cup canola oil",
            "\u00bd cup molasses",
            "2 eggs",
            "2 cups whole wheat flour",
            "2 cups all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "1 tablespoon ground ginger",
            "2 teaspoons baking soda",
            "2 teaspoons ground cinnamon",
            "1 teaspoon salt",
            "1 (8 ounce) package Neufchatel cheese, softened",
            "1 cup confectioners' sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1 cup heavy cream, whipped",
            "1 small bunch fresh mint leaves, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease 36 mini muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oil, molasses, and eggs in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine whole wheat flour, all-purpose flour, sugar, ginger, baking soda, cinnamon, and salt in a separate bowl. Stir into egg mixture until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll cookie dough into 1 1/4-inch balls. Place balls into the prepared muffin cups. Press dough up the sides of the cups using your fingers or a wooden spoon handle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until no longer doughy but still soft and edges are set, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven. Gently reform the holes in each cookie cup using a wooden spoon handle. Let cool for 2 minutes. Remove cookie cups from the pans and transfer to wire racks to cool completely, 15 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat Neufchatel cheese in a medium bowl using an electric mixer until fluffy. Beat in confectioners' sugar and vanilla extract. Gently fold in whipped cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon filling into a pastry bag; pipe into each cooled cookie cup. Garnish each cookie cup with a mint sprig.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Gingersnap Recipes"
        ],
