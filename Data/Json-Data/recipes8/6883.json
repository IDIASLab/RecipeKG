    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281665/apricot-jam-bars/",
        "name": "Apricot Jam Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8574948.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-09-22T03:27:11.000Z",
        "description": "Delicious, simple jam bars are made with a bit of almond flour for a nutty flavour and rustic texture.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 stick butter",
            "\u00bd cup white sugar",
            "1 large egg",
            "\u00bd teaspoon almond extract",
            "\u00bd teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "\u00bd cup almond flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "1\u2009\u00bc cups apricot jam, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Line an 8x8-inch baking dish with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and caster sugar together in a bowl until smooth; beat in egg, almond extract, and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, almond flour, baking powder, and salt together in a bowl. Stir flour mixture into butter mixture until a smooth dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press 1/2 of the dough into the prepared baking dish. Spread apricot jam evenly over the dough. Drop remaining dough in small spoonfuls evenly over the jam.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, about 25 minutes. Allow to cool before cutting into squares and serving.\n"
            }
        ],
