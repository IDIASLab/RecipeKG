        "recipeIngredient": [
            "nonstick cooking spray",
            "3 tablespoons all-purpose flour",
            "3 tablespoons powdered sugar",
            "2 tablespoons unsweetened cocoa powder",
            "\u00bc teaspoon baking powder",
            "1 pinch salt",
            "3 tablespoons milk",
            "1 tablespoon vegetable oil",
            "4 drops chocolate extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray a regular size mug with nonstick spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add flour, powdered sugar, cocoa powder, a little less than &frac14; teaspoon baking powder, and a pinch of salt to the mug. Mix everything well using a fork. Add milk, oil, and chocolate essence to taste. Stir until just smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave until firm, about 1 minute 30 seconds. Let sit for a few minutes to cool down before eating.\n"
            }
