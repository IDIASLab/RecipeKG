    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280525/homemade-hawaiian-buns/",
        "name": "Homemade Hawaiian Buns",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Homemade Hawaiian Buns"
        },
        "datePublished": "2020-06-20T03:46:45.000Z",
        "description": "Delicious, homemade pineapple buns, similar to King's Hawaiian&reg; Bread.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT2H55M",
        "recipeYield": "36 buns",
        "recipeIngredient": [
            "3 large eggs",
            "2 cups pineapple juice, at room temperature",
            "\u00be cup white sugar",
            "\u00be cup unsalted butter, melted, divided",
            "1 teaspoon vanilla extract",
            "6\u2009\u00bd cups all-purpose flour, divided, or more as needed",
            "2 (.25 ounce) packages instant yeast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a bowl. Add pineapple juice, sugar, 1/2 cup butter, and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Measure 3 cups of flour into a large bowl and stir in egg mixture with a wooden spoon until well combined. Sprinkle in yeast packages one at a time, mixing well. Add 3 more cups flour and mix well, using your hands if needed. Make sure all flour is well incorporated; the dough should be tacky to the touch, but not overly sticky. If dough is too wet, mix in a little extra flour, 1 tablespoon at a time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover dough with a clean towel and set in a warm place to rise for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove dough from the bowl and knead in remaining 1/2 cup flour, approximately 10 to 15 times, making sure flour is well incorporated. Form dough into rolls and place on buttered pans. Cover and place in a warm area to rise until doubled in size, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops are golden brown, 25 to 30 minutes. Brush tops with remaining 1/4 cup melted butter.\n"
            }
        ],
