    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277275/filipino-pandesal/",
        "name": "Filipino Pandesal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9307063.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:43:09.000Z",
        "description": "A favorite Filipino breakfast roll, this pandesal is a bit sweet and crusted with fine bread crumbs before baking.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT3H45M",
        "recipeYield": "24 rolls",
        "recipeIngredient": [
            "1 teaspoon active dry yeast",
            "1 cup lukewarm water",
            "1 teaspoon white sugar",
            "\u00bc cup white sugar",
            "\u215b cup vegetable oil",
            "1 teaspoon fine salt",
            "3 cups all-purpose flour, divided",
            "\u215b cup fine bread crumbs, divided, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour yeast into water. Add 1 teaspoon sugar and set aside to soften, about 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1/4 cup sugar, oil, and salt together in a bowl. Add softened yeast mixture and 1/2 of the flour. Mix well. Add remaining flour little by little, mixing constantly, until dough does not stick to the sides of the mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer dough to a lightly floured work surface and knead until smooth. Form into a ball and place in a lightly greased bowl. Brush surface of dough with more vegetable oil and cover with a damp cloth. Let rise in a warm place until doubled in size, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Punch dough down and roll into 2-inch rounds. Cover rolls with 1/2 of the fine bread crumbs and let rise again, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut rounds into 1 1/2-inch pieces and arrange on a slightly greased baking sheet, cut-side up. Sprinkle with remaining bread crumbs and let rise until doubled in size, about 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until slightly brown, 20 to 25 minutes.\n"
            }
        ],
