    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281903/rich-mushroom-stuffing/",
        "name": "Rich Mushroom Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rich Mushroom Stuffing"
        },
        "datePublished": "2012-12-19T02:32:58.000Z",
        "description": "Someone passed me this recipe years ago, but I've just started to use it. It's a vegetarian alternative to a meat-based stuffing. Rich and light.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P1DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (1 pound) loaf egg bread",
            "10 sprigs fresh thyme, stemmed",
            "3 sprigs fresh rosemary, stemmed ",
            "6 tablespoons unsalted butter",
            "2 cups diced onion",
            "2 cups celery, diced",
            "2 cups finely chopped portobello mushrooms",
            "3 cups vegetable broth",
            "\u00bc cup unsalted butter, melted",
            "1 teaspoon salt, or more to taste ",
            "1 teaspoon ground black pepper, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Tear bread into 1-inch chunks. Place in a bowl and leave out until stale, 1 to 2 days.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop thyme and rosemary leaves; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 6 tablespoons butter in a large, heavy saute pan over medium heat. Add onion and saute until soft, 3 to 4 minutes. Add onion, celery, mushrooms, and thyme and rosemary leaves; cook until mixture starts to soften, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer vegetable mixture to a large bowl and add bread, broth, melted butter, salt, and pepper. Taste and adjust seasonings. Press into a suitable baking dish. Cover with buttered parchment paper, then aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 45 to 55 minutes, removing foil and parchment paper for the last 10 minutes.\n"
            }
        ],
