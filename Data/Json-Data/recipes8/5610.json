        "recipeIngredient": [
            "1 cup olive oil",
            "\u2154 cup ketchup",
            "\u00bd cup white sugar",
            "\u00bd cup white vinegar",
            "2 tablespoons finely chopped onion",
            "1 tablespoon lemon juice",
            "1 teaspoon dry mustard",
            "1 teaspoon paprika",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper"
