        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the smoker to 250 degrees F (120 degrees C). Add wood chips according to manufacturer's directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place asparagus inside a disposable foil pan. Drizzle oil, salt, and pepper over the top. Toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the pan on the smoker rack and smoke for 30 minutes.\n"
            }
