    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276535/classic-gingerbread-cookies/",
        "name": "Classic Gingerbread Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Classic Gingerbread Cookies"
        },
        "datePublished": "2020-06-19T04:42:01.000Z",
        "description": "These classic gingerbread cookies are a holiday staple.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "30 cookies",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup butter",
            "\u2153 cup molasses",
            "1 egg",
            "2 cups all-purpose flour",
            "1\u2009\u00bd teaspoons ground ginger",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground cloves",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 3 cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter together in a bowl using an electric mixer until fluffy. Beat in molasses and egg until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, ginger, cinnamon, cloves, and salt together in a separate bowl. Stir into butter mixture until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into teaspoon-sized balls and place 2 inches apart onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 1 sheet at a time in the preheated oven until edges of cookies are firm, 10 to 12 minutes. Let cool for 5 minutes before transferring to wire racks to cool completely.\n"
            }
        ],
