    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281084/lemon-pie-without-condensed-milk/",
        "name": "Lemon Pie without Condensed Milk",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon Pie without Condensed Milk"
        },
        "datePublished": "2013-01-02T03:57:42.000Z",
        "description": "Easy lemon pie to transport, made without condensed milk.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) unbaked pie crust",
            "1 (8 ounce) package cream cheese, softened",
            "1 cup milk",
            "1 (3.4 ounce) package instant lemon pudding mix",
            "\u00bd cup lemon pie filling",
            "2\u2009\u00bc cups frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Place pie crust into a 9-inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 9 to 11 minutes. Remove from the oven and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While crust cools, beat cream cheese in a medium bowl with an electric mixer until creamy, about 30 seconds. Gradually beat in milk until well blended. Add pudding mix and beat for 30 seconds. Beat in lemon pie filling on low speed until blended. Fold in whipped topping. Spoon mixture into the cooled pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for at least 2 hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes",
            "Pudding Pie Recipes"
        ],
