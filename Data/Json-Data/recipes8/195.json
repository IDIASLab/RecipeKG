    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274258/pumpkin-cranberry-scones/",
        "name": "Pumpkin-Cranberry Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1067972.jpg",
            "width": 1280,
            "height": 1280
        },
        "datePublished": "2020-06-19T04:38:10.000Z",
        "description": "All the goodness of fall in one treat! Baking these pumpkin-cranberry scones will make your home smell so delightful.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "10 scones",
        "recipeIngredient": [
            "2 cups unbleached all-purpose flour",
            "\u2153 cup lightly packed brown sugar",
            "2 tablespoons white sugar",
            "1 tablespoon baking powder",
            "\u00be teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon salt",
            "6 tablespoons unsalted butter",
            "\u00bd cup pumpkin puree",
            "\u00bd cup coarsely chopped fresh cranberries ",
            "\u2153 cup sour cream",
            "3 tablespoons milk",
            "1 teaspoon vanilla extract",
            "\u00bd cup powdered sugar",
            "1 tablespoon milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, brown sugar, white sugar, baking powder, cinnamon, ginger, baking soda, and salt together in a bowl. Cut in butter using a pastry cutter until the mixture looks like oatmeal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add pumpkin, cranberries, sour cream, 3 tablespoons milk, and vanilla extract to the flour mixture and gently combine using a fork. Roll dough out onto a floured surface and form a rectangle. Cut into 10 triangles or squares and place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine powdered sugar and milk in a small bowl until smooth. Set glaze aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove scones from the oven and let cool 10 minutes before drizzling glaze on top. Serve warm.\n"
            }
        ],
