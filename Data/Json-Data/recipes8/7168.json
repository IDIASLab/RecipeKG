    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281715/pumpkin-harvest-bars/",
        "name": "Pumpkin Harvest Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Harvest Bars"
        },
        "datePublished": "2008-06-25T15:44:22.000Z",
        "description": "Lovely fall flavors! This recipe for pumpkin bars made with applesauce will have you thinking of autumn all year long!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "24 bars",
        "recipeIngredient": [
            "nonstick cooking spray",
            "1\u2009\u00be cups whole wheat flour",
            "2 teaspoons baking powder",
            "1 teaspoon grated orange zest",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon salt",
            "\u00be cup white sugar",
            "\u00bd cup pumpkin puree",
            "\u00bd cup applesauce",
            "2 tablespoons vegetable oil",
            "1 large egg",
            "1 large egg white",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a 9x13-inch baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, orange zest, cinnamon, nutmeg, and salt in a small bowl; stir gently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar, pumpkin puree, applesauce, vegetable oil, egg, and egg white together in a large bowl. Add flour mixture and stir until well combined and smooth. Stir in raisins. Spread batter in the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 25 to 30 minutes. Let cool for 15 minutes before cutting into 24 bars.\n"
            }
        ],
