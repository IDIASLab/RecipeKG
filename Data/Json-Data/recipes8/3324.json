    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278844/steak-flautas/",
        "name": "Steak Flautas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7807936.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-23T21:53:22.000Z",
        "description": "This is an adaptation of Mad Hungry with Lucinda Scala Quinn. These steak flautas are crispy and delicious with Spanish rice and guacamole!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "12 flautas",
        "recipeIngredient": [
            "1 pound beef round steak, cut into thin strips",
            "1 pinch ground cumin, or to taste",
            "1 pinch chili powder, or to taste",
            "1 pinch garlic powder, or to taste",
            "1 pinch onion powder, or to taste",
            "salt to taste",
            "3 tablespoons vegetable oil, divided",
            "1 onion, thinly sliced",
            "1 bell pepper, thinly sliced",
            "nonstick cooking spray",
            "12 corn tortillas",
            "6 ounces grated Monterey Jack cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season steak with cumin, chili powder, garlic powder, onion powder, and salt. Set aside and allow to marinate for about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon of oil in a pan over medium-high heat. Saute onion and bell pepper in the hot oil until they are soft, 5 to 7 minutes. Transfer vegetables to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a large baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat another tablespoon of oil in the same pan over medium-high heat. Saute the steak until no longer pink, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 6 tortillas on the prepared baking sheet and add a handful of Monterey Jack cheese to each.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the cheese melts, 2 to 3 minutes. Remove from the oven and add veggies and meat to each tortilla. Roll up and turn seam side-down. Brush with remaining vegetable oil. Return to the oven and bake until tortillas crisp up, 8 to 10 minutes. Repeat with remaining tortillas.\n"
            }
        ],
