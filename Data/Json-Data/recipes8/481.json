    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274804/apple-scones-with-oats-and-maple-syrup/",
        "name": "Apple Scones with Oats and Maple Syrup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F364233.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T04:39:31.000Z",
        "description": "Delicious oat-apple scones with a hint of cinnamon and maple. A true breakfast treat.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 scones",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 cup finely ground oats",
            "1 tablespoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground cinnamon",
            "3 tablespoons cold butter",
            "2 medium apples, peeled and shredded",
            "\u00bc cup half-and-half",
            "1 large egg, lightly beaten",
            "2 tablespoons vanilla-flavored maple syrup",
            "\u00bc cup white sugar",
            "\u00bc teaspoon ground cinnamon",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Lightly grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, oats, baking powder, salt, and cinnamon together in a large bowl. Cut in cold butter with a pastry blender. Add shredded apples and mix to distribute. Stir in half-and-half, egg, and maple syrup. Mix until dough holds its shape but is still fairly sticky; do not overmix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out onto a floured board and pat into a circle about 1/2-inch thick. Mix sugar and cinnamon together in a small bowl. Brush the top of the dough with melted butter and sprinkle with cinnamon-sugar. Cut into 12 wedges and place on baking sheets at least 1 inch apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 13 to 15 minutes.\n"
            }
        ],
