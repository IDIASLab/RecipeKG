        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (12 ounce) package flour tortillas",
            "1 drizzle olive oil",
            "1 pinch salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut tortillas into 6 wedges each. Place on a baking sheet and drizzle with olive oil. Season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and crisp, about 5 minutes.\n"
            }
