    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277938/banana-muffins-with-chocolate-chips/",
        "name": "Banana Muffins with Chocolate Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F959344.jpg",
            "width": 2240,
            "height": 2240
        },
        "datePublished": "2020-06-19T04:43:56.000Z",
        "description": "This muffin is very moist and fluffy with a good banana flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 eggs",
            "1 cup white sugar",
            "3 medium ripe bananas, mashed",
            "\u2153 cup oil",
            "1 teaspoon vanilla extract",
            "\u00bd cup chocolate chips",
            "\u00bd cup yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, baking soda, and salt in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the eggs with an electric mixer until foamy. Add sugar; beat until fluffy. Continue beating while adding mashed bananas, oil, and vanilla extract.  Add flour mixture in 2 batches, alternating with yogurt, beating batter briefly after each addition, and until creamy following last flour addition. Stir in chocolate chips. Fill the prepared muffin cups to the top with batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, 22 to 25 minutes. Cool in the tin for 5 minutes. Transfer to a wire rack to cool completely.\n"
            }
        ],
