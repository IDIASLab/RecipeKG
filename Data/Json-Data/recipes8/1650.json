    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278160/chocolate-ganache-cupcakes/",
        "name": "Chocolate Ganache Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Ganache Cupcakes"
        },
        "datePublished": "2006-10-05T17:15:28.000Z",
        "description": "Delicious fudgy cupcakes dipped in chocolate ganache.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup unsalted butter, at room temperature",
            "4 large eggs, at room temperature",
            "1 (16 ounce) can chocolate syrup",
            "1 tablespoon vanilla extract",
            "1 cup all-purpose flour",
            "\u00bd cup semisweet chocolate chips",
            "1 cup semisweet chocolate chips",
            "\u00bd cup heavy cream",
            "\u00bd teaspoon instant coffee granules"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar and butter in a large bowl; beat with an electric mixer until light and fluffy. Add eggs, 1 at a time, beating briefly after each addition. Mix in chocolate syrup and vanilla extract. Add flour; mix until combined. Stir in chocolate chips. Scoop batter into the prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, about 30 minutes; do not overbake. Cool in the tin for 5 minutes. Transfer to a wire rack to cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cupcakes are cooling, combine chocolate chips, heavy cream, and instant coffee granules for the ganache in a microwave-safe bowl. Microwave at 30 second intervals, stirring between, until melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip the top of each cupcake in the ganache. Do not refrigerate.\n"
            }
        ],
