    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274940/chocolate-orange-cupcakes/",
        "name": "Chocolate-Orange Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate-Orange Cupcakes"
        },
        "datePublished": "2020-06-19T04:39:36.000Z",
        "description": "Chocolate and orange are a natural together. This is a really easy recipe that will wow your guests.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "\u00bd cup all-purpose flour",
            "\u00bd cup white sugar",
            "5 tablespoons white sugar, divided",
            "1 teaspoon baking powder",
            "3 tablespoons unsalted butter",
            "\u00bd cup milk",
            "2 ounces dark chocolate, melted",
            "1 egg",
            "1 orange, juiced",
            "3 tablespoons white sugar",
            "\u00bd cup unsalted butter, softened",
            "1 tablespoon unsalted butter, softened",
            "9 ounces confectioners' sugar",
            "2 tablespoons milk, or more as needed",
            "2 ounces white chocolate, melted",
            "1 orange, zested",
            "4 ounces orange-flavored chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, 1/2 cup plus 2 tablespoons sugar, and baking powder together in a food processor. Add butter and pulse until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk, melted dark chocolate, and egg together in a measuring cup. Stir chocolate mixture into the flour mixture until just combined. Spoon batter into the prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of a cupcake comes out clean, 15 to 20 minutes. Remove from the oven and let cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat orange juice and remaining 3 tablespoons sugar together over low heat until sugar has dissolved, 3 to 4 minutes. Carefully spoon orange juice mixture over warm cupcakes and set aside to cool completely, 15 to 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1/2 cup plus 1 tablespoon butter together in a bowl using an electric mixer until light and fluffy. Carefully stir in confectioners' sugar and continue to beat for 5 minutes. Beat in milk, melted white chocolate, and orange zest.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Decorate cooled cupcakes with icing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut orange-flavored chocolate into shavings using a sharp knife or a vegetable peeler and use to decorate cupcakes.\n"
            }
        ],
