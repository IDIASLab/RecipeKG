    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274808/espresso-ice-cream/",
        "name": "Espresso Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Espresso Ice Cream"
        },
        "datePublished": "2020-06-19T04:39:31.000Z",
        "description": "I love coffee and ice cream, so this espresso ice cream recipe combines the best of both worlds.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT5H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bc cup white sugar",
            "2 tablespoons butter, melted",
            "1 tablespoon cornstarch",
            "1 tablespoon instant espresso powder",
            "1 cup milk",
            "1 teaspoon vanilla extract",
            "1 (14 ounce) can sweetened condensed milk",
            "3 tablespoons coffee-flavored liqueur (such as Kahlua\u00ae)",
            "2 cups whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar, butter, cornstarch, and espresso powder together in a saucepan until blended. Stir in milk. Bring to a boil over medium heat; cook and stir until thickened, about 2 minutes. Remove mixture from heat; stir in vanilla extract and let cool completely, 20 to 30 minutes. Stir in condensed milk and coffee-flavored liqueur.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat whipping cream using an electric mixer in a mixing bowl until stiff peaks form; fold into cooled milk mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into an ice cream maker and freeze according to manufacturer's instructions, about 20 minutes. Transfer to an airtight container and freeze until firm, about 4 hours.\n"
            }
        ],
