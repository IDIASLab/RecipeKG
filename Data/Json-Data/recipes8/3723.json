    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280447/frozen-blueberry-crisp/",
        "name": "Frozen Blueberry Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8269004.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-07-02T02:48:14.000Z",
        "description": "This recipe uses frozen blueberries and is quick and easy to prepare. Serve it warm over vanilla ice cream for a light, delicious summer dessert.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 8x8-inch crisp",
        "recipeIngredient": [
            "2 (16 ounce) packages frozen blueberries, thawed",
            "\u00bc cup white sugar",
            "2 teaspoons fresh lemon juice",
            "\u00be cup rolled oats",
            "\u2153 cup all-purpose flour",
            "\u2153 cup brown sugar",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bc cup unsalted butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease the bottom and halfway up the sides of an 8x8-inch glass baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain thawed blueberries and put in a medium mixing bowl. Sprinkle with white sugar and lemon juice and gently combine using your hands. Layer berries evenly in the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, flour, brown sugar, cinnamon, and nutmeg together in a separate bowl. Cut in butter until mixture is crumbly and cover the berries with it.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 40 to 45 minutes. Serve warm.\n"
            }
        ],
