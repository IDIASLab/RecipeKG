        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274903/pumpkin-spice-cookies-with-brown-sugar-frosting/",
        "name": "Pumpkin Spice Cookies with Brown Sugar Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Spice Cookies with Brown Sugar Frosting"
        },
        "datePublished": "2020-06-19T04:39:34.000Z",
        "description": "Light cookies that satisfy your pumpkin spice tastes!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup pumpkin puree",
            "\u00bd cup butter, softened",
            "1 egg",
            "1 teaspoon vanilla extract",
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00be cup packed brown sugar",
            "\u00bc cup butter",
            "1\u2009\u00bc cups powdered sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine white sugar, pumpkin, butter, egg, and vanilla extract in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, and baking soda in a separate bowl. Add to pumpkin mixture until a soft dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by heaping teaspoonfuls 2 inches apart onto ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 10 to 12 minutes. Remove from the oven and let cool completely, 15 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat brown sugar and butter in a saucepan over medium-low heat until smooth, about 5 minutes. Remove from heat and stir in powdered sugar and vanilla extract. Spread frosting over cooled cookies.\n"
            }
        ],
