        "recipeIngredient": [
            "1 (750 milliliter) bottle Spanish white wine",
            "\u2153 cup brandy",
            "\u00bc cup berry-flavored schnapps",
            "1 pint fresh strawberries",
            "1 pint fresh raspberries",
            "2 fresh peaches",
            "\u00bd cup raspberry-flavored seltzer",
            "ice as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix wine, brandy, and schnapps together in a large pitcher.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice strawberries, raspberries, and peaches into medium chunks. Add to the wine mixture. Chill for a minimum of 3 to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in seltzer just prior to serving. Serve over ice with fruit added from the pitcher.\n"
            }
