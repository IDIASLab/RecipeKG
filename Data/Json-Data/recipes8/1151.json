    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276734/frosted-lemon-sugar-cookies/",
        "name": "Frosted Lemon Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9366672.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:42:31.000Z",
        "description": "I just find the sugar cookie recipes a bit too rich for my taste, so I modified them by adding some lemon.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 cup sugar",
            "\u00bd cup vegetable oil",
            "2 tablespoons lemon juice",
            "1 teaspoon vanilla extract",
            "1 egg, lightly beaten",
            "2 cups flour",
            "\u00bd teaspoon cream of tartar",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bc cup white sugar, or as needed",
            "\u00bd cup powdered sugar",
            "1 tablespoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter in a bowl with an electric mixer until creamy. Add 1 cup sugar, vegetables oil, lemon juice, and vanilla extract; mix to combine. Add egg and mix until thoroughly incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together flour, cream of tartar, baking soda, and salt in a bowl. Add flour mixture to the butter mixture; mix until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1/4 cup sugar into a small bowl. Form dough into 1-inch balls. Roll each ball into sugar to coat and place on an ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 10 minutes. Remove from oven, transfer to a wire rack and cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together powdered sugar and lemon juice for the frosting in a small bowl until smooth. Pour frosting over cookies and allow to harden for at least 5 minutes before serving.\n"
            }
        ],
