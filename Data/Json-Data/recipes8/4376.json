    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280993/honey-garlic-shrimp-and-pineapple-skewers/",
        "name": "Honey-Garlic Shrimp and Pineapple Skewers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8335993.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-07-16T22:20:18.000Z",
        "description": "Nothing says summer like shrimp on the grill. This quick recipe will have dinner on the table in under 30 minutes. Serve over rice, pasta, or a salad.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "2 skewers",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 tablespoons raw honey",
            "1 tablespoon fresh lemon juice",
            "1 tablespoon pineapple juice",
            "1 large clove garlic, minced",
            "2 teaspoons low-sodium soy sauce (such as Bragg\u00ae)",
            "1 pinch red pepper flakes",
            "4 ounces large shrimp, peeled and deveined",
            "8 (1 inch) cubes fresh pineapple",
            "8 (1 inch) pieces red bell pepper",
            "2 each bamboo skewers, soaked in water for 20 minutes",
            "1  serving cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine butter, honey, lemon juice, pineapple juice, garlic, soy sauce, and red pepper flakes in a small saucepan over medium-low heat. Simmer until sauce begins to thicken, 3 to 4 minutes. Set aside 1/2 of the sauce for dipping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Thread pineapple, shrimp, and bell pepper alternately onto both skewers and baste with the remaining sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat (400 degrees F (200 degrees C)) and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill until shrimp are cooked through and opaque in color, turning halfway, 5 to 7 minutes. Serve with dipping sauce.\n"
            }
        ],
