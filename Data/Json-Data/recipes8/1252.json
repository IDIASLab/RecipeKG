    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277060/coconut-lime-cupcakes/",
        "name": "Coconut-Lime Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1115933.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T04:42:58.000Z",
        "description": "This is a combination/variation on my coconut and lemon cake recipes. It goes great with mango-lime filling and coconut cream cheese frosting.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "36 cupcakes",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup butter",
            "4 eggs",
            "1 tablespoon coconut extract",
            "1 tablespoon vanilla extract",
            "3 cups all-purpose flour",
            "1 tablespoon baking powder",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon ground nutmeg",
            "1 cup buttermilk",
            "\u00bd cup fresh lime juice",
            "2 cups sweetened shredded coconut",
            "2 tablespoons freshly grated lime zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 36 cupcake cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter together in the bowl of an electric stand mixer until fluffy, about 10 minutes. Beat in eggs, 1 at a time, adding coconut extract and vanilla extract with the last egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, 1 tablespoon plus 1/2 teaspoon baking powder, and nutmeg in another bowl. Beat into the egg mixture until thoroughly combined; beat in buttermilk and lime juice. Stir in coconut and lime zest.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill each of the prepared cupcake cups with about 1/4 cup of batter using a large cookie scoop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of a cupcake comes out clean, 20 to 25 minutes. Remove from the oven and let cool before serving.\n"
            }
        ],
