        "mainEntityOfPage": "https://www.allrecipes.com/recipe/214966/watermelon-and-tomato-salad/",
        "name": "Watermelon and Tomato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F572606.jpg",
