    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273808/glazed-lemon-cookies/",
        "name": "Glazed Lemon Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Glazed Lemon Cookies"
        },
        "datePublished": "2020-06-19T04:37:40.000Z",
        "description": "Delicious shortbread-like cookies with a sweet-tart lemon glaze! These are a favorite of everyone in my family!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "\u00be cup butter",
            "\u00be cup white sugar",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "1 teaspoon lemon zest",
            "\u00bc teaspoon salt",
            "1 cup icing sugar",
            "2 tablespoons fresh lemon juice",
            "1 teaspoon lemon zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter and sugar in a large bowl; beat with an electric mixer until creamy. Beat in egg and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, lemon zest, and salt in a separate bowl. Pour over butter mixture; beat until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out on a lightly floured surface and cut into cookies. Arrange on the lined baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set and bottoms are golden brown, 12 to 15 minutes. Allow cookies to cool, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cookies are cooling, mix icing sugar, lemon juice, and lemon zest together in a small bowl. Spread glaze over the cookies.\n"
            }
        ],
