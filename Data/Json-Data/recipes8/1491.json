    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277709/rhapple-pie/",
        "name": "Rhapple Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F292227.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-01-08T14:32:23.000Z",
        "description": "Delicious sweet and tart combination of apples and rhubarb with a wonderful spicy crumb topping flavored with Chinese five-spice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) refrigerated unbaked pie crust ",
            "4 each apples, peeled and thinly sliced",
            "2 cups 1/2-inch-thick slices trimmed rhubarb",
            "\u00be cup white sugar",
            "3 tablespoons all-purpose flour",
            "1 teaspoon all-purpose flour",
            "2 teaspoons Chinese five-spice powder",
            "\u00be cup unsifted all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd cup quick-cooking oats",
            "\u00bc teaspoon salt",
            "1 teaspoon Chinese five-spice powder",
            "\u00bd cup butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place rack in lowest position in the oven. Preheat the oven to 375 degrees F (190 degrees C). Line a 9-inch pie plate with pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples and rhubarb in a large bowl. Toss with sugar, 3 tablespoons plus 1 teaspoon flour, and Chinese five-spice. Pour filling over the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine unsifted flour, sugar, oats, salt, and five-spice together in a large bowl. Cut in butter until crumbly. Scatter crumb topping over the pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping is golden brown and filling is bubbly, 50 to 60 minutes. Let cool completely before serving.\n"
            }
        ],
