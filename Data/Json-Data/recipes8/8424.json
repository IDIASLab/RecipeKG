        "name": "Fresh Tomato and Cucumber Salad with Buttery Garlic Croutons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8717692.jpg",
            "width": 0,
