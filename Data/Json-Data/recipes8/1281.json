        "recipeYield": "2 cups",
        "recipeIngredient": [
            "\u00bd cup fresh blueberries",
            "\u00bd cup fresh strawberries, stemmed",
            "\u00bd cup fresh raspberries",
            "1 teaspoon white sugar",
            "\u00bc cup balsamic vinegar",
            "\u00bc cup extra-virgin olive oil",
            "2 tablespoons Dijon mustard",
            "2 tablespoons lemon juice",
            "2 tablespoons honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Muddle blueberries, strawberries, and raspberries together with sugar in a bowl until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into a blender and add balsamic vinegar, oil, Dijon, lemon juice, and honey. Blend until smooth.\n"
            }
