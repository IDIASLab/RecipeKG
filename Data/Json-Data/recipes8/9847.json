        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284502/sparkling-rum-runner/",
        "name": "Sparkling Rum Runner",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9214633.jpg",
            "width": 0,
