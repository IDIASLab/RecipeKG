    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280707/school-cafeteria-hot-rolls/",
        "name": "School Cafeteria Hot Rolls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9084710.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-27T05:34:21.000Z",
        "description": "Recipe for hot rolls (from school food service 1968-1990).",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT2H55M",
        "recipeYield": "42 rolls",
        "recipeIngredient": [
            "2 (.25 ounce) packages active dry yeast",
            "2 cups warm water",
            "6\u2009\u00bd cups all-purpose flour",
            "\u00bd cup white sugar",
            "1 teaspoon salt",
            "\u00bd cup melted shortening",
            "2 large eggs",
            "4 tablespoons unsalted butter, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve yeast in warm water in a mixing bowl. Let stand until yeast is frothy, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 2 cups flour, sugar, and salt to the yeast mixture. Beat with an electric mixer for 2 minutes. Add shortening and eggs; beat for 2 minutes more. Slowly add remaining flour and mix well. Let dough rise in a warm place, undisturbed, until doubled in size, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Punch down dough and divide into 42 pieces. Shape each piece into a ball and arrange on a rimmed baking sheet. Let rolls rise in a warm spot, undisturbed, until doubled in size, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 25 to 30 minutes. Remove from the oven and gently brush tops with butter.\n"
            }
        ],
