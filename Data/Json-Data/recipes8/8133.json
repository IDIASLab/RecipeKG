    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282126/sumneys-deer-jerky/",
        "name": "Sumney's Deer Jerky",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sumney's Deer Jerky"
        },
        "datePublished": "2011-11-13T21:10:29.000Z",
        "description": "Deer jerky with a very flavorful marinade using A1 Bold and Spicy Steak Sauce&reg;.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT4H0M",
        "totalTime": "P0DT12H10M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4 pounds venison (deer meat)",
            "\u2154 cup low-sodium soy sauce",
            "\u2154 cup Worcestershire sauce",
            "\u00bd cup steak sauce (such as A1\u00ae)",
            "1 tablespoon seasoned salt, or more to taste",
            "1 tablespoon onion powder, or more to taste",
            "1 tablespoon coarsely ground black pepper, or more to taste",
            "2 teaspoons flavor enhancer (such as Accent\u00ae)",
            "2 teaspoons garlic powder, or more to taste",
            "2 teaspoons liquid smoke",
            "1 teaspoon hot pepper sauce (such as Tabasco\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice venison into 1/4 inch strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine soy sauce, Worcestershire sauce, steak sauce, seasoned salt, onion powder, pepper, flavor enhancer, garlic powder, and liquid smoke in a large glass or ceramic bowl; mix well. Add venison and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set a dehydrator to 155 degrees F (68 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove venison from the marinade and shake off excess. Discard the remaining marinade. Arrange venison on the dehydrator racks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in the dehydrator until desired dryness, 4 to 6 hours.\n"
            }
        ],
