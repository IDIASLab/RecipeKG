    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282452/beef-chili-no-beans/",
        "name": "Beef Chili (No Beans)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8895932.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-10-31T10:23:09.000Z",
        "description": "When the fall weather moves in, it's time to make this delicious chili. Serve over a piping hot sweet potato.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H20M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons coconut oil",
            "2 cups diced onion",
            "4 cloves garlic, minced",
            "2 pounds ground beef",
            "2 tablespoons chili powder",
            "2 tablespoons ground cumin",
            "1\u2009\u00bd tablespoons unsweetened cocoa powder",
            "1 teaspoon dried oregano",
            "1 teaspoon ground allspice",
            "1 teaspoon ancho chile powder",
            "1 teaspoon salt",
            "1 (6 ounce) can tomato paste",
            "2 cups beef broth",
            "1 (14.5 ounce) can fire-roasted diced tomatoes, undrained",
            "1 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large, deep pot over medium-high heat. Add coconut oil. When oil is shimmering, add onions. Cook and stir with a wooden spoon until slightly browned, about 5 minutes. Add garlic; cook until fragrant, about 30 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crumble ground beef into the pot. Stir to combine and cook until beef is no longer pink, 5 to 7 minutes. Add chili powder, cumin, cocoa powder, oregano, allspice, ancho chili powder, and salt; stir until well combined. Mix in tomato paste and simmer for 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add beef broth, diced tomatoes with juice, and water. Stir well and bring to a boil. Reduce heat to low and simmer for 2 hours.\n"
            }
        ],
