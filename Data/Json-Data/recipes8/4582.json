        "mainEntityOfPage": "https://www.allrecipes.com/recipe/146115/classic-crab-and-shrimp-salad/",
        "name": "Classic Crab and Shrimp Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7042773.jpg",
            "width": 3024,
