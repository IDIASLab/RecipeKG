    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282312/slow-cooker-hot-cocoa/",
        "name": "Slow Cooker Hot Cocoa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9004480.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2007-10-21T17:13:18.000Z",
        "description": "If your family is like mine and you have several slow cookers going Christmas Eve then this hot cocoa is perfect. Rich, creamy, and versatile.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT7H0M",
        "totalTime": "P0DT7H5M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "1 (12 ounce) package chocolate chips",
            "1 gallon milk",
            "8 fluid ounces heavy cream, or more to taste",
            "1 (16 ounce) package large marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set a 6-quart slow cooker on Low or Warm and add chocolate chips. Cook until melted, 1 to 2 hours, checking frequently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in milk and cream, about 1 cup at a time, and continue to cook, making sure mixture stays hot and melted until all the milk is used and incorporated, about 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in marshmallows and continue to cook on Low or Warm, stirring frequently, until all is smooth and marshmallows are melted, about 2 hours. Serve with a ladle.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Hot Chocolate Recipes"
        ],
