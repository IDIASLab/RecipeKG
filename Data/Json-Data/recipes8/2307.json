        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215570/dans-wpb-sandwich/",
        "name": "Dan's W.P.B. Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F657443.jpg",
            "width": 2048,
            "height": 1536
