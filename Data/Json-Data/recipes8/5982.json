        "mainEntityOfPage": "https://www.allrecipes.com/recipe/35304/pico-de-gallo/",
        "name": "Pico De Gallo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F377426.jpg",
