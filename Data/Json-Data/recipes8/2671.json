        "name": "Strawberry and Spinach Salad with Honey-Poppy Seed Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8230030.jpg",
            "width": 0,
            "height": 0
