    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282012/pineapple-curd/",
        "name": "Pineapple Curd",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8657150.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-10-22T02:24:37.000Z",
        "description": "Pineapple curd is a delicious alternative to lemon curd. Use it as a cake filling, tart topping, or fold in some whipped cream to make a fruit dip!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "1 1/3 cups",
        "recipeIngredient": [
            "1\u2009\u00bd cups fresh pineapple chunks",
            "2 large egg yolks",
            "2 tablespoons white sugar",
            "1 tablespoon lemon juice",
            "1 tablespoon cornstarch",
            "1 pinch salt",
            "2 tablespoons unsalted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine pineapple, egg yolks, sugar, lemon juice, cornstarch, and salt in a blender; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a saucepan and bring to a simmer over medium heat, whisking constantly. Cook and stir until thickened, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and whisk in butter until fully incorporated. Transfer to a small bowl, cover with plastic wrap, and chill until set, 2 to 3 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Pineapple Dessert Recipes"
        ],
