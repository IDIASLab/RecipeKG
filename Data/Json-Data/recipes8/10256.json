    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285561/nanny-rosalinds-cornbread-dressing/",
        "name": "Nanny Rosalind's Cornbread Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Nanny Rosalind's Cornbread Dressing"
        },
        "datePublished": "2021-09-22T00:41:48.000Z",
        "description": "This is my aunt's recipe for my grandmother's fabulous dressing. It has a spicy Louisiana kick.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 teaspoons salt",
            "1 teaspoon dried basil",
            "1 teaspoon dried oregano",
            "1 teaspoon ground dried thyme",
            "1 teaspoon ground white pepper",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon cayenne pepper",
            "\u00bd teaspoon onion powder",
            "1 pound ground pork",
            "\u00bd cup extra-virgin olive oil",
            "\u00be cup finely chopped onion",
            "\u00be cup finely chopped green bell pepper",
            "\u00bd cup finely chopped celery",
            "1 tablespoon minced garlic",
            "1 cup reduced sodium chicken broth",
            "2 large bay leaves",
            "5 cups crumbled cornbread",
            "1 tablespoon chopped fresh thyme, or more to taste",
            "1 tablespoon chopped fresh oregano, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together salt, dried basil, oregano, thyme, white pepper, black pepper, cayenne pepper, and onion powder in a small bowl for seasoning mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook pork in a 5- to 6-quart pot over medium-high heat, stirring to break up meat, until browned, 8 to 10 minutes. Transfer to paper towels; discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 2-quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add oil to the same pot and heat over medium-high heat. Add onion, bell pepper, celery, and garlic. Cook, stirring, until softened, about 5 minutes. Stir in seasoning mix. Add cooked pork, broth, and bay leaves; bring to a boil. Reduce heat and simmer, covered, for 20 minutes. Remove and discard bay leaves. Mix in cornbread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer dressing mixture to the prepared baking dish. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 minutes. Uncover; bake until lightly browned, about 10 minutes more. Garnish with fresh thyme and oregano.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Stuffing and Dressing Recipes",
            "Cornbread Stuffing and Dressing Recipes"
        ],
