        "mainEntityOfPage": "https://www.allrecipes.com/recipe/86919/sarahs-pasta-salad/",
        "name": "Sarah's Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F424257.jpg",
