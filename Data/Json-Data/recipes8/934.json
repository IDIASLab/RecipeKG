    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276138/apple-cider-cupcakes/",
        "name": "Apple Cider Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7072968.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:41:12.000Z",
        "description": "A simple, tasty fall treat--these cupcakes are easy to make and use basic pantry staples.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "\u2154 cup brown sugar",
            "\u00bd cup butter, melted",
            "\u2153 cup white sugar",
            "2 eggs",
            "1\u2009\u00bd cups all-purpose flour",
            "2 teaspoons ground cinnamon",
            "1 teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00be cup apple cider",
            "2 teaspoons vanilla extract",
            "4 ounces cream cheese, softened",
            "4 tablespoons butter, softened",
            "1\u2009\u00bd cups powdered sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together brown sugar, melted butter, and white sugar in a stand mixer until thoroughly combined. Add eggs and mix well. Add flour, cinnamon, baking soda, and salt; mix until combined. Add apple cider and vanilla extract and beat on high speed for 2 minutes. Divide batter evenly into the prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, about 20 minutes. Cool in the tin for 5 minutes. Transfer to a wire rack and let cool, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cupcakes are cooling, combine cream cheese and softened butter in the bowl of a stand mixer and beat until blended. Add powdered sugar and vanilla extract; beat until frosting is creamy. Top cooled cupcakes with frosting.\n"
            }
        ],
