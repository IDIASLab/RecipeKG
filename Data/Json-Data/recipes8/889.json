    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276035/chocolate-fruit-tart/",
        "name": "Chocolate Fruit Tart",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F612296.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:41:04.000Z",
        "description": "Chocolate shortbread tart with pastry cream and fresh fruit.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 tart",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bd cup white sugar",
            "4 tablespoons unsweetened cocoa powder",
            "1 pinch salt",
            "\u00bd cup butter",
            "3 tablespoons butter",
            "1 egg",
            "1 egg yolk",
            "2 cups milk",
            "2 eggs",
            "\u2153 cup white sugar",
            "6 tablespoons all-purpose flour",
            "1 (2-inch) strip of lemon zest",
            "7 pitted fresh cherries, or more to taste",
            "2 fresh peaches - peeled, pitted, and sliced",
            "2 large kiwis, peeled and sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, sugar, cocoa powder, and salt together in a bowl. Cut in 1/2 cup plus 3 tablespoons butter using a pastry cutter or 2 knives until mixture resembles coarse meal. Mix in egg and egg yolk until dough comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out the dough on a lightly floured work surface. Transfer to a 9-inch tart pan with a removable bottom.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until dry to the touch, about 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, prepare the pastry cream. Bring milk to a simmer in a medium saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs, sugar, and flour together in a large bowl until smooth. Whisk in hot milk, a little at a time, until custard is smooth. Pour custard back into the saucepan; add lemon zest. Cook, whisking constantly, until pastry cream thickens and comes to a boil, 5 to 7 minutes. Remove from heat, discard lemon zest, and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove tart crust from the oven and let cool to room temperature, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unmold crust and fill with cooled pastry cream. Pile cherries in the center. Arrange peach slices in a ring around the cherries. Arrange kiwi slices in a ring around the peaches.\n"
            }
        ],
