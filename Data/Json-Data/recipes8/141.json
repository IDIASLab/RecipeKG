    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274123/mini-banana-bread-loaves/",
        "name": "Mini Banana Bread Loaves",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mini Banana Bread Loaves"
        },
        "datePublished": "2020-06-19T04:37:57.000Z",
        "description": "Delicious recipe I created when I had no eggs but wanted banana loaf for my new mini loaf pans!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 mini loaves",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "\u00bd cup whole wheat flour",
            "\u00bc cup quick-cooking oats, or to taste",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "4 ripe bananas",
            "\u00bd cup white sugar",
            "\u00bd cup brown sugar",
            "\u00bc cup margarine",
            "\u00bc cup milk",
            "1 teaspoon vanilla extract",
            "\u00bd cup raisins, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flours, oats, baking soda, cinnamon, and salt in a medium bowl. Mash bananas into the dry ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugars and margarine in a large bowl; beat with an electric mixer until creamy. Mix in milk and vanilla extract. Add the flour mixture and raisins; mix well. Pour batter into 4 mini silicone loaf pans. Place loaf pans on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 25 to 30 minutes.\n"
            }
        ],
