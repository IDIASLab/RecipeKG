    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22941/confetti-corn/",
        "name": "Confetti Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F485818.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-19T02:43:31.000Z",
        "description": "My grandmother always had this for Thanksgiving, and it has been a family tradition for over 30 years.  We serve this flavorful, colorful baked mixture of sweet corn and croutons every year.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 tablespoons minced green bell pepper",
            "2 tablespoons minced red bell pepper",
            "1 (14.75 ounce) can creamed corn",
            "\u2154 cup seasoned croutons",
            "\u00bd cup milk",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease an 8x8 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan over medium heat, melt the butter. Slowly cook and stir the green bell pepper and red bell pepper until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the green bell pepper, red bell pepper, creamed corn, seasoned croutons, milk and ground black pepper in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered in the preheated oven 40 minutes, until bubbly and lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Corn"
        ],
