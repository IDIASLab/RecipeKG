    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277265/gluten-free-cranberry-walnut-muffins/",
        "name": "Gluten-Free Cranberry Walnut Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1147710.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:43:08.000Z",
        "description": "A delicious gluten-free muffin filled with dried cranberries, cinnamon, crunchy walnuts, and fragrant vanilla.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "18 muffins",
        "recipeIngredient": [
            "cooking spray",
            "\u00bc cup milk, at room temperature",
            "1 tablespoon white vinegar",
            "\u00bd cup brown sugar",
            "\u00bc cup white sugar",
            "\u00bd cup butter, softened",
            "2 eggs, at room temperature",
            "1 egg yolk, at room temperature",
            "1 tablespoon vanilla extract",
            "1\u2009\u00bd cups brown rice flour",
            "1 cup white rice flour",
            "\u00bd cup ground flax seeds",
            "4 tablespoons baking powder",
            "1 teaspoon xanthan gum",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "1 cup dried cranberries",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Spray 18 muffin cups with cooking spray or line with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place milk and vinegar in a small bowl; stir and set soured milk aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar, white sugar, and butter together in a bowl using an electric hand mixer or stand mixer until creamy and smooth. Add soured milk, eggs, egg yolk, and vanilla extract; beat until smooth, scraping down the sides of the bowl as needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown rice flour, white rice flour, flax seeds, baking powder, xanthan gum, cinnamon, and salt together in another bowl. Stir slowly into creamed butter mixture until just mixed. Stir in cranberries and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling each about 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 18 to 20 minutes.\n"
            }
        ],
