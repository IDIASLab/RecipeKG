    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281132/chocolate-coconut-meringue-cookies/",
        "name": "Chocolate-Coconut Meringue Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6124801.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-12-17T06:01:07.000Z",
        "description": "My grandmother used to make these cookies at Christmas time every year. This recipe is a family treasure!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "4 large egg whites",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon vanilla extract",
            "2 cups powdered sugar",
            "2 (6 ounce) packages chocolate chips",
            "4 cups cornflakes cereal",
            "2 cups sweetened flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Grease 3 to 4 cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites, salt, and vanilla extract in a bowl with an electric mixer until soft peaks form. Add powdered sugar and beat until peaks are stiff. Fold in chocolate chips, cornflakes, and coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by heaping teaspoonfuls onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 20 minutes. Cool slightly before removing from the cookie sheets.\n"
            }
        ],
