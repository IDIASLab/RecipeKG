        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14305/broccoli-salad-ii/",
        "name": "Broccoli Salad II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3375935.jpg",
            "width": 1334,
            "height": 1335
        },
        "datePublished": "1999-08-13T21:06:16.000Z",
        "description": "A tasteful salad which can be made early in the day and keeps well.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "5 cups fresh chopped broccoli",
            "1 pound bacon - cooked and crumbled",
            "1 cup raisins",
            "1 cup mayonnaise",
            "\u00bd cup white sugar",
            "2 tablespoons white wine vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the broccoli, bacon and raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the mayonnaise, sugar and vinegar. Pour dressing over broccoli mixture, toss and refrigerate.\n"
            }
        ],
