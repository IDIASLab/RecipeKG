    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277715/bourbon-bread-pudding/",
        "name": "Bourbon Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7775631.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2004-03-21T22:57:16.000Z",
        "description": "A sweet bourbon bread pudding for large get-togethers. My family requests that I bring this every holiday!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 9x13-inch bread pudding",
        "recipeIngredient": [
            "4 cups milk",
            "\u00bd cup butter",
            "1 cup white sugar",
            "4 large eggs, beaten slightly",
            "2 teaspoons ground cinnamon",
            "\u00bc teaspoon salt",
            "12 cups dry bread cubes",
            "\u00bd cup raisins",
            "\u00bc cup finely chopped hazelnuts",
            "2 cups packed brown sugar",
            "1 cup butter",
            "\u00bd cup bourbon whiskey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat milk and 1/2 cup butter in a saucepan over medium heat until butter is completely melted, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar, eggs, cinnamon, and salt together in a large mixing bowl with a whisk until well blended. Stir in milk mixture. Stir in bread cubes, raisins, and hazelnuts. Let sit for 5 minutes. Pour into an ungreased 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, until a toothpick inserted into the center comes out clean, 40 to 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While pudding is baking, heat brown sugar, 1 cup butter, and bourbon whiskey in a pot over medium heat, stirring constantly, until boiling and sugar is dissolved, about 5 minutes. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauce over bread pudding when done.\n"
            }
        ],
