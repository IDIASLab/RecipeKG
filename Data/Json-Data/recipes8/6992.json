    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281535/chicken-fajita-casserole/",
        "name": "Chicken Fajita Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8671813.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-07-21T02:03:04.000Z",
        "description": "Wonderful, hearty, and tasty chicken fajita casserole that combines chicken and Tex-Mex elements. Serve with lime wedges.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4 cups chicken stock",
            "3 pounds bone-in chicken breast halves",
            "2 cups uncooked white rice",
            "1 tablespoon olive oil, or as needed",
            "1 medium green bell pepper, chopped",
            "1 medium red bell pepper, chopped",
            "1 medium yellow bell pepper, chopped",
            "1 medium yellow onion, chopped",
            "1 medium red onion, chopped",
            "5 medium jalapeno peppers, diced, or more to taste",
            "1 teaspoon minced garlic",
            "1 (15 ounce) can black beans, rinsed and drained",
            "1 (8 ounce) jar salsa",
            "1 (1.12 ounce) package fajita seasoning (such as McCormick\u00ae)",
            "2 teaspoons salt",
            "2 teaspoons ground black pepper",
            "1 (1 pound) package shredded Mexican cheese blend",
            "1 (16 ounce) container sour cream",
            "1 cup tortilla strips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring stock to a boil in a large pot. Add chicken breasts and cook until very tender, no longer pink in the centers, and juices run clear, 15 to 20 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C). Remove chicken to a plate, keeping stock in the pot. Remove and discard bones, fat, and skin. Chop meat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add rice to the stock and bring to a boil over medium-high heat. Reduce heat to medium-low, cover, and simmer until rice is tender and stock has been absorbed, 20 to 25 minutes. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat. Add bell peppers, onions, jalapeno peppers, and garlic; saute until slightly tender, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chicken, rice, vegetables, black beans, salsa, fajita seasoning, salt, and pepper in a large mixing bowl. Transfer to a large casserole dish and top with cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned and bubbly, about 30 minutes. Let rest for 10 minutes; serve with sour cream and tortilla strips on top.\n"
            }
        ],
