    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71193/cedar-plank-salmon/",
        "name": "Cedar Plank Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2428619.jpg",
            "width": 3888,
            "height": 2592
        },
        "datePublished": "2020-03-05T04:58:39.000Z",
        "description": "Cedar plank salmon can be baked in the oven or barbequed on the grill with this easy recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (12 inch) cedar plank",
            "2 tablespoons sesame oil",
            "3 tablespoons brown sugar",
            "1 teaspoon dried dill weed",
            "1 tablespoon sesame seeds",
            "3 tablespoons water",
            "4 (6 ounce) salmon fillets, with skin",
            "1 teaspoon freshly ground black pepper",
            "1 tablespoon soy sauce",
            "1 lemon, cut into wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak the cedar plank in water for at least 2 hours and up to 12.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C), or see Footnote for grilling instructions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the salmon on the plank, skin-side down. In a small bowl, stir together the sesame oil, brown sugar, dill weed and sesame seeds. Stir in just enough water to make it liquid. Season with pepper and soy sauce if desired. Spoon the mixture over the salmon so it is fully coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 to 40 minutes in the preheated oven, until the fillet can be flaked with a fork.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Salmon",
            "Salmon Fillet Recipes"
        ],
