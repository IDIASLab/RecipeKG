    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241269/gluten-free-power-bars/",
        "name": "Gluten-Free Power Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Power Bars"
        },
        "datePublished": "2020-03-05T05:12:59.000Z",
        "description": "Easy and economical option to store-bought power or granola bars. This gluten-free version is great for families like ours that struggle with food allergies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "18 bars",
        "recipeIngredient": [
            "1 cup packed brown sugar",
            "\u00bd cup butter, softened",
            "1 egg",
            "2 tablespoons honey",
            "1 teaspoon vanilla extract",
            "1 cup gluten-free all-purpose flour",
            "2 tablespoons vanilla whey protein powder",
            "2 tablespoons coconut flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 cup quick-cooking oats",
            "1 cup crispy rice cereal",
            "1 cup chocolate chips",
            "\u00bd cup raisins",
            "\u00bd cup chopped almonds",
            "2 tablespoons ground flax seed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar and butter together in a bowl using an electric mixer until light and fluffy. Add egg, honey, and vanilla extract and mix until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine gluten-free all-purpose flour, protein powder, coconut flour, baking powder, and salt together in a bowl. Slowly mix flour mixture into creamed butter mixture; fold in oats, rice cereal, chocolate chips, raisins, almonds, and flax seeds. Press mixture into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 20 to 30 minutes. Cool on a wire rack.\n"
            }
        ],
