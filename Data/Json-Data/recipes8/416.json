    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274702/pumpkin-bread-pudding-with-cinnamon-whipped-cream/",
        "name": "Pumpkin Bread Pudding with Cinnamon Whipped Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Bread Pudding with Cinnamon Whipped Cream"
        },
        "datePublished": "2020-06-19T04:39:20.000Z",
        "description": "This pumpkin bread pudding is a new Thanksgiving favorite.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "8 ramekins",
        "recipeIngredient": [
            "6 cups 1/2-inch cubes French bread",
            "1 cup whipping cream",
            "1 cup whole milk",
            "1 (15 ounce) can pumpkin puree",
            "1 cup brown sugar",
            "3 eggs",
            "1 teaspoon ground cinnamon",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon salt",
            "\u00bd cup raisins",
            "2 cups whipping cream, chilled",
            "2 tablespoons white sugar",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss bread cubes with 1 cup whipping cream and milk in a large bowl until all bread is moistened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together pumpkin puree, brown sugar, eggs, cinnamon, vanilla extract, and salt in another large bowl. Pour pumpkin mixture over bread cubes, add raisins, and mix until combined. Scoop mixture into 8 ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a knife inserted near the center of the pudding comes out clean, about 1 hour. Remove from the oven and allow to sit for at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, beat 2 cups chilled whipping cream, sugar, and cinnamon in a glass or metal bowl with an electric mixer until soft peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve puddings topped with cinnamon whipped cream.\n"
            }
        ],
