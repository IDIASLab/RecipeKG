    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262293/easy-mexican-tortilla-soup/",
        "name": "Easy Mexican Tortilla Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5013196.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-23T21:52:29.000Z",
        "description": "This easy Mexican tortilla soup features fried tortilla strips in a tomato-based broth seasoned with pasilla chiles. Delicious served as tradition dictates: with panela cheese and avocado.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "8 (6 inch) corn tortillas",
            "3 tablespoons avocado oil, divided",
            "1 small onion, finely chopped",
            "3 pasilla chiles - seeded, deveined, and cut into strips",
            "1 small small onion, finely chopped",
            "4 tomatoes, seeded and diced",
            "2 sprigs fresh mint",
            "1 sprig fresh oregano",
            "2 quarts chicken broth",
            "2 teaspoons chicken bouillon granules",
            "2 avocados, diced",
            "1 (8 ounce) package Mexican queso fresco, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut tortillas in half and slice crosswise into 1/8-inch strips. Place in a bowl and toss with 1 tablespoon oil until fully coated. Arrange tortilla strips in a single layer on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crisp and light brown, about 15 minutes. Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in a saucepan over medium heat. Add pasilla chiles; cook and stir until light brown, about 4 minutes. Transfer chiles to a plate. Add onion to the saucepan; cook until translucent, about 5 minutes. Add tomatoes; cook until soft, stirring occasionally, about 10 minutes. Reduce heat to medium-low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return chiles to the pan; add mint and oregano. Simmer until fragrant, about 3 minutes. Pour in chicken broth, increase heat to medium-high, and bring to a boil. Add chicken bouillon; reduce heat and simmer until flavors meld, about 10 minutes more. Discard mint and oregano. Garnish with the tortilla strips, avocados, and queso fresco.\n"
            }
        ],
