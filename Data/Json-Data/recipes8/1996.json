    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279150/italian-knot-cookies/",
        "name": "Italian Knot Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Italian Knot Cookies"
        },
        "datePublished": "2020-06-19T04:44:53.000Z",
        "description": "These Italian knot cookies are soft, sweet, glazed Sicilian biscotti, from a recipe my great-grandmother gave me, not hard like traditional biscotti, not baked twice...These should be kept in an airtight container.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "60 cookies",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup unsalted butter, at room temperature",
            "\u00bd cup margarine",
            "\u00bc cup milk",
            "1 teaspoon pure vanilla extract",
            "\u00bc teaspoon almond extract",
            "6 large eggs",
            "4\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "1 cup confectioners' sugar",
            "1 tablespoon milk, or more as needed",
            "1 teaspoon lemon juice",
            "1 tablespoon colored sugar or festive sprinkles"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar, butter, margarine, milk, vanilla extract, and almond extract together in a large bowl. Add eggs, 1 at a time, beating briefly after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, and salt; add 1 cup at a time to the creamed mixture until a dough forms, using your hands if and when necessary. Turn out onto a floured board and knead briefly until smooth and a bit sticky, but not sticking to your hands. Set aside to rest for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pull off small pieces of dough, 1 at a time, and roll into a rope. Gently place one end towards the middle, followed by the other end, in a way to slightly overlap each other. Repeat to form remaining cookies and place on the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven in batches until just turning a light brown on the bottom, 10 to 15 minutes. Remove to a wire rack and allow to cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile mix confectioners sugar, milk, and lemon juice together in a small bowl to form a thin glaze. Dip warm cookies in the glaze and sprinkle with sugar or sprinkles. Let set on waxed paper or a cooling rack to dry.\n"
            }
        ],
