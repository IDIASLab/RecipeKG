        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233386/toss-together-broccoli-salad/",
        "name": "Toss-Together Broccoli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2307792.jpg",
