        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283077/easy-meaty-sheet-pan-meal/",
        "name": "Easy, Meaty Sheet Pan Meal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8979545.jpg",
            "width": 0,
            "height": 0
