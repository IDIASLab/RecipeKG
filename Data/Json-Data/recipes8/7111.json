    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281685/sweet-potato-cobbler/",
        "name": "Sweet Potato Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F02%2F01%2FSweet-Potato-Cobbler82-2000.jpg",
            "width": 2000,
            "height": 2000
        },
        "datePublished": "2010-01-20T06:54:37.000Z",
        "description": "A warm, comforting cobbler dessert with all the flavors of autumn. A beautiful marriage of sweet potatoes and pumpkin pie.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9-inch cobbler",
        "recipeIngredient": [
            "3\u2009\u00bd medium sweet potatoes, peeled and cut into chunks",
            "\u00bc cup white sugar",
            "\u00bd teaspoon vanilla extract",
            "1 (2 inch) cinnamon stick",
            "\u00bc cup loosely packed brown sugar",
            "1 teaspoon pumpkin pie spice",
            "1 teaspoon cornstarch",
            "\u00bd cup all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd teaspoon baking powder",
            "1 pinch salt",
            "\u00bd cup cold unsalted butter, cut into small pieces",
            "1 large egg",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sweet potatoes, sugar, vanilla extract, cinnamon stick, and water to cover in a medium pot over medium heat. Bring to a simmer and cook until sweet potatoes are barely tender, 8 to 10 minutes. Remove from heat, discard cinnamon stick, and strain potatoes. Allow potatoes to cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 375 degrees F (190 degrees C). Grease a 9-inch pie plate, baking dish, or casserole dish of your choice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, baking powder, and salt for crust in a mixing bowl. Cut cold butter into the mixture until crumbly. Stir in egg and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine warm potatoes, brown sugar, pumpkin pie spice, and cornstarch in a separate bowl; mix until thoroughly combined. Spoon filling into the prepared pie plate and spoon crust mixture over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is golden brown and filling is bubbly, about 35 minutes.\n"
            }
        ],
