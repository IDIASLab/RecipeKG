        "name": "Fruit-Topped Whole Wheat Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2020%2F11%2F18%2FFruit-Topped-Whole-Wheat-Pancakes-2000.jpg",
            "width": 1500,
