        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277887/easy-chocolate-pudding/",
        "name": "Easy Chocolate Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7824741.jpg",
