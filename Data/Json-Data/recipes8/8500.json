    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282381/chocolate-candy-cane-cupcakes-with-whipped-topping/",
        "name": "Chocolate-Candy Cane Cupcakes with Whipped Topping",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate-Candy Cane Cupcakes with Whipped Topping"
        },
        "datePublished": "2008-11-13T15:10:59.000Z",
        "description": "Your family will love the chocolate flavor with a hint of peppermint. Great for Christmas, or any cold day.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "30 cupcakes",
        "recipeIngredient": [
            "1 (15.25 ounce) package chocolate cake mix",
            "1 (3.9 ounce) package instant chocolate pudding mix",
            "1 cup sour cream",
            "\u00bd cup vegetable oil",
            "\u00bd cup water",
            "4 large eggs",
            "4 (1 ounce) squares semisweet chocolate, chopped",
            "6 small peppermint candy canes, crushed, divided",
            "1 (1 ounce) square semisweet chocolate",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 30 muffin cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cake mix, pudding mix, sour cream, oil, water, and eggs for cupcakes with a mixer on low speed until moistened. Beat on medium speed for 2 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in chopped chocolate and 2 tablespoons of the crushed candy canes. Reserve remaining crushed candy for the topping. Spoon batter into the prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the centers comes out clean, 20 to 23 minutes. Cool in the tins for 5 minutes. Transfer to a wire rack and let cool completely, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate square for topping in a microwave-safe bowl in a microwave, 10 to 20 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost cooled cupcakes with whipped topping. Drizzle with melted chocolate and top with remaining crushed candy canes.\n"
            }
        ],
