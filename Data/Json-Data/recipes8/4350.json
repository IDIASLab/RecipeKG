    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270662/turkey-bacon-zucchini-pasta/",
        "name": "Turkey Bacon Zucchini Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6415717.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2019-02-13T04:42:43.000Z",
        "description": "Next time you're looking to eat a little lighter without sacrificing flavor, give this dish featuring Pure Farmland&reg; Turkey Bacon a try.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 slices Pure Farmland\u00ae Uncured Turkey Bacon, diced",
            "1 tablespoon sunflower seeds, toasted",
            "1 cup fresh basil",
            "\u00bd cup parsley",
            "1 cup green peas, cooked, cooled",
            "1 clove garlic",
            "\u00bc cup pine nuts",
            "1 lemon, zested and juiced",
            "\u00bc cup grated Parmesan cheese",
            "\u00bc cup olive oil",
            "Salt and pepper, to taste",
            "1 zucchini, spiralized",
            "1 teaspoon kosher salt",
            "2 tablespoons olive oil",
            "8 sun-dried tomatoes, sliced",
            "Salt and pepper, to taste",
            "\u00bd cup arugula leaves",
            "2 ounces smoked mozzarella, torn into pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prep: Cook turkey bacon in a skillet over medium heat until browned and crispy, 5 to 7 minutes. Dice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oven to 325 degrees F. Place sunflower seeds on baking sheet, and toast for 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make Pea Pesto: In food processor, add basil, parsley, peas, garlic, pine nuts, lemon zest and juice, and cheese. Pulse to combine. With machine running, add in olive oil and mix to combine. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make Zucchini Pasta: In colander, toss zucchini with kosher salt and allow to sit for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large saute pan over medium heat and add olive oil. Add zucchini to pan and cook for 3 minutes, or until slightly softened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add in 1 cup Pea Pesto and stir to combine. Add sun-dried tomatoes and cook for additional 2 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove to platter and top with arugula, smoked mozzarella, sunflower seeds, and turkey bacon.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
