    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231630/chocolate-covered-almond-meringue-cookies/",
        "name": "Chocolate Covered Almond Meringue Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Covered Almond Meringue Cookies"
        },
        "datePublished": "2020-06-19T03:53:33.000Z",
        "description": "A light crisp cookie covered in milk chocolate that's scrumptious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT3H0M",
        "recipeYield": "4 dozen cookies",
        "recipeIngredient": [
            "6 egg whites at room temperature",
            "1\u2009\u00bc cups white sugar",
            "8 ounces ground almonds",
            "2\u2009\u00bc cups confectioners' sugar",
            "1 pound milk chocolate, broken into pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C). Line 4 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a glass or metal bowl until foamy and soft peaks form. Gradually add white sugar, continuing to beat until mixture holds stiff peaks, about 5 minutes. Lift your beater or whisk straight up: the egg whites will form sharp peaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground almonds and confectioners' sugar together in a separate bowl; fold into egg white mixture. Transfer mixture to a piping bag; pipe mixture into 2-inch long by 1/2-inch thick mounds onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are dry and lightly golden brown, about 45 minutes. Transfer cookies to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in the top of a double boiler over simmering water, stirring frequently and scraping down the sides with a rubber spatula to avoid scorching.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each cookie in the melted chocolate using a fork until cookie is completely coated. Shake off excess chocolate. Arrange cookies on the parchment-lined baking sheets; refrigerate until set.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Almond Dessert Recipes"
