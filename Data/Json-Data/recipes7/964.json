    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/87629/hot-dog-relish/",
        "name": "Hot Dog Relish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F968892.jpg",
            "width": 1150,
            "height": 1150
        },
        "datePublished": "2020-06-19T03:12:24.000Z",
        "description": "This is my mom's relish recipe. It is easy. Use it on hot dogs or in macaroni salad. Delicious!  It makes approximately 9 to 12 pints.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "10 pints",
        "recipeIngredient": [
            "4 cups chopped onions",
            "4 cups coarsely chopped cabbage",
            "12 chopped green bell peppers",
            "10 green tomatoes, chopped",
            "\u00bd cup salt",
            "2 tablespoons mustard seed",
            "1 teaspoon celery seed",
            "1\u2009\u00bd teaspoons turmeric  ",
            "6 cups sugar",
            "4 cups white vinegar",
            "2 cups cold water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place onions, cabbage, bell peppers, and tomatoes in a food processor, and pulse until finely minced. Drain, and remove contents to a large bowl. Sprinkle with salt, cover, and let stand overnight. Pour contents into a colander; rinse, and drain. Return contents to bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, stir together mustard seed, celery seed, turmeric, sugar, white vinegar, and water. Pour over vegetable mixture, and stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into a large pot, and bring to a boil over high heat. Boil for 5 to 10 minutes. Store, refrigerated, in sterilized, sealed pint jars.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Relish Recipes"
        ],
