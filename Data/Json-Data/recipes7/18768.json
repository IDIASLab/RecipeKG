        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268579/easy-chocolate-custard/",
        "name": "Easy Chocolate Custard",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5865149.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T04:33:13.000Z",
        "description": "A quick, lower-fat, rich chocolate custard that does not require baking or water baths. No one will ever guess it takes 10 minutes to make! Top with whipped cream before serving.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT4H25M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "2 large eggs",
            "1 teaspoon vanilla extract",
            "2 cups low-fat milk",
            "\u00bc cup white sugar",
            "\u00bc cup Dutch-process cocoa powder",
            "2 tablespoons cornstarch",
            "1 tablespoon brandy",
            "\u215b teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat eggs and vanilla extract together in a medium bowl until combined; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk, sugar, cocoa powder, cornstarch, brandy, and salt together in a saucepan. Cook over medium heat, stirring constantly, until boiling and thickened, 6 to 8 minutes. Reduce heat to low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly pour 1/2 of the milk mixture into the beaten eggs while stirring; pour mixture back into the saucepan. Cook, stirring constantly, until thick, 2 to 3 minutes. Do not boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let custard cool to room temperature, at least 10 minutes. Refrigerate until set, at least 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes",
            "Brandy"
