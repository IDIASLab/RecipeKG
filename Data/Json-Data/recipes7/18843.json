    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268990/matcha-green-tea-pancake/",
        "name": "Matcha Green Tea Pancake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5930251.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:33:27.000Z",
        "description": "A delicious pancake infused with matcha green tea flavor to give your breakfast a colorful Japanese flair! These pancakes are a vibrant color, so flipping them earlier will prevent them from browning as much and will maintain the bright green exterior. Serve with powdered sugar or whipped cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 pancakes",
        "recipeIngredient": [
            "1 teaspoon green tea powder (matcha)",
            "1 cup boiling water",
            "\u00bd cup milk",
            "2 tablespoons melted butter",
            "1 teaspoon vanilla extract",
            "1 egg",
            "1 cup all-purpose flour",
            "2 tablespoons white sugar",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon baking soda",
            "1 teaspoon butter, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place matcha powder in a bowl and slowly add boiling water while whisking continuously until tea is frothy and matcha is fully incorporated. Refrigerate or set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix milk, melted butter, and vanilla extract together in a large mixing bowl. Beat egg into the wet ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, salt, and baking soda together in a bowl. Sift into another bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cooled tea mixture into the milk mixture. Add sifted flour mixture to the milk mixture and mix batter thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a lightly buttered frying pan or griddle over medium heat, but do not let it smoke. Pour 1/4 cup batter into the pan and cook until small bubbles are visible near the center of the pancake, 1 to 3 minutes. Flip and cook on the second side 1 to 3 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
