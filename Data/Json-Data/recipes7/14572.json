    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254669/raspberry-peach-jam/",
        "name": "Raspberry Peach Jam",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5435891.jpg",
            "width": 3343,
            "height": 2595
        },
        "datePublished": "2020-06-19T04:17:22.000Z",
        "description": "One of my favorite homemade jams. A summery fresh taste and easy to make. Store jars in a cool, dark area.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H41M",
        "totalTime": "P0DT13H11M",
        "recipeYield": "10 12-ounce jam jars",
        "recipeIngredient": [
            "7 pounds fresh peaches",
            "10 cups white sugar",
            "5 cups fresh raspberries",
            "\u2153 cup lemon juice",
            "1 (6 fluid ounce) container liquid pectin (such as CERTO\u00ae)",
            "3 tablespoons powdered pectin"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Immerse ten 12-ounce jars in simmering water until jam is ready. Wash lids and rings in warm soapy water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add peaches, 3 or 4 at a time; cook for 30 seconds. Immerse in ice water for several minutes to stop the cooking process. Drain. Peel off skins, core and dice peaches into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine diced peaches, sugar, raspberries, and lemon juice in a large pot. Cook over medium heat, stirring occasionally, until sugar is dissolved, about 5 minutes. Increase heat to medium-high; cook at a rapid boil for 10 minutes. Stir in liquid and powdered pectin; bring back to a boil. Remove from heat and skim foam off the jam with a slotted spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack jam into hot jars, filling to within 1/4 inch of the top. Wipe the rims of the jars with a moist paper towel to remove any food residue. Top with lids and screw on rings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a rack in the bottom of a large stockpot and fill halfway with water. Bring to a boil and lower jars 2 inches apart into the boiling water using a holder. Pour in more boiling water if necessary to bring the water level to at least 1 inch above the tops of the jars. Bring the water to a rolling boil, cover the pot, and process for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the jars from the stockpot and place onto a cloth-covered or wood surface, several inches apart, until cool, 12 to 24 hours. Press the top of each lid with a finger, ensuring that the seal is tight (lid does not move up or down at all).\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Jams and Jellies Recipes"
        ],
