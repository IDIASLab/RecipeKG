        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221315/kiwi-lime-pork-ribs/",
        "name": "Kiwi-Lime Pork Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F873242.jpg",
            "width": 3872,
            "height": 2592
        },
        "datePublished": "2020-06-19T03:48:26.000Z",
        "description": "This lovely dish pops with flavor and brings the tastes of the tropics to your dinner table!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 kiwis, peeled and sliced",
            "\u00bd cup white sugar",
            "2 tablespoons cider vinegar",
            "2 tablespoons lime juice",
            "2 tablespoons salt",
            "2 tablespoons chili powder",
            "2 tablespoons cornstarch",
            "1 clove garlic, minced",
            "1 (3 pound) package country-style pork ribs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend kiwis, sugar, vinegar, lime juice, salt, chili powder, cornstarch, and garlic in a blender until thickened into a sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pork ribs on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle blended kiwi sauce over ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Loosely cover ribs with a sheet of aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in preheated oven until tender, about 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes",
            "Country-Style Ribs"
        ],
