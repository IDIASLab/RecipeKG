    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235973/mahogany-cake/",
        "name": "Mahogany Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5994196.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:57:54.000Z",
        "description": "Original recipe from my grandmother Sallie Ayers Kelly. It is a depression-era or before recipe. One of the simplest cake recipes I have tried. Cool cake before frosting.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "2 cups buttermilk",
            "2 teaspoons baking soda",
            "2 cups white sugar",
            "2\u2009\u00bd tablespoons butter, softened",
            "2 eggs",
            "3 cups all-purpose flour",
            "2\u2009\u00bd tablespoons unsweetened cocoa powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x12-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir buttermilk and baking soda together in a bowl until dissolved and foaming.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter together in a bowl using an electric mixer until smooth and creamy. Beat the first egg into butter mixture until completely blended; beat in the second egg until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour and cocoa powder together in a bowl. Beat flour mixture, alternating with buttermilk mixture, into creamed butter mixture using an electric mixer until batter is smooth. Pour batter into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the cake comes out clean, 30 to 40 minutes. Cool cake for 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Sheet Cake Recipes"
