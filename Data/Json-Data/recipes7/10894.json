    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236761/pecan-cajun-cake/",
        "name": "Pecan Cajun Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pecan Cajun Cake"
        },
        "datePublished": "2020-06-19T03:58:18.000Z",
        "description": "Great for a Mardi Gras-themed dinner party! I was a member of the Barksdale AFB Officers' Wive's gourmet club. This cake was made for one of our luncheons. Guaranteed Cajun!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 (15 ounce) can crushed pineapple",
            "1\u2009\u00bd cups white sugar",
            "2 eggs, beaten",
            "1\u2009\u00bd teaspoons baking soda",
            "1 pinch salt",
            "1 cup white sugar",
            "\u00be cup evaporated milk",
            "\u00bd cup margarine",
            "1 cup chopped pecans",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Grease an 11x14-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, pineapple, 1 1/2 cups white sugar, eggs, baking soda, and salt together in a large bowl; pour into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the cake comes out clean, about 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup sugar, evaporated milk, and margarine together in a saucepan; bring to a boil for 1 minute. Remove saucepan from heat and stir pecans and vanilla extract into sugar mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pierce the top of the cake with a toothpick and spread pecan topping over cake.\n"
            }
        ],
