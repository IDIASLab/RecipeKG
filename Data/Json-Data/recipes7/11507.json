    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238324/red-cabbage-slow-slaw/",
        "name": "Red Cabbage Slow Slaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2884060.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:59:45.000Z",
        "description": "This slaw takes days to make, so it's hard to keep up with having it ready for my family! We eat this several times a week! This goes great with my husband's Texas BBQ!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P2DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 head red cabbage, cored and sliced as thinly as possible",
            "\u00be cup red wine vinegar",
            "5 tablespoons white sugar",
            "2 tablespoons balsamic vinegar",
            "1 tablespoon salt",
            "1\u2009\u00bd teaspoons onion powder",
            "1 teaspoon seasoned salt (such as LAWRY'S\u00ae)",
            "\u00bd teaspoon ground black pepper",
            "\u00bd cup olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put sliced cabbage into a large sealable plastic bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk red wine vinegar, sugar, balsamic vinegar, salt, onion powder, seasoned salt, and black pepper together in a bowl; pour into bag with the cabbage. Massage the bag to ensure cabbage is completely coated with liquid. Squeeze as much air from the bag as possible and seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate cabbage in refrigerator, massaging the bag several times a day, until the cabbage is softened and the marinade is deep purple in color, at least 2 days.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour olive oil into the bag and mix with the cabbage mixture. Seal bag and refrigerate 1 hour more before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Coleslaw Recipes",
            "No Mayo"
        ],
