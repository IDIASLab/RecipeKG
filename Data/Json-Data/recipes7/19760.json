    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273077/breakfast-blueberry-cheesecake-muffins/",
        "name": "Breakfast Blueberry Cheesecake Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1125563.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2020-06-19T04:36:28.000Z",
        "description": "A very light cheesecake muffin with juicy blueberries that give the muffins a nice fruity flavor for breakfast or brunch.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "2 eggs, beaten",
            "1 (8 ounce) package cream cheese, softened",
            "1 cup white sugar",
            "\u00bd cup heavy whipping cream",
            "1 cup fresh blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 12-cup muffin tin or line cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, baking powder, and baking soda together in a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs with an electric mixer in a bowl on medium speed until light and frothy. Beat cream cheese in a large bowl with an electric mixer until fluffy. Add sugar and mix to form a smooth paste. Add beaten eggs and beat with an electric mixer until a smooth creamy batter forms, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold cream and blueberries into the batter and mix until just combined. Add flour mixture and fold in just until absorbed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling each 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed and muffins are lightly browned, about 20 minutes. Cool in the tin for 5 minutes. Transfer to a wire rack to cool completely.\n"
            }
        ],
