        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239002/bran-cereal-fruit-muffins/",
        "name": "Bran Cereal Fruit Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bran Cereal Fruit Muffins"
        },
        "datePublished": "2020-06-19T04:00:26.000Z",
        "description": "I keep this recipe for large gatherings, even picky eaters like these muffins!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "54 muffins",
        "recipeIngredient": [
            "1 (17 ounce) package wheat bran flakes cereal with raisins",
            "1 quart buttermilk",
            "5 cups all-purpose flour",
            "3 cups white sugar",
            "1 (15 ounce) can fruit cocktail, drained",
            "1 cup vegetable oil",
            "4 large eggs",
            "4 teaspoons ground cinnamon",
            "2 teaspoons salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease 54 muffin cups or line with paper muffin liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat bran cereal, buttermilk, flour, sugar, fruit cocktail, vegetable oil, eggs, cinnamon, and salt together in a bowl with an electric mixer on medium speed until batter is blended, about 2 minutes. Spoon batter into prepared muffin cups, filling each about 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and set, 16 to 18 minutes.\n"
            }
        ],
