    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222739/nikkis-best-rhubarb-dessert-ever/",
        "name": "Nikki's Best Rhubarb Dessert Ever",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1005634.jpg",
            "width": 2593,
            "height": 2592
        },
        "datePublished": "2020-06-19T03:49:50.000Z",
        "description": "If you are not a real big fan of rhubarb, you HAVE to try this!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1\u2009\u00bd cups brown sugar",
            "1 cup butter, softened",
            "4 eggs",
            "\u00bc cup all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "1 cup half-and-half",
            "\u00bd teaspoon salt",
            "4 cups chopped rhubarb"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3 cups flour and brown sugar in a large bowl; cut in softened butter with a knife or pastry blender until mixture resembles coarse crumbs. Reserve 1 1/2 cups flour mixture; pat remainder into the bottom of a 9x13-inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until crust is lightly browned, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a large bowl; mix 1/4 cup flour, white sugar, half-and-half, and salt into eggs, beating until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle chopped rhubarb over baked crust. Pour egg mixture over rhubarb; top with reserved flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until eggs are set and lightly browned, 40 to 45 minutes.\n"
            }
        ],
