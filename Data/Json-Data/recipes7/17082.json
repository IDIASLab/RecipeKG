    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261352/cinnamon-roll-bread-pudding/",
        "name": "Cinnamon Roll Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1781718.jpg",
            "width": 1280,
            "height": 1110
        },
        "datePublished": "2020-06-19T04:25:23.000Z",
        "description": "A twist on a classic recipe. I've made this for breakfast and dessert and my family loves it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT10H30M",
        "recipeYield": "1 2-quart baking dish",
        "recipeIngredient": [
            "cooking spray",
            "1 (12.4 ounce) package refrigerated cinnamon rolls with icing (such as Pillsbury\u2122 Grands!\u2122)",
            "2 cups heavy whipping cream",
            "1 cup light brown sugar",
            "\u2154 cup whole milk",
            "5 egg yolks",
            "2 teaspoons vanilla extract",
            "\u00be teaspoon ground cinnamon",
            "1 pinch salt",
            "2 tablespoons white sugar",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc cup unsalted butter, at room temperature",
            "2 tablespoons butter",
            "\u00bd cup white sugar",
            "1 tablespoon cornstarch",
            "1 cup milk",
            "\u00bc cup bourbon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a baking sheet with cooking spray; place rolls on top, 2 inches apart. Reserve icing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 8 to 10 minutes. Cool on the baking sheet for 5 minutes. Transfer to a wire rack. Let stand, uncovered, until stale, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Grease a 2-quart baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut stale cinnamon rolls into 1/2-inch cubes. Whisk heavy cream, brown sugar, 2/3 cup milk, egg yolks, vanilla extract, 3/4 teaspoon cinnamon, and salt in a large bowl. Toss in cinnamon roll cubes until evenly coated. Let stand, tossing occasionally, until cubes being to absorb cream mixture, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into the prepared baking dish. Cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until almost set, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 2 tablespoons white sugar and 1/4 teaspoon cinnamon together in a small bowl. Rub in 1/4 cup butter with your fingers until topping is in pea-sized crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Uncover baking dish and sprinkle topping on top. Continue baking, uncovered, until custard is set, 20 to 25 minutes. Increase oven temperature to 450 degrees F (230 degrees C). Bake until bread pudding forms a golden crust, about 2 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm reserved icing in a small microwave-safe bowl, about 15 seconds. Drizzle hot icing carefully over bread pudding. Cool on a wire rack, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 2 tablespoons butter in a saucepan over medium heat. Mix 1/2 cup sugar and cornstarch together; stir into the melted butter. Pour in milk and bourbon, stirring frequently until mixture begins to boil. Cook, stirring constantly, until thickened, about 5 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon cooled bread pudding onto serving plates. Drizzle warm sauce on top.\n"
            }
        ],
