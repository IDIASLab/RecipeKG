    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254908/green-bean-casserole-tartlets/",
        "name": "Green Bean Casserole Tartlets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3894447.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-08-02T00:30:13.000Z",
        "description": "A delicious miniature puff pastry appetizer.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "60 tartlets",
        "recipeIngredient": [
            "2 sheets frozen puff pastry, thawed",
            "1 tablespoon butter",
            "2 tablespoons minced onion",
            "\u00bd teaspoon sugar",
            "1 tablespoon all-purpose flour",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon black pepper",
            "\u00bd cup sour cream",
            "1 (14.5 ounce) can Libby's\u00ae Cut Green Beans",
            "1 cup shredded sharp Cheddar cheese, divided",
            "\u00bd cup canned French fried onions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On lightly floured surface, gently roll out each pastry sheet to 10-by-12 inches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each sheet into 30 2-inch squares, ending with a total of 60 squares (a pizza cutter works great for this).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly score each square about 1/8 inch around the edges and prick liberally with a fork. This will keep the pastry from puffing up too much during cooking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer pastry squares to 2 parchment-lined baking sheets and put into the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add onions to pan and cook for 2 minutes, or until onions are translucent.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in sugar, flour, salt, and pepper and cook, stirring constantly, for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sour cream, green beans, and half of cheese to onion mixture and stir until well combined and heated through. Remove from heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take pastry squares out of refrigerator and top each one with a small spoonful of the green bean mixture and shredded cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 7 minutes, remove from the oven. Top each square with a few French fried onions.  Return to the oven and continue cooking for an additional 6-8 minutes, until the pastry is puffed and golden.  Let sit 5 minutes before serving.\n"
            }
        ],
