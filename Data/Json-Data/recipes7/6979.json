        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222418/the-mojito-cocktail/",
        "name": "The Mojito Cocktail",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F993575.jpg",
            "width": 960,
