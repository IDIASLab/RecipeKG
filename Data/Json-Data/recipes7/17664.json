    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263619/white-asparagus-salad-with-strawberries/",
        "name": "White Asparagus Salad with Strawberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "White Asparagus Salad with Strawberries"
        },
        "datePublished": "2020-06-19T04:27:33.000Z",
        "description": "White asparagus has a much milder taste and works well with the sweet fruitiness of strawberries. This salad is served with a creamy yogurt-mayo dressing with orange juice.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 teaspoon salt, divided",
            "1 teaspoon white sugar, divided",
            "1\u2009\u00be pounds white asparagus - peeled, trimmed, and cut into 1-inch pieces",
            "3 tablespoons orange juice",
            "2 tablespoons plain yogurt",
            "2 tablespoons mayonnaise",
            "2 tablespoons creme fraiche with herbs",
            "salt and ground black pepper to taste",
            "2 cups hulled and quartered strawberries",
            "4 leaves green leaf lettuce",
            "1 tablespoon chopped fresh chives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a pot with water; add 1/2 teaspoon each salt and sugar. Bring to a boil. Add white asparagus, reduce heat to low, and simmer until soft and cooked through, 10 to 15 minutes. Drain and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine orange juice, yogurt, mayonnaise, and creme fraiche in a large bowl. Season with remaining salt, sugar, and pepper. Add cooled asparagus and strawberries and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange lettuce leaves on plates and distribute asparagus salad on top. Sprinkle with chives.\n"
            }
        ],
