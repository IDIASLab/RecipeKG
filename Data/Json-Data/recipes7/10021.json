    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234491/pear-pie/",
        "name": "Pear Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2597871.jpg",
            "width": 2400,
            "height": 1600
        },
        "datePublished": "2020-06-19T03:56:13.000Z",
        "description": "Such a great change of pace from other more typical fruit pies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) unbaked pie crust",
            "3 cups sliced Bartlett pears",
            "1 cup sour cream",
            "\u00bd cup white sugar",
            "2 tablespoons all-purpose flour",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "\u00bd teaspoon salt",
            "\u00bd cup all-purpose flour",
            "\u00bd cup butter",
            "\u00bc cup white sugar",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Press pie crust into a 9-inch pie dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange pear slices in the pie crust. Stir sour cream, 1/2 cup sugar, 2 tablespoons flour, egg, vanilla extract, and salt together in a bowl; pour over pears.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1/2 cup flour, butter, 1/4 cup sugar, and cinnamon together in a bowl until topping resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake pie in the preheated oven for 35 minutes. Sprinkle topping onto pie and continue baking until topping is golden brown, about 10 more minutes.\n"
            }
        ],
        "recipeCategory": [
