    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245600/mimis-cheesecake-torte/",
        "name": "Mimi's Cheesecake Torte",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3305618.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T04:04:06.000Z",
        "description": "Mimi gave this recipe to my mother, who then gave it to me. This is my Papa's cake of choice for his birthday. A sour cream topping makes this cheesecake unique. Garnish with almonds if you like.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "1 9-inch cheesecake",
        "recipeIngredient": [
            "3 (8 ounce) packages cream cheese, softened",
            "5 egg whites",
            "1\u2009\u00bd cups white sugar",
            "1 teaspoon vanilla extract",
            "1 (9 inch) prepared graham cracker crust",
            "1 pint sour cream",
            "2 tablespoons white sugar, or more to taste",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese in the bowl of a stand mixer fitted with a paddle attachment on medium speed until smooth, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a separate bowl on high speed until stiff peaks form, 3 to 5 minutes. Blend in 1 1/2 cups sugar. Fold egg whites into cream cheese, and stir in 1 teaspoon vanilla extract. Pour cream cheese filling into crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until filling is set, about 20 minutes. Remove from oven; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase oven temperature to 475 degrees F (246 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sour cream, 2 tablespoons white sugar, and 1/2 teaspoon vanilla extract together in a large bowl. Spread evenly on top of the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, about 5 minutes. Remove from oven and cool to room temperature, about 45 minutes. Cover with plastic wrap and chill in the refrigerator until firm, at least 2 hours.\n"
            }
        ],
