    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242321/secret-ingredient-cookies/",
        "name": "Secret Ingredient Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3027961.jpg",
            "width": 1762,
            "height": 1757
        },
        "datePublished": "2020-06-19T04:03:06.000Z",
        "description": "These cookies are unbelievably good, due to two special secret ingredients. Enjoy these warm and chewy right from the oven or put some vanilla malt ice cream between two for a delicious sandwich.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "\u00bd cup packed brown sugar",
            "\u00bd cup butter, softened",
            "\u00bd cup vanilla ice cream",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd cup malt powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup white chocolate chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat white sugar, brown sugar, and butter with an electric mixer in a large bowl until light and fluffy. Beat ice cream, egg, and vanilla extract into butter mixture until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, malt powder, baking soda, and salt together in a bowl. Add flour mixture to butter mixture and stir until just combined. Drop spoonfuls of dough 2 inches apart onto baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until lightly browned, about 8 minutes. Cool cookies on a wire rack.\n"
            }
        ],
