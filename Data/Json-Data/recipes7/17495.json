    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262511/rose-pavlova-cakes/",
        "name": "Rose Pavlova Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4534480.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:26:57.000Z",
        "description": "Try out these rose-flavored pavlova mini cakes for any celebration or holiday. You can store them in an airtight container for several days (without whipping cream) and decorate them whenever you want.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "18 pavlova cakes",
        "recipeIngredient": [
            "3 egg whites, at room temperature",
            "1 teaspoon dried rose petal powder",
            "\u00bd cup white sugar, or more to taste",
            "1 teaspoon vanilla extract, or to taste",
            "\u00bd teaspoon rose extract, or to taste",
            "1 teaspoon cornstarch",
            "1\u2009\u00bd tablespoons cream cheese",
            "1 cup heavy whipping cream",
            "2 tablespoons white sugar",
            "1 tablespoon dried rose petals"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 225 degrees F (110 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine egg whites and rose petal powder in the bowl of a stand mixer; beat until soft peaks form. Start beating in 1/2 cup sugar gradually. Increase speed to high and beat until stiff and glossy peaks form. Mix in vanilla extract and rose extract. Fold cornstarch very gently into the meringue using a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon meringue into a piping bag fitted with a fine round tip. Pipe into 18 individual rounds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges appear dry, 1 hour 15 minutes. Turn off oven and leave pavlovas inside the oven for another 20 to 25 minutes. Remove from oven and let cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese in a bowl for 30 seconds. Add heavy cream and 2 tablespoons sugar; beat until fluffy. Spoon whipped cream into another piping bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pipe whipped cream over cooled pavlovas. Decorate with dried rose petals.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Meringue Cookies"
        ],
