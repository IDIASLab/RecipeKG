    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260585/bbq-country-style-pork-ribs-sous-vide/",
        "name": "BBQ Country-Style Pork Ribs - Sous Vide",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4470803.jpg",
            "width": 1080,
            "height": 1920
        },
        "datePublished": "2020-06-19T04:24:46.000Z",
        "description": "Slow cooked with just a bit of flavor and then finished in the oven with your favorite sauce, these pork ribs are easy and delicious.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT8H2M",
        "totalTime": "P0DT8H12M",
        "recipeYield": "1 pound of ribs",
        "recipeIngredient": [
            "1 tablespoon kosher salt",
            "1 tablespoon white sugar",
            "1 pound pork spareribs",
            "\u00bd teaspoon red pepper flakes, or to taste",
            "\u00bc teaspoon five-spice powder, or to taste",
            "\u00bc teaspoon ground black pepper, or to taste",
            "2 teaspoons liquid smoke flavoring, or to taste",
            "\u00bd cup barbecue sauce, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix salt and sugar together in a small bowl. Sprinkle all over ribs. Coat with red pepper flakes, five-spice powder, and black pepper. Slide ribs into a large vacuum-seal bag; pour in liquid smoke. Seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Immerse bag in a heat-proof container of water with a sous vide cooker. Set temperature to 140 degrees F (60 degrees C); cook for 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Set oven rack about 6 inches from the heat source and turn on the broiler. Place a wire rack on top of a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pull ribs out of the bag; spread onto the prepared baking sheet and brush with barbeque sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly, 4 to 5 minutes. Flip and continue baking until other side bubbles, 2 to 3 minutes more.\n"
            }
        ],
