    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223513/jimmys-clam-and-corn-fritters/",
        "name": "Jimmy's Clam and Corn Fritters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1260303.jpg",
            "width": 2375,
            "height": 1836
        },
        "datePublished": "2020-06-19T03:50:23.000Z",
        "description": "Came up with these because my son loves clam fritters and I love corn hush puppies! So instead of French fries for your next fried seafood dinner, give these a try!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bc cups finely ground cornmeal",
            "1 (14.75 ounce) can cream-style corn",
            "1 (6.5 ounce) can minced clams, drained",
            "1 small onion, finely chopped",
            "\u00bd cup all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "1 teaspoon white sugar",
            "\u00bc cup milk, or as needed",
            "1 quart vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cornmeal, cream-style corn, clams, onion, flour, baking powder, salt, and sugar in a bowl until batter is thoroughly combined. If batter is too thick, stir in milk, about 1 tablespoon at a time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop about 1 heaping tablespoon of batter per fritter and gently drop into the hot oil; deep-fry in small batches of 3 or 4 fritters until golden brown, 3 to 4 minutes. Drain fritters on paper towels.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Corn"
        ],
