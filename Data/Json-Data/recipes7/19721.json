    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273043/easy-rhubarb-pie/",
        "name": "Easy Rhubarb Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6929907.jpg",
            "width": 1200,
            "height": 1600
        },
        "datePublished": "2020-06-19T04:36:25.000Z",
        "description": "A spring tradition in my family. This wonderful recipe passed down from my great-grandmother combines the sweetness of a crumb topping with a tart custard-style rhubarb pie.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 cup white sugar",
            "2 tablespoons all-purpose flour",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "\u00be cup all-purpose flour",
            "\u00bd cup brown sugar",
            "\u2153 cup butter, softened",
            "2 cups 1-inch pieces rhubarb",
            "1 (9 inch) unbaked pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix white sugar, 2 tablespoons flour, egg, and vanilla in a small bowl until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 3/4 cup flour, brown sugar, and butter in another small bowl; mix until crumbly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place rhubarb into pie crust. Pour white sugar mixture over the rhubarb. Sprinkle with brown sugar mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until filling is bubbly and crust is golden brown, 30 to 40 minutes.\n"
            }
        ],
