        "recipeIngredient": [
            "\u00bd cup balsamic vinegar",
            "1 small onion, chopped",
            "1 tablespoon soy sauce",
            "3 tablespoons honey",
            "1 tablespoon white sugar",
            "2 cloves garlic, minced",
            "\u00bd teaspoon crushed red pepper flakes",
            "\u2154 cup extra-virgin olive oil"
