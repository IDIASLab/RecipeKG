    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269000/easy-three-berry-crumble-slab-pie/",
        "name": "Easy Three-Berry Crumble Slab Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5934446.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:33:28.000Z",
        "description": "This informal pie is baked on a sheet pan and serves a crowd! Use frozen berries for a fresh flavor any time of year. You may serve this with whipped cream or ice cream, if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "2 tablespoons all-purpose flour",
            "2 9-inch pie crusts",
            "7 cups frozen mixed berries (blackberries, blueberries, and raspberries)",
            "\u00be cup granulated sugar",
            "\u2153 cup arrowroot powder",
            "\u00bd teaspoon salt",
            "1 cup rolled oats",
            "1 cup all-purpose flour",
            "\u00be cup packed light brown sugar",
            "1 teaspoon apple pie spice",
            "\u00bd teaspoon salt",
            "7 tablespoons cold unsalted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly dust an 11x15-inch half-sheet pan with flour. Carefully place the 2 pie crusts into the bottom of the sheet pan, gently stretching and trimming the crusts to fit the bottom and up the sides of the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine berries, sugar, arrowroot powder, and salt in a large bowl; stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats, flour, brown sugar, apple pie spice, and salt in a medium bowl. Use a pastry knife to cut butter into the oat mixture until butter is evenly distributed throughout and the crumble topping mixture resembles small, uneven pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour berry mixture evenly onto the crust and add crumble mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until brown and bubbly, 45 to 55 minutes. Remove and allow to cool for 30 to 45 minutes\n"
            }
        ],
