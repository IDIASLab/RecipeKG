    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245454/butter-pecan-fudge/",
        "name": "Butter Pecan Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6033449.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T04:03:58.000Z",
        "description": "A smooth and deliciously-flavored fudge. You could also use walnuts instead of pecans. A co-worker shared this wonderful recipe with me and the entire office loved it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "64 1-inch pieces",
        "recipeIngredient": [
            "\u00bd cup butter",
            "\u00bd cup brown sugar",
            "\u00bd cup white sugar",
            "\u215b teaspoon salt",
            "\u00bd cup heavy whipping cream",
            "1 teaspoon vanilla extract",
            "2 cups confectioners' sugar",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter an 8-inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, brown sugar, white sugar, and salt in a saucepan; pour in cream. Bring to a boil, stirring until butter melts and sugar is dissolved. Let mixture boil, stirring often, for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove saucepan from heat; add vanilla extract and mix well. Stir in confectioners' sugar until smooth. Fold pecans into fudge.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour fudge into prepared pan and cool until firm, 1 to 2 hours. Cut into 1-inch squares.\n"
            }
        ],
