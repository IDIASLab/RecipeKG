    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257433/best-banana-yogurt-bread/",
        "name": "Best Banana Yogurt Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7730182.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:20:28.000Z",
        "description": "This bread is different because I use banana-flavored yogurt and sliced bananas instead of mashed. The consistency is lighter, not dense - definitely different and delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup butter, softened",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd teaspoons ground allspice",
            "3 overripe bananas, thinly sliced",
            "\u00bd cup banana-flavored yogurt",
            "3 tablespoons white sugar",
            "1 teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease an 8x4-inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup sugar and butter in a bowl; beat with an electric mixer until creamy. Beat in eggs and vanilla extract. Mix in flour and allspice gradually until batter is blended. Fold in bananas and banana-flavored yogurt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3 tablespoons sugar and cloves together in a small bowl; sprinkle over batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour.\n"
            }
        ],
