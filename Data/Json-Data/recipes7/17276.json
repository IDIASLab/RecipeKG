        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261842/moms-cheesecake-cookie-bars/",
        "name": "Mom's Cheesecake Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8248420.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:26:05.000Z",
        "description": "Found this cheesecake cookie bars recipe going through my mom's recipe box. Good memories...",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9x13-inch baking pan",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup chopped walnuts",
            "\u2154 cup butter",
            "\u2154 cup brown sugar",
            "2 (8 ounce) packages cream cheese, softened",
            "\u00bd cup white sugar",
            "2 eggs",
            "\u00bc cup milk",
            "1 teaspoon vanilla extract",
            "2 tablespoons lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, walnuts, butter, and brown sugar together in a bowl. Line a 9x13-inch baking pan with half the crust mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned, about 15 minutes. Remove crust and leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese and white sugar together in another bowl using an electric mixer until blended. Add eggs, milk, and vanilla extract; mix until blended. Pour on top of the baked crust. Sprinkle with remaining crust mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until set, about 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
