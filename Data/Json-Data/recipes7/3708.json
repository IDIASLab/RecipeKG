        "mainEntityOfPage": "https://www.allrecipes.com/recipe/181891/sweet-lime-iced-tea/",
        "name": "Sweet Lime Iced Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5250651.jpg",
            "width": 4752,
            "height": 4752
