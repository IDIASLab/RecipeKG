    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265471/date-muffins/",
        "name": "Date Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5645972.jpg",
            "width": 2709,
            "height": 2709
        },
        "datePublished": "2020-06-19T04:29:36.000Z",
        "description": "A variation of an old family recipe and my daughter's favorite. These are addictive if you love dates! Use fresh whole dates for best results.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "10 muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bc cup white sugar",
            "\u00be teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u215b teaspoon baking soda",
            "\u00bd cup pitted and chopped dates",
            "1 cup buttermilk, at room temperature",
            "1 egg, at room temperature",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease or line 10 cups of a muffin tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, sugar, baking powder, salt, and baking soda together in a bowl. Add dates, separating the pieces as you drop them into the flour mixture. Mix to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk buttermilk, egg, and butter together in a small bowl. Fold buttermilk mixture into flour mixture until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into prepared muffin tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the largest muffin comes out clean, about 15 minutes.\n"
            }
        ],
