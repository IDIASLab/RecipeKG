        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246470/wheat-free-passover-chocolate-chip-cookies/",
        "name": "Wheat-Free Passover Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Wheat-Free Passover Chocolate Chip Cookies"
        },
        "datePublished": "2020-06-19T04:06:04.000Z",
        "description": "This is a Passover recipe that is popular year round!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H36M",
        "recipeYield": "48 cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup margarine",
            "2 eggs",
            "1\u2009\u00bd cups matzo cake meal",
            "1 tablespoon orange juice",
            "1 teaspoon potato starch",
            "1 teaspoon salt",
            "8 ounces chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and margarine together in a large bowl with an electric mixer until light and fluffy. Beat eggs into the margarine mixture. Add matzo cake meal, orange juice, potato starch, and salt; mix until smooth dough forms. Stir in chocolate chips. Drop teaspoonfuls of dough 2 inches apart onto prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are light brown, 20 to 25 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
