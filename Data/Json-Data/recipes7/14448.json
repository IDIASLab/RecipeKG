    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254319/banana-brownie-cookies/",
        "name": "Banana Brownie Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Banana Brownie Cookies"
        },
        "datePublished": "2020-06-19T04:16:26.000Z",
        "description": "I was looking for a brownie recipe that used bananas and had lots of fiber. My 4-year-old is a chocoholic but needs some more fiber in her diet. Couldn't find anything so I experimented and discovered these cookies.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H38M",
        "recipeYield": "5 dozen brownies",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 cup whole wheat flour",
            "1 cup unsweetened cocoa powder",
            "1 cup oats",
            "1 cup ground flax seeds",
            "2 very ripe bananas, mashed",
            "2 eggs",
            "\u00bc cup vegetable oil",
            "2 teaspoons vanilla extract",
            "1 cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift all-purpose flour, whole wheat flour, and cocoa powder together in a large bowl. Stir in oats and ground flax seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat bananas, eggs, vegetable oil, and vanilla extract together in a large bowl with an electric mixer until well-blended. Beat in sugar. Mix in flour mixture until dough is sticky.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll a teaspoonful of dough into a ball with your hands and place on a nonstick baking sheet. Repeat with remaining dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges look dry but cookies are still soft, about 8 minutes. Cool on the baking sheets until set and easily handled, about 10 minutes.\n"
            }
        ],
