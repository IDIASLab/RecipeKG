        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237910/pudding-in-a-mug/",
        "name": "Pudding in a Mug",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1200479.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:59:19.000Z",
        "description": "For those cravings that need to be answered fast. This is a quick single-serving answer. Watch closely while heating to prevent boil over.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H8M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "\u00bd cup milk",
            "\u00bd (1 ounce) square semisweet baking chocolate",
            "2 teaspoons cornstarch",
            "1 tablespoon sugar",
            "1 dash salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place milk and chocolate in a 12-ounce microwave-safe mug or a 2-cup glass measuring cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on High until hot but not boiling, 60 to 90 seconds. Stir to melt chocolate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cornstarch, sugar, and salt together in a small bowl; blend into the milk mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on High for 30 seconds, stir, and continue to heat until mixture thickens and just begins to boil, about 15 seconds more.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Custards and Puddings",
            "Chocolate Pudding Recipes"
