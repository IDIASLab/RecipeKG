    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223312/nutella-hazelnut-cookies/",
        "name": "Nutella&reg; Hazelnut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2168521.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2020-06-19T03:50:15.000Z",
        "description": "These cookies are great for chocolate-hazelnut lovers. I make these all the time and everyone absolutely loves them.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H57M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bc cup unsweetened cocoa powder",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 cup butter, room temperature",
            "\u00be cup brown sugar",
            "\u00be cup white sugar",
            "2 large eggs",
            "2 tablespoons vanilla extract",
            "\u00bd cup chocolate-hazelnut spread, such as Nutella\u00ae",
            "\u00bd cup chopped toasted hazelnuts",
            "1 cup chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a baking sheet or line it with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, cocoa powder, baking soda, and salt in a bowl. Mix with a whisk to break up any lumps.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, brown sugar, and white sugar with an electric mixer in a large bowl until smooth. Beat in eggs one at a time until completely incorporated. Mix in vanilla and chocolate-hazelnut spread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly add in flour mixture and stir just until combined. Fold in chopped hazelnuts and chocolate chips. Drop the dough onto prepared baking sheet by tablespoons or with a cookie scoop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until edges look dry and cookies are fragrant, about 12 minutes. Allow to cool for 1 minute on baking sheet before transferring to a wire rack.\n"
            }
        ],
