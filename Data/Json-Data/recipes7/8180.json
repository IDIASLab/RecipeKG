    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229820/easy-cookie-brittle/",
        "name": "Easy Cookie Brittle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F965874.jpg",
            "width": 1936,
            "height": 1940
        },
        "datePublished": "2020-06-19T03:52:00.000Z",
        "description": "These easy and buttery cookie bars require only 5 ingredients and take just minutes to make. Perfect for potlucks or snacks, these are loved by everyone.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "2 dozen bars",
        "recipeIngredient": [
            "1  serving cooking spray",
            "1 cup butter, softened",
            "1 cup white sugar",
            "2 cups all-purpose flour",
            "1 cup semi-sweet chocolate chips, divided",
            "1 cup peanut butter chips, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a 15x10-inch baking pan with aluminum foil; spray aluminum foil layer with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar together with an electric mixer until light and fluffy; gradually beat in flour until combined. Fold 3/4 cup chocolate chips and 3/4 cup peanut butter chips into dough. Press dough into the prepared pan; sprinkle with remaining 1/4 cup chocolate chips and 1/4 cup peanut butter chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 25 minutes. Cool in pan for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove brittle by lifting aluminum foil out of pan and transferring to a wire rack to cool completely. Cut into 24 bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Brittle"
