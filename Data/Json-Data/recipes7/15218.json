    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256449/gluten-free-lemon-raspberry-cheesecake-squares/",
        "name": "Gluten-Free Lemon Raspberry Cheesecake Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Lemon Raspberry Cheesecake Squares"
        },
        "datePublished": "2020-06-19T04:19:16.000Z",
        "description": "A luscious treat with just the right blend of lemon and raspberry.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT11H50M",
        "recipeYield": "1 8x8-inch pan",
        "recipeIngredient": [
            "\u00be cup walnuts",
            "\u00be cup gluten-free oats",
            "6 tablespoons butter, melted",
            "\u00be cup superfine brown rice flour",
            "\u00bc cup white sugar",
            "1 teaspoon salt",
            "\u00bc cup seedless raspberry preserves",
            "2 (8 ounce) packages cream cheese, at room temperature  ",
            "\u00bd cup white sugar",
            "2 eggs at room temperature, beaten",
            "\u00bc cup pure maple syrup",
            "1 lemon, zested",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spread walnuts on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast walnuts in the preheated oven until fragrant, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine toasted walnuts and oats in a food processor; grind into a fine meal. Mix with melted butter in a bowl. Mix in brown rice flour, 1/4 cup sugar, and 1 teaspoon salt. Press mixture into an 8-inch glass baking dish to form crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until edges start to brown, about 30 minutes. Let cool, about 10 minutes. Spread raspberry preserves on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and 1/2 cup sugar in a bowl with an electric mixer until smooth. Beat in eggs, maple syrup, lemon zest, and 1/2 teaspoon salt. Spread cream cheese mixture evenly over the raspberry preserves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the edges start to brown and the center is mostly set, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool cheesecake to room temperature, at least 2 hours. Refrigerate until firm, 8 hours to overnight. Cut into squares.\n"
            }
        ],
