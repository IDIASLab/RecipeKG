        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216427/cream-of-mushroom-sauce/",
        "name": "Cream of Mushroom Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3860447.jpg",
