        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238427/simple-strawberry-salsa/",
        "name": "Simple Strawberry Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2220499.jpg",
