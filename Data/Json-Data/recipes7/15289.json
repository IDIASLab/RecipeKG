        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256678/caribbean-sorrel-tea/",
        "name": "Caribbean Sorrel Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4573955.jpg",
            "width": 2870,
            "height": 2868
        },
        "datePublished": "2020-06-19T04:19:26.000Z",
        "description": "Get dried sorrel flowers from Asian, African, or Caribbean stores.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT8H10M",
        "recipeYield": "1 pitcher",
        "recipeIngredient": [
            "9 ounces dried red sorrel buds",
            "3 teaspoons grated ginger",
            "3 strips dried orange zest",
            "1 white clove",
            "1 quart water",
            "\u2154 cup white sugar",
            "ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sorrel, ginger, dried orange zest, and clove in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a saucepan or kettle; pour over sorrel mixture. Stir in sugar until dissolved. Let mixture steep at room temperature, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain sorrel mixture through a fine-mesh sieve into a pitcher; discard solids. Serve over ice.\n"
            }
        ],
