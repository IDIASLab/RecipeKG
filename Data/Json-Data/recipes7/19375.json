    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/271000/sweet-easter-pineapple-casserole/",
        "name": "Sweet Easter Pineapple Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sweet Easter Pineapple Casserole"
        },
        "datePublished": "2020-06-19T04:35:12.000Z",
        "description": "Saw this on the local news and my mom made it for Easter as a side dish. Very tasty! Made it myself and realized that it tastes better the second day after being in the fridge.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup white sugar",
            "6 tablespoons all-purpose flour",
            "2 (8 ounce) packages cream cheese, softened",
            "2 (20 ounce) cans pineapple chunks, drained, 6 tablespoons juice reserved",
            "1 stick butter, melted",
            "1 cup crushed buttery round crackers (such as Ritz\u00ae), or more as needed",
            "2 cups grated sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a medium casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar and flour together in a large bowl. Gradually stir in cream cheese. Add drained pineapple chunks and stir until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine reserved pineapple juice, butter, and cracker crumbs in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread crumb mixture over the bottom of the casserole dish to make a crust. Pour pineapple mixture over crust and top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 25 to 30 minutes.\n"
            }
        ],
