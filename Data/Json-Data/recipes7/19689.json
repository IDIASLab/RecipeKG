    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272935/rhubarb-angel-dessert/",
        "name": "Rhubarb Angel Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rhubarb Angel Dessert"
        },
        "datePublished": "2020-06-19T04:36:21.000Z",
        "description": "This is always a hit on our 'Snack Mom Friday' at work. Perfect summertime recipe to use up rhubarb and delicious too!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "\u00bd cup cold butter",
            "\u00bc teaspoon salt",
            "1 cup white sugar",
            "2 tablespoons all-purpose flour",
            "\u00bc teaspoon salt",
            "3 egg yolks",
            "\u00bd cup heavy whipping cream",
            "2\u2009\u00bd cups chopped rhubarb",
            "3 egg whites",
            "1 teaspoon vanilla extract",
            "6 tablespoons white sugar",
            "\u00bc cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup flour and sugar in a bowl. Cut in butter until mixture resembles coarse crumbs. Press crust into an ungreased 8-inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 15 to 20 minutes. Remove from oven, leaving heat on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, 2 tablespoons flour, and salt in a bowl. Beat in egg yolks and heavy cream. Stir in rhubarb. Pour over the hot crust in the baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pan to the hot oven and bake 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites and vanilla extract together in a bowl at medium speed until soft peaks form. Gradually add sugar and beat meringue until stiff peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pan from the oven, leaving heat on. Spread meringue over hot filling in the baking pan, spreading to seal edges. Sprinkle coconut on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pan to the hot oven and bake 15 minutes more.\n"
            }
        ],
