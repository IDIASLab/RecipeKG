    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247102/sencha-green-tea-cake/",
        "name": "Sencha Green Tea Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sencha Green Tea Cake"
        },
        "datePublished": "2020-06-19T04:06:42.000Z",
        "description": "An amazingly flavored cake made from the Japanese green tea called Sencha. I used loose Sencha green tea which I bought in a duty-free store at Narita airport. But you can always buy them in any Asian grocery store together with the ice green tea powder.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 cake",
        "recipeIngredient": [
            "\u215c cup boiling water",
            "2 tablespoons Sencha green tea leaves",
            "cooking spray",
            "\u00bd cup vegetable oil",
            "3 eggs",
            "\u00be cup white sugar",
            "1\u2009\u2153 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "1 (16 ounce) package prepared white frosting",
            "1 teaspoon green tea powder, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine boiling water and tea leaves in a bowl; allow to steep for about 5 minutes. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray a cake pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vegetable oil in to tea. Strain tea leaves from tea using a strainer or paper coffee filter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs in a bowl; add sugar and mix well. Sift flour and baking powder together in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix half of the flour mixture into the egg mixture, stirring quickly. Add half of the cooled tea mixture to flour-egg mixture, mixing well. Add the remaining flour mixture to the batter, mixing well. Stir the remaining cooled tea mixture to batter, mixing well. Pour batter into the prepared pan and place on top of a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the cake comes out clean, about 35 minutes. Cool cake for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread frosting onto cake and sprinkle green tea powder on top.\n"
            }
        ],
