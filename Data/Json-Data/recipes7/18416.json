    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267581/peach-sorbet/",
        "name": "Peach Sorbet",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5650713.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "2020-06-19T04:31:47.000Z",
        "description": "Use very ripe peaches for this sorbet; one way of knowing they are ripe is when the skins come off effortlessly.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT5H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ripe peaches - peeled, pitted, and chopped",
            "2 tablespoons lemon juice",
            "\u00bd cup water",
            "\u00bd cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Add peaches to a blender; blend until smooth. Measure out 1 1/2 cups peach puree into a bowl. Immediately stir in lemon juice and refrigerate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water and sugar in a small saucepan and bring to a boil. Stir until sugar is dissolved, about 1 minute. Remove from stove and cool to room temperature. Refrigerate simple syrup until chilled, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chilled peach puree and simple syrup into an ice cream maker and freeze according to manufacturer's instructions, about 20 minutes. Transfer to an airtight container and freeze until firm, about 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Peach Dessert Recipes"
        ],
