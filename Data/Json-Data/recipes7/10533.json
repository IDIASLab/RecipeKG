    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235739/bushman-traditional-breakfast-christmas-tree/",
        "name": "Bushman Traditional Breakfast Christmas Tree",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bushman Traditional Breakfast Christmas Tree"
        },
        "datePublished": "2020-06-19T03:57:29.000Z",
        "description": "Every year we make this for part of our big Christmas breakfast. We also sing Happy Birthday to Jesus.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages refrigerated crescent rolls",
            "3 tablespoons white sugar",
            "2 tablespoons melted butter",
            "1 teaspoon grated orange zest",
            "1 cup confectioners' sugar",
            "2 ounces cream cheese, softened",
            "2 tablespoons orange juice",
            "1 teaspoon red decorating sugar, or as needed",
            "1 teaspoon green decorating sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice each roll of crescent roll dough into 6 equal pieces for a total of 12 pieces and flatten each piece. Arrange 10 pieces into a triangle forming the 'tree' on a baking sheet. Arrange the remaining 2 dough pieces as the 'trunk.'\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk white sugar, butter, and orange zest together in a bowl until smooth; brush onto 'tree.'\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat confectioners' sugar, cream cheese, and orange juice together in a bowl using an electric mixer until smooth and creamy; brush into baked 'tree.' Sprinkle 'tree' with red and green decorating sugar.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
