        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/155551/macaroons/",
        "name": "Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F219487.jpg",
            "width": 2272,
            "height": 1704
