    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229901/our-apple-butter/",
        "name": "Our Apple Butter",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1665779.jpg",
            "width": 1224,
            "height": 1632
        },
        "datePublished": "2020-06-19T03:52:05.000Z",
        "description": "My 9-year-old son said 'Mom, you have to share this with America!' It's a huge hit at our house and easy to make.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT9H0M",
        "totalTime": "P0DT9H15M",
        "recipeYield": "11 4-ounce jars",
        "recipeIngredient": [
            "5\u2009\u00bd pounds McIntosh apples - peeled, cored, and chopped",
            "\u00be cup white sugar",
            "\u00be cup dark brown sugar",
            "1 tablespoon vanilla extract",
            "2\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground ginger"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend apples in batches in a blender until no large chunks remain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine blended apples, white sugar, brown sugar, vanilla, cinnamon, cloves, nutmeg, and ginger in a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook uncovered on High for 1 hour. Place cover on slow cooker and continue cooking, stirring about twice per hour, until smooth, 6 to 8 hours. Remove the cover and continue cooking and stirring until thick and spreadable, 2 to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon into jars leaving about an inch of space at the top; seal and freeze.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Fruit Butter Recipes"
        ],
