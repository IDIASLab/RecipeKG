        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218128/5-minute-cookies/",
        "name": "5 Minute Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F856056.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:45:02.000Z",
        "description": "Cookies to make in just 5 minutes!!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "5 dozen cookies",
        "recipeIngredient": [
            "2 cups rolled oats",
            "1 cup flaked coconut",
            "1 cup peanut butter",
            "2 cups white sugar",
            "\u00bc cup unsweetened cocoa powder",
            "\u00bc cup butter",
            "\u00bd cup milk",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line baking sheets with waxed paper. Mix the oats, coconut, and peanut butter in a  bowl until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sugar, cocoa powder, butter, and milk in a large saucepan, and bring to a boil over medium heat. Stir until the mixture is smooth and the sugar has dissolved, about 1 minute; remove from heat. Mix the oat mixture into the liquid mixture; stir in vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop up about 1 tablespoonful of the dough, roll it into a ball, and flatten. Place on waxed paper, and allow to cool.\n"
            }
        ],
