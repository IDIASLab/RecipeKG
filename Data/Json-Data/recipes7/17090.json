        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd red bell pepper, minced",
            "\u00bd red onion, minced",
            "2 cloves garlic, minced",
            "\u00bd (15.25 ounce) can sweet corn, drained",
            "\u00bd (15 ounce) can black beans, drained and rinsed",
            "3 ripe avocados, peeled, pitted and sliced",
            "2 tablespoons fresh lime juice",
            "\u00bc teaspoon salt",
            "1 pinch ground black pepper, or to taste",
            "\u00bd cup chopped fresh cilantro, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir red bell pepper, red onion, garlic, sweet corn, black beans, and avocado slices together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lime juice, salt, black pepper, and cilantro into avocado mixture until desired texture is achieved.\n"
            }
