    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230890/midgs-mouth-watering-beef-jerky/",
        "name": "Midg's Mouth Watering Beef Jerky",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2301289.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:53:03.000Z",
        "description": "Better than store-bought! My version is mouthwatering and full of flavor!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT5H0M",
        "totalTime": "P0DT17H45M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "5 pounds boneless beef sirloin",
            "2 cups soy sauce",
            "1 cup water",
            "3 dashes Worcestershire sauce",
            "3 tablespoons white sugar",
            "3 tablespoons salt",
            "\u00bd teaspoon onion powder",
            "\u00bc teaspoon garlic powder",
            "2 teaspoons liquid smoke flavoring",
            "mesquite or hickory wood chips, as needed for smoker"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut beef into 1/4-inch thick slices. Combine soy sauce, water, Worcestershire sauce, sugar, salt, onion powder, garlic powder, and liquid smoke. Pour marinade into heavy-duty resealable bag. Add beef and mix well; seal bag and refrigerate 12 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove beef from marinade, pat dry with paper towels, and allow to stand for 30 minutes. Discard used marinade. Meanwhile, soak wood chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat smoker for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange beef on drying racks. Smoke for 5 to 7 hours, depending on how you like it, replenishing wood chips as necessary.\n"
            }
        ],
