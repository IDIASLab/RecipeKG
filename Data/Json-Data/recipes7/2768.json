    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/152210/pumpkin-cake-with-apple-top/",
        "name": "Pumpkin Cake with Apple Top",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F822772.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "2020-06-19T03:32:58.000Z",
        "description": "This is my own idea for a pumpkin cake, based on my favorite carrot cake recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups flour",
            "1 cup sugar",
            "2 teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground cardamom",
            "2 teaspoons lemon zest",
            "2 teaspoons ground ginger",
            "1 egg yolk",
            "2 cups pumpkin puree",
            "\u00be cup vegetable oil",
            "3 egg whites, beaten",
            "1 apple - peeled, cored, and sliced",
            "2 teaspoons white sugar",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Lightly grease a 9 inch round cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together the flour, sugar, baking powder, cinnamon, cardamom, lemon zest, and ginger in a large bowl. Add the egg yolk, pumpkin puree, and oil; mix until smooth. Gently stir the egg whites into the batter; pour into prepared pan. Arrange the apple slices atop the batter; sprinkle with sugar and cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until a knife inserted into the center of the cake comes out clean, about 40 minutes. Allow to cool before serving.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
