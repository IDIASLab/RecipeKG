        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229599/classic-daiquiri/",
        "name": "Classic Daiquiri",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1016644.jpg",
            "width": 1478,
