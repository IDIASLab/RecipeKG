        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 small roma (plum) tomatoes, chopped",
            "1 cucumber, chopped",
            "1 green bell pepper, chopped",
            "1 small yellow onion, chopped",
            "5 tablespoons malt vinegar",
            "1\u2009\u00bd tablespoons white sugar",
            "1 tablespoon olive oil",
            "1 teaspoon ground black pepper",
            "1 teaspoon salt",
            "\u00bd teaspoon minced garlic",
            "1 (12 ounce) jar kalamata olives",
            "1 (4 ounce) container crumbled feta cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix tomatoes, cucumber, green bell pepper, and onion together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk vinegar, sugar, olive oil, black pepper, salt, and garlic together in a small bowl; pour over vegetables. Add olives; toss to coat. Divide salad among four plates; top each serving with feta cheese.\n"
            }
        ],
