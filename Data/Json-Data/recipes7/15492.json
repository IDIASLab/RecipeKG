    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257287/cheesecake-de-dulce-de-leche/",
        "name": "Cheesecake De Dulce De Leche",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheesecake De Dulce De Leche"
        },
        "datePublished": "2020-06-19T04:20:13.000Z",
        "description": "This cheesecake is to-die-for. A velvety, scrumptious dulce de leche cheesecake baked to perfection, ideal for serving as a dessert or with coffee among friends.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT5H20M",
        "recipeYield": "1 9-inch cheesecake",
        "recipeIngredient": [
            "2 cups crushed digestive biscuits",
            "\u00bc cup finely chopped walnuts",
            "3\u2009\u00bd tablespoons melted butter",
            "7 ounces double cream",
            "2 (8 ounce) packages cream cheese, softened",
            "\u2154 cup white sugar",
            "1 (11.5 ounce) jar dulce de leche",
            "4 eggs",
            "2 tablespoons dulce de leche, or as desired",
            "1 tablespoon chopped walnuts, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9-inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine digestive biscuits, 1/4 cup chopped walnuts, and melted butter in a bowl; mix well. Press biscuit mixture into the bottom of the prepared springform pan using the back of a spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is set, about 5 minutes. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat double cream in a bowl using an electric mixer until soft peaks form. Combine cream cheese and sugar together in a separate bowl until well mixed; add 11.5-ounce jar dulce de leche and eggs, 1 at a time and mixing well after each addition. Avoid over-mixing the cream cheese mixture. Fold double cream mixture into cream cheese mixture until well combined; spoon over the cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the oven for 1 hour 15 minutes. Do not open the oven door while baking. Cool at room temperature for 30 minutes; refrigerate until chilled, at least 3 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Decorate cheesecake with 2 tablespoons dulce de leche and 1 tablespoon chopped walnuts.\n"
            }
        ],
