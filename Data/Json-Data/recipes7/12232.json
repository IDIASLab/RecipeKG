        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 prepared 9-inch single pie crust",
            "1 cup light corn syrup",
            "1 cup chopped pecans",
            "3 eggs, beaten",
            "\u00bc cup butter, melted",
            "\u00bc cup white sugar",
            "1 tablespoon orange juice",
            "1 tablespoon orange zest",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Press prepared pie crust into a pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat corn syrup, pecans, eggs, butter, sugar, orange juice, orange zest, and salt together in a bowl; pour into the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until pie filling is set in the center, about 45 minutes.\n"
            }
        ],
