    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266083/rebeccas-tropical-granola-grabbers/",
        "name": "Rebecca's Tropical Granola Grabbers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2290841.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T04:30:34.000Z",
        "description": "Made these as an alternative to chocolate chip cookies and the kids loved them! You can mix and match the nuts and dried fruits you prefer. Store tightly covered.",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "20 2-ounce cookies",
        "recipeIngredient": [
            "\u00be cup shredded coconut",
            "3 cups granola",
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup dried banana chips",
            "\u00bd cup coarsely chopped macadamia nuts",
            "\u00bd cup coarsely chopped dried pineapple",
            "\u00bd cup coarsely chopped dried mango",
            "1\u2009\u00be sticks butter",
            "\u00be cup firmly packed brown sugar",
            "\u00bd cup white sugar",
            "2 eggs",
            "\u00bd teaspoon coconut extract",
            "\u00bd teaspoon banana extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper; spread coconut on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend granola in a food processor until finely ground. Set aside in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast coconut in the preheated oven until lightly browned, about 5 minutes. Let cool. Transfer to the food processor and pulse until finely chopped. Mix into granola. Add flour, baking soda, and salt; whisk until mixed evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend banana chips in the food processor until finely chopped. Transfer to a bowl; add macadamia nuts, dried pineapple, and dried mango. Stir until mixed evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place butter, brown sugar, and white sugar in a large bowl. Beat with an electric mixer on medium speed until creamy. Add eggs, coconut extract, and banana extract; beat well. Add granola mixture and mix well. Fold in banana chip mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop rounded teaspoonfuls of the cookie dough onto ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 8 to 10 minutes. Cool on the baking sheets for a few minutes before removing to a wire rack. Let cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
        ],
