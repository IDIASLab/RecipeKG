        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273441/blueberry-compote/",
        "name": "Blueberry Compote",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9346900.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:36:59.000Z",
        "description": "This blueberry compote goes beautifully in the morning on hot pancakes.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 1/2 cups compote",
        "recipeIngredient": [
            "1 (16 ounce) package frozen blueberries, unthawed, divided",
            "\u2153 cup white sugar",
            "\u2153 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 the blueberries, sugar, and water in a small, heavy saucepan. Bring to a boil. Reduce heat and let simmer, stirring often, until blueberries burst, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir remaining blueberries into the simmering mixture. Continue to simmer until compote thickens and coats the back of a spoon, about 8 minutes.\n"
            }
        ],
