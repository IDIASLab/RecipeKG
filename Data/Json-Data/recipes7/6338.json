    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220396/moms-ranger-cookies/",
        "name": "Mom's Ranger Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F820387.jpg",
            "width": 1838,
            "height": 2441
        },
        "datePublished": "2020-06-19T03:47:39.000Z",
        "description": "Mom makes these and I love them so I thought I'd share them with you. They don't stay around long! They are soft and yummy and kids love them.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 dozen cookies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup brown sugar",
            "1 cup white sugar",
            "2 large eggs",
            "1\u2009\u00be teaspoons vanilla extract",
            "2 cups all-purpose flour",
            "2\u2009\u00bd tablespoons all-purpose flour",
            "\u00bc teaspoon salt",
            "1\u2009\u00bd teaspoons baking soda",
            "\u00bc teaspoon baking powder",
            "1\u2009\u00be cups rolled oats",
            "1\u2009\u00be cups corn flake cereal",
            "\u00bd cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, brown sugar, and white sugar together in a bowl until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in eggs and vanilla extract until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, whisk together both amounts of flour, salt, baking soda, and baking powder. Mix flour mixture into the liquid ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently mix rolled oats, corn flakes, and coconut into the dough until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop the dough by tablespoons onto baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the cookies are light golden brown, 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool in the pans for 10 minutes before removing to cool completely on a wire rack.\n"
            }
        ],
