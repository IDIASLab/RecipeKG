    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273061/chinese-egg-tarts/",
        "name": "Chinese Egg Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F578759.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T04:36:27.000Z",
        "description": "These Chinese egg tarts are a very good dessert that's almost like a custard! Use caution when eating the tart since the inside can still be very hot.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 tarts",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon white sugar",
            "\u00bc teaspoon baking powder",
            "\u215b teaspoon salt",
            "4 ounces butter, softened",
            "3 tablespoons cold water",
            "\u00be cup hot water",
            "\u00bd cup white sugar",
            "3 eggs",
            "2 tablespoons evaporated milk",
            "\u00bc teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease 12 small (3 1/2-inch) fluted brioche pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, sugar, baking powder, and salt together in a mixing bowl. Add butter and cold water; mix until dough forms into a ball.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough on a lightly floured work surface with a lightly floured rolling pin to 1/8-inch thickness. Cut dough into rounds and press lightly into the prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix hot water and sugar in a bowl until sugar melts. Whisk in eggs; pour in evaporated milk and vanilla extract and mix well. Divide egg mixture between dough shells.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until egg is almost firm in the middle, 14 to 16 minutes. Let cool 10 minutes before transferring to a wire rack.\n"
            }
        ],
