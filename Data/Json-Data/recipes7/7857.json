    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228940/tommys-tomato-gravy/",
        "name": "Tommy's Tomato Gravy",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tommy's Tomato Gravy"
        },
        "datePublished": "2020-06-19T03:51:17.000Z",
        "description": "Delicious Southern-style gravy to serve over biscuits or rice. It's been my youngest son's favorite since he was a little boy. Serve while still hot!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 teaspoons white sugar",
            "1 (16 ounce) can crushed tomatoes",
            "3\u2009\u00bd tablespoons self-rising flour",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2\u2009\u00bd tablespoons margarine",
            "1\u2009\u00bd cups milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle sugar directly into opened can of tomatoes. Mix flour, salt, and black pepper together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat margarine in a large skillet over medium-low heat. Sprinkle flour mixture into melted margarine, stirring constantly. Increase heat to medium-high; continue stirring flour mixture until dissolved and bubbling, 5 to 10 minutes. Add tomatoes; cook and stir until flour mixture is fully incorporated, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir milk, 2 to 3 tablespoons at a time, into tomato mixture; increase heat to high. Cook and stir sauce until warm and thickened, 2 to 3 minutes.\n"
            }
        ],
