    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263768/grilled-pineapple-dessert-with-greek-yogurt/",
        "name": "Grilled Pineapple Dessert with Greek Yogurt",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6040914.jpg",
            "width": 2340,
            "height": 2925
        },
        "datePublished": "2020-06-19T04:27:42.000Z",
        "description": "Grilled pineapple is one of my favorite desserts on a warm summer night when we are throwing a barbeque. I serve them with homemade mint sugar and sweetened Greek yogurt.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "6 savings",
        "recipeIngredient": [
            "2 (16 ounce) cans pineapple slices, drained",
            "2 lemons, juiced",
            "4 tablespoons white sugar, divided, or more to taste",
            "1 tablespoon minced fresh ginger",
            "3 tablespoons fresh mint leaves",
            "1 (7 ounce) container Greek yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place pineapple slices in a shallow bowl. Combine lemon juice, 2 tablespoons sugar, and ginger in a cup and pour over pineapple. Cover and marinate for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 2 tablespoons sugar and mint leaves in the bowl of a food processor; pulse until finely chopped. Drain marinated pineapple well, reserving marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate. Grill pineapple until soft and heated through, 2 to 3 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 3 to 4 tablespoons of the marinade into the Greek yogurt and sweeten with extra sugar if desired. Sprinkle grilled pineapple with mint sugar and serve with yogurt.\n"
            }
        ],
