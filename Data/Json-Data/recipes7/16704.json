    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260335/beanless-chili/",
        "name": "Beanless Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Beanless Chili"
        },
        "datePublished": "2020-06-19T04:24:37.000Z",
        "description": "This is a quick recipe to throw together and is great for chilly days. The optional broccoli bulks up the chili's nutrition but isn't noticeable in the final product. All recipes I post are made for those who need to live sulfite-free, which means these recipes are also clean-eating recipes.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 butternut squash, halved and seeded",
            "2\u2009\u00bd tablespoons extra-virgin olive oil, divided",
            "1 head broccoli",
            "1 pound ground beef",
            "\u00bc cup chopped yellow onion, or to taste",
            "1 clove garlic, minced",
            "1 tablespoon chili powder, or to taste",
            "2 teaspoons ground cumin, or to taste",
            "\u00bd teaspoon cayenne pepper, or to taste",
            "\u00bd teaspoon ground chipotle pepper, or to taste",
            "3 cups filtered water, or as needed",
            "1 (24 ounce) container crushed tomatoes",
            "1 red bell pepper, chopped",
            "1 yellow bell pepper, chopped",
            "1 teaspoon sea salt",
            "1 jalapeno pepper, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place squash halves on a baking sheet. Coat the flesh with 1 1/2 tablespoons olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, 25 to 30 minutes. Allow to cool until safe to handle. Scrape flesh into a bowl and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove broccoli florets from stems. Grind florets in a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the remaining olive oil in a skillet over medium heat. Add ground beef, onion, garlic, chili powder, cumin, cayenne pepper, and chipotle pepper. Cook and stir until browned and crumbly, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the beef to a stock pot over medium heat. Add the mashed squash, ground broccoli, water, tomatoes, red bell pepper, yellow bell pepper, and jalapeno pepper. Bring to a boil. Lower heat; simmer until flavors meld and vegetables are tender, at least 30 minutes.\n"
            }
        ],
