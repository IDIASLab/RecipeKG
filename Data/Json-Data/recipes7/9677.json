        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233523/mojitos-by-the-pitcher/",
        "name": "Mojitos by the Pitcher",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1249577.jpg",
