    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/76338/slow-cooker-cherry-cobbler/",
        "name": "Slow Cooker Cherry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4999507.jpg",
            "width": 5184,
            "height": 5184
        },
        "datePublished": "2020-06-19T03:11:19.000Z",
        "description": "This is a fun way to make cherry pie with this very easy and simple slow cooker recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (21 ounce) can cherry pie filling",
            "1 cup all-purpose flour",
            "\u00bc cup white sugar",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00bc cup butter, melted",
            "\u00bd cup milk",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Coat the inside of your slow cooker with cooking spray. Pour in the cherry pie filling. In a medium bowl, stir together the flour, sugar, baking powder, and salt. Make a well in the center, and pour in the melted butter, milk, and vanilla. Mix until well blended. Spread evenly over the cherry pie filling. Cover, and cook on High for 1 1/2 to 2 hours, or until a toothpick inserted into the topping comes out clean.\n"
            }
        ],
