    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254201/mulberry-spice-pie/",
        "name": "Mulberry Spice Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8066980.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:16:15.000Z",
        "description": "The entire back property of my family's farm is lined with mulberry trees. We picked quarts of these deliciously messy berries last year. Be prepared for purple stained hands if you want to get in on this recipe! Serve warm with a scoop of French vanilla bean ice cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (14.1 ounce) package pastry for a 9-inch double crust pie",
            "6 cups mulberries",
            "1 cup white sugar",
            "\u2154 cup all-purpose flour",
            "2 teaspoons ground nutmeg",
            "1 teaspoon ground allspice",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lay bottom pie crust in pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mulberries, sugar, flour, nutmeg, allspice, and salt together in a bowl; gently fold mixture with a wooden spoon until blended, 3 to 5 minutes. Pour mixture into bottom pie crust and cover with top crust. Seal the edges; make slits in the top with a sharp knife.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate pie until sealed edges appear firm, 10 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake pie in the preheated oven for 15 minutes. Reduce oven temperature to 350 degrees F (175 degrees C), and continue baking until crust is golden brown, about 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Fruit Pies"
        ],
