        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215353/honeydew-grape-tomato-salad/",
        "name": "Honeydew-Grape Tomato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2646638.jpg",
