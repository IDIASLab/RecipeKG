    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237046/broccoli-bacon-salad/",
        "name": "Broccoli Bacon Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Broccoli Bacon Salad"
        },
        "datePublished": "2020-06-19T03:58:27.000Z",
        "description": "This is a sweet and tangy salad with broccoli, raisins, and bacon. There's never any left when I make it for guests.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "7 strips bacon, or more to taste",
            "2 bunches broccoli, cut into florets",
            "\u00bd cup raisins",
            "1 cup heavy whipping cream",
            "\u2154 cup white sugar",
            "1\u2009\u00bd tablespoons all-purpose flour",
            "3 tablespoons white vinegar",
            "1 egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels; crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bacon, broccoli, and raisins together in a plastic bowl with a tight-fitting lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream, sugar, and flour together in a skillet over medium heat; cook, whisking constantly, until sugar and flour are dissolved, 2 to 3 minutes. Slowly whisk vinegar into cream mixture until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk about 1 tablespoon cream mixture and egg together in a bowl; add about 2 more tablespoons cream mixture until egg starts to warm. Whisk egg mixture into cream mixture in skillet; cook until dressing is thickened, about 5 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large bowl with ice and add water, creating an ice bath. Place skillet in ice bath to cool dressing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour dressing over broccoli mixture. Secure lid on bowl and shake until salad is evenly coated. Refrigerate until chilled, at least 30 minutes.\n"
            }
        ],
