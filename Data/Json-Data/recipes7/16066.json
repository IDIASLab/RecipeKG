    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258676/chinese-toffee-apple-fritters/",
        "name": "Chinese Toffee Apple Fritters",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chinese Toffee Apple Fritters"
        },
        "datePublished": "2020-06-19T04:22:19.000Z",
        "description": "This dessert is a wonderful change from banana fritters. Apple chunks are fried in batter, then coated in caramel and sprinkled with sesame seeds.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H11M",
        "totalTime": "P0DT0H26M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3\u2009\u00bd tablespoons all-purpose flour",
            "1 apple - peeled, cored, and cut into chunks",
            "\u00be cup cornstarch",
            "1 tablespoon water, divided",
            "1 egg, beaten",
            "oil for frying",
            "\u00be cup white sugar",
            "3 tablespoons vegetable oil",
            "\u00bd teaspoon toasted sesame seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour flour into a shallow bowl. Dredge apple chunks in flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cornstarch and 2 teaspoons water thoroughly in a small bowl. Mix in egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saucepan to 320 degrees F (160 degrees C). Dip apple chunks in the cornstarch mixture; cook in the hot oil until browned, about 3 minutes. Transfer to a plate using a slotted spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, 3 tablespoons vegetable oil, and remaining 1 teaspoon water in a separate saucepan over medium-low heat. Cook until sugar melts, turns light golden, and starts to bubble, 8 to 10 minutes. Mix in fried apple chunks until evenly coated with caramel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer caramel-coated apple chunks to a large serving dish; lift with chopsticks to separate and create sugar threads. Garnish with sesame seeds.\n"
            }
        ],
