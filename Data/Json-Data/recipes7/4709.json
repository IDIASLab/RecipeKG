    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/214104/fresh-rhubarb-torte/",
        "name": "Fresh Rhubarb Torte",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3651908.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "2020-06-19T03:42:00.000Z",
        "description": "I got this recipe several years ago from a cousin. It's wonderful served warm with ice cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 9x9-inch pan",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "2 tablespoons white sugar",
            "\u00bd cup butter, melted",
            "1\u2009\u00bc cups white sugar",
            "2\u2009\u00bd cups diced rhubarb",
            "2 tablespoons all-purpose flour",
            "\u2153 cup milk",
            "3 egg yolks, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 325 degrees F (165 degrees C). Prepare a 9x9-inch baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together 1 cup of flour, 2 tablespoons of sugar, and the melted butter until well combined; pat the mixture into the bottom of the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the crust in the preheated oven until very lightly browned, about 25 minutes. Set aside to cool slightly. Increase oven heat to 350 degrees F (175 degrees C.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together 1 1/4 cup of sugar, the rhubarb, 2 tablespoons flour, the milk, and beaten egg yolks until thoroughly combined; pour over the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the oven until set, 40 to 45 minutes. Serve warm.\n"
            }
        ],
