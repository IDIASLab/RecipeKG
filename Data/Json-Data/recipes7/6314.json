    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220260/dynamite-halibut/",
        "name": "Dynamite Halibut",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F801797.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:47:34.000Z",
        "description": "I created this recipe because my family just loves the dynamite dishes served in sushi restaurants. It's a hit for all ages! This would even be great on top of chicken breasts! Serve atop of sushi rice and a side of steamed broccoli. Garnish with a lemon wedge.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds halibut, cut into bite size pieces",
            "1 cup mayonnaise",
            "1 teaspoon fresh lemon juice",
            "\u00be teaspoon chile-garlic sauce",
            "\u00bd teaspoon white sugar",
            "\u00bd teaspoon soy sauce",
            "\u215b teaspoon sesame oil",
            "4 cups thinly sliced mushrooms",
            "2 cups thinly sliced onions",
            "1 lemon, cut in half",
            "Kosher salt, to taste",
            "1 tablespoon sesame seeds",
            "\u00be cup sliced green onions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack into the center of the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with aluminum foil and a layer of cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange halibut pieces on the prepared baking sheet so that all pieces are touching.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the mayonnaise, lemon juice, chili garlic sauce, sugar, soy sauce, and sesame oil in a bowl; fold the mushrooms and onion into the mayonnaise mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the mushroom mixture over the halibut pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze one lemon half over the mushroom mixture; sprinkle with Kosher salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until almost opaque, 15 to 20 minutes; remove the baking sheet from the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Raise the oven rack with the baking sheet to the next-highest level switch the oven to broil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue cooking the fish under the broiler until the top is browned, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze the remaining lemon half evenly over the fish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with sesame seeds and green onions.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Asian"
        ],
        "recipeCuisine": [
            "Asian"
        ],
