    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229463/peanut-butter-cookies-with-truvia-baking-blend/",
        "name": "Peanut Butter Cookies with Truvia&reg; Baking Blend",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6371128.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T03:51:40.000Z",
        "description": "These classic peanut butter cookies are tender, fulfilling and simple to make. Made with Truvia&reg; Baking Blend, these cookies have 70% less sugar* than the full sugar version.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "\u00be cup Truvia\u00ae Baking Blend",
            "1 cup peanut butter",
            "3 tablespoons shortening",
            "3 tablespoons unsalted butter plus",
            "2 teaspoons unsalted butter",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "2 cups all-purpose flour, sifted",
            "\u00be teaspoon baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Truvia&reg; Baking Blend, peanut butter, shortening, butter, egg, and vanilla in a large bowl and mix on low speed until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a second smaller bowl, whisk together flour, baking soda, and salt.  Add slowly to the larger bowl and mix until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1 1/4 inch balls, flattening slightly on the top and bottom.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookies 3 inches apart on a parchment-lined or ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a fork, press a crisscross pattern in each cookie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 12-14 minutes or until lightly browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow to rest 5 minutes on pan, then place cookies on a wire rack to cool completely.\n"
            }
        ],
