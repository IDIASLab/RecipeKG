        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238274/grandmothers-sour-cream-cucumber-salad/",
        "name": "Grandmother's Sour Cream Cucumber Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1247288.jpg",
