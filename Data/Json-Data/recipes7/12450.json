    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240814/peanut-butter-and-amaranth-cookies/",
        "name": "Peanut Butter and Amaranth Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2514674.jpg",
            "width": 1296,
            "height": 968
        },
        "datePublished": "2020-06-19T04:02:01.000Z",
        "description": "A chewy peanut butter cookie that has a crunchy texture with the goodness of amaranth cereal in every bite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups water",
            "\u00bd cup amaranth",
            "1 cup peanut butter",
            "1 cup white sugar",
            "\u00bd cup butter, softened",
            "1 large egg",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups whole wheat pastry flour",
            "1 cup quick-cooking oats",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water and amaranth together in a saucepan; bring to a boil, reduce heat to medium-low, and simmer until water is absorbed and amaranth grains are fluffy, about 20 minutes. Cool for 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cooked amaranth and peanut butter together in a large bowl until smooth. Beat in sugar until completely dissolved. Add butter to amaranth mixture and mix; stir in egg until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk all-purpose flour, pastry flour, oats, baking powder, baking soda, and salt together in a bowl. Gradually add flour mixture to amaranth mixture, stirring until the dough starts to come away from the sides of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into small balls and place 2-inches apart on a baking sheet. Dip a fork into flour and press dough balls with fork to flatten.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 15 minutes. Cool on the baking sheet for 3 to 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
