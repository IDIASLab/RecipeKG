    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262009/beef-bulgogi-lettuce-wraps/",
        "name": "Beef Bulgogi Lettuce Wraps",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Beef Bulgogi Lettuce Wraps"
        },
        "datePublished": "2020-06-19T04:26:19.000Z",
        "description": "Set out the bulgogi meat, rice, lettuce, and dipping sauce and let everyone wrap the fixings into bundles.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds beef flank steak",
            "\u2153 cup reduced-sodium soy sauce",
            "4 green onions, cut in 1-inch pieces ",
            "1 tablespoon sake",
            "1 tablespoon sesame oil",
            "4 teaspoons white sugar",
            "3 cloves garlic, minced",
            "2 teaspoons grated ginger root",
            "1 teaspoon ground black pepper",
            "1 tablespoon gochujang (Korean hot pepper paste)",
            "1 teaspoon soy sauce",
            "1 teaspoon sesame oil",
            "1 tablespoon sesame seeds, toasted",
            "4 cups hot cooked short-grain rice",
            "\u00bd English cucumber, sliced",
            "1 head Boston lettuce, separated into leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Freeze beef partially, until firm, about 1 hour. Slice across the grain into 1/2-inch slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/3 cup soy sauce, green onions, sake, 1 tablespoon sesame oil, sugar, garlic, ginger, and black pepper in a large bowl. Add beef; toss to coat. Cover with plastic wrap and refrigerate until sauce flavors are absorbed, 1 to 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir gochujang, 1 teaspoon soy sauce, and 1 teaspoon sesame oil together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler. Arrange marinated beef on a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil beef; stirring frequently, until glazed and dark brown, and nearly all marinade has evaporated, 15 to 20 minutes. Sprinkle with sesame seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange cooked beef, rice, and cucumber in serving bowls. Wrap portions of each in lettuce and dip into the gochujang sauce.\n"
            }
        ],
