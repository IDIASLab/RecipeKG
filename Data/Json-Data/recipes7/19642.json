    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272842/quick-lemon-cheesecake/",
        "name": "Quick Lemon Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5151667.jpg",
            "width": 1200,
            "height": 1600
        },
        "datePublished": "2020-06-19T04:36:16.000Z",
        "description": "A quick lemon cheesecake.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "1 8-inch cheesecake",
        "recipeIngredient": [
            "1 (3 ounce) package lemon-flavored gelatin ",
            "1 cup hot water",
            "\u00bc pound butter",
            "8 tablespoons white sugar",
            "2 (14.4 ounce) packages graham crackers, crushed into fine crumbs",
            "2 lemons, zested",
            "2 (8 ounce) packages cream cheese, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour gelatin mix into a bowl and add 1 cup hot water. Stir until dissolved. Set aside and do not chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a heat-resistant measuring cup in a microwave, about 30 seconds. Transfer melted butter to a bowl. Add sugar and mix well. Pour in graham cracker crumbs and mix well. Place crumb mixture into the bottom of an 8-inch pie pan and shape into a crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir lemon zest into warm gelatin mixture. Add cream cheese and mix until any lumps are gone. Pour filling into the prepared crust. Chill in the refrigerator for at least 2 hours.\n"
            }
        ],
