        "name": "Broccoli Cranberry Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4542396.jpg",
            "width": 2000,
            "height": 2000
