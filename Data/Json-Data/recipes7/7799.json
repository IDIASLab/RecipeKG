    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228807/blueberry-freezer-jam/",
        "name": "Blueberry Freezer Jam",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2782702.jpg",
            "width": 3156,
            "height": 4248
        },
        "datePublished": "2020-06-19T03:51:06.000Z",
        "description": "A quick and easy way to enjoy your favorite fruit freezer jam.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": "P1DT0H10M",
        "recipeYield": "50 servings",
        "recipeIngredient": [
            "4 cups white sugar",
            "2 cups crushed blueberries",
            "2 tablespoons lemon juice",
            "1 (2 ounce) package powdered fruit pectin"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir sugar and blueberries together in a bowl; let stand for 10 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir lemon juice and pectin together in a separate small bowl; add to blueberry mixture. Stir constantly until sugar is completely dissolved and no longer grainy, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sterilize the jars and lids in boiling water for at least 5 minutes. Pack the blueberry into the hot, sterilized jars, filling the jars to within 1/2 inch of the top. Run a knife or a thin spatula around the insides of the jars after they have been filled to remove any air bubbles. Wipe the rims of the jars with a moist paper towel to remove any food residue. Top with lids, and screw on rings. Let stand at room temperature for 24 hours until set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for up to 3 weeks or freeze for up to 1 year. Thaw in refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Jams and Jellies Recipes"
        ],
