        "mainEntityOfPage": "https://www.allrecipes.com/recipe/158940/the-ultimate-shaken-bloody-mary/",
        "name": "The Ultimate Shaken Bloody Mary",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F554778.jpg",
