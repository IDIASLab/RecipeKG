        "name": "Halloween Fruit Snacks",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5506091.jpg",
            "width": 2448,
            "height": 3264
