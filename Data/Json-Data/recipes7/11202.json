    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237624/golden-fruit-pizza/",
        "name": "Golden Fruit Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3236059.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T03:59:05.000Z",
        "description": "This fruit pizza is like no other! As opposed to the traditional cookie crust, this one is made with a delicious corn flake crust that is to die for! Best served with the fruit chilled. Any fruit you choose will work! You've got to try this one! Happy eating!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup crushed cornflakes",
            "2 tablespoons butter, softened",
            "2 tablespoons light corn syrup",
            "2 tablespoons white sugar",
            "1 tablespoon honey",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (7 ounce) jar marshmallow fluff",
            "\u00bd cup sliced strawberries, or to taste",
            "2 apricots, sliced, or to taste",
            "2 kiwi, peeled and sliced, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cornflakes, butter, corn syrup, and sugar in a bowl until evenly combined; press onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is golden brown, about 5 minutes. Drizzle crust with honey and cool in refrigerator, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cream cheese and marshmallow fluff together in a bowl until smooth and creamy; spread over cooled crust, keeping a 1/2-inch border of crust. Chill crust in refrigerator until completely cooled, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange strawberries, apricots, and kiwi over the cream cheese layer.\n"
            }
        ],
