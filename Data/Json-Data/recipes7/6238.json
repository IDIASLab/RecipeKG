        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219969/valentines-day-cookies/",
        "name": "Valentine's Day Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6341392.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T03:47:18.000Z",
        "description": "This cookie recipe is my own personal recipe I would just love to share with the world. What makes them so special is that the eggs are replaced with milk. Without the eggs it give the outside a cookie like shell but with a moist cakey inside.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "18 servings",
        "recipeIngredient": [
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bc cup white sugar",
            "\u00bd teaspoon baking soda",
            "2 pinches salt",
            "\u00bd cup honey",
            "\u2153 cup milk",
            "\u00bc cup butter, softened",
            "\u00bc teaspoon vanilla extract",
            "\u00bd cup semisweet chocolate chips",
            "\u00bd cup vanilla chips",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the flour, white sugar, baking soda, and salt in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat honey, milk, butter, and vanilla extract together in a large bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour mixture into the honey mixture until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in the chocolate and vanilla chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the dough into walnut-sized balls and place 2 inches apart onto nonstick baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 15 to 18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow cookies to cool; dust with confectioners' sugar.\n"
            }
        ],
