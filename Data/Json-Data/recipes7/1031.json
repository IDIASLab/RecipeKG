        "recipeIngredient": [
            "1 cup boiling water",
            "1 tea bag",
            "1 (1.5 fluid ounce) jigger Irish whiskey",
            "1 tablespoon milk",
            "1 teaspoon white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour boiling water into a mug, and place the tea bag in to steep for about 1 minute. Remove and discard the tea bag. Pour in the whiskey, milk and sugar as desired. Stir, drink, give me good rating, then relax.\n"
            }
