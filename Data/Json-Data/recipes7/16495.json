    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259748/gluten-free-vegan-banana-chocolate-chip-muffins/",
        "name": "Gluten-Free Vegan Banana Chocolate Chip Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Vegan Banana Chocolate Chip Muffins"
        },
        "datePublished": "2020-06-19T04:24:01.000Z",
        "description": "Great for people with allergies and other sensitivities. Also Celiac friendly. No one needs to know that these are special, they'll never know. Please enjoy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "cooking spray",
            "1 cup rice flour",
            "\u00be cup white sugar",
            "\u00bc cup blanched almond flour",
            "\u00bc cup cornstarch",
            "\u00be cup water, divided",
            "1 tablespoon dry vegan egg replacer",
            "2 ripe bananas, mashed",
            "6 tablespoons vegetable oil",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon baking soda",
            "\u00bd cup vegan chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 12-cup muffin tin or line cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift rice flour, sugar, almond flour, and cornstarch together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 1/4 cup water and egg replacer in a bowl until smooth. Stir in remaining 1/2 cup water, mashed bananas, and vegetable oil. Add rice flour mixture, cinnamon, salt, and baking soda; mix until completely blended. Fold in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling each 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, about 30 minutes. Let cool slightly before serving.\n"
            }
        ],
