    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244350/apple-pudding-with-rum-butter-sauce/",
        "name": "Apple Pudding with Rum Butter Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Pudding with Rum Butter Sauce"
        },
        "datePublished": "2020-06-19T04:03:19.000Z",
        "description": "This is a wonderful apple pudding cake served with a very yummy rum butter sauce and is ready in about 1 hour.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup butter, divided",
            "2 cups white sugar, divided",
            "2 eggs",
            "2 cups grated peeled apple",
            "1 cup all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "1 teaspoon nutmeg",
            "1 pinch salt",
            "\u00bd cup milk",
            "1 tablespoon rum",
            "1 pinch ground nutmeg, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a deep 4-quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1/2 cup butter and 1 cup sugar together in a large bowl with an electric mixer until light and fluffy. Stir eggs into butter mixture until smooth; stir in grated apple.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking soda, cinnamon, 1 teaspoon nutmeg, and salt together. Stir flour mixture into apple mixture; spoon into prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted near the center comes out clean, about 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining 1 cup sugar, remaining 1/2 cup butter, and milk in a small saucepan over low heat until sugar is melted, about 10 minutes; stir in rum and remaining pinch nutmeg just before serving. Serve sauce over pudding.\n"
            }
        ],
