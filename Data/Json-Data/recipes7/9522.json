        "name": "Hot Vidalia&reg; Onion Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2534260.jpg",
            "width": 1920,
            "height": 1920
