    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219055/cherry-chutney-grilled-chops/",
        "name": "Cherry Chutney Grilled Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F710722.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:45:53.000Z",
        "description": "Marinated pork chops, grilled to crisp perfection and basted in a sweet and spicy black cherry glaze. Serve with your favorite autumn ale alongside grilled veggies and or roasted potatoes!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT6H35M",
        "recipeYield": "8 pork chops",
        "recipeIngredient": [
            "2 cups cider vinegar",
            "2 teaspoons salt",
            "1 teaspoon garlic powder",
            "1 teaspoon dried basil",
            "\u00bd teaspoon crushed red pepper flakes",
            "8 bone-in pork chops",
            "1 (12 ounce) package frozen black cherries, thawed",
            "1 cup water",
            "\u00bd cup white sugar",
            "2 tablespoons chopped fresh mint"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the vinegar, salt, garlic powder, basil, and red pepper flakes in a large glass or ceramic bowl. Add the pork chops and toss to coat. Cover the bowl with plastic wrap; marinate in the refrigerator 6 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat; lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the grill heats, combine the cherries, water, and sugar in a saucepan over medium-low heat. Cook, stirring occasionally, until the sugar dissolves completely, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the pork chops from the marinade and shake off excess moisture. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the pork chops on the preheated grill until no longer pink in the center, 8 to 10 minutes per side. An instant-read thermometer inserted into the center should read 160 degrees F (70 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle the chutney over the pork chops; garnish with the mint.\n"
            }
        ],
