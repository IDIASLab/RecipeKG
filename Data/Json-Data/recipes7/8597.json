    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230708/zucchini-crisp/",
        "name": "Zucchini Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2411215.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2020-06-19T03:52:52.000Z",
        "description": "Just like apple crisp! We first made this when a neighbor gave us a HUGE zucchini he grew in his garden.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch crisp",
        "recipeIngredient": [
            "8 cups cubed peeled zucchini",
            "\u00bd cup white sugar",
            "\u2153 cup lemon juice",
            "2 teaspoons ground cinnamon",
            "1 teaspoon ground nutmeg",
            "1 cup brown sugar, packed",
            "1 cup rolled oats",
            "1 cup all-purpose flour",
            "\u00bd cup margarine"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix zucchini, white sugar, lemon juice, cinnamon, and nutmeg in a large bowl. Pour mixture into baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, oats, and flour in another bowl. Cut in margarine until mixture resembles coarse crumbs; sprinkle over zucchini.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until bubbly and zucchini is tender, 40 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
