    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232897/classic-key-lime-pie/",
        "name": "Classic Key Lime Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1017818.jpg",
            "width": 300,
            "height": 300
        },
        "datePublished": "2020-06-19T03:55:01.000Z",
        "description": "Creamy, sweet, and tart, this classic lime dessert is always a favorite.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1\u2009\u00bc cups graham cracker crumbs",
            "2 tablespoons sugar",
            "5 tablespoons butter, melted",
            "2 (14 ounce) cans EAGLE BRAND\u00ae Sweetened Condensed Milk",
            "\u2154 cup fresh lime juice",
            "\u2153 cup sour cream",
            "2 teaspoons grated lime peel",
            "1 cup heavy whipping cream",
            "\u00bc cup powdered sugar",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 325 degrees F. Combine graham cracker crumbs and sugar. Add melted butter and stir until evenly moistened. Press mixture into bottom and sides of a 9-inch pie plate to form crust. Bake 10 minutes or until crust begins to brown. Cool 15 minutes on wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk sweetened condensed milk, lime juice and sour cream in large bowl until blended. Stir in lime peel. Pour filling into crust. Bake 7 to 10 minutes or until tiny bubbles begin to form on surface of pie. Cool completely on wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream, powdered sugar and vanilla in large mixing bowl with electric mixer on medium speed until stiff. Top pie with whipped cream. Chill at least 1 hour before serving. Best served the same day.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "EAGLE BRAND\u00ae"
        ],
