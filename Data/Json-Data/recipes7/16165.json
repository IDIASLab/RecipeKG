    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258831/make-ahead-cauliflower-salad/",
        "name": "Make Ahead Cauliflower Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4537915.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2020-06-19T04:22:59.000Z",
        "description": "My mom's famous make-ahead cauliflower salad for picnics and barbeques.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P1DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound bacon",
            "1 head iceberg lettuce, finely chopped",
            "1 head cauliflower, florets finely chopped",
            "1 large red onion, shaved thin",
            "1 cup mayonnaise",
            "3 tablespoons white sugar",
            "\u2153 cup grated Parmesan cheese",
            "1 dash ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Chop into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer iceberg lettuce, cauliflower, red onion, bacon, mayonnaise, sugar, and Parmesan cheese in a large bowl, in that order. Cover tightly with plastic wrap. Place a heavy pan on top of the salad to press it down.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for 24 to 36 hours, checking occasionally that salad is still pressed down.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss salad; season with black pepper.\n"
            }
        ],
