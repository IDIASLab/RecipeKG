    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264153/chocolate-beet-cake/",
        "name": "Chocolate Beet Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2778036.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:28:01.000Z",
        "description": "This is a moist, rich, dense chocolate cake with an earthy flavor. People are surprised to learn that there are beets in it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9x13-inch cake",
        "recipeIngredient": [
            "3 medium beets",
            "1 cup butter",
            "4 (1 ounce) squares unsweetened chocolate",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup whole wheat flour",
            "1 tablespoon baking soda",
            "2 teaspoons salt",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "1 teaspoon vanilla extract",
            "1 cup bittersweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place beets in a pot with water to cover. Bring to a boil and cook for 30 minutes. Remove skin and puree beets in a blender or food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Butter a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over low heat. Add unsweetened chocolate and stir until melted. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine all-purpose flour, whole wheat flour, baking soda, and salt in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and eggs together in a large bowl using an electric mixer until creamy. Add the melted chocolate, beets, and vanilla extract. Beat until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour mixture and chocolate chips into the beet mixture just until combined. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a fork inserted into the center comes out clean, about 40 minutes.\n"
            }
        ],
