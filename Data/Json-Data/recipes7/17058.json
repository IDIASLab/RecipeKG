    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261247/carrot-cupcakes-or-cake-with-cream-cheese-frosting/",
        "name": "Carrot Cupcakes or Cake with Cream Cheese Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Carrot Cupcakes or Cake with Cream Cheese Frosting"
        },
        "datePublished": "2020-06-19T04:25:19.000Z",
        "description": "Delicious carrot cupcakes/muffins/cake!",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "24 cupcakes",
        "recipeIngredient": [
            "\u00bd cup white sugar, or to taste",
            "\u00be cup buttermilk",
            "3 eggs",
            "\u2153 cup vegetable oil",
            "2 teaspoons vanilla extract, divided",
            "1 (15 ounce) can pineapple chunks, drained",
            "2 cups grated carrots",
            "1 cup sweetened flaked coconut",
            "1 cup all-purpose flour",
            "1 cup whole wheat flour",
            "2 teaspoons baking soda",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon ground cinnamon",
            "2 cups confectioners' sugar",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line two 12-cup muffin tins with cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar, buttermilk, eggs, oil, and 1 teaspoon vanilla extract together in a large bowl. Stir in pineapple, carrots, and coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine all-purpose flour, whole wheat flour, baking soda, salt, and cinnamon in a separate bowl. Stir flour mixture gently into the carrot mixture. Fill lined muffin cups about 3/4-full of batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into a cupcake comes out clean, about 15 minutes. Remove from oven and set aside to cool, at least 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend confectioners' sugar, cream cheese, butter, and remaining 1 teaspoon vanilla extract together in a bowl using an electric mixer until creamy. Frost the cooled cupcakes.\n"
            }
        ],
