    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239331/idahoan-potato-crusted-rib-roast/",
        "name": "Idahoan Potato Crusted Rib Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Idahoan Potato Crusted Rib Roast"
        },
        "datePublished": "2014-09-12T18:18:14.000Z",
        "description": "Idahoan Potato Crusted Rib Roast is a special dish for a special occasion and developed for us by a special lady, Chef Casey Thompson of Top Chef fame.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (4 ounce) package Idahoan\u00ae Buttery Homestyle Flavored Mashed Potatoes",
            "1 tablespoon mayonnaise",
            "3 tablespoons Parmesan cheese, grated",
            "1 teaspoon kosher salt",
            "Black pepper",
            "2 eggs, beaten",
            "\u00bd cup all-purpose flour",
            "1 (4 ounce) package Idahoan\u00ae Buttery Homestyle Flavored Mashed Potatoes",
            "1 cup panko bread crumbs",
            "1 cup potato chips, crushed",
            "1 (5 pound) bone-in standing rib roast",
            "Salt and pepper",
            "\u00bc cup all-purpose flour",
            "3 tablespoons canola oil",
            "\u00bc cup whole-grain mustard"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees .\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the mashed potato crust:\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make Buttery Homestyle Flavored Mashed Potatoes to package directions substituting boiling milk for the water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the milk is incorporated, add the mayonnaise and the Parmesan. Season with the salt and pepper to taste. Cool for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Once cooled, add the eggs, and the flour. Mix until blended - the potatoes should appear thick and dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the crispy outer potato crust:\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the 3 ingredients and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the Roast:\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season the rib roast generously with salt and black pepper. Dust the outside with the all-purpose flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a cast iron or a large heavy saute pan over med-high heat. Add the canola oil and allow it to heat until simmering.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the rib roast carefully bone-side up or fat side down. Sear until golden in color.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the roast and place on a sheet tray. Allow it to cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Once cooled, brush the mustard onto the fat side of the roast and the under side. Coat the roast with the potato and egg mixture. Then coat the potato mixture with the crispy crust mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the roast in the preheated oven and roast for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn off the oven and making sure to keep the oven door closed, leave the roast in the oven for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After 3 hours, turn the oven back on to 375 degrees to reheat the roast - about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the roast and let it rest for 15 minutes. Slice and serve.\n"
            }
        ],
