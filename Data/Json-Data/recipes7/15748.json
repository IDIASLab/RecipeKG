        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257896/marshmallows-with-coconut/",
        "name": "Marshmallows with Coconut",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6224325.jpg",
            "width": 2567,
            "height": 2568
        },
        "datePublished": "2020-06-19T04:21:02.000Z",
        "description": "Better than marshmallows from the store.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "cooking spray",
            "\u00bd cup cold water",
            "2 (.25 ounce) packages unflavored gelatin",
            "\u00be cup water",
            "2 cups white sugar",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "2 cups toasted coconut, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray an 8-inch square baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cold water and gelatin together in a bowl until gelatin is dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 3/4 cup water to a boil in a saucepan; add sugar and bring back to a boil. Pour sugar mixture over gelatin mixture; add vanilla extract and salt. Beat with an electric mixer until fluffy, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mixture in the prepared baking pan; chill in the refrigerator for 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut marshmallows into squares with a knife dipped in hot water. Press squares into the toasted coconut.\n"
            }
        ],
