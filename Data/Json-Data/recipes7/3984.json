        "recipeIngredient": [
            "4 pounds cantaloupe, shredded",
            "1 (12 fluid ounce) can evaporated milk",
            "2 quarts water",
            "1\u2009\u00bc cups white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cantaloupe, evaporated milk, water, and sugar in a large pitcher, stirring until combined. Refrigerate for 10 minutes. Divide shredded cantaloupe and liquid into molds and freeze until firm, about 6 hours.\n"
            }
