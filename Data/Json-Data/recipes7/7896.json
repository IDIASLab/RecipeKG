    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229050/summery-corn-salad/",
        "name": "Summery Corn Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6931049.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2020-06-19T03:51:26.000Z",
        "description": "A light and refreshing salad perfect for a warm summer evening. I like to serve this as a side with Mexican meals.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 teaspoons butter",
            "3 ears fresh corn, husks and silks removed",
            "1 teaspoon garlic powder",
            "1 (15 ounce) can black beans, drained",
            "\u00bc cup chopped fresh cilantro",
            "\u00bc cup olive oil",
            "2 tablespoons red wine vinegar",
            "2 tablespoons lime juice",
            "1\u2009\u00bd teaspoons minced garlic",
            "1 teaspoon white sugar",
            "\u00bd teaspoon ground cumin",
            "\u00bd teaspoon chili powder",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1 teaspoon butter on each ear of corn and sprinkle ears with garlic powder. Wrap corn in aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill wrapped ears until corn kernels are hot and steaming, about 15 minutes. Unwrap ears and grill until kernels are lightly browned, about 5 more minutes, turning once. Let corn cool and cut kernels from the cobs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently stir corn, black beans, and cilantro together in a salad bowl. Whisk olive oil, red wine vinegar, lime juice, garlic, sugar, cumin, chili powder, salt, and black pepper in a separate bowl; pour dressing over corn mixture and toss gently to coat.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Corn Salad Recipes"
        ],
