        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216421/slow-cooker-cranberry-sauce/",
        "name": "Slow Cooker Cranberry Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2877919.jpg",
            "width": 3264,
