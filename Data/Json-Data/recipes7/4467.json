    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213013/rumaki-teriyaki/",
        "name": "Rumaki Teriyaki",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rumaki Teriyaki"
        },
        "datePublished": "2020-06-19T03:41:20.000Z",
        "description": "This is a tasty Filipino appetizer made with chicken livers, water chestnuts, and pineapple wrapped in bacon.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bc cup soy sauce",
            "\u00bc cup vinegar",
            "2 tablespoons white sugar",
            "1 pound chicken livers, rinsed and trimmed",
            "10 water chestnuts",
            "3 ounces pineapple chunks, drained",
            "5 slices bacon, cut in half crosswise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk the soy sauce, vinegar, and sugar together in a bowl until the sugar is completely dissolved; pour into a large skillet and place over medium heat. Cook the chicken livers in the sauce until no longer pink in the center and the juices run clear, about 15 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Slice the livers into bite-sized pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place one piece of liver, a water chestnut, and a pineapple chunk on a piece of bacon and secure the bacon wrap with a toothpick by inserting through all the ingredients. Place on a baking tray, and repeat until all the ingredients have been wrapped.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven on the second rack until the bacon is crispy, about 5 minutes. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Chicken"
        ],
