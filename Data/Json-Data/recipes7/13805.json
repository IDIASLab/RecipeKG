    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246084/chocolate-creme-brulee/",
        "name": "Chocolate Creme Brulee",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4569512.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T04:05:10.000Z",
        "description": "A great take on classic creme brulee, with just the right amount of chocolate.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H6M",
        "totalTime": "P0DT5H31M",
        "recipeYield": "8 6-ounce ramekins",
        "recipeIngredient": [
            "4 cups heavy whipping cream",
            "1 vanilla bean, split in half lengthwise",
            "\u00bc teaspoon salt",
            "8 egg yolks",
            "\u00be cup white sugar",
            "2 tablespoons white sugar",
            "\u00bd cup cocoa powder",
            "\u00bd cup brown sugar, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine heavy cream, vanilla bean, and salt in a saucepan over medium heat. Heat until surface of cream begins to shimmer, about 5 minutes. Remove from heat and take out vanilla bean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg yolks, 3/4 cup plus 2 tablespoons white sugar, and cocoa together in a large bowl until well-blended. Pour hot cream over egg mixture a little at a time, stirring constantly, until all cream is incorporated and custard is smooth. Strain custard into a large glass measuring cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange eight 6-ounce ramekins in a roasting pan. Pour custard into ramekins. Pour enough warm water into the pan to come halfway up the sides of the ramekins. Cover pan loosely with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until custard is firm around the edges, but still wobbly in the center, about 1 hour. Remove from oven and let cool to room temperature, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover ramekins with plastic wrap and refrigerate until chilled, about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove ramekins from refrigerator and blot any liquid on the surface with a paper towel. Arrange ramekins on a baking sheet. Top each custard with 1 tablespoon of brown sugar, spreading evenly to the edges.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until sugar is melted and crisp, 30 seconds to 2 minutes. Cool briefly, about 5 minutes, before serving.\n"
            }
        ],
