        "mainEntityOfPage": "https://www.allrecipes.com/recipe/109190/smooth-sweet-tea/",
        "name": "Smooth Sweet Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F216335.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:13:48.000Z",
        "description": "Southern sweet tea, perfect for hot summer days!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "2 quarts",
        "recipeIngredient": [
            "1 pinch baking soda",
            "2 cups boiling water",
            "6 tea bags",
            "\u00be cup white sugar",
            "6 cups cool water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle a pinch of baking soda into a 64-ounce, heat-proof, glass pitcher. Pour in boiling water, and add tea bags. Cover, and allow to steep for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove tea bags, and discard; stir in sugar until dissolved. Pour in cool water, then refrigerate until cold.\n"
            }
        ],
