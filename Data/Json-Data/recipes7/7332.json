    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223283/little-anns-peach-and-blueberry-pie/",
        "name": "Little Ann's Peach and Blueberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8390829.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:50:13.000Z",
        "description": "The very best peach pie you will find. The blueberries make the difference.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 pastry for a double-crust 9-inch pie",
            "3 cups sliced peaches",
            "1 cup blueberries",
            "2 tablespoons lemon juice",
            "1 cup white sugar",
            "2 tablespoons quick-cooking tapioca",
            "\u00bd teaspoon salt",
            "2 tablespoons butter, cut into pieces",
            "1 egg yolk, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C). Place one pie crust on the bottom of a pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss peach slices and blueberries in a large bowl with lemon juice. Combine sugar, tapioca, and salt in a small bowl; pour over peach mixture and toss to coat. Transfer to prepared pie plate and scatter butter pieces over fruit. Cover pie with remaining crust; press and seal the top and bottom crusts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut several slits in the top crust to allow steam to escape, then brush with egg yolk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until golden brown, 45 to 50 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
