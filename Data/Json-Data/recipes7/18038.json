    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265428/blueberry-custard-pie/",
        "name": "Blueberry Custard Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6708444.jpg",
            "width": 2756,
            "height": 2756
        },
        "datePublished": "2020-06-19T04:29:33.000Z",
        "description": "Blueberry pie is delicious, but add a custard for a little something different... I love this for a quick breakfast snack for guests!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "3 cups fresh blueberries",
            "1 (9 inch) prepared graham cracker crust",
            "1 cup all-purpose flour, divided",
            "\u00bd cup white sugar",
            "\u215b teaspoon salt",
            "\u2154 cup sour cream",
            "2 eggs",
            "\u00bd cup brown sugar",
            "\u00bd cup cold butter, cut into cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread blueberries over the bottom of the graham cracker crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1/2 cup flour, white sugar, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk sour cream and eggs together in a separate bowl. Fold in flour mixture. Pour batter over the blueberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 1/2 cup flour and brown sugar in a bowl. Cut in butter using a pastry cutter or 2 knives until mixture forms coarse crumbs. Scatter crumb topping over the pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until custard and topping are golden brown, 50 to 55 minutes. Transfer to a rack and cool completely before serving.\n"
            }
        ],
