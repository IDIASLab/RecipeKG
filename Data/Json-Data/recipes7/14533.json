    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254500/classic-bing-cherry-pie/",
        "name": "Classic Bing Cherry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Classic Bing Cherry Pie"
        },
        "datePublished": "2020-06-19T04:17:04.000Z",
        "description": "A nice variation to the sour cherry pie.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (15 ounce) package double crust ready-to-use pie crust",
            "4 cups Bing cherries, pitted and sliced",
            "1 cup white sugar",
            "\u00bc cup instant tapioca",
            "2 tablespoons lemon juice",
            "\u00bc teaspoon almond extract",
            "1 tablespoon unsalted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Press the bottom pie crust into a pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cherries, sugar, tapioca, lemon juice, and almond extract together in bowl. Refrigerate until cherry filling begins to set, about 10 minutes. Stir filling again; chill until flavors combine, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the filling into the bottom half of the pie crust; cover with top crust, crimp the edges to seal, and dot with butter. Cut vents into the top with a sharp knife.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes; rotate the pie 1/4 of a turn. Continue baking until the crust is golden brown, about 20 minutes.\n"
            }
        ],
