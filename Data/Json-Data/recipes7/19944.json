    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273638/double-chocolate-bread-pudding/",
        "name": "Double-Chocolate Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Double-Chocolate Bread Pudding"
        },
        "datePublished": "2020-06-19T04:37:19.000Z",
        "description": "The best chocolate bread pudding dessert! Easy to make and a people pleaser! Serve with whipped cream if you like, or ice cream - or both!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 8-inch square baking dish",
        "recipeIngredient": [
            "2 cups heavy cream",
            "\u2153 cup white sugar",
            "\u00bc cup whole milk",
            "6 ounces semisweet chocolate, chopped",
            "2 large eggs, lightly beaten",
            "1 teaspoon vanilla extract",
            "6 ounces day-old French bread, cut into 3/4-inch cubes",
            "\u00bc cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat heavy cream, sugar, and milk in a 2-quart saucepan over medium heat until sugar dissolves, stirring occasionally, about 5 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chopped chocolate into warm cream mixture using a wire whisk until smooth and chocolate has melted completely. Whisk eggs and vanilla extract into mixture gradually until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place French bread into the bottom of a shallow 8-inch square glass or ceramic baking dish. Pour chocolate mixture over bread. Cover and refrigerate for 30 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pudding from the refrigerator, uncover, and sprinkle with chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, uncovered, until set, 40 to 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven. Let cool 10 minutes before serving warm, or cover and refrigerate to serve cold later.\n"
            }
        ],
