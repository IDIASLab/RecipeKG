    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220818/patsys-half-baked-blueberry-pie/",
        "name": "Patsy's Half-Baked Blueberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3869370.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2020-06-19T03:48:03.000Z",
        "description": "Best blueberry pie ever created by my friend Patsy. Most blueberry pies lose the depth of taste of the original fruit and dissolve into sweetness. This one explodes with fresh blueberry flavor. Top with fresh cut strawberries and whipped cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 refrigerated crust for a 9-inch pie",
            "\u00bd (8 ounce) package cream cheese",
            "2\u2009\u00bd cups fresh blueberries",
            "3 tablespoons cornstarch",
            "1 cup cold water",
            "2\u2009\u00bd cups fresh blueberries",
            "1 cup white sugar",
            "1 pinch lemon zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press pie crust dough into a pie pan. Poke small holes into dough with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 15 minutes. Cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese in a small microwave-safe bowl; heat in microwave on High to soften, about 20 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread softened cream cheese in the bottom of cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer 2 1/2 cups blueberries over the cream cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cornstarch and cold water in a small bowl until dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cornstarch mixture, 2 1/2 cups blueberries, sugar, and lemon zest in a saucepan over medium heat; simmer until thickened, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the thickened blueberry mixture over the raw blueberries in the pie crust and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool pie in refrigerator until set, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
