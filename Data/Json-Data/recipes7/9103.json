        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232114/white-peach-sorbet/",
        "name": "White Peach Sorbet",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1032361.jpg",
