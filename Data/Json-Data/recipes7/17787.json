        "recipeYield": "12 ice pops",
        "recipeIngredient": [
            "\u2153 cup frozen strawberries",
            "\u2153 cup frozen raspberries",
            "\u00bd frozen banana",
            "2 tablespoons plain yogurt",
            "\u2153 cup water",
            "\u00bd cup frozen mango",
            "\u00bd frozen banana",
