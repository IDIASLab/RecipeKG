    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257984/poached-pear-pizza/",
        "name": "Poached Pear Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Poached Pear Pizza"
        },
        "datePublished": "2020-06-19T04:21:12.000Z",
        "description": "A yummy way to enjoy poached pears at a potluck or picnic!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H24M",
        "totalTime": "P0DT2H54M",
        "recipeYield": "1 sheet pan pizza",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bc teaspoon salt",
            "1 egg",
            "2 teaspoons vanilla extract",
            "1 cup butter, softened",
            "\u00bd cup white sugar",
            "3 tablespoons confectioners' sugar",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup confectioners' sugar",
            "1 tablespoon vanilla extract",
            "3 very firm Bartlett pears, cored and sliced 1/8-inch thick",
            "5 tablespoons brown sugar, divided",
            "\u00bc cup dry red wine",
            "2 teaspoons ground cinnamon",
            "1 teaspoon ground ginger",
            "1 pinch freshly grated nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour and salt together together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg and 2 teaspoons vanilla extract together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, white sugar, and 3 tablespoons confectioners' sugar in a large bowl; beat with an electric mixer until fluffy. Beat in egg mixture. Add flour and salt gradually, beating until dough comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out on a large piece of plastic wrap; wrap and chill until firm, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a rimmed baking sheet with parchment paper; spread dough in an even layer on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until center is firm and edges are golden, 10 to 15 minutes. Let cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cream cheese, 1/4 confectioners' sugar, and 1 tablespoon vanilla extract in a bowl until smooth. Spread icing over the cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover icing with sliced pears, overlapping slices slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 3 tablespoons brown sugar and red wine in a small saucepan over low heat; stir until brown sugar is dissolved, 3 to 5 minutes. Add cinnamon and ginger. Bring to a boil; reduce heat and simmer until reduced to a syrup, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat pear slices with syrup using a pastry brush. Sprinkle remaining 2 tablespoons brown sugar on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil pizza until brown sugar melts and browns, about 3 minutes. Grate fresh nutmeg over pizza; cut into squares.\n"
            }
        ],
