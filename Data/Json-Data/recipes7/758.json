    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/84858/hawaiian-pork-hash/",
        "name": "Hawaiian Pork Hash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6459899.jpg",
            "width": 5936,
            "height": 3963
        },
        "datePublished": "2020-06-19T03:12:03.000Z",
        "description": "Very popular Hawaiian pupu - you can even get pork hash at the 7-11 here.  My recipe is better - it includes shrimp!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "25 dumplings",
        "recipeIngredient": [
            "\u00bc pound ground pork",
            "\u00bc pound shrimp - peeled, deveined and minced to a paste",
            "1 egg white",
            "2 tablespoons chopped water chestnuts",
            "2 tablespoons chopped green onion",
            "2 tablespoons cornstarch",
            "2 teaspoons soy sauce",
            "1 teaspoon white sugar",
            "1 teaspoon minced garlic",
            "1 teaspoon oyster sauce",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bc teaspoon sesame oil",
            "1 (14 ounce) package round dumpling skins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the ground pork, shrimp, egg white, water chestnuts, green onion, cornstarch, soy sauce, sugar, garlic, oyster sauce, salt, pepper, and sesame oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place about 1 tablespoon of this filling onto the center of each dumpling wrapper, and bring the sides up to the top. Do not seal the top, as these dumplings are left open. Place dumplings in a steamer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set the steamer basket over a pan or wok of boiling water. Steam for 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Ground Pork Recipes"
        ],
