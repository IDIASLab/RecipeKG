    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241290/simply-good-eggs/",
        "name": "Simply Good Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2099753.jpg",
            "width": 7056,
            "height": 7056
        },
        "datePublished": "2020-06-19T04:02:18.000Z",
        "description": "Deviled eggs! For a little extra zing use spicy brown mustard.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 dozen eggs",
        "recipeIngredient": [
            "12 eggs",
            "3 tablespoons mayonnaise",
            "1\u2009\u00bd tablespoons dill pickle relish",
            "1 tablespoon white sugar",
            "1 teaspoon yellow mustard",
            "1 teaspoon cider vinegar",
            "salt and ground black pepper to taste",
            "1 pinch paprika, or as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a large pot and cover with cold salted water. Bring salt water to a boil and immediately remove pot from heat. Cover pot and let eggs stand in hot water for 15 minutes. Pour hot water out of pot and run cold water over eggs until completely cool. Peel eggs and cut in half lengthwise.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate the yolks from the whites of each egg. Mash the yolks in a bowl. Add mayonnaise, pickle relish, sugar, yellow mustard, vinegar, salt, and pepper to egg yolks and mix until smooth. Spoon yolk mixture into the egg whites and sprinkle paprika over yolk filling. Refrigerate until chilled, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Deviled Eggs"
        ],
