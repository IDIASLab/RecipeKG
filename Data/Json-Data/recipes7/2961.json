    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/157816/super-easy-chocolate-cupcakes/",
        "name": "Super Easy Chocolate Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F101296.jpg",
            "width": 1407,
            "height": 2048
        },
        "datePublished": "2020-06-19T03:33:39.000Z",
        "description": "An easy chocolate cupcake that will cure any chocolate lover's craving. I find that, instead of using frosting, a bit of chocolate syrup on the top works best!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "18 cupcakes",
        "recipeIngredient": [
            "\u00be cup shortening",
            "1\u2009\u2154 cups white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 teaspoon instant coffee granules",
            "1 pinch cayenne pepper",
            "2\u2009\u00bc cups all-purpose flour",
            "\u2154 cup unsweetened cocoa powder",
            "\u00bc teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "1 teaspoon salt",
            "1\u2009\u00bd cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease two 12 cup muffin pans or line with 18 paper baking cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, beat the shortening and sugar with an electric mixer until light and fluffy. Mix in the eggs, one at a time, then stir in vanilla, instant coffee and cayenne pepper. Combine the flour, cocoa, baking powder, baking soda and salt; stir into the batter, alternating with the water, just until blended. Spoon the batter into the prepared cups, dividing evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the tops spring back when lightly pressed, 20 to 25 minutes. Cool in the pan set over a wire rack. When cool, arrange the cupcakes on a serving platter and frost as desired.\n"
            }
        ],
