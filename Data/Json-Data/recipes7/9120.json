    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232171/brown-bears/",
        "name": "Brown Bears",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7826490.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:53:59.000Z",
        "description": "My kids love making these brown bear biscuits for breakfast whenever we go camping. They're super simple and no prep for mom!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bc cup ground cinnamon",
            "1 cup white sugar",
            "\u00bd cup butter, melted",
            "1 (10 ounce) can refrigerated biscuit dough"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cinnamon and sugar together in a bowl. Pour melted butter into another bowl. Separate biscuits and form each piece of dough into a rope 4 to 5 inches long. Wrap the dough pieces around sticks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Hold sticks over campfire and slowly turn until the biscuit dough is browned and set, 8 to 10 minutes. Dip biscuits into melted butter and then into cinnamon sugar. Eat biscuits from sticks.\n"
            }
        ],
