    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261646/german-stollen/",
        "name": "German Stollen",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4589918.jpg",
            "width": 1750,
            "height": 984
        },
        "datePublished": "2020-06-19T04:25:45.000Z",
        "description": "A very traditional stollen that I make every Christmas. This stollen is loaded with dried fruit, candied citrus and orange peel, and almonds.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT3H0M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "8 cups all-purpose flour",
            "3 ounces compressed fresh yeast",
            "1 pinch white sugar",
            "1 tablespoon lukewarm milk",
            "1\u2009\u00bd cups milk",
            "1 cup unsalted butter",
            "2 tablespoons unsalted butter",
            "1 cup white sugar",
            "2 egg yolks",
            "\u00bd teaspoon salt",
            "1\u2009\u00be cups chopped blanched almonds",
            "1\u2009\u00bc cups raisins",
            "6 tablespoons candied lemon peel",
            "6 tablespoons chopped candied orange peel",
            "2 tablespoons melted butter",
            "2 tablespoons confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place flour in a large bowl, make a well in the center, and crumble fresh yeast into it. Sprinkle in sugar and add 1 tablespoon milk. Cover and let rise at a warm place for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 1/2 cups milk and 1 cup plus 2 tablespoons unsalted butter in a saucepan over low heat until butter is melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour milk-butter mixture over yeast mixture and add 1 cup sugar, egg yolks, and salt. Knead until a soft dough forms. Cover with a clean dish towel and let rise in a warm place until doubled in volume, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix almonds, raisins, candied lemon peel, and candied orange peel together and fold into the dough. Shape dough into a loaf and place on the prepared baking sheet. Cover and let rest until the loaf has risen again slightly, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until toothpick inserted in the middle comes out clean, 45 to 60 minutes. Remove from oven. Brush hot stollen immediately with 2 tablespoons melted butter and dust with confectioners' sugar.\n"
            }
        ],
