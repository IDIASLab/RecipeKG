        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (15 ounce) can crushed pineapple",
            "1 cup white sugar",
            "2 eggs, beaten",
            "\u00bd cup milk",
            "5 slices bread, cut into cubes",
            "1 teaspoon vanilla extract",
            "\u00bd cup butter, melted",
            "ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pineapple, sugar, eggs, milk, bread, and vanilla extract together in a casserole dish. Pour melted butter over the pineapple mixture. Sprinkle cinnamon over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until set in the middle, about 30 minutes.\n"
            }
        ],
