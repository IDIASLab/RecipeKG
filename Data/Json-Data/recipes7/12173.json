    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240161/cream-cheese-pumpkin-pie/",
        "name": "Cream Cheese Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cream Cheese Pumpkin Pie"
        },
        "datePublished": "2020-06-19T04:01:28.000Z",
        "description": "A light, creamy take on pumpkin pie with a delicious oat crust.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT4H40M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "2 cups quick-cooking oats",
            "\u00bd cup confectioners' sugar",
            "\u00bc cup unsalted butter, melted",
            "2 tablespoons vegetable shortening, melted",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "1\u2009\u00bc cups brown sugar",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup pumpkin puree",
            "\u00bc cup white sugar",
            "1 teaspoon pumpkin pie spice",
            "1 cup frozen whipped topping (such as Cool Whip\u00ae), or more to taste, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9-inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, confectioners' sugar, butter, shortening, cinnamon, and salt in a bowl until the dry ingredients are entirely moistened; press evenly into the bottom and up the sides of the prepared pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until beginning to brown, about 20 minutes. Cool for at least 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar, cream cheese, pumpkin puree, white sugar, and pumpkin pie spice together with a mixer until smooth. Fold whipped topping into the pumpkin mixture until smooth; pour into the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate pie until filling is set, at least 4 hours.\n"
            }
        ],
