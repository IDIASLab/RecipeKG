        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup orange flavored drink mix (such as Tang\u00ae)",
            "\u00bd cup instant tea powder",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sugar, orange flavored drink mix, tea powder, cinnamon, and cloves in a large bowl. Store in a sealed container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, mix 2 to 3 tablespoons of the mixture with a cup of hot water.\n"
            }
