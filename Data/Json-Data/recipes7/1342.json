        "mainEntityOfPage": "https://www.allrecipes.com/recipe/99052/no-bake-peach-pie/",
        "name": "No Bake Peach Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2574495.jpg",
