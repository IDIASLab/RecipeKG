        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221316/vanessas-horchata/",
        "name": "Vanessa's Horchata",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4461733.jpg",
            "width": 3318,
            "height": 2212
        },
        "datePublished": "2020-06-19T03:48:26.000Z",
        "description": "Just as yummy as taqueria horchata!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT4H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2\u2009\u2154 cups uncooked white rice",
            "10 cups water, divided",
            "4 cinnamon sticks",
            "1 cup soy milk",
            "1 tablespoon vanilla extract",
            "1 cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place about half the rice and 2 cups water in a blender; pulse until rice is roughly ground.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour about 3 cups more water in blender and add 2 cinnamon sticks; blend until cinnamon sticks are broken to pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour rice mixture into a large pitcher.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Repeat the steps until all the rice, water, and cinnamon sticks are blended and poured into the pitcher.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and refrigerate pitcher for at least 4 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain rice mixture through a sieve into another pitcher or bowl and discard rice and cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir soy milk, sugar, and vanilla extract into the strained liquid; chill until ready to serve.\n"
            }
        ],
