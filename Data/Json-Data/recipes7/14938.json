        "totalTime": "P0DT0H20M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "cooking spray",
            "\u00bd cup rolled oats",
            "\u00bc cup pumpkin puree",
            "\u00bc cup white sugar",
            "1 egg white",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "1 pinch ground cinnamon, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, pumpkin puree, sugar, egg white, baking powder, baking soda, and cinnamon together in a bowl. Divide into 2 portions on the greased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 10 minutes.\n"
            }
        ],
