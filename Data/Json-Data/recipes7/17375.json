    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262163/moms-russian-tea/",
        "name": "Mom's Russian Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1879829.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:26:27.000Z",
        "description": "Nothing powdered or instant here. Just pure natural flavors to sip on a cold winter day. Smells great while it's brewing too. Vitamin C never tasted so good!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 cups",
        "recipeIngredient": [
            "9 cups water, separated",
            "4 family-size black tea bags (such as Lipton\u00ae)",
            "1 cup white sugar",
            "1 cup pineapple juice",
            "1 (6 ounce) can frozen orange juice concentrate, thawed",
            "\u00bd cup lemon juice",
            "6 cloves whole cloves, or more to taste",
            "2 cinnamon sticks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring 8 cups water to a boil in a large stockpot. Remove from heat. Add tea bags; steep, 5 to 7 minutes. Remove and discard tea bags and return pot to medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 1 cup water to a boil in a separate pot. Add sugar; stir well until dissolved. Pour mixture into the stockpot with the tea. Add pineapple juice, orange juice concentrate, and lemon juice. Stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put cloves in a tea ball and place into the stockpot with the tea-juice mixture. Cook for 10 minutes; do not boil. Turn heat down to very low; let simmer until flavors blend, 15 to 20 minutes more.\n"
            }
        ],
