    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/217933/classic-crisp-bread-and-butter-pickles/",
        "name": "Classic Crisp Bread and Butter Pickles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5598261.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2011-05-06T23:45:55.000Z",
        "description": "Classic 'bread and butter' pickles are perfect with lunches, picnics and barbecues.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "7 pints",
        "recipeIngredient": [
            "4 pounds 4- to 6-inch cucumbers, cut into 1/4-inch slices ",
            "2 pounds onions, thinly sliced",
            "\u00bd cup canning salt",
            "2 cups sugar",
            "2 tablespoons mustard seed",
            "2 teaspoons turmeric  ",
            "2 teaspoons celery seed",
            "1 teaspoon ginger",
            "1 teaspoon peppercorns",
            "3 cups vinegar",
            "Ball\u00ae Pickle Crisp\u00ae Granules",
            "7 Ball\u00ae or Kerr\u00ae Pint (16 oz) Jars with lids and bands"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cucumber and onion slices in a large bowl, layering with salt: cover with ice cubes. Let stand 11/2 hours. Drain; rinse; drain again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining ingredients except Pickle Crisp in a large saucepot; bring to a boil. Add drained cucumbers and onions and return to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack hot pickles and liquid into hot jars, 1/2-inch headspace. Add rounded 1/8 tsp Pickle Crisp&reg; Granules to each jar. Remove air bubbles. Adjust two-piece caps.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Process 10 minutes in a boiling-water canner, adjusting for altitude.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Ball&reg; Canning &amp; Recipes"
        ],
