        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223089/crunchy-curry-tuna-sandwich/",
        "name": "Crunchy Curry Tuna Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F858334.jpg",
