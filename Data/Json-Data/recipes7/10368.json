    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235314/roasted-cinnamon-spice-pumpkin-seeds/",
        "name": "Roasted Cinnamon Spice Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1602318.jpg",
            "width": 4928,
            "height": 3264
        },
        "datePublished": "2020-06-19T03:57:07.000Z",
        "description": "So fun to make with the kids! These sweet treats were a big hit with my kids and husband.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "6 cups boiling water",
            "1 cup raw pumpkin seeds",
            "1 teaspoon salt",
            "1 teaspoon olive oil",
            "1 tablespoon white sugar",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water, pumpkin seeds, and salt together in a pot; bring to a boil, reduce heat to low, and simmer until seeds are tender, about 10 minutes. Strain seeds, discarding water, and lightly pat seeds dry with a towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Lightly oil a baking sheet with olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread pumpkin seeds out in a single layer on the prepared baking sheet. Sprinkle sugar, cinnamon, and ginger evenly over seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every ten minutes, until seeds are fragrant and roasted, about 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Nuts and Seeds",
            "Pumpkin Seed Recipes"
        ],
