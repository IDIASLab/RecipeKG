    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272867/smores-pancakes/",
        "name": "S'mores Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1099827.jpg",
            "width": 1836,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:36:18.000Z",
        "description": "Quick and easy chocolate pancakes, garnished with the key components of s'mores.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup milk",
            "1 (.55 ounce) package instant hot chocolate mix",
            "2 cups baking mix (such as Bisquick \u00ae)",
            "2 eggs",
            "3 tablespoons white sugar",
            "1 tablespoon unsweetened baking cocoa",
            "1 teaspoon vanilla extract",
            "cooking spray",
            "3 tablespoons chocolate syrup, or to taste",
            "3 tablespoons whipped cream, or to taste",
            "3 tablespoons miniature marshmallows, or to taste",
            "1 graham cracker, crushed into fine crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk milk and hot chocolate together in a large bowl until smooth. Add baking mix, eggs, sugar, cocoa, and vanilla extract; whisk until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a large skillet with cooking spray and set over medium-high heat. Pour in 1/4 cup batter for each pancake. Cook until bubbles form on the top, 2 to 3 minutes. Flip with a spatula and continue cooking until golden brown on the edges, 2 to 3 minutes more. Transfer pancakes to a plate. Repeat with remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle chocolate syrup over pancakes. Top with whipped cream, marshmallows, and graham cracker crumbs.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
