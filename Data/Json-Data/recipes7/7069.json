    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222647/black-forest-cheesecakes/",
        "name": "Black Forest Cheesecakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1006021.jpg",
            "width": 2068,
            "height": 2071
        },
        "datePublished": "2020-06-19T03:49:42.000Z",
        "description": "A Yuletide treat, these easy mini-cheesecakes combine the perfect match of chocolate and cherries.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "12 chocolate sandwich cookies with creme filling",
            "2 (8 ounce) packages cream cheese, softened",
            "\u00be cup white sugar",
            "\u2153 cup baking cocoa",
            "1 teaspoon vanilla extract",
            "2 eggs",
            "1 (21 ounce) can cherry pie filling",
            "\u00bd cup whipped topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (160 degrees C). Line muffin cups with paper or foil muffin liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cookie top from each sandwich cookie; crush and set aside. Place cream-topped cookies in lined muffin cups, cream side up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, sugar, baking cocoa, and vanilla extract with an electric mixer in a large bowl until fluffy. Mix in eggs until blended. Fill prepared muffin cups 3/4 full; sprinkle 1/4 cup reserved cookie crumbs over top. Discard remaining crumbs or save for another use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until set, 20 to 25 minutes. Cool completely; cover and refrigerate for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each cheesecake with 2 tablespoons pie filling and a dollop of whipped topping just before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cherry Dessert Recipes"
        ],
