        "recipeYield": "3 servings",
        "recipeIngredient": [
            "2 limes",
            "3 cups water",
            "\u00bd cup white sugar",
            "3 tablespoons sweetened condensed milk",
            "1 cup ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Wash limes thoroughly. Cut ends off and discard, then slice into about 8 pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place limes, water, sugar, and sweetened condensed milk in a blender. Pulse 5 times. Strain mixture through a sieve into a large pitcher. Serve over ice.\n"
            }
        ],
