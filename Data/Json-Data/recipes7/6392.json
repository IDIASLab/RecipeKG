        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ground beef",
            "1 small onion, chopped",
            "\u00be cup ketchup",
            "\u00bc cup barbeque sauce",
            "1 tablespoon vinegar",
            "1 tablespoon prepared mustard",
            "1 tablespoon white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir the ground beef and onion in a large skillet over medium heat until the beef is completely browned and the onion softens, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the ketchup, barbeque sauce, vinegar, mustard, and white sugar through the ground beef mixture; simmer 15 minutes.\n"
            }
        ],
