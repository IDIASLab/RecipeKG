    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221150/chef-johns-chocolate-mint-brownies/",
        "name": "Chef John's Chocolate Mint Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4253976.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2012-04-12T21:39:09.000Z",
        "description": "This stripped-down recipe for brownies requires almost zero technique, and uses only cocoa powder to achieve a dense, chewy, very chocolaty treat. Top with mint icing for a unique spin on an old favorite.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "16 brownies",
        "recipeIngredient": [
            "\u00bd cup unsalted butter",
            "1\u2009\u215b cups sugar",
            "\u00be cup unsweetened cocoa powder",
            "2 large eggs",
            "\u00bd teaspoon vanilla extract",
            "\u00bc teaspoon salt",
            "\u2154 cup all-purpose flour",
            "1 cup powdered sugar",
            "2 tablespoons milk",
            "\u00bc teaspoon mint extract, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan over medium-low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar and cocoa powder in a large bowl; stir in melted butter until mixture is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in 1 egg, vanilla, and salt. Mix in second egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in flour and transfer to ungreased 8x8-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for about 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and cool in the baking dish for 10 minutes before removing to cool completely on a wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix powdered sugar, milk, and mint extract in a bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour icing over cooled brownies. Spread evenly and allow to set, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice into 16 brownies.\n"
            }
        ],
