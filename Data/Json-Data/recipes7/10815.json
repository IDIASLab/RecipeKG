    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236487/the-best-caesar-salad-dressing/",
        "name": "The Best Caesar Salad Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1104372.jpg",
            "width": 3400,
            "height": 3400
        },
        "datePublished": "2020-06-19T03:58:11.000Z",
        "description": "I have been working with chefs for years and found out how to make the best Caesar dressing. I thought I would share!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 cup mayonnaise",
            "\u00bc cup egg substitute",
            "\u00bc cup freshly grated Parmesan cheese",
            "2 tablespoons water",
            "3 cloves garlic, finely chopped",
            "1\u2009\u00bd tablespoons lemon juice",
            "1 tablespoon anchovy paste",
            "2 teaspoons white sugar",
            "\u00bd teaspoon dried parsley",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend mayonnaise, egg substitute, Parmesan cheese, water, garlic, lemon juice, anchovy paste, sugar, parsley, salt, and pepper in a blender until smooth, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer dressing to an airtight container and refrigerate 8 hours to overnight.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Salad Dressing Recipes",
            "Caesar Dressing Recipes"
        ],
