        "name": "Sliwkowka Czyli Nalewka ze Sliwek (Polish Purple Plum Liqueur)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4537039.jpg",
