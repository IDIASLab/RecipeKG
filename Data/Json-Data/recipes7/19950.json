    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273651/old-fashioned-bread-pudding/",
        "name": "Old-Fashioned Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Old-Fashioned Bread Pudding"
        },
        "datePublished": "2020-06-19T04:37:20.000Z",
        "description": "This may be an old-fashioned bread pudding dessert, but good taste never goes out of style!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 10x12-inch baking pan",
        "recipeIngredient": [
            "2 cups half-and-half",
            "8 large eggs",
            "1\u2009\u00bc cups white sugar",
            "1 cup heavy cream",
            "2 tablespoons raisins",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon vanilla extract",
            "1 loaf white bread, torn into large chunks",
            "1 stick butter",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Lightly grease a 10x12-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine half-and-half, eggs, sugar, heavy cream, raisins, cinnamon, and vanilla extract in a bowl; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange bread pieces on the bottom of the prepared pan. Pour cream mixture over bread. Allow to soak 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the baking pan into a larger pan; fill the larger pan with water halfway up the sides of the smaller pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a knife inserted into the center comes out clean, 60 to 75 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt butter in a small skillet over low heat. Pour into a small bowl; add confectioners' sugar and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove bread pudding from the oven. Poke holes throughout using a butter knife. Pour butter sauce over the top.\n"
            }
        ],
