    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235477/natalie-powells-knishes/",
        "name": "Natalie Powell's Knishes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1842552.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T03:57:15.000Z",
        "description": "My grandmother is of eastern European Jewish descent so this is a very authentic recipe!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "25 knishes",
        "recipeIngredient": [
            "1 cube chicken bouillon",
            "1 tablespoon water",
            "2 tablespoons butter",
            "2 onions, diced",
            "3 large potatoes, peeled and cut into chunks",
            "1 egg",
            "1 clove garlic, minced",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "4 cups all-purpose flour",
            "\u00bc cup white sugar",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "\u2154 cup vegetable oil",
            "2 eggs",
            "\u00be cup warm water",
            "1 tablespoon vegetable oil for brushing, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve chicken bouillon in 1 tablespoon water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over medium-high heat. Saute onions in hot butter until soft, 7 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes; drain and return potatoes to pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash potatoes, onion mixture, 1 egg, garlic, chicken bouillon, 1/4 teaspoon salt, and pepper together until potatoes are smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, sugar, baking powder, and 1/4 teaspoon salt together in a bowl; make a well in the center. Stir oil and 2 eggs into flour mixture. Add 3/4 cup water and beat until dough is smooth and elastic. Divide dough into 5 balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly oil a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll 1 dough ball out on a lightly floured surface into a very thin rectangle. Brush surface of the dough with oil. Place about 5 spoonfuls of the potato mixture in a line on the dough about 2 inches from the long edge. Roll dough around filling. Pinch one end of the roll. Use the edge of your hand to cut dough roll into 5 knishes and pinch the edges of each. Repeat for remaining dough balls and potato filling. Place knishes on prepared baking sheet and brush each knish with oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown and dough is cooked through, about 20 minutes.\n"
            }
        ],
