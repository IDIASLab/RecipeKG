        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218003/quick-strawberry-shortcake/",
        "name": "Quick Strawberry Shortcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8229097.jpg",
