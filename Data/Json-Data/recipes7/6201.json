    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219814/dads-banana-nut-bread/",
        "name": "Dad's Banana Nut Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F785162.jpg",
            "width": 4592,
            "height": 3056
        },
        "datePublished": "2020-06-19T03:47:08.000Z",
        "description": "My dad made this bread for years to the delight to our family. Pair it with a dish of fruit and a cup of coffee for breakfast. Yum! The flax seed meal gives a great texture without altering the flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "1 cup white sugar",
            "2 eggs",
            "2 very <a href=\"https://www.allrecipes.com/article/how-to-ripen-bananas-faster/\" aria-label=\"ripe bananas\">ripe bananas</a>, mashed",
            "1 cup self-rising flour",
            "\u00bc cup flax seed meal",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 325 degrees F (165 degrees C). Grease a 9x5-inch loaf pan, or line a muffin pan with cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, beat the butter and sugar with an electric mixer until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in the eggs one at a time, followed by the bananas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in the self-rising flour and flax seed meal until just incorporated; fold in the walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 20 minutes for muffins, or 1 hour for a loaf. Cool in the pan for 10 minutes before removing to cool completely on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
