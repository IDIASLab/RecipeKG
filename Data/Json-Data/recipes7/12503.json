        "name": "Microwave Chocolate Mug Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F10%2F26%2FMicrowave-Chocolate-Mug-Cake.jpg",
            "width": 1241,
