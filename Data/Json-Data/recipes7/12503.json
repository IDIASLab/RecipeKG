        "recipeYield": "1 cake",
        "recipeIngredient": [
            "\u00bc cup all-purpose flour",
            "\u00bc cup white sugar",
            "2 tablespoons unsweetened cocoa powder",
            "\u215b teaspoon baking soda",
            "\u215b teaspoon salt",
            "3 tablespoons milk",
            "2 tablespoons canola oil",
            "1 tablespoon water",
            "\u00bc teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, cocoa powder, baking soda, and salt in a large microwave-safe mug; stir in milk, canola oil, water, and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in microwave until cake is done in the middle, about 1 minute 45 seconds.\n"
            }
        ],
