        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230464/easy-eggnog-pancakes/",
        "name": "Easy Eggnog Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4457558.jpg",
            "width": 2420,
            "height": 2420
        },
        "datePublished": "2020-06-19T03:52:36.000Z",
        "description": "A twist on buttermilk pancakes and the perfect thing to do with the leftover eggnog from the night before!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 teaspoons vegetable oil, or as needed",
            "1 cup all-purpose flour",
            "1 tablespoon white sugar",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1\u2009\u00bc cups eggnog"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a griddle or shallow frying pan over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, sugar, baking powder, baking soda, and salt together in a bowl; stir eggnog into flour mixture until batter is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour enough batter into the heated griddle to make a 4- to 5-inch circle. Cook until bubbles appear around the edges and bottom is browned, about 5 minutes. Flip and cook until other side is evenly browned, about 5 more minutes. Repeat with remaining batter.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
