    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262946/majorcan-almond-cake/",
        "name": "Majorcan Almond Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5178394.jpg",
            "width": 1066,
            "height": 960
        },
        "datePublished": "2020-06-19T04:27:14.000Z",
        "description": "This naturally gluten-free cake is made with ground almonds, which are found aplenty on the Spanish island of Majorca (Mallorca). The cake should be beautiful and moist.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "1 teaspoon butter",
            "2 tablespoons gluten-free bread crumbs",
            "6 eggs",
            "1\u2009\u00bd cups white sugar",
            "2\u2009\u00be cups finely ground almonds (with skins)",
            "1 tablespoon lemon juice",
            "1 teaspoon ground cinnamon",
            "2 tablespoons rum"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a loaf pan or wide casserole dish and sprinkle with breadcrumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a large bowl with an electric mixer until light and fluffy. Add sugar and beat in well. Stir in almonds and continue beating until batter is well combined. Add lemon juice, cinnamon, and rum, one after the other, beating until well combined. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out with a few moist crumbs attached, 40 to 50 minutes. Cool on a wire rack for 5 minutes. Run a table knife around the edges to loosen. Invert carefully onto a serving plate or cooling rack. Let cool completely.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Spanish"
        ],
