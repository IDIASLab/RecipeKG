    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75425/hallelujah-noodles/",
        "name": "Hallelujah Noodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F579003.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:11:12.000Z",
        "description": "This is a recipe that has been passed down through my church. Great and easy to cook for large gatherings. Everyone I've cooked it for loves it.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package uncooked linguini pasta",
            "1 cup soy sauce",
            "\u00bd cup extra virgin olive oil",
            "\u00bd cup white sugar",
            "\u2153 cup distilled white vinegar",
            "1 tablespoon sesame seeds",
            "1 (10 ounce) package mixed baby salad greens",
            "1 (8 ounce) package imitation crabmeat, flaked",
            "1 bunch chopped cilantro",
            "1 avocado - peeled, pitted and diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Place linguini pasta in the pot, cook for 8 to 10 minutes, until al dente, and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, whisk together the soy sauce, olive oil, sugar, vinegar, and sesame seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, toss the cooked pasta with the soy sauce dressing, greens, imitation crabmeat, cilantro, and avocado. Chill until serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Seafood Salad Recipes",
            "Crab Salad Recipes"
