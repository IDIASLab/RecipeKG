    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/142412/rhubarb-and-strawberry-cobbler/",
        "name": "Rhubarb and Strawberry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2277932.jpg",
            "width": 2141,
            "height": 1440
        },
        "datePublished": "2020-06-19T03:30:38.000Z",
        "description": "This is a rhubarb cobbler recipe that is quick, simple, and has a lot of flavor. Serve warm with whipped cream or ice cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (18 ounce) package refrigerated sugar cookie dough",
            "8 cups chopped fresh rhubarb (1/2-inch pieces)",
            "\u2153 cup white sugar",
            "2 teaspoons ground cinnamon",
            "1 teaspoon lemon juice",
            "1 (21 ounce) can strawberry pie filling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place 8 large custard cups onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop the chilled cookie dough into 1/4 inch cubes and set aside. Mix the rhubarb with the sugar, cinnamon, and lemon juice. Fill each of the custard cups half full with the rhubarb mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the strawberry pie filling over the rhubarb. Arrange the cubed cookie dough on top of the filling. Leave some open spaces between the cubes, so that the filling does not overflow when baking. There may be some cookie dough left over.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the dough is fully cooked and starting to brown around the edges, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Strawberry Dessert Recipes"
        ],
