        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235879/campfire-smores-latte/",
        "name": "Campfire S'Mores Latte",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2278932.jpg",
