    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262025/easy-honey-baked-ham/",
        "name": "Easy Honey Baked Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Honey Baked Ham"
        },
        "datePublished": "2020-06-19T04:26:20.000Z",
        "description": "This recipe produces a nice, sweet, glazed honey-baked ham; the soaking reduces some of the salt from the ham.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT3H0M",
        "totalTime": "P2DT3H10M",
        "recipeYield": "1 8-pound ham",
        "recipeIngredient": [
            "2 cups white sugar",
            "water",
            "1 (8 pound) fully-cooked bone-in ham, or more to taste",
            "1 cup honey",
            "1 (6 ounce) can frozen orange juice concentrate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sugar and water in a bowl and stir to dissolve sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice ham just shy of the bone into 1/2- to 1-inch slices. Place ham into the water and sugar solution; add more water, so ham is completely covered with liquid. Seal with plastic wrap and let soak in the refrigerator for a minimum of 48 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine honey and orange juice in a bowl. Line a roasting pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain the ham slices, pat dry, and place in the roasting pan. Wrap aluminum foil around the bottom of the ham and pour 1/2 the orange juice mixture over the ham. Wrap ham completely in foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, basting with the remaining orange juice mixture, until a sticky, browned glaze forms, 3 to 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Ham",
            "Whole"
        ],
