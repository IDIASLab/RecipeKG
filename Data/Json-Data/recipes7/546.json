        "mainEntityOfPage": "https://www.allrecipes.com/recipe/79735/hot-and-sweet-dipping-sauce/",
        "name": "Hot and Sweet Dipping Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F370691.jpg",
