    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219930/grilled-corn-off-the-cob-salad/",
        "name": "Grilled Corn Off the Cob Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8248415.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:47:16.000Z",
        "description": "Every summer we have a 4th of July party. There are several dishes that our guests look forward to eating. This is one that makes everyone say Aaaaah, the corn salad...",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT12H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "12 ears fresh corn with husks",
            "5 stalks celery, diced",
            "\u00bd green bell pepper, diced",
            "1 small onion, diced",
            "2 tablespoons chopped pimento peppers",
            "\u00bd cup olive oil",
            "2 tablespoons balsamic vinegar",
            "1 teaspoon sea salt",
            "1 teaspoon Dijon mustard",
            "1 teaspoon white sugar",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat, and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill corn on preheated grill until husks are charred on all sides, about 20 minutes. Remove corn from grill and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove husks and silk from corn, then cut corn kernels from the cob.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss corn kernels, celery, green bell pepper, onion, and pimento peppers together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, balsamic vinegar, sea salt, Dijon mustard, sugar, and black pepper together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour dressing into the corn mixture and toss well. Cover and refrigerate for at least 12 hours and up to 3 days before serving.\n"
            }
        ],
        "recipeCategory": [
