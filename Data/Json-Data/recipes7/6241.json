        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219977/romantic-lemon-cheesecake-pancakes/",
        "name": "Romantic Lemon Cheesecake Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5136863.jpg",
            "width": 1780,
            "height": 1584
        },
        "datePublished": "2020-06-19T03:47:18.000Z",
        "description": "These delicate pancakes are elegant enough for dessert!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 ounces cream cheese, softened",
            "2 eggs, beaten",
            "5 teaspoons all-purpose flour",
            "1\u2009\u00bd teaspoons white sugar",
            "2 teaspoons butter, melted - divided",
            "confectioners' sugar for dusting",
            "1 teaspoon lemon juice",
            "\u00bd lemon, cut into wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat the cream cheese with an electric mixer until smooth. Blend in the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the flour, sugar, and 1 teaspoon of melted butter to make a batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining 1 teaspoon of butter on a griddle or in a heavy frying pan over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop the batter onto the griddle in silver dollar-sized dollops; cook until lightly golden brown, about 3 minutes. Turn the pancakes over and cook an additional 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, sprinkle with confectioners' sugar and drizzle with lemon juice. Serve with lemon wedges.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes"
        ],
