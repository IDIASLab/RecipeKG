    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269648/evoo-sugar-cookies/",
        "name": "EVOO Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3063915.jpg",
            "width": 1448,
            "height": 1448
        },
        "datePublished": "2020-06-19T04:34:03.000Z",
        "description": "Extra-virgin olive oil replaces butter in a simple sugar cookie recipe. The result is a rich, dense cookie to sink your teeth into. These are great for dunking in your tea.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H24M",
        "totalTime": "P0DT1H4M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "2 eggs",
            "\u2154 cup extra-virgin olive oil",
            "1 cup sugar",
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 pinch salt",
            "\u00bd cup chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 365 degrees F (185 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together eggs, olive oil, and sugar in a bowl using an electric mixer until smooth and pale gold in color. Add flour, baking powder, and salt and mix until just combined. Fold in chocolate chips. Shape dough into a ball and refrigerate for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a teaspoon to drop spoonfuls of dough onto the prepared baking sheet, leaving about 2 inches of space in between as cookies will spread. Refrigerate leftover dough to bake in batches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until just golden around the edges, about 8 minutes. Allow to cool for 2 minutes on the baking sheet before transferring to a cooling rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
