    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232718/wonderful-toffee-bars/",
        "name": "Wonderful Toffee Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Wonderful Toffee Bars"
        },
        "datePublished": "2020-06-19T03:54:54.000Z",
        "description": "This is a easy recipe, but better than that, they taste wonderful! Everyone that tastes them always wants the recipe. Try them once and you'll be hooked!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "48 bars",
        "recipeIngredient": [
            "12 graham crackers, broken into quarters",
            "\u00bd cup butter",
            "\u00bd cup margarine",
            "\u00bd cup white sugar",
            "\u00bc cup chopped walnuts, or to taste",
            "1 (12 ounce) package milk chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a 10x15-inch baking sheet with graham cracker quarters.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring butter, margarine, and sugar to a boil in a saucepan, stirring constantly, until sugar is dissolved, about 3 minutes. Pour butter mixture over graham crackers and sprinkle with walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until butter is browned and nuts are toasted, about 10 minutes. Immediately transfer toffee bars from the baking sheet to a platter lined with aluminum foil or wax paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in 2 batches in a microwave-safe bowl in 30-second intervals, stirring after each melting, for 1 to 3 minutes (depending on your microwave). Drop spoonfuls of melted chocolate over each toffee bar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "Milk Chocolate"
        ],
