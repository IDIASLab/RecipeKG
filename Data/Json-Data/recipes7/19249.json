    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270584/vegan-sugar-and-spice-muffins/",
        "name": "Vegan Sugar and Spice Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6326883.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T04:34:50.000Z",
        "description": "These muffins will be a sure hit for with everyone. No one will ever know they are vegan.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "9 muffins",
        "recipeIngredient": [
            "cooking spray",
            "1\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon pumpkin pie spice",
            "\u00bd teaspoon salt",
            "\u2153 cup white sugar",
            "\u2153 cup avocado oil",
            "3 tablespoons aquafaba (chickpea water)",
            "\u00bd cup white sugar",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd cup vegan margarine (such as Earth Balance\u00ae), melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a muffin tin with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking powder, pumpkin pie spice, and salt together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk sugar, avocado oil, and aquafaba together in a bowl until sugar is dissolved. Pour in flour mixture; mix until evenly incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared tin, filling each cup halfway full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar and cinnamon in a shallow dish to make the topping. Remove hot muffins from the tin and dip in melted margarine; roll in cinnamon sugar to coat. Serve warm.\n"
            }
        ],
