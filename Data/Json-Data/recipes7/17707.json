    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263726/no-egg-blueberry-mug-cake/",
        "name": "No-Egg Blueberry Mug Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5290545.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:27:39.000Z",
        "description": "The perfect dessert when you're home alone and have a craving for sometime homemade and fruity. It is also egg free so perfect for those with allergies.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT0H7M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "4 tablespoons all-purpose flour",
            "3 tablespoons white sugar",
            "1 teaspoon vanilla sugar",
            "1 pinch baking powder",
            "3 tablespoons milk",
            "\u00bd tablespoon olive oil",
            "10 blueberries",
            "1 teaspoon confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, vanilla sugar, and baking powder in a large mug. Add milk and oil; mix batter well with a fork or a small whisk. Carefully fold in blueberries so they are completely covered with batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on high until cake has risen by 50%, 30 seconds to 2 minutes, depending on strength of your microwave. Allow to cool in the mug for a few minutes. Dust with confectioners' sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Blueberry Dessert Recipes"
        ],
