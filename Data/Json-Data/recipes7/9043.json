    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231963/catchy-coleslaw/",
        "name": "Catchy Coleslaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1031752.jpg",
            "width": 1300,
            "height": 973
        },
        "datePublished": "2020-06-19T03:53:47.000Z",
        "description": "This cole slaw has a vinaigrette-style dressing instead of a traditional creamy dressing. It would be good for picnics or where a salad needs to sit out. If you like vinegar-based dressings, this one can be addictive.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT3H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "6 cups shredded green cabbage",
            "1\u2009\u00bd cups shredded carrot",
            "1 cup shredded red cabbage",
            "\u00bd cup shredded onion",
            "\u00bc cup kosher salt",
            "1 cup cider vinegar",
            "1 cup white sugar",
            "1 tablespoon celery seed",
            "1 tablespoon dry mustard",
            "\u00bc cup vegetable oil",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Toss green cabbage, carrot, red cabbage, and onion in a colander and sprinkle with kosher salt. Set aside to drip for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat cider vinegar, sugar, celery seed, and dry mustard together in a small saucepan over medium-low heat, stirring until sugar has dissolved. Remove from heat and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze as much juice as possible from cabbage mixture and transfer vegetables to a serving bowl. Pour vinegar mixture to taste over vegetables and drizzle vegetable oil over the top. Sprinkle with black pepper and stir coleslaw to coat with dressing. Refrigerate 2 hours before serving for flavors to blend.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Coleslaw Recipes",
            "No Mayo"
        ],
