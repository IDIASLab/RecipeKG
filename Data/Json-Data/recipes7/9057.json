        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231996/kool-aid-popcorn/",
        "name": "Kool-Aid&reg; Popcorn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1218918.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:53:50.000Z",
        "description": "You can't stop eating it! Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 quarts popcorn",
        "recipeIngredient": [
            "6 quarts popped popcorn",
            "2 cups white sugar",
            "1 cup light corn syrup",
            "\u2154 cup butter",
            "2 (.13 ounce) envelopes unsweetened soft drink mix, any flavor (such as KOOL-AID\u00ae)",
            "1 teaspoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 225 degrees F (110 degrees C). Grease a large baking sheet. Place popped popcorn in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, corn syrup, and butter in a saucepan; bring to a boil until sugar dissolves, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine drink mix and baking soda in a large bowl; pour sugar mixture into drink mixture, stirring carefully. Sugar mixture will bubble. Pour mixture over popcorn; toss to evenly coat. Transfer coated popcorn to baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every 10 minutes, until coating has hardened, about 30 minutes. Cool and break popcorn into pieces.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Popcorn Recipes"
        ],
