        "cookTime": null,
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons rice vinegar",
            "2 tablespoons Thai sweet chili sauce",
            "1 teaspoon sesame oil",
            "\u00bc teaspoon white sugar",
            "\u00bc teaspoon granulated garlic",
            "1 pinch salt to taste",
            "1 cucumber, thinly sliced",
            "1 green onion, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk rice vinegar, chili sauce, sesame oil, sugar, and garlic together in a small bowl; season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cucumber and onion in a separate bowl. Pour vinegar mixture over cucumber and onion mixture and toss to coat. Refrigerate at least 1 hour. Toss before serving.\n"
            }
        ],
