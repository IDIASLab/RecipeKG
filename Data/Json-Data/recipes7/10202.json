    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234913/curried-sweet-potato-soup/",
        "name": "Curried Sweet Potato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5049824.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T03:56:41.000Z",
        "description": "A delicious blend of sweet potatoes, onion, and carrots with just the right spices for a perfect autumn soup.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT7H20M",
        "totalTime": "P0DT7H40M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "5 cups chicken broth",
            "2 large sweet potatoes, peeled and cut into cubes",
            "2 cups baby carrots",
            "1 onion, chopped",
            "1 teaspoon red curry powder",
            "\u00be teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger",
            "1 cup half-and-half",
            "2 tablespoons real maple syrup",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir chicken broth, sweet potatoes, baby carrots, onion, red curry powder, cinnamon, and ground ginger together in a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 7 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup into a blender no more than half full. Cover and hold lid in place. Pulse blender a few times before leaving on to blend. Puree in batches until smooth. Return pureed soup to slow cooker. Stir half-and-half and maple syrup into the soup; season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on High for 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Sweet Potato Soup Recipes"
        ],
