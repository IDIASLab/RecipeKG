    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257207/sinfully-moist-banana-muffins/",
        "name": "Sinfully Moist Banana Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4487161.jpg",
            "width": 3648,
            "height": 3648
        },
        "datePublished": "2020-06-19T04:20:01.000Z",
        "description": "Super moist recipe. Great for breakfast on the go! Even tastier if you spread some cream cheese on them.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "17 muffins",
        "recipeIngredient": [
            "cooking spray",
            "1 cup applesauce",
            "1 cup mashed bananas",
            "\u00bd cup butter, melted",
            "2 eggs",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd cup brown sugar",
            "1 tablespoon ground cinnamon",
            "1 teaspoon vanilla extract",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray 2 muffin tins with cooking spray or line with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk applesauce, mashed bananas, butter, and eggs together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, white sugar, brown sugar, cinnamon, vanilla extract, baking soda, and salt in a separate bowl. Fold into the applesauce mixture until batter is just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide batter evenly among muffin tins, filling each cup almost full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 15 minutes.\n"
            }
        ],
