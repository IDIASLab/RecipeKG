    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241737/rhubarb-shortcake/",
        "name": "Rhubarb Shortcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rhubarb Shortcake"
        },
        "datePublished": "2020-06-19T04:02:35.000Z",
        "description": "Old-fashioned rhubarb shortcake recipe (from scratch) that uses fresh rhubarb in the shortcake as well as in the sauce!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H48M",
        "totalTime": "P0DT2H28M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "2 cups diced rhubarb",
            "1 cup white sugar",
            "2 cups all-purpose flour",
            "1 cup sour cream",
            "2 teaspoons baking powder",
            "1 pinch salt",
            "2\u2009\u00bd cups white sugar, divided",
            "1 pint fresh strawberries, hulled and sliced thin",
            "3 cups diced rhubarb",
            "\u00bd cup water",
            "1 tablespoon cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 2 cups rhubarb and 1 cup sugar together in a bowl; set aside to allow juices to release, about 10 minutes. Combine flour, sour cream, baking powder, and salt in a bowl. Add rhubarb-sugar mixture to flour mixture; mix until just blended. Pour into an 8-inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 25 to 35 minutes. Cut shortcake into 9 squares.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1 1/2 cups sugar and strawberries together in a bowl; set aside to allow flavors to meld, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 3 cups rhubarb and water in a saucepan over medium heat. Stir in 1 cup sugar; bring to a boil. Lower heat; simmer until tender, about 15 minutes. Remove from heat; mash with a potato masher. Return to medium heat; add cornstarch and return to boiling. Simmer, stirring, until sauce is thick, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour strawberry-sugar mixture into rhubarb sauce; stir until well mixed. Cool completely in the refrigerator, about 1 hour. Spoon sauce over shortcake squares.\n"
            }
        ],
