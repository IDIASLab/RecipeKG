    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245296/chocolate-sugar-drop-cookies/",
        "name": "Chocolate Sugar Drop Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2823222.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T04:03:52.000Z",
        "description": "My daughter and I had fun experimenting in the kitchen one night creating our own recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 dozen cookies",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "\u00bc cup butter, softened",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "\u00bc cup cocoa powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bd cup chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar, butter, egg, and vanilla together in a large bowl. Mix flour, cocoa, baking soda, and salt in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat flour mixture into sugar mixture slowly until well-combined, about 2 minutes. Stir chocolate chips into cookie dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon cookie dough onto the baking sheet, spaced about 2-inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are crisp, 10 to 12 minutes. Cool on the baking sheet for 5 minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
