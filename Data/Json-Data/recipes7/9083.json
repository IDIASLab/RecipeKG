    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232104/chocolate-chip-mini-muffins/",
        "name": "Chocolate Chip Mini Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3832907.jpg",
            "width": 2742,
            "height": 2741
        },
        "datePublished": "2020-06-19T03:53:54.000Z",
        "description": "My kids love when I make these yummy muffins. After trial and error of several muffin recipes, I've come up with what I think is my favorite one. Enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 dozen mini muffins",
        "recipeIngredient": [
            "cooking spray",
            "2 cups all-purpose flour",
            "\u00bd cup white sugar",
            "2 tablespoons brown sugar",
            "1 tablespoon baking powder",
            "\u00bd teaspoon salt",
            "1 egg",
            "1\u2009\u00bc cups milk",
            "\u2153 cup vegetable oil",
            "2 teaspoons vanilla extract",
            "\u00be cup mini chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly spray 48 miniature muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, white sugar, brown sugar, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly beat egg in a separate bowl; stir in milk, vegetable oil, and vanilla extract. Mix liquid mixture into flour mixture until incorporated. Fold in chocolate chips. Fill muffin cups 3/4 full with batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in a muffin comes out clean, 10 to 11 minutes. Cool for 1 minute before transferring muffins to a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Muffin Recipes",
            "Chocolate Muffin Recipes"
        ],
