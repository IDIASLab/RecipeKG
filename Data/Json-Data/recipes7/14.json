    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72600/better-burger-sauce/",
        "name": "Better Burger Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F359575.jpg",
            "width": 3008,
            "height": 2000
        },
        "datePublished": "2020-06-19T03:10:53.000Z",
        "description": "For a little added burger excitement, this sauce goes great with any of your favorites!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "2 teaspoons red wine vinegar",
            "2 teaspoons sugar",
            "1 cup mayonnaise",
            "\u00bd cup ketchup",
            "\u2153 cup sweet pickle relish",
            "1 tablespoon chopped fresh parsley",
            "\u00bd bunch green onions, chopped",
            "2 hard-cooked eggs, peeled and chopped",
            "salt and pepper to taste",
            "Worcestershire sauce to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a saucepan and cover with cold water. Bring water to a boil and immediately remove from heat. Cover and let eggs stand in hot water for 10 to 12 minutes. Drain water, and allow to cool, then peel, chop, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, stir together the vinegar and sugar until sugar is completely dissolved. Stir in mayonnaise, ketchup, relish, parsley, green onions, and chopped egg, and stir until well blended. Season to taste with salt, pepper, and Worcestershire sauce. Cover and refrigerate until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
