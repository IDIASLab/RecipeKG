    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247042/baked-nashville-hot-chicken-breasts/",
        "name": "Baked Nashville Hot Chicken Breasts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6063659.jpg",
            "width": 1536,
            "height": 1536
        },
        "datePublished": "2020-06-19T04:06:40.000Z",
        "description": "This is one of my recipes that I keep getting requests for. We also eat it without the sauce. I hope that you enjoy.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 quart water",
            "\u00bc cup salt",
            "\u00bc cup white sugar",
            "\u00bc cup hot sauce",
            "4 skinless, boneless chicken breast halves, cut into strips",
            "1 cup all-purpose flour",
            "\u00bd teaspoon cayenne pepper",
            "\u00bd teaspoon paprika",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon ground black pepper",
            "3 tablespoons vegetable oil",
            "1 tablespoon cayenne pepper",
            "\u00bd teaspoon paprika",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon white sugar",
            "\u00bc teaspoon garlic powder",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix water, 1/4 cup salt, 1/4 cup sugar, and hot sauce together in a large bowl until sugar and salt are dissolved into the brine. Place chicken pieces in the brine and refrigerate, 30 minutes to 1 hour. Pour brine from the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place a baking rack onto a baking sheet and coat rack with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, 1/2 teaspoon cayenne pepper, 1/2 teaspoon paprika, 1/2 teaspoon salt, 1/4 teaspoon garlic powder, and black pepper in a shallow bowl until coating is well mixed. Press chicken pieces into the coating until both sides are evenly coated; place on the prepared rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Spray each chicken piece with cooking spray; flip and spray the other side. Continue baking until chicken is no longer pink in the center, about 10 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a small pan over medium heat until shimmering. Mix 1 tablespoon cayenne pepper, 1/2 teaspoon paprika, 1/2 teaspoon salt, 1/2 teaspoon sugar, and 1/4 teaspoon garlic powder together in a bowl; add to oil and cook for 30 seconds. Brush oil mixture onto cooked chicken, adding more layers for extra heat.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
