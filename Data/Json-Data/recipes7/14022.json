        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246643/gluten-free-kalbi-beef/",
        "name": "Gluten-Free Kalbi Beef",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5528131.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T04:06:13.000Z",
        "description": "Great gluten-free option for a day of grilling.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 tablespoons sesame seeds",
            "1\u2009\u00bd cups gluten-free soy sauce (tamari)",
            "\u2154 cup white sugar",
            "\u00bd cup chopped green onions",
            "\u00bc cup sesame oil",
            "2 tablespoons minced garlic",
            "\u00bd teaspoon red pepper flakes",
            "5 pounds Korean short ribs (cut flanken-style into 1/2-inch slices across the bones)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Toast sesame seeds in a small skillet over medium heat until lightly browned, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sesame seeds, soy sauce, sugar, green onions, sesame oil, garlic, and red pepper flakes in a saucepan. Simmer over medium-low heat, stirring constantly, until flavors combine, about 10 minutes. Remove from heat and let marinade cool completely, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place short ribs in large resealable plastic bags and cover with cooled marinade. Seal and let marinate in the refrigerator, at least 1 hour or overnight for maximum flavor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain short ribs, discarding marinade, and cook on the preheated grill until browned, about 5 minutes per side.\n"
            }
        ],
