    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258381/apple-cranberry-crisp-with-lavender-honey-mascarpone/",
        "name": "Apple Cranberry Crisp with Lavender Honey Mascarpone",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Cranberry Crisp with Lavender Honey Mascarpone"
        },
        "datePublished": "2020-06-19T04:21:37.000Z",
        "description": "Unique apple cranberry crisp.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 9x13-inch baking dish",
        "recipeIngredient": [
            "2 large Braeburn apples, peeled and thinly sliced",
            "2 cups fresh cranberries",
            "1\u2009\u00bc cups all-purpose flour, divided",
            "1 cup white sugar",
            "1 tablespoon lemon juice",
            "1\u2009\u00bd teaspoons ground nutmeg",
            "1 cup firmly packed brown sugar",
            "1 cup quick-cooking oats",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "1 teaspoon vanilla extract",
            "\u00bd cup butter, melted",
            "1 cup heavy whipping cream",
            "1 (8 ounce) container mascarpone cheese",
            "2 tablespoons lavender honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples, cranberries, 1/4 cup flour, white sugar, lemon juice, and nutmeg in a large bowl; toss until apples are well coated. Spread over the bottom of a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 1 cup flour, brown sugar, oats, cinnamon, and vanilla extract in another bowl. Pour in melted butter; mix thoroughly. Spoon evenly over apple-cranberry mixture in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until apples are tender and top is browned, 50 to 60 minutes. Let cool slightly, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine heavy cream, mascarpone cheese, and lavender honey in a bowl; beat with an electric mixer on medium speed until soft peaks form. Serve with warm crisp.\n"
            }
        ],
