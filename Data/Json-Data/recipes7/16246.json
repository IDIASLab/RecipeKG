    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259041/lemon-pie-cookies/",
        "name": "Lemon Pie Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon Pie Cookies"
        },
        "datePublished": "2020-06-19T04:23:14.000Z",
        "description": "Yummy lemon-orange cookies made with whole wheat flour and lemon pie filling. Crisp on the outside, soft on the inside; they'll vanish from your kitchen very quickly!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 cookies",
        "recipeIngredient": [
            "1 (4.3 ounce) package lemon pie filling (such as JELL-O\u00ae)",
            "1 cup whole wheat pastry flour",
            "\u2153 cup white sugar",
            "1 tablespoon arrowroot powder",
            "1 teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "\u2153 cup vegetable oil",
            "1 orange, zested and juiced",
            "1 teaspoon lemon juice",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lemon pie filling, whole wheat pastry flour, sugar, arrowroot powder, baking powder, and baking soda together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk vegetable oil, orange zest, orange juice, lemon juice, and salt together in a separate bowl. Pour over lemon pie filling mixture; stir until dough is crumbly and moist throughout.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop 12 spoonfuls of dough 1 inch apart onto an ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly golden, about 10 minutes. Cool on the baking sheet for 5 minutes. Transfer gently to a wire rack; let cool until firm.\n"
            }
        ],
