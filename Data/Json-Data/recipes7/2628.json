    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149841/cranberry-cinnamon-cookies/",
        "name": "Cranberry Cinnamon Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F45043.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2020-06-19T03:32:31.000Z",
        "description": "I got this recipe from my aunt. It is amazingly good. It comes out perfect every time you bake it. You will be surprised how easy it is!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "60 cookies",
        "recipeIngredient": [
            "3\u2009\u00bc cups all-purpose flour",
            "1 teaspoon ground cinnamon",
            "1 cup butter, room temperature",
            "2\u2009\u215b cups white sugar",
            "2 eggs",
            "\u2154 cup sweetened dried cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift together the flour and cinnamon in a bowl. Cream together the butter and sugar in a separate bowl. Mix in 1 egg thoroughly, then mix in the second egg. Slowly stir the flour mixture into the butter mixture. Stir in the cranberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the dough into a log with a diameter of about 1 1/2 inches; wrap in plastic; place in freezer for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Cut the cookie dough into 1/2-inch-thick slices using a sharp knife. Arrange the cookies onto a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until edges turn brown, about 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
