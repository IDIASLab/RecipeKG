    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245306/homemade-peach-tea/",
        "name": "Homemade Peach Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7005265.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:03:53.000Z",
        "description": "This delicious peach iced tea tastes eerily similar to a famous Italian chain restaurant's peach tea. Serve tea over ice and garnish with peach slices, if desired.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 cups",
        "recipeIngredient": [
            "1 cup water",
            "1 cup white sugar",
            "2 slices fresh peach, or more to taste",
            "6 cups water",
            "3 tea bags"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix 1 cup water, sugar, and peach slices together in a saucepan; bring to a boil. Reduce heat to medium and simmer, crushing peach slices as you stir, until sugar is dissolved, about 5 minutes. Remove saucepan from heat, cover, and allow peaches to infuse syrup, about 30 minutes. Strain to remove peach slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 6 cups remaining water to a boil in a separate pot. Remove from heat, add tea bags, cover, and steep for about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove tea bags from the pot, pour in peach syrup, and cool to room temperature. Refrigerate until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Tea",
            "Iced Tea Recipes"
        ],
