    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234848/lactose-free-peanut-brittle/",
        "name": "Lactose Free Peanut Brittle",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lactose Free Peanut Brittle"
        },
        "datePublished": "2020-06-19T03:56:38.000Z",
        "description": "This is something I had made for my mom. At age 76 she became lactose-intolerant. I did not want her to go without during the holiday season.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 tablespoons shortening",
            "2 teaspoons butter flavoring",
            "1 cup white sugar",
            "1 cup raw peanuts",
            "\u00bd cup corn syrup",
            "\u00bc cup water",
            "\u00bc teaspoon salt",
            "1 teaspoon baking soda",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir shortening and butter flavoring together in a bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar, peanuts, corn syrup, water, and salt together in a 2-quart saucepan. Heat mixture over medium-high heat, stirring frequently, until temperature reaches 300 degrees F (149 degrees C), or until a small amount of syrup dropped into cold water forms hard, brittle threads, about 15 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully stir baking soda and vanilla into peanut mixture until fully incorporated. Stir shortening mixture into peanut mixture until dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peanut mixture in a single layer onto prepared baking sheet; cool until hard. Crack brittle into pieces.\n"
            }
        ],
