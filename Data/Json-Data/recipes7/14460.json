    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254351/brazilian-milk-pudding/",
        "name": "Brazilian Milk Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Brazilian Milk Pudding"
        },
        "datePublished": "2020-06-19T04:16:35.000Z",
        "description": "This pudding is a very common dessert in Brazil, probably the most popular pudding of all. It is simple and absolutely delicious.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT5H25M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 tablespoons white sugar",
            "1 (14 ounce) can sweetened condensed milk",
            "28 fluid ounces milk",
            "3 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sugar in a saucepan over medium heat. Cook, stirring frequently, until sugar melts into a golden caramel, about 10 minutes. Pour into a ring mold, tilting to coat the bottom evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour condensed milk into a blender. Fill the empty can with milk and pour in, twice. Add eggs. Blend until smooth, about 30 seconds. Pour over caramel in the mold.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mold in a larger baking pan. Fill baking pan with hot water to a depth of 1 inch to make a water bath.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake pudding in the preheated oven until set and firm to the touch, 60 to 90 minutes. Remove mold from the water bath and let cool to room temperature, about 1 hour. Unmold pudding carefully onto a plate and chill before serving, about 3 hours.\n"
            }
        ],
