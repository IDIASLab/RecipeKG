        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236648/simple-french-toast-in-a-cup/",
        "name": "Simple French Toast in a Cup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1119191.jpg",
            "width": 1935,
            "height": 1935
        },
        "datePublished": "2020-06-19T03:58:15.000Z",
        "description": "A quick and easy breakfast or lunch that you can modify to suite your taste. My 4 kids all love this. Try it with raisin bread too!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H10M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 tablespoon butter",
            "\u00bc cup milk",
            "1 egg, beaten",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon white sugar",
            "\u00bc teaspoon vanilla extract",
            "2 slices bread, cut into cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in the bottom of a large microwave-safe mug in the microwave, about 30 seconds. Tilt mug to coat all sides with melted butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir melted butter, milk, egg, cinnamon, sugar, and vanilla extract together in the mug. Press bread cubes into milk mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on High until set, about 90 seconds.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "French Toast Recipes"
        ],
