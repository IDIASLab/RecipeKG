        "mainEntityOfPage": "https://www.allrecipes.com/recipe/112133/best-baconless-broccoli-salad/",
        "name": "Best Baconless Broccoli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1209754.jpg",
