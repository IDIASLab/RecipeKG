    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255498/chunky-halloween-candy-cookies/",
        "name": "Chunky Halloween Candy Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8843910.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:18:18.000Z",
        "description": "What do you do with all of that Halloween candy? Make these fabulous cookies of course! Leftover halloween candy and a chewy cookie recipe serve up quickly and taste oh-so-delicious. This also works great with Easter candy too.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup brown sugar, packed",
            "\u00bd cup white sugar",
            "2 eggs",
            "2 cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon vanilla extract",
            "20 (.3 ounce) miniature bars fun-sized candy bars, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix butter, brown sugar, and white sugar together in a bowl until creamy. Beat in eggs, one at a time, until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour and baking powder in a bowl. Mix gradually into the butter mixture. Beat in vanilla extract until mixed. Fold in candy a little at a time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of dough 2 inches apart onto 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are golden, about 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
