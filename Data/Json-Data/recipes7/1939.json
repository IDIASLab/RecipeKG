    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/137363/easy-chocolate-cupcakes/",
        "name": "Easy Chocolate Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3249193.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2020-06-19T03:28:50.000Z",
        "description": "These chocolate cupcakes are easy to make and very tasty.  You can also bake them as a cake.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 cupcakes",
        "recipeIngredient": [
            "10 tablespoons butter",
            "1\u2009\u00bc cups white sugar",
            "4 eggs",
            "\u00bc teaspoon almond extract",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00be cup unsweetened cocoa powder",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00be cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease two muffin pans or line with 20 paper baking cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, beat the butter and sugar with an electric mixer until light and fluffy. Mix in the eggs, almond extract and vanilla. Combine the flour, cocoa, baking powder and salt; stir into the batter, alternating with the milk, just until blended. Spoon the batter into the prepared cups, dividing evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the tops spring back when lightly pressed, 20 to 25 minutes. Cool in the pan set over a wire rack. When cool, arrange the cupcakes on a serving platter. Frost with your favorite frosting.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cupcake Recipes",
            "Chocolate"
        ],
