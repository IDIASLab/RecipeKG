    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259750/gluten-free-dense-and-chewy-vegan-pumpkin-cookies/",
        "name": "Gluten-Free Dense and Chewy Vegan Pumpkin Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4557145.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:24:01.000Z",
        "description": "This is one of the best cookies I have ever had...the texture is divine and not a bit cakey!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H32M",
        "totalTime": "P0DT1H32M",
        "recipeYield": "12 cookies",
        "recipeIngredient": [
            "1 cup pumpkin puree",
            "\u00bd cup brown sugar",
            "\u00bd cup shortening",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00bd cup dried cranberries, or to taste",
            "\u00bc cup pumpkin seeds, or to taste",
            "1 cup oat flour, or more as needed",
            "\u00be cup tapioca flour, or more as needed",
            "\u00bd cup brown rice flour, or more as needed",
            "1\u2009\u00bd teaspoons guar gum",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00bd cup confectioners' sugar",
            "\u00bd cup confectioners' sugar",
            "1 tablespoon margarine, melted",
            "1 teaspoon soy milk, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat pumpkin puree in a saucepan over low heat, stirring occasionally, until condensed to about 1/2 cup, about 20 minutes. Let cool, at least 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar, shortening, and white sugar together until smooth and creamy. Add the cooled pumpkin and vanilla extract; blend well. Sprinkle in oat flour, tapioca flour, brown rice flour, guar gum, cinnamon, and salt. Add cranberries and pumpkin seeds. Mix until dough is very stiff and dry; it should not stick to your hands. Add more flour, 2 tablespoons at a time, if needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place golf-ball size balls of dough on the prepared baking sheet. Press tops down slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges just begin to turn golden, about 12 minutes. Let cool, at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix confectioners' sugar and melted margarine together in a bowl. Add enough milk so that icing is spreadable but not runny. Dollop icing on top of the cookies.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
