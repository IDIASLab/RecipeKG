        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 cup white sugar",
            "\u00bd cup maple syrup",
            "\u2153 cup chocolate chips",
            "\u2153 cup brownie mix",
            "1 egg",
            "1 teaspoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a small baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, sugar, maple syrup, chocolate chips, brownie mix, egg, and baking soda together in a bowl until batter is evenly-mixed. Spoon batter onto prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, 7 to 10 minutes. Cool to room temperature and break into pieces.\n"
            }
        ],
