    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245572/double-dark-chocolate-hazelnut-cookies/",
        "name": "Double Dark Chocolate Hazelnut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3598689.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T04:04:03.000Z",
        "description": "These are sort of like Nutella&reg; in cookie form.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00bd cup butter, at room temperature",
            "\u00be cup white sugar",
            "\u00bc teaspoon salt",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00be ounces 70% dark chocolate, melted",
            "1 tablespoon cocoa powder",
            "\u00bc teaspoon instant espresso powder",
            "1 cup all-purpose flour",
            "\u00bd cup semisweet chocolate chips ",
            "\u00bd cup roasted hazelnuts, chopped",
            "\u00bc cup roasted hazelnuts, finely ground"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter in a large bowl with an electric mixer until light and fluffy. Add sugar and salt; beat until combined. Beat in egg and vanilla extract until well-blended. Blend in melted chocolate, cocoa powder, and espresso. Mix in flour gradually, until batter is smooth. Fold in chocolate chips, chopped hazelnuts, and ground hazelnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop heaping tablespoons of cookie batter 2 inches apart onto baking sheets. Use the back of a wet fork to flatten each cookie slightly, making a crisscross pattern.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set and edges start to darken, about 15 minutes. Let cool a few minutes before serving.\n"
            }
        ],
