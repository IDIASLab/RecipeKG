    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242514/best-unique-grilled-cheese/",
        "name": "Best, Unique Grilled Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2373043.jpg",
            "width": 1781,
            "height": 1781
        },
        "datePublished": "2020-06-19T04:03:13.000Z",
        "description": "The best sandwich I've ever had that is a spin off of a grilled cheese I had at a ski chalet.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "1 sandwich",
        "recipeIngredient": [
            "1 tablespoon water",
            "1 tablespoon rice vinegar",
            "1 tablespoon white sugar",
            "1 teaspoon red pepper flakes",
            "\u00bd teaspoon minced garlic",
            "1 tablespoon butter, softened",
            "2 slices bread",
            "2 tablespoons cream cheese, softened",
            "2 slices Cheddar cheese",
            "4 slices pickles, or as desired",
            "4 slices tomato"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water, vinegar, sugar, red pepper flakes, and garlic in a small saucepan over medium heat; cook and stir until sugar is dissolved and sauce is slightly reduced, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread butter onto 1 side of each bread slice. Spread cream cheese on the opposite side of 1 of the bread slices. Layer Cheddar cheese and pickles on top of the cream cheese layer. Drizzle 1 to 2 tablespoons sauce on the other slice of bread on the side opposite the butter; place bread on top of pickle layer, butter-side on the outside, creating a sandwich.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook sandwich in the hot skillet until cheese is melted and bread is browned, about 5 minutes per side. Remove top bread piece, add tomatoes to middle, and close sandwich.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Sandwich Recipes",
            "Grilled Cheese"
