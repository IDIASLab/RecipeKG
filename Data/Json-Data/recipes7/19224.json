    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270449/bumbleberry-jam/",
        "name": "Bumbleberry Jam",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6919856.jpg",
            "width": 3264,
            "height": 3262
        },
        "datePublished": "2020-06-19T04:34:46.000Z",
        "description": "This yummy jam is made with a combination of rhubarb, strawberries, peaches, blackberries, and blueberries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT13H0M",
        "recipeYield": "10 8-ounce jars",
        "recipeIngredient": [
            "1 cup chopped rhubarb",
            "1 cup mashed strawberries",
            "1 cup mashed peaches",
            "1 cup mashed blackberries",
            "1 cup mashed blueberries",
            "1 cup chopped apples",
            "6 cups white sugar",
            "1 (1.75 ounce) package fruit pectin (such as Sure-Jell\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine rhubarb, strawberries, peaches, blackberries, blueberries, apples, and sugar in a saucepan over medium heat. Bring to a boil. Boil for 10 minutes, stirring the whole time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add pectin to the saucepan and boil until jam coats the back of a metal spoon, about 10 minutes more. Remove from heat and transfer jam to sterilized 8-ounce canning jars and cover with new lids and rings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a rack in the bottom of a large stockpot and fill halfway with water. Bring to a boil and lower jars 2 inches apart into the boiling water using a holder. Pour in more boiling water to cover jars by at least 1 inch. Bring to a rolling boil, cover, and process for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the jars from the stockpot and let rest, several inches apart, for 12 to 24 hours. Press the center of each lid with a finger to ensure the lid does not move up or down. Remove the rings for storage and store in a cool, dark area.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Jams and Jellies Recipes"
        ],
