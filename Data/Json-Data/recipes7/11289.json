        "name": "Chef John's Homemade Mayonnaise",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fcf-images.us-east-1.prod.boltdns.net%2Fv1%2Fstatic%2F1033249144001%2Fab444347-3baf-4a16-a510-f09c67ca43e5%2Fb02095ac-69f7-46b0-9b1e-6427f70d4073%2F1280x720%2Fmatch%2Fimage.jpg",
