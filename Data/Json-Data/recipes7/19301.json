    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270691/mocha-white-chocolate-chunk-cookies/",
        "name": "Mocha White Chocolate Chunk Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9422655.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:35:01.000Z",
        "description": "Traditional chocolate chip cookies get a makeover with cocoa powder and instant coffee in the batter. White chocolate chips add contrast and a burst of extra sweetness to brighten the bitter cocoa and coffee flavors.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1\u2009\u2153 cups white sugar",
            "\u2154 cup dark brown sugar",
            "2 large eggs",
            "4 tablespoons instant coffee granules",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bd cup cocoa powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups white chocolate chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine butter, sugars, eggs, instant coffee, and vanilla extract in a large bowl. Beat with an electric mixer until fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, cocoa powder, baking soda, and salt together in a separate bowl. Pour over butter mixture and beat until combined. Fold in white chocolate chunks using a spatula. Cover dough and chill for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease 2 cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are lightly browned, 12 to 15 minutes. Allow to cool slightly before removing from the cookie sheets.\n"
            }
        ],
