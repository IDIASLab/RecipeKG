    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257262/fresh-strawberry-shortcake-cupcakes/",
        "name": "Fresh Strawberry Shortcake Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fresh Strawberry Shortcake Cupcakes"
        },
        "datePublished": "2020-06-19T04:20:07.000Z",
        "description": "Soft vanilla cake with a strawberry filling and whipped cream topping. Enjoy!",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "24 cupcakes",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "1 cup butter, softened",
            "1 cup white sugar",
            "2 eggs",
            "\u00bd teaspoon vanilla extract",
            "\u00bd cup milk",
            "1 (8 ounce) container strawberry-flavored cream cheese",
            "\u2153 cup white sugar",
            "2 tablespoons heavy whipping cream",
            "1 teaspoon strawberry jam",
            "8 strawberries",
            "1 (7 ounce) can whipped cream",
            "10 strawberries, sliced crosswise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Line two 12-cup muffin tins with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter and 1 cup sugar in a bowl; beat with an electric mixer until creamy. Beat in eggs one at a time. Mix in vanilla extract. Add the flour mixture alternately with milk, beating batter very briefly after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter evenly into the lined cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 20 minutes. Cool in the tins for 5 minutes. Transfer to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, 1/3 cup sugar, heavy cream, and strawberry jam in a bowl; beat with an electric mixer until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice off the sides of 8 strawberries to remove seeds. Mash strawberries in a small bowl with a fork. Beat into the cream cheese mixture until fully incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut a small hole in the center of each cooled cupcake; scoop out about 1/2 inch of cake. Fill each gap with a spoonful of the cream cheese frosting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each cupcake with a swirl of whipped cream. Place 1 strawberry slice in the center of each cupcake.\n"
            }
        ],
