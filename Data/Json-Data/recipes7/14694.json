    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255005/easy-apple-pie-bars/",
        "name": "Easy Apple Pie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7149473.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:17:55.000Z",
        "description": "Shortbread crust with dense apple pie filling.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "1 cup shortening",
            "4 egg yolks",
            "8 tablespoons cold water, or more as needed ",
            "2 tablespoons lemon juice",
            "7 cups finely chopped apples",
            "2 cups white sugar",
            "\u00bc cup all-purpose flour",
            "2 teaspoons ground cinnamon",
            "1 tablespoon milk, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 4 cups flour, baking powder, and salt in a large bowl. Cut in shortening with a fork until coarse crumbs form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg yolks, cold water, and lemon juice together in another bowl. Stir into flour mixture with a fork until dough comes together into a ball; divide in half. Roll out 1 half and press into the bottom of a 10x15-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples, sugar, 1/4 cup flour, and ground cinnamon in a bowl to make filling. Spread over dough in the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out remaining dough and lay on top of filling. Brush with milk. Cut slits in the dough with a sharp knife.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 45 to 50 minutes. Cool before cutting into 24 bars, at least 20 minutes.\n"
            }
        ],
