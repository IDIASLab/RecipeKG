    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233753/raspberry-applesauce-squares/",
        "name": "Raspberry Applesauce Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1123736.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "2020-06-19T03:55:39.000Z",
        "description": "A sweet raspberry filling surrounded by oats makes a good snack for tea time.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "1 cup fresh raspberries",
            "1 cup applesauce",
            "\u00bd cup oat bran",
            "\u00bd cup white sugar",
            "1\u2009\u00bd cups quick-cooking oats",
            "1 cup brown sugar",
            "\u00bd cup butter",
            "\u00bd cup all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix raspberries, applesauce, oat bran, and white sugar together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats, brown sugar, and butter together in a bowl using a pastry blender; add flour. Continue cutting into mixture using pastry blender until crumbly. Spread half the crumb topping into the bottom of the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is lightly browned, about 20 minutes. Remove from oven and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the raspberry filling onto the cooled crust and sprinkle with remaining crumb topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping is lightly browned, about 20 more minutes.\n"
            }
        ],
