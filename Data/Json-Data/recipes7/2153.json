    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/141708/mixed-berry-trifle/",
        "name": "Mixed Berry Trifle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1237453.jpg",
            "width": 2071,
            "height": 2071
        },
        "datePublished": "2020-06-19T03:30:13.000Z",
        "description": "This creamy berry dessert is very easy to make and simply delicious.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT1H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (12 ounce) bag mixed frozen berries",
            "\u00bc cup cream sherry",
            "1 teaspoon fresh lemon juice",
            "1 teaspoon grated lemon zest",
            "2 cups heavy cream, chilled",
            "1 tablespoon white sugar",
            "1 (12 ounce) loaf prepared pound cake",
            "\u00bd cup toasted coconut "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the berries, sherry, lemon juice, and lemon zest in a bowl; toss to blend. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the heavy cream in a large bowl until soft peaks form. Gradually beat in the sugar until stiff peaks form. Refrigerate cream until needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut pound cake into slices 1/3 inch wide, discarding cake ends.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Assemble the trifle by placing a layer of cake slices in the bottom of a glass serving bowl. Spoon half the berry mixture with juice over the cake slices. Cover with half of the whipped cream, and sprinkle with half the coconut. Repeat layering with the remaining cake slices, berries and juice, whipped cream, and coconut. Refrigerate at least 1 hour before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Trifle Recipes"
        ],
