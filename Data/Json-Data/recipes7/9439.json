    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233058/simple-shortbread-squares/",
        "name": "Simple Shortbread Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1023928.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:55:07.000Z",
        "description": "My favorite Christmas cookie! They absolutely melt in your mouth!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H45M",
        "recipeYield": "120 cookie squares",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "2 tablespoons all-purpose flour",
            "2 cups butter at room temperature",
            "1 cup white sugar",
            "1 tablespoon colored sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 275 degrees F (135 degrees C). Butter a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 4 cups and 2 tablespoons flour, butter, and white sugar together in a bowl using your hands until completely mixed. Spread dough into the prepared baking pan; cover with waxed paper and press firmly into pan. Prick dough through the waxed paper using a fork; remove waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until shortbread is lightly browned, about 1 1/2 hours. Turn off oven and remove pan. Cut shortbread into 1-inch squares. Return pan to oven until oven is completely cooled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle shortbread with colored sugar, remove squares from pan, and store in an airtight container.\n"
            }
        ],
