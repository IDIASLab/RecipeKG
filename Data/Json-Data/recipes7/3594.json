    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/176440/red-hot-sugar-cookies/",
        "name": "Red Hot Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4465542.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T03:37:39.000Z",
        "description": "Perfect for Valentine's Day or any day you want to give someone a special red hot treat!!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "48 cookies",
        "recipeIngredient": [
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon baking powder",
            "1 cup butter, softened",
            "1\u2009\u00bd cups white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 (2.25 ounce) package cinnamon red hot candies"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together flour, baking soda, and baking powder in a small bowl.  Using an electric mixer, beat butter and sugar in a large bowl until smooth.  Beat in egg and vanilla.  Stir in the dry ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out on lightly floured surface.  Cut into heart shapes and place on ungreased baking sheets.  Dot cookies with red hot candies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until golden, 8 to 10 minutes.  Let stand on cookie sheet for 2 minutes before removing to cooling racks.\n"
            }
        ],
