    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260270/easy-and-delicious-apple-crisp/",
        "name": "Easy and Delicious Apple Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559092.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:24:33.000Z",
        "description": "This crisp is delicious as a dessert on a chilly day. It's tasty with some vanilla ice cream and caramel sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9-inch baking dish",
        "recipeIngredient": [
            "2 apples, peeled and diced",
            "\u00bc cup water",
            "1 lime, juiced",
            "1\u2009\u00bd teaspoons white sugar",
            "1 teaspoon ground cinnamon",
            "\u00bd cup all-purpose flour",
            "\u2153 cup white sugar",
            "\u00bc cup rolled oats",
            "2 tablespoons rolled oats",
            "1 teaspoon ground cinnamon",
            "1 pinch salt",
            "\u00bc cup butter, cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples, water, lime juice, and 1 1/2 teaspoons white sugar in a pot over medium heat. Stir until well combined. Add 1 teaspoon cinnamon and cook over medium-low heat, stirring occasionally, until apples are softened and water has evaporated, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, 1/3 cup white sugar, 1/4 cup plus 2 tablespoons oats, 1 teaspoon cinnamon, and salt in a bowl. Mix in butter until crumbles form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour apple filling into a 9-inch baking dish. Sprinkle crumble mixture over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the top is golden brown, about 25 minutes.\n"
            }
        ],
