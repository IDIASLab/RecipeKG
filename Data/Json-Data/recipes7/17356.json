        "recipeYield": "1 gallon",
        "recipeIngredient": [
            "1 gallon water, divided",
            "4 family-size tea bags",
            "2 cups white sugar",
            "1 cinnamon stick",
            "1 teaspoon whole cloves",
            "2 cups pineapple juice",
            "2 cups orange juice",
            "\u00bc cup lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Boil 2 quarts water in a pot. Remove from heat, add tea bags, and steep for 20 minutes. Remove and discard tea bags.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 2 quarts water, sugar, cinnamon stick, and cloves in a separate pot. Bring to a boil. Add the brewed tea, pineapple juice, orange juice, and lemon juice.\n"
            }
        ],
