    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237274/pineapple-bread-pudding/",
        "name": "Pineapple Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1156449.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2020-06-19T03:58:53.000Z",
        "description": "This is our family recipe (Grandma Mel's) side dish for Easter dinner, served alongside ham and green beans. It is very sweet and great for a holiday meal accompaniment.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT3H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "7 slices white bread, cut into cubes",
            "\u00bd cup butter, melted",
            "30 ounces crushed pineapple",
            "4 eggs",
            "\u00bd cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 2-quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread bread cubes in the bottom of the prepared casserole dish. Pour butter over bread cubes and top with pineapple.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and sugar together in a bowl until foamy; pour over bread mixture. Cover dish with aluminum foil and refrigerate, about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes. Remove aluminum foil and bake until browned, 5 to 20 minutes.\n"
            }
        ],
