    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257570/summertime-dessert-lasagna/",
        "name": "Summertime Dessert Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Summertime Dessert Lasagna"
        },
        "datePublished": "2020-06-19T04:20:40.000Z",
        "description": "Delicious lasagna made with graham crackers, banana pudding, cream cheese frosting, and fresh fruit layers. The whole family will love this treat on a hot summer day. Great for get-togethers as well.",
        "prepTime": "P0DT1H0M",
        "cookTime": null,
        "totalTime": "P0DT6H0M",
        "recipeYield": "1 9x13-inch lasagna",
        "recipeIngredient": [
            "8 kiwis, peeled",
            "\u00be cup white sugar, divided",
            "4 cups hulled fresh strawberries",
            "4 cups fresh blackberries",
            "1 (14.4 ounce) package graham crackers (such as Honey Maid\u00ae)",
            "2 cups ricotta cheese",
            "1 (16 ounce) container cream cheese frosting (such as Duncan Hines\u00ae)",
            "3 (3.5 ounce) containers banana cream pudding (such as Snack Pack\u00ae)",
            "\u00bd cup whipped cream",
            "\u00bd cup toasted sliced almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 4 kiwis and 2 tablespoons sugar in a food processor; puree until smooth. Transfer puree to a bowl. Rinse the food processor bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 cups strawberries and 2 tablespoons sugar in the food processor; puree until smooth. Strain puree into a bowl. Rinse the food processor bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 cups blackberries and 2 tablespoons sugar in the food processor; puree until smooth. Strain puree into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut remaining kiwis into 1/4-inch slices. Cut remaining strawberries into 1/8-inch slices. Cut remaining blackberries in half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 4 graham crackers in a resealable plastic bag; seal. Crush with a rolling pin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining sugar, ricotta cheese, cream cheese frosting, and banana cream pudding together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the bottom of a 9x13-inch glass pan with whole graham crackers. Spread 1/3 of the ricotta cheese mixture evenly on top. Cover with kiwi puree and kiwi slices. Lay more whole graham crackers on top. Spread 1/2 of the remaining ricotta cheese mixture over crackers. Cover with strawberry puree and strawberry slices. Repeat layers with blackberry puree and halved blackberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread whipped cream over blackberries. Garnish with crushed graham crackers and toasted almonds. Cover tightly with plastic wrap and refrigerate until flavors combine, 5 hours to overnight.\n"
            }
        ],
