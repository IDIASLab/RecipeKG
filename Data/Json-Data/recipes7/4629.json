        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213743/swedish-butter-crisps/",
        "name": "Swedish Butter Crisps",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F566483.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:41:49.000Z",
        "description": "This is one of my favorite cookies. They are sweet and buttery and addicting!!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "1 cup butter",
            "\u00be cup white sugar",
            "1 egg yolk",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "1 egg white, stiffly beaten",
            "\u00bc cup brown sugar",
            "\u00bc cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). Grease a cookie sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the butter and white sugar with an electric mixer in a bowl until smooth. Add the egg yolk and vanilla extract, then mix in the flour until just incorporated. Press the dough into the prepared cookie sheet, and spread with the beaten egg white. Sprinkle with brown sugar and pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and set, about 30 minutes. Cool completely before cutting into squares.\n"
            }
        ],
