        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260046/new-england-brown-bread/",
        "name": "New England Brown Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4554746.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:24:24.000Z",
        "description": "A New England classic; lightly sweet and delicious.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "1 large loaf",
        "recipeIngredient": [
            "1 cup water",
            "1 tablespoon water",
            "\u00bd cup molasses",
            "1 tablespoon vegetable oil",
            "1 tablespoon white sugar",
            "1 teaspoon salt",
            "1 cup quick-cooking oats",
            "3 cups bread flour",
            "2\u2009\u00bc teaspoons bread machine yeast",
            "1 tablespoon quick-cooking oats, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place 1 cup plus 1 tablespoon water, molasses, oil, sugar, salt, 1 cup oats, bread flour, and yeast in a bread machine in the order listed. Run \"Dough\" cycle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove dough from the bread machine. Shape into a round loaf, tucking ends in underneath. Transfer to a baking sheet and cover loosely with lightly oiled plastic wrap. Let rise until puffy, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Uncover dough and sprinkle 1 tablespoon oats on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 40 minutes.\n"
            }
        ],
