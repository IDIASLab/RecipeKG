    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272330/slow-cooker-creamy-tomato-soup/",
        "name": "Slow Cooker Creamy Tomato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6082354.jpg",
            "width": 1164,
            "height": 1014
        },
        "datePublished": "2020-06-19T04:35:53.000Z",
        "description": "This rich, creamy, tomato soup is easily made in a slow cooker and features Italian plum tomatoes and half-and-half. It makes for a great go-to on a cold winter day.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT3H20M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon extra-virgin olive oil",
            "2 onions, sliced",
            "2 cloves garlic cloves, smashed and peeled, or more to taste",
            "2 (28 ounce) cans Italian-style plum tomatoes, drained",
            "4 cups vegetable broth",
            "1 (28 ounce) jar tomato sauce",
            "6 sprigs flat-leaf parsley",
            "1 teaspoon salt",
            "1 teaspoon white sugar",
            "2 cups half-and-half"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a medium skillet over medium-high heat. Add onions and garlic. Cook, stirring often until fragrant, about 30 seconds. Place in the bottom of a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add plum tomatoes, vegetable broth, tomato sauce, parsley sprigs, salt, and sugar to the slow cooker. Cover and cook on High until tomatoes are stewed, 3 to 4 hours. Remove parsley sprigs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree the soup using an immersion blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir half-and-half into pureed soup. Cover and cook until heated through, 20 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Tomato Soup Recipes"
        ],
