        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218182/saralees-beet-salad/",
        "name": "SARALEE'S Beet Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F805108.jpg",
