        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263727/vegan-mug-cake/",
        "name": "Vegan Mug Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9356489.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:27:39.000Z",
        "description": "This vegan mug cake is made without egg or milk and uses applesauce for moisture. Within 5 minutes you have a great vegan dessert for 1!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H8M",
        "recipeYield": "1 mug cake",
        "recipeIngredient": [
            "4 tablespoons all-purpose flour",
            "3 tablespoons white sugar",
            "2 tablespoons unsweetened cocoa powder",
            "\u00bc teaspoon baking powder",
            "4 tablespoons applesauce",
            "3 tablespoons soy milk",
            "1 tablespoon vegan chocolate chips, or more to taste",
            "1 tablespoon toasted flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, cocoa powder, and baking powder in a mug. Stir applesauce and soy milk together in a bowl and add to flour mixture. Stir until well combined. Fold in chocolate chips and sprinkle with coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave at the highest setting until mug cake has set and risen well, about 3 minutes.\n"
            }
        ],
