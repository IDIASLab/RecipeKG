    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239296/easy-tomato-beet-soup/",
        "name": "Easy Tomato-Beet Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1546080.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T04:00:41.000Z",
        "description": "This is a hearty warm soup with just a hint of beets that's quick and easy and makes a healthy filling meal. Served with grilled cheese sandwiches, it makes a weeknight meal you can have on the table in no time. Garnish with sour cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 (14.5 ounce) cans fire-roasted diced tomatoes, drained",
            "1 (15 ounce) can whole beets, drained",
            "1 (6 ounce) can tomato paste",
            "26 ounces beef stock, divided",
            "1 tablespoon white sugar, or more to taste",
            "\u00bd teaspoon dried dill weed",
            "3 drops hot pepper sauce, or more to taste",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree tomatoes, beets, tomato paste, and 1/2 cup beef stock together in a blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour tomato mixture into a saucepan with the remaining beef stock. Stir sugar, dill, hot pepper sauce, salt, and pepper into the tomato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the soup to a simmer, reduce heat to medium-low, and cook until the flavors meld, about 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Tomato Soup Recipes"
        ],
