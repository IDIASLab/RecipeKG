    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/103805/scottish-pudding/",
        "name": "Scottish Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1986904.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T03:13:29.000Z",
        "description": "My Granny's dumpling brought into the 20th century, no longer hours on end cooking, just ten minutes in microwave! She makes it every year on Hogmanay.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup water",
            "\u00be cup white sugar",
            "1 tablespoon mixed spice",
            "1 tablespoon cinnamon",
            "1\u2009\u00bc cups chopped mixed dried fruit ",
            "1\u2009\u00bc cups golden raisins",
            "1 cup margarine",
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "2 eggs, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine the water, sugar, mixed spice, cinnamon, mixed fruit, golden raisins and margarine. Bring to a boil and let simmer for one minute. Remove from the heat and stir in the flour, baking soda and eggs. Pour into a microwave-safe bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the bowl into the microwave, uncovered. Cook for 10 minutes on full power. Let stand for 5 minutes. Serve warm with your favorite custard.\n"
            }
        ],
