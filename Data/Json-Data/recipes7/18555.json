    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268056/swedish-pepparkakor/",
        "name": "Swedish Pepparkakor",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Swedish Pepparkakor"
        },
        "datePublished": "2020-06-19T04:32:22.000Z",
        "description": "Crunchy, but not hard, spicy gingerbread cookies from Sweden. You can omit the egg to make it more cracker-like. If you prefer a little softer cookie, leave on the tray for a minute or two before moving to the cooling rack.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "48 cookies",
        "recipeIngredient": [
            "1\u2009\u00be cups all-purpose flour",
            "\u00bc cup pastry flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground ginger",
            "1 dash ground black pepper, or to taste",
            "\u00bc cup molasses",
            "\u2153 cup white sugar",
            "\u215b cup maple syrup",
            "\u00bc cup butter",
            "egg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix all-purpose flour, pastry flour, baking soda, cloves, cinnamon, salt, ginger, and black pepper together in a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine molasses, sugar, and maple syrup in a small saucepan over low heat. Stir constantly to dissolve and remove from heat when a small foam rises to the surface, about 3 minutes. Continue stirring and allow to cool slightly; add butter and let cool to room temperature. Add egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour molasses mixture into flour mixture in the bowl and mix until crumbles hold together when pressed. Knead for 2 minutes, focusing on any dry bits. Form into a flat log and wrap tightly in plastic wrap. Place in the refrigerator for 1 hour to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut dough log into small sections and allow to warm up slightly before rolling out between 2 layers of parchment paper to 1/8-inch thickness. Cut shapes with a floured cookie cutter or pastry wheel. Place cookies on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 8 to 12 minutes; remove cookies before they brown. Cool on a rack.\n"
            }
        ],
