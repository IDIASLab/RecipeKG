        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258200/sunbutter-monster-cookies/",
        "name": "SunButter&reg; Monster Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4518111.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-05-11T06:59:52.000Z",
        "description": "Crunchy, sweet SunButter Sunflower Butter Monster Cookies are peanut free and scrumptious! Add your favorite monster ingredients to the cookie dough.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": "P0DT0H40M",
        "recipeYield": "30 cookies",
        "recipeIngredient": [
            "\u00bc cup softened organic butter",
            "\u00be cup organic brown sugar",
            "\u00be cup organic white sugar",
            "2 organic eggs",
            "1 teaspoon gluten-free vanilla extract",
            "1\u2009\u00bc teaspoons baking soda",
            "1 cup SunButter\u00ae, any variety",
            "3 cups gluten-free rolled oats",
            "1\u2009\u00bd cups combined total monster ingredients: raisins, chocolate candies, etc."
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl cream butter and sugars until creamy. Add eggs, vanilla, and baking soda and mix well. Add SunButter&reg; and mix until thoroughly blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in oats. Add your monster ingredients and stir until well mixed throughout cookie dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop heaping tablespoon full onto parchment lined baking sheets. Bake for 15 to 18 minutes until edges are golden brown and cookies are somewhat firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool and store in airtight container.\n"
            }
        ],
