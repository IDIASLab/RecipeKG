        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (750 milliliter) bottle spiced apple wine",
            "2 cups cranberry juice",
            "\u00bd cup white sugar",
            "12 whole cloves, or as desired",
            "1 orange ",
            "3 cinnamon sticks",
            "1 apple, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine apple wine, cranberry juice, and sugar in a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press whole cloves into the orange. Float the orange, cinnamon sticks, and apple slices in the wine mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat on Low until wine mixture is heated through, at least 30 minutes.\n"
            }
