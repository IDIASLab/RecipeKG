        "mainEntityOfPage": "https://www.allrecipes.com/recipe/171700/cucumbers-in-sour-cream/",
        "name": "Cucumbers in Sour Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F253689.jpg",
