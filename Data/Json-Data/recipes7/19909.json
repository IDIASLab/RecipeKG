    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273552/vanilla-chai-cheesecake/",
        "name": "Vanilla Chai Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2020%2F12%2F14%2FVanilla_Chai_Cheesecake_001.jpg",
            "width": 640,
            "height": 636
        },
        "datePublished": "2020-06-19T04:37:11.000Z",
        "description": "Vanilla chai cheesecake adapted from Rachel's Diary.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "1 9-inch cheesecake",
        "recipeIngredient": [
            "2 (6 ounce) containers vanilla yogurt",
            "1 cup crushed gingersnap cookies",
            "\u00bd cup crushed amaretti cookies",
            "3 tablespoons butter, melted",
            "4 (8 ounce) packages cream cheese, softened",
            "\u00be cup white sugar",
            "2 teaspoons vanilla extract",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground ginger",
            "\u00bc teaspoon ground cardamom",
            "\u215b teaspoon salt",
            "4 eggs",
            "1 (8 ounce) jar caramel sauce, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place yogurt in a strainer lined with a coffee filter and set over a bowl. Refrigerate for at least 40 minutes to allow liquid to drain. Discard liquid and set yogurt aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine gingersnap cookies, amaretti cookies, and butter in a bowl; mix well. Press crust into the bottom of a 9-inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from oven and reduce temperature to 325 degrees F (165 degrees C). Let crust cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While crust cools, beat cream cheese, sugar, vanilla extract, cinnamon, ginger, cardamom, and salt together in a mixing bowl using an electric mixer until well blended. Add eggs 1 at a time, beating on low speed and scraping the sides of the bowl after each addition. Stir in yogurt. Pour filling into the cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cake is set on sides but slightly shimmers in center, about 50 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cool completely and remove from pan. Serve with caramel sauce.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cheesecake Recipes"
        ],
