    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246080/citrus-creme-brulee/",
        "name": "Citrus Creme Brulee",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Citrus Creme Brulee"
        },
        "datePublished": "2020-06-19T04:05:07.000Z",
        "description": "This is an excellent creme brulee with a slightly tart kick. Serve with fresh fruit slices (orange or kiwi suggested) and whipped cream, if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H9M",
        "totalTime": "P0DT5H24M",
        "recipeYield": "6 ramekins",
        "recipeIngredient": [
            "2 oranges, juiced",
            "lemon, juiced",
            "1 lime, juiced",
            "2 cups heavy whipping cream",
            "\u2153 cup white sugar",
            "5 egg yolks",
            "6 tablespoons white sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring orange juice, lemon juice, and lime juice to a boil in a saucepan; cook until reduced to about 2 tablespoons liquid, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat cream and 1/3 cup sugar together in a separate saucepan over medium heat, stirring until sugar completely dissolves, 1 to 2 minutes; remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk egg yolks together in a bowl until frothy. Whisk reduced juice mixture into egg yolks until fully combined. Pour cream mixture in gradually, whisking constantly, until thin custard forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain custard mixture through a fine sieve; divide into six 5-ounce ramekins. Place ramekins in a baking dish; pour in enough boiling water to come half way up the sides of ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until just set, about 1 hour. Remove from oven, cool slightly, and cover ramekins with plastic wrap; refrigerate at least 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove plastic wrap from ramekins. Sprinkle 1 tablespoon of sugar in a thin layer over top of each ramekin. Brown sugar under a broiler until golden and caramelized, 30 seconds to 1 minute.\n"
            }
        ],
