        "mainEntityOfPage": "https://www.allrecipes.com/recipe/217195/simply-lasagna/",
        "name": "Simply Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F980194.jpg",
