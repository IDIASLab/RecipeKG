    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256031/mom-spragginss-blackberry-cobbler/",
        "name": "Mom Spraggins's Blackberry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4394748.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:18:38.000Z",
        "description": "You can use just about any kind of fresh berries you like. My favorite is the black and red raspberries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9x13-inch baking pan",
        "recipeIngredient": [
            "2 cups fresh blackberries, or more to taste",
            "2 cups white sugar, divided",
            "\u00bd cup butter",
            "\u00be cup milk",
            "\u00be cup all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix blackberries and 1 cup sugar together in a bowl; let stand until mixture becomes juicy, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place butter in a 9x13-inch baking dish and put dish in the preheating oven until butter is melted, 2 to 5 minutes. Remove dish from oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 1 cup sugar, milk, flour, baking powder, and salt in a large bowl until batter is smooth; pour over melted butter. Do not stir. Spoon blackberry mixture over batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling and cooked through, about 1 hour.\n"
            }
        ],
