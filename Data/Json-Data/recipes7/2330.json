        "cookTime": null,
        "totalTime": "P0DT0H5M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "2 teaspoons cocoa powder",
            "1 teaspoon hot water",
            "1 cup milk",
            "4 tablespoons frozen blueberries",
            "1 teaspoon white sugar",
            "4 ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together the cocoa powder and water in a small bowl until the cocoa is dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the cocoa mixture, milk, blueberries, sugar, and ice cubes into a blender; cover and blend until smooth, 30 to 45 seconds. Serve cold.\n"
            }
        ],
