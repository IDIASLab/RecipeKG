        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257454/quick-pan-seared-scallops/",
        "name": "Quick Pan-Seared Scallops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8248419.jpg",
