    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235662/sweet-potato-crunch-casserole/",
        "name": "Sweet Potato Crunch Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1744383.jpg",
            "width": 3264,
            "height": 2387
        },
        "datePublished": "2020-06-19T03:57:25.000Z",
        "description": "A Southern take on the traditional sweet potato casserole; a creamy sweet potato base covered liberally with a crunchy pecan praline topping. This dish is great even at room temperature.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 (29 ounce) cans sweet potatoes, drained and mashed",
            "1 cup white sugar",
            "\u00bd cup butter, melted",
            "\u00bd cup milk",
            "2 eggs",
            "1 teaspoon salt",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon ground cinnamon",
            "1 cup brown sugar",
            "1 cup chopped pecans",
            "\u2153 cup butter, melted",
            "1 cup all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x9-inch casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sweet potatoes, white sugar, 1/2 cup butter, milk, eggs, salt, vanilla extract, and cinnamon together in a bowl until smooth; spread into the prepared casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir brown sugar, pecans, and 1/3 cup butter together in a bowl. Mix flour into pecan mixture using your hands until evenly coated and crumbly; sprinkle over sweet potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cooked through and praline topping is lightly browned, about 40 minutes. Cool slightly before serving, at least 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Sweet Potato Casserole Recipes"
        ],
