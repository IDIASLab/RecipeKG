    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258635/hamburger-rice-casserole/",
        "name": "Hamburger Rice Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5202296.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2020-06-19T04:22:16.000Z",
        "description": "My mom made this many times for Saturday dinner when we were growing up. It is quick to prep and makes a satisfying meal with fresh bread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H25M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "1 head green cabbage, shredded",
            "\u00bd cup uncooked long-grain white rice",
            "1 pound ground beef",
            "1 small onion, chopped",
            "1\u2009\u00bd teaspoons dried oregano",
            "\u00bd teaspoon dried sweet basil",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon dried thyme",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon white sugar",
            "\u00bc teaspoon ground black pepper",
            "1 (28 ounce) can whole tomatoes",
            "10 slices American cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread cabbage in the bottom of a 9x13-inch baking dish; sprinkle rice on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease. Add onion, oregano, basil, garlic powder, and thyme; cook and stir until onion is transparent, about 5 minutes. Season with sugar, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon beef mixture over rice and cabbage in the baking dish. Break up tomatoes in the can with your hand; pour over beef mixture. Cover baking dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until rice is tender, about 1 hour 15 minutes. Uncover and arrange American cheese slices over casserole. Let stand until cheese melts and absorbs juices, 15 to 20 minutes.\n"
            }
        ],
