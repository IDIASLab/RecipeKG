    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/205651/thai-cucumber-tomato-salad/",
        "name": "Thai Cucumber Tomato  Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F407103.jpg",
            "width": 2364,
            "height": 2363
        },
        "datePublished": "2020-06-19T03:40:03.000Z",
        "description": "This is a delicious light summer salad that is a little on the sweet side rather than than the tangy-vinegary side.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 large cucumber",
            "2 tomatoes, seeded and cut into wedges",
            "\u00bc red onion, thinly sliced",
            "\u00bc cup rice vinegar",
            "2 tablespoons lime juice",
            "1 teaspoon white sugar, or to taste",
            "3 tablespoons chopped fresh cilantro",
            "3 tablespoons chopped peanuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel the cucumber in stripes lengthwise with a vegetable peeler, alternating skinned stripes with peel for a decorative effect. Slice the cucumber in half lengthwise, and then thinly slice. Place the cucumber in a salad bowl with the tomato and red onion, and mix together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the rice vinegar and lime juice into a separate bowl, and stir in the sugar until dissolved. Pour the dressing over the salad;mix, cover, and refrigerate until chilled, at least 30 minutes. Just before serving, stir in the cilantro and sprinkle with chopped peanuts.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Tomato Salad Recipes"
        ],
