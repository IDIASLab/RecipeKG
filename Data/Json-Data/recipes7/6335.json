    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220377/chipotle-chicken-sandwich/",
        "name": "Chipotle Chicken Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F814367.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2020-06-19T03:47:38.000Z",
        "description": "This chicken sandwich has a nice kick to it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 teaspoons olive oil",
            "4 skinless, boneless chicken breast halves",
            "1 tablespoon red wine vinegar",
            "1 tablespoon fresh lime juice",
            "\u00bd teaspoon white sugar",
            "salt and ground black pepper to taste",
            "1 green onion, chopped",
            "1 clove garlic, minced",
            "\u00bd teaspoon dried oregano",
            "\u2153 cup light mayonnaise",
            "1 tablespoon canned chipotle peppers in adobo sauce, seeded and minced",
            "1\u2009\u00bd tablespoons chopped green onion",
            "1\u2009\u00bd tablespoons sweet pickle relish",
            "8 slices  sourdough bread",
            "4 slices mozzarella cheese",
            "1 cup torn lettuce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat and pan-fry chicken breasts until browned and no longer pink inside, about 10 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle chicken breasts with red wine vinegar, lime juice, sugar, salt, black pepper, 1 chopped green onion, garlic, and oregano; pan-fry until green onion is soft, an additional 5 minutes per side. Transfer chicken breasts to a plate and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place light mayonnaise and chipotle pepper into a blender and blend until smooth. Transfer to a bowl and stir in 1 1/2 tablespoon of chopped green onion and sweet pickle relish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast sourdough bread slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer 4 bread slices each with 1/4 cup of lettuce, a chicken breast, and 1 mozzarella cheese slice. Spread remaining slices with chipotle mayonnaise and place on top to make sandwiches. Serve while still warm.\n"
            }
        ],
