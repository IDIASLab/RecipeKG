        "recipeYield": "1 doughnut",
        "recipeIngredient": [
            "1 egg, beaten",
            "2 tablespoons brown sugar",
            "2 tablespoons self-rising flour",
            "1\u2009\u00bd tablespoons milk",
            "1 tablespoon white sugar",
            "1 tablespoon olive oil",
            "\u00bc teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat egg, brown sugar, self-rising flour, milk, white sugar, olive oil, and cinnamon together in a large mug.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mug in microwave; cook on high heat for 2 minutes.\n"
            }
        ],
