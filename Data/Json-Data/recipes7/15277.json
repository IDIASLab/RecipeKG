        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256651/magic-syrup/",
        "name": "Magic Syrup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4458089.jpg",
