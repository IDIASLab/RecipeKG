    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258704/four-layer-strawberry-cream-cake/",
        "name": "Four-Layer Strawberry Cream Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Four-Layer Strawberry Cream Cake"
        },
        "datePublished": "2020-06-19T04:22:29.000Z",
        "description": "Since I first made this cake, everyone at work requests it for special occasions and I have even sold some! Keep refrigerated.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 layer cake",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup butter, softened",
            "4 eggs",
            "3 cups self-rising flour",
            "1 cup 2% milk ",
            "1 teaspoon vanilla extract",
            "1 (16 ounce) container sour cream",
            "1 (16 ounce) package frozen sweetened sliced strawberries, unthawed",
            "1 (16 ounce) package frozen whipped topping, thawed",
            "1 pint whole fresh strawberries",
            "2 sprigs fresh mint, stemmed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease four 9-inch round cake pans and line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar and butter in a stand mixer fitted with the paddle attachment; beat until smooth and creamy. Beat in eggs one at a time. Mix in 1 cup flour. Beat in 1/2 cup milk. Repeat with remaining flour and milk, beating briefly after each addition. Scrape down sides of bowl and mix in vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter evenly into the prepared cake pans. Smooth the tops with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 15 minutes. Cool cakes in the pans for 10 minutes; invert onto a wire rack to cool completely, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sour cream and sliced strawberries together in a large bowl. Spread evenly between the cooled cake layers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost top and sides of cake with whipped topping. Decorate top of cake with whole strawberries and mint leaves.\n"
            }
        ],
