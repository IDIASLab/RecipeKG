        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239792/bite-sized-cheesecake-cupcakes/",
        "name": "Bite-Sized Cheesecake Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2367127.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:01:11.000Z",
        "description": "These bite-sized cheesecakes are just the thing to bring to parties or to have as a dessert during the holidays!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "12 vanilla wafer cookies",
            "2 (8 ounce) packages cream cheese, softened",
            "\u00bd cup white sugar",
            "2 eggs, beaten",
            "\u00bd teaspoon vanilla extract",
            "\u00be cup cherry pie filling, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line 12 muffin cups with foil cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a vanilla wafer cookie into the bottom of each cupcake liner.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, sugar, eggs, and vanilla extract together in a bowl until smooth; pour atop the cookies until the liners are about 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until center is nearly set and slightly collapsed, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate cupcakes 2 hours to overnight. Spoon cherry pie filling onto the cupcakes.\n"
            }
        ],
