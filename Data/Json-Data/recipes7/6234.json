    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219971/old-fashioned-butter-valentine-cookies-dipped-in-chocolate/",
        "name": "Old Fashioned Butter Valentine Cookies Dipped in Chocolate",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F788196.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:47:18.000Z",
        "description": "This is my mother's recipe. She handed it down to me years ago and I have been making it with a few of my own changes for about 25 years now.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour, sifted",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "1 cup butter, softened",
            "\u00be cup white sugar",
            "1 large egg",
            "2 tablespoons milk",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bd cup seedless raspberry jam",
            "4 ounces semisweet chocolate chips",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift the flour, baking powder, and salt together in a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the 1 cup butter with the sugar, egg, milk, and vanilla extract in another bowl until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually mix the flour mixture into the butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape the dough into a disk; wrap with plastic wrap and refrigerate for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Working with half the dough at a time, roll out dough to about 1/8-inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut into cookies with a large heart shaped cookie cutter. Repeat with the remaining half of the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the center of half of the cookies with a smaller heart shaped cookie cutter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place an open centered cookie on top of a whole cookie to form a sandwich. Arrange cookies on an ungreased baking sheet; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the raspberry jam in a microwave save bowl and heat on high until jam is runny, about 35 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill the center of the cookies with the jam. Refrigerate cookies for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the cookies are firm to the touch, 5 to 8 minutes. Cookies will be pale in color.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cookies to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the chocolate and 2 tablespoons butter in a microwave-safe glass or ceramic bowl in 30-second intervals, stirring after each melting until smooth, for 1 to 3 minutes, depending on your microwave.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chocolate decoratively over half of each cookie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookie on a waxed paper lined baking sheet and refrigerate until the chocolate has set, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
