    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256534/butterfly-cakes/",
        "name": "Butterfly Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Butterfly Cakes"
        },
        "datePublished": "2020-06-19T04:19:20.000Z",
        "description": "Light, tasty individual cakes with a jam centre, vanilla buttercream topping, and sponge 'butterfly wings'. Always a hit with kids!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "16 cakes",
        "recipeIngredient": [
            "\u00be cup butter, softened",
            "\u00be cup white sugar",
            "2 tablespoons white sugar",
            "3 eggs",
            "1 teaspoon vanilla extract, or more to taste",
            "1\u2009\u00bd cups self-rising flour",
            "1 cup confectioners' sugar",
            "2\u2009\u00bd tablespoons confectioners' sugar",
            "\u00bc cup butter, softened",
            "1 teaspoon vanilla extract, or to taste",
            "\u00bd cup raspberry jam, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Line a muffin tin with medium-sized cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 3/4 cup butter and 3/4 cup plus 2 tablespoons white sugar in a large bowl; beat with an electric mixer until fluffy. Beat in eggs and 1 teaspoon vanilla extract. Beat in flour gradually. Divide batter evenly among the cupcake liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 15 to 20 minutes. Transfer to a wire rack to cool, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1 cup plus 2 1/2 tablespoons confectioners' sugar, 1/4 cup butter, and 1 teaspoon vanilla extract together in a bowl to make buttercream filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop some of the center out of each cupcake, keeping it in one piece. Fill each hole with raspberry jam. Top cupcakes with buttercream. Cut the scooped-out centers into 2 pieces and position them in the buttercream to look like butterfly wings.\n"
            }
        ],
