    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270684/tropical-trifle-with-fresh-pineapple/",
        "name": "Tropical Trifle with Fresh Pineapple",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tropical Trifle with Fresh Pineapple"
        },
        "datePublished": "2020-06-19T04:35:00.000Z",
        "description": "A refreshing and delicious tropical trifle made from fresh pineapple.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "4 tablespoons vanilla custard powder",
            "4 cups milk",
            "2 tablespoons white sugar",
            "1 fresh pineapple, cut into 1/2-inch pieces",
            "water to cover",
            "1 tablespoon brown sugar",
            "\u00bd teaspoon ground cinnamon",
            "2 (12 ounce) loaves prepared pound cake, crusts trimmed and cut into 1/2-inch squares",
            "1 cup heavy whipping cream, whipped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix custard powder with a few tablespoons cold milk in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour remaining milk into a saucepan, stir in sugar, and bring to a boil over medium heat, about 5 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add custard mixture to hot milk, stirring continuously until liquid has thickened. Set aside to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pineapple in a large heavy pan, cover with water, and bring to a boil over medium heat. Remove from heat and allow to cool, about 30 minutes. Strain the juice and set aside. Place pineapple pieces in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar and cinnamon together in a small bowl. Sprinkle over pineapple pieces and toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer 1/2 the pound cake in a trifle bowl. Sprinkle with 1 to 2 tablespoons of the reserved pineapple juice. Spread 1/2 the pineapple pieces over the cake. Spoon 1/2 the cooled custard over pineapple. Repeat layers. Spread whipped cream on top and smooth out evenly. Cover and chill in the refrigerator at least 1 hour.\n"
            }
        ],
