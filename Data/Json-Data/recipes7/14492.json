    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254414/grandmas-no-bake-cheesecake/",
        "name": "Grandma's No-Bake Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5315372.jpg",
            "width": 1932,
            "height": 2576
        },
        "datePublished": "2020-06-19T04:16:50.000Z",
        "description": "This is a quick and easy cheesecake recipe created by my grandmother and handed down through 3 generations. Each has put their own special touch on it but the overall taste is the same. Blue Ribbon flavor for years of happy memories!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT4H11M",
        "recipeYield": "1 8-inch cake or 8 mini cakes",
        "recipeIngredient": [
            "1 (8 ounce) container whipped topping (such as Cool Whip\u00ae)",
            "1 cup sour cream",
            "1 cup white sugar",
            "1 tablespoon vanilla extract",
            "1 (8 ounce) package cream cheese, cut into 6 pieces",
            "1 (9 inch) graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix whipped topping, sour cream, sugar, and vanilla extract together in a large bowl until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese pieces on a microwave-safe plate. Microwave at 30-second intervals until warm and soft, but not discolored, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese into whipped topping mixture; beat with an electric bender on medium speed until light and fluffy, about 2 minutes. Pour into graham cracker crust, smoothing the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate cheesecake until set, 4 to 5 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cheesecake Recipes"
        ],
