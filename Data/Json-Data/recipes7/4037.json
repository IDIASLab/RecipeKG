    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/202247/portabella-basil-sub/",
        "name": "Portabella Basil Sub",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F415806.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2020-06-19T03:39:42.000Z",
        "description": "Portabella mushrooms are cooked in butter with spinach, yellow pepper, garlic, basil and shallots, and served on a roll.",
        "prepTime": "P0DT0H23M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 sandwich",
        "recipeIngredient": [
            "1 tablespoon butter",
            "\u00bc cup chopped red shallots",
            "2 tablespoons chopped fresh basil leaves",
            "1 tablespoon minced fresh garlic",
            "1 teaspoon hot paprika",
            "1 teaspoon salt",
            "1 teaspoon fresh ground black pepper ",
            "1 cup fresh spinach leaves",
            "\u00bc cup diced portabella mushroom caps",
            "\u00bc cup diced yellow bell pepper",
            "1 tablespoon sweet Jamaican pepper sauce (such as Pickapeppa Sauce \u00ae)",
            "1 tablespoon balsamic vinegar",
            "1 hoagie roll, split lengthwise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a skillet with a lid over medium heat, and cook and stir the red shallots, basil, and garlic until fragrant, about 1 minute. Stir in the hot paprika, salt, and pepper. Add the spinach, portabella mushrooms, and yellow bell pepper, stir a few times, cover with lid, and reduce the heat to medium-low. Cook the mixture until the spinach and mushrooms give up their liquid and the peppers are softened, about 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Uncover the skillet, and pour in the pepper sauce and balsamic vinegar. Increase the heat to medium, and let the mixture cook down and thicken slightly, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the hot mushroom mixture onto the split hoagie roll, and serve.\n"
            }
        ],
        "recipeCategory": [
