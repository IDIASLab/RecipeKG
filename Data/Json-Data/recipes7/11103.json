        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237309/homemade-italian-red-sauce/",
        "name": "Homemade Italian Red Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6443381.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2020-06-19T03:58:54.000Z",
        "description": "You will love my Italian-style sauce. You can add meatballs to this sauce; it comes out really good.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H10M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bd cup olive oil",
            "1 large onion, minced",
            "3 cloves garlic, minced",
            "4 cups water",
            "2 (32 ounce) cans crushed diced tomatoes",
            "1 (16 ounce) can tomato paste",
            "\u00bc cup chopped fresh basil",
            "1 teaspoon baking soda",
            "1 teaspoon white sugar",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large saucepan over medium-high heat. Saute onion and garlic in hot oil until onion is translucent, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat to medium-low. Add water, crushed tomatoes, tomato paste, basil, baking soda, and sugar; season with salt and pepper. Stir mixture, bring to a simmer, and cook until the sauce is thickened, about 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces",
            "Tomato"
        ],
