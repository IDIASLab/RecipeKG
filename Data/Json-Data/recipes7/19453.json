    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272185/blueberry-dutch-baby/",
        "name": "Blueberry Dutch Baby",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8400697.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:35:45.000Z",
        "description": "Spruce up your Dutch baby by adding blueberries - this easy puffed pancake is quick and easy to make and will surely delight your family.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 large eggs",
            "\u00be cup whole milk",
            "\u00bc cup white sugar",
            "\u00bd teaspoon vanilla extract",
            "\u00bd cup all-purpose flour",
            "\u00bc teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "2 tablespoons unsalted butter",
            "\u00bd cup blueberries",
            "confectioners' sugar for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine eggs, milk, sugar, and vanilla extract in a bowl; whisk to combine. Combine flour, salt, and cinnamon in a second bowl. Whisk flour mixture into the egg mixture until batter is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in an 8-inch cast iron skillet over medium-high heat. Pour in the batter and sprinkle blueberries on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake Dutch baby in the preheated oven until puffed and golden, 20 to 25 minutes. Remove from the oven, dust with confectioners' sugar, and serve immediately.\n"
            }
        ],
