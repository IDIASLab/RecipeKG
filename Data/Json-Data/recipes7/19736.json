    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273051/bake-sale-worthy-lemon-bars/",
        "name": "Bake-Sale-Worthy Lemon Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9423127.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:36:26.000Z",
        "description": "The next time you need something amazing to contribute to a bake sale, or maybe just a last-minute birthday party, try this simple lemon bars recipe.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "16 bars",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u2153 cup powdered sugar",
            "\u00be cup butter, softened",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "\u00bc cup lemon juice",
            "3 tablespoons all-purpose flour",
            "\u2153 cup powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 1/2 cups flour, 1/3 cup powdered sugar, and butter in a bowl. Pat dough into the prepared pan to form a crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven for 20 minutes. Remove from the oven and leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk white sugar, eggs, lemon juice, and 3 tablespoons flour together in a bowl and pour filling over crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until set, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and cool completely, about 30 minutes. Dust with powdered sugar and cut into 16 bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
