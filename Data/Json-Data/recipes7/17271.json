    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261821/vegan-chocolate-crinkle-cookies/",
        "name": "Vegan Chocolate Crinkle Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5152024.jpg",
            "width": 3742,
            "height": 2788
        },
        "datePublished": "2020-06-19T04:26:05.000Z",
        "description": "Crispy, chewy, and very chocolaty, these vegan-friendly chocolate crinkle cookies are impossible to resist; just a perfect Christmas treat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H11M",
        "totalTime": "P0DT0H31M",
        "recipeYield": "20 cookies",
        "recipeIngredient": [
            "2 tablespoons ground flax seeds",
            "\u2154 cup water, divided",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup unsweetened cocoa powder",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00be cup vegan chocolate chips",
            "3 tablespoons coconut oil",
            "\u00be cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground flax seeds with 1/3 cup water in a bowl; let sit until thickened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, cocoa powder, baking powder, and salt into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips and coconut oil in a microwave-safe glass or ceramic bowl in 15-second intervals, stirring after each melting, 1 to 2 minutes. Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat flax seed mixture, remaining 1/3 cup water, sugar, and vanilla extract in a bowl with an electric mixer until creamy, about 3 minutes. Add flour mixture and melted chocolate; mix until well combined. Shape dough into 1-inch balls. Roll in confectioner's sugar and place on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until firm to the touch, 11 to 13 minutes.\n"
            }
        ],
