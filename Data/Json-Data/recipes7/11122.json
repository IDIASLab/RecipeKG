        "recipeIngredient": [
            "12 fresh strawberries, hulled and quartered",
            "\u00bc cup white sugar",
            "7 cups water, divided",
            "\u00be cup white sugar",
            "1 cup lemon juice",
            "2 cups ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix strawberries and 1/4 cup sugar together in a bowl; set aside for juices to release from strawberries, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 1 cup water to a boil in a small saucepan; add 3/4 cup sugar. Cook and stir sugar mixture until sugar is dissolved, about 5 minutes. Remove saucepan from heat and cool simple syrup to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir simple syrup, 6 cups water, lemon juice, and ice together in a pitcher. Add sweetened strawberries and stir.\n"
            }
        ],
