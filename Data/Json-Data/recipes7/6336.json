    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220390/strawberry-delight/",
        "name": "Strawberry Delight",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F809683.jpg",
            "width": 1936,
            "height": 1948
        },
        "datePublished": "2020-06-19T03:47:38.000Z",
        "description": "A delightful strawberry dessert that is sure to be eaten quickly.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "1 tablespoon white sugar",
            "1 cup sour cream",
            "\u00bc teaspoon almond extract",
            "1\u2009\u00bd teaspoons vanilla extract, divided",
            "2 cups confectioners' sugar",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "1 (9 inch) prepared angel food cake, torn into bite-size pieces",
            "2 quarts thinly sliced strawberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together cream cheese and white sugar in a bowl until smooth. Stir in sour cream, almond extract, and 1/2 teaspoon vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold remaining 1 teaspoon vanilla extract and confectioners' sugar into thawed whipped topping in a separate bowl until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently fold the whipped topping mixture into the cream cheese mixture until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer the bottom of a glass serving bowl with 1/4 angel food cake pieces. Top with 1/4 cream mixture and a layer of strawberries; continue layering three more times until all the cake and cream mixture is used. Finish with a layer of strawberries. Chill until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Trifle Recipes"
        ],
