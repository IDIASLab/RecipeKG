        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254513/pickled-jalapenos/",
        "name": "Pickled Jalapenos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3766414.jpg",
            "width": 2439,
            "height": 2439
        },
        "datePublished": "2020-06-19T04:17:06.000Z",
        "description": "Deliciously fresh and simple pickled peppers! Peppers will keep in the refrigerator for up to 2 months as long as they are covered by the pickling liquid.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 1/2 quart",
        "recipeIngredient": [
            "1 cup vinegar",
            "1 cup water",
            "2 tablespoons white sugar",
            "1 tablespoon salt",
            "2 cloves garlic, smashed",
            "1 pound jalapeno peppers, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine vinegar, water, sugar, salt, and garlic in a pot over medium-high heat. Bring to a boil, stirring until sugar and salt dissolve. Remove pickling liquid from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir jalapeno peppers into the pot. Let stand until slightly cooled, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer jalapeno peppers to a large Mason jar; pour in pickling liquid to cover. Let cool to room temperature, about 30 minutes. Cover and refrigerate.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Pickled"
        ],
