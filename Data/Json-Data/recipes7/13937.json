    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246441/choco-coco-cupcakes/",
        "name": "Choco-Coco Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Choco-Coco Cupcakes"
        },
        "datePublished": "2020-06-19T04:06:02.000Z",
        "description": "These are decadently chocolaty cupcakes, infused with coconut and topped with chocolate ganache. Using 85% cocoa chocolate ensures a very rich cupcake.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2\u2009\u2153 cups all-purpose flour",
            "1\u2009\u00bd tablespoons all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "1\u2009\u00bd tablespoons white sugar",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "1 cup vegetable oil",
            "3 eggs, beaten",
            "\u2153 cup coconut milk",
            "1\u2009\u2153 cups dried shredded coconut",
            "1 teaspoon vanilla extract",
            "6\u2009\u00bc ounces 85% dark chocolate, chopped",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 1/3 cups plus 1 1/2 tablespoon flour, 1 1/2 cup plus 1 1/2 tablespoon white sugar, baking powder, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vegetable oil, eggs, and coconut milk together in another bowl. Pour over flour mixture; stir well to combine. Fold shredded coconut and vanilla extract into batter. Spoon batter into prepared muffin tin, filling each liner 2/3 of the way.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, about 25 minutes. Remove from oven and transfer to a wire rack until cooled completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chopped chocolate in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat cream in a small saucepan over medium heat until edges are bubbling, about 5 minutes. Pour over chocolate in the bowl. Mix until ganache is smooth and shiny. Chill until firm, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove ganache from the refrigerator. Beat with an electric mixer on medium speed until fluffy and slightly lightened in color, 3 to 5 minutes. Transfer to a piping bag fitted with a round tip.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost cooled cupcakes with whipped ganache.\n"
            }
        ],
