    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218663/bobs-pineapple-pie/",
        "name": "Bob's Pineapple Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2900159.jpg",
            "width": 1920,
            "height": 2560
        },
        "datePublished": "2020-06-19T03:45:25.000Z",
        "description": "A wonderful double-crust pie that is my husband's all-time favorite.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bc cup cornstarch",
            "2 (20 ounce) cans crushed pineapple, drained with juice reserved",
            "1\u2009\u00bd cups white sugar",
            "\u00bc teaspoon salt",
            "4 egg yolks",
            "pastry for a (10 inch) double crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve the cornstarch in 1/4 cup of the reserved pineapple juice. Combine the drained pineapple, sugar, salt, egg yolks, cornstarch mixture, and 1 cup of the reserved pineapple juice in a large saucepan. Cook and stir over medium heat just until the mixture has thickened slightly and turned shiny. Do not boil. Remove from heat, and cool 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a 10-inch pie plate with half of the pastry. Pour the cooled pineapple mixture into the crust. Top with second crust, and seal and crimp the edge. Use a fork to poke holes in the top crust in a few places.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until the crust is golden brown, about 35 to 40 minutes. Cool before serving.\n"
            }
        ],
