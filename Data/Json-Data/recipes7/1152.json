    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/91471/no-yolk-deviled-eggs/",
        "name": "No Yolk Deviled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8170625.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:12:48.000Z",
        "description": "Deviled eggs without the yolks, using potato instead of yolks.",
        "prepTime": null,
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "6 eggs",
            "1 medium potato, peeled and diced",
            "1 tablespoon sweet pickle relish",
            "1 teaspoon sugar",
            "1 tablespoon mayonnaise",
            "1 teaspoon prepared yellow mustard, or to taste",
            "1 pinch paprika, for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the eggs in a pan of cold water and bring to a boil. Once they boil, remove from the heat, cover and let stand for 10 minutes. Drain, cool and peel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the potato in a saucepan with enough water to cover. Bring to a boil, and cook until tender. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the eggs in half lengthwise, and remove the yolks; reserve for other uses or discard. Place the potatoes in a medium bowl. Mash with a whisk until smooth, then mix in the relish, sugar, mayonnaise and mustard. Spoon into the egg whites, and dust with paprika. Refrigerate or serve.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Deviled Eggs"
        ],
