    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270231/nutella-bread-pudding/",
        "name": "Nutella&reg; Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5053775.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T04:34:22.000Z",
        "description": "A delicious Nutella&reg; spin on classic bread pudding!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 teaspoon butter, or as needed",
            "6 slices bread, or more to taste",
            "1 (12 fluid ounce) can evaporated milk",
            "\u00bc cup white sugar",
            "\u00bc cup brown sugar",
            "3 eggs",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon ground cinnamon",
            "1 dash salt",
            "3 tablespoons melted butter",
            "\u2153 cup chocolate-hazelnut spread (such as Nutella\u00ae)",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Butter a pie dish and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Tear bread into 1- to 2-inch pieces and place in the bottom of the prepared pie dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine evaporated milk, white sugar, brown sugar, eggs, vanilla extract, cinnamon, and salt in a bowl. Mix well and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add melted butter to bread pieces in the pie dish and toss to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate-hazelnut spread in a microwave-safe bowl in a microwave to soften slightly, 15 to 30 seconds. Place dollops on top of bread mixture, spreading to make an even layer. Sprinkle with walnuts. Pour milk-egg mixture on top, making sure bread is completely covered.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 45 to 50 minutes.\n"
            }
        ],
