    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220978/passover-chocolate-walnut-cookies/",
        "name": "Passover Chocolate Walnut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Passover Chocolate Walnut Cookies"
        },
        "datePublished": "2020-06-19T03:48:11.000Z",
        "description": "These cookies are so delicious you'll want to make them all year round! Crispy, fudgy, with just the right amount of crunch!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 dozen cookies",
        "recipeIngredient": [
            "8 ounces bittersweet chocolate",
            "2 tablespoons margarine",
            "1\u2009\u00bd tablespoons potato starch",
            "\u00bc teaspoon baking powder",
            "2 eggs",
            "\u2154 cup white sugar",
            "1 teaspoon vanilla extract",
            "1\u2009\u2154 cups chocolate chips",
            "2 cups chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt bittersweet chocolate and margarine together in the top of a double boiler over barely simmering water, stirring frequently and scraping down the sides with a rubber spatula, about 5 minutes. Remove from heat and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine potato starch and baking powder in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs, sugar, and vanilla extract together in a separate large bowl with an electric mixer on medium speed until light and fluffy, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold the cooled chocolate mixture, potato starch mixture, chocolate chips, and walnuts into egg mixture until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of batter onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies in the preheated oven for 4 minutes, turn baking sheets the opposite way in the oven, and bake until cookies are crisp on the edges, about 6 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cookies cool on sheets for about 5 minutes; transfer to wire racks to finish cooling.\n"
            }
        ],
