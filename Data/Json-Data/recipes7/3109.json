    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/161418/apple-crunch-a-la-mode/",
        "name": "Apple Crunch a la Mode",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Crunch a la Mode"
        },
        "datePublished": "2020-06-19T03:34:39.000Z",
        "description": "This is a fantastic recipe, my kids love it!  I love making this in the fall; and serving it over ice cream!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "6 tart apples, peeled and thinly sliced",
            "1 cup white sugar",
            "1 cup cold water",
            "1 tablespoon cornstarch",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 cup packed brown sugar",
            "\u00be cup old-fashioned rolled oats",
            "\u00bd cup butter, melted",
            "\u00bd cup chopped walnuts",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). Grease a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scatter sliced apples in the prepared baking dish. Whisk together the white sugar, cold water, cornstarch, and vanilla extract in a small saucepan. Cook over medium heat until syrup is clear, and all sugar had dissolved, about 5 minutes. Pour hot syrup over the fruit.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the flour, brown sugar, oats, melted butter, walnuts, and cinnamon until crumbly. Scatter crumble over fruit mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until the top is brown and the crunch is bubbly, about 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
