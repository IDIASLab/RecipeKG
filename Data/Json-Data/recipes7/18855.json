    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269046/inarizushi-inari-sushi/",
        "name": "Inarizushi (Inari Sushi)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Inarizushi (Inari Sushi)"
        },
        "datePublished": "2020-06-19T04:33:28.000Z",
        "description": "Sushi rice is stuffed inside pouches of fried tofu! A delicious snack any day of the week.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "6 aburaage squares (fried tofu) ",
            "2 tablespoons all-purpose flour",
            "salt and freshly ground black pepper to taste",
            "2 cups uncooked white rice",
            "5 cups water, divided",
            "\u2153 cup rice vinegar",
            "2 tablespoons olive oil",
            "5 tablespoons white sugar",
            "3 tablespoons soy sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Press each fried tofu square using a rolling pin to flatten. Place flour in a bowl and toss tofu in the flour. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat. Add tofu and cook until golden brown, turning from time to time, 4 to 6 minutes. Set aside to cool while you prepare the rice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 4 cups salted water to a boil in a pot, add rice, cover, and reduce heat to low. Simmer until rice is tender, about 18 minutes. Remove from heat and fluff, draining any excess water. Stir in rice vinegar and additional salt. Keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut fried tofu squares in half and carefully open to make pouches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 cup water in a saucepan over medium heat; stir in sugar and soy sauce. Add tofu and cook until liquid has evaporated, 10 to 15 minutes. Remove tofu pouches and stuff with rice.\n"
            }
        ],
