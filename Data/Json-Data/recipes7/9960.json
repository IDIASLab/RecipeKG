    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234372/fresh-mango-pie-dessert/",
        "name": "Fresh Mango Pie Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6658425.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:56:08.000Z",
        "description": "Mango gelatin on a cream cheese-Coolwhip&reg; mixture sits on a yummy crust.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "2 cups all-purpose flour, sifted",
            "\u00bd cup confectioners' sugar",
            "\u00be cup butter",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00be (12 ounce) container whipped topping (such as Cool Whip\u00ae)",
            "1 cup cold water",
            "2 envelopes  unflavored gelatin",
            "1 cup boiling water",
            "\u00bd cup white sugar, or to taste",
            "\u00bc teaspoon salt",
            "\u00bc cup lemon juice",
            "5 cups diced mango"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour and confectioners' sugar together in a bowl. Cut butter into flour mixture using a pastry blender or 2 knives until mixture resembles coarse crumbs; press into a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is lightly browned, 20 to 25 minutes. Remove from oven and cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, 1/2 cup white sugar, and vanilla extract together in a bowl using an electric mixer until smooth and creamy. Fold whipped topping into cream cheese mixture and pour over crust. Refrigerate mixture until cream cheese filling is chilled, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cold water into a bowl and sprinkle with gelatin; stir well. Pour hot water over gelatin mixture and stir until gelatin is dissolved. Stir 1/2 cup sugar and salt into gelatin mixture until sugar is dissolved; add lemon juice and cool to room temperature. Fold mango into gelatin mixture and refrigerate until gelatin begins to set, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mango gelatin over cream cheese filling and refrigerate until completely set, about 1 hour.\n"
            }
        ],
