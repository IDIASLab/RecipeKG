    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220801/poppy-onion-biscuits/",
        "name": "Poppy Onion Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F815935.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2020-06-19T03:48:02.000Z",
        "description": "A recipe that has been in our family for years.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "24 small rolls",
        "recipeIngredient": [
            "2 large onions, grated",
            "1 teaspoon white sugar",
            "1 egg",
            "\u00be cup vegetable oil",
            "\u00bc cup warm water",
            "4 teaspoons poppy seeds",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2 cups flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly grease a baking sheet or line with baking parchment.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix onions, sugar, egg, vegetable oil, and warm water in a large bowl with an electric mixer until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the baking powder, salt, pepper, and poppy seeds into the onion mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the flour into the onion mixture in batches with the mixer set to Low speed until incorporated into a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by the tablespoon onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown on top, 20 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Biscuits"
        ],
