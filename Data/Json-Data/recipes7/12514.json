        "recipeYield": "1 cake",
        "recipeIngredient": [
            "5 tablespoons white sugar",
            "\u00bc cup all-purpose flour",
            "3 tablespoons milk",
            "3 tablespoons olive oil",
            "2 tablespoons unsweetened cocoa powder",
            "1 egg",
            "1 dash pure vanilla extract",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar, flour, milk, olive oil, cocoa powder, egg, vanilla extract, and salt together in a large microwave-safe mug.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat in microwave until center of cake is done, about 2 minutes.\n"
            }
