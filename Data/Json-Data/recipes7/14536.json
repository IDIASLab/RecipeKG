        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup white sugar",
            "1 (10 ounce) package frozen strawberries in juice, thawed",
            "\u00bd cup crushed pineapple, drained",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and sugar in a bowl; add strawberries and pineapple. Fold in the whipped topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a loaf pan with parchment paper. Pour the strawberry mixture into the loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place in the freezer until firm, about 6 hours. Set loaf pan in a warm water bath to loosen. Flip mixture onto a cutting board to remove; cut into slices.\n"
            }
