        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261779/apple-pie-bites/",
        "name": "Apple Pie Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4584175.jpg",
            "width": 2261,
            "height": 2745
        },
        "datePublished": "2020-06-19T04:26:02.000Z",
        "description": "A mini apple pie wrapped in a wonton. Drizzle melted caramel over the top and serve warm.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "20 apple pie bites",
        "recipeIngredient": [
            "2 apples, peeled and diced",
            "2 tablespoons white sugar",
            "1\u2009\u00bd tablespoons ground cinnamon, divided, or more to taste",
            "1 tablespoon melted butter",
            "1 egg white",
            "20 wonton wrappers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples, sugar, 1 tablespoon cinnamon, and butter in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wet all edges of wonton wrappers with water. Place 1 tablespoon of apple mixture in center of each wonton. Fold all 4 corners of wonton to the middle. Squeeze together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush egg white onto the wonton wrappers and sprinkle with remaining cinnamon. Place wontons on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 15 minutes.\n"
            }
        ],
