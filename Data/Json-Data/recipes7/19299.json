        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270692/dark-mocha-crinkles/",
        "name": "Dark Mocha Crinkles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1078896.jpg",
            "width": 1440,
            "height": 960
        },
        "datePublished": "2020-06-19T04:35:01.000Z",
        "description": "Chewy dark chocolate mocha goodness.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter, at room temperature",
            "\u00be cup brown sugar",
            "\u2153 cup white sugar",
            "\u00bc cup instant coffee powder ",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "\u00be cup cocoa powder ",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup white sugar",
            "24 dark chocolate wafers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, brown sugar, 1/3 cup sugar, and coffee powder in a large bowl; beat with an electric mixer until creamy. Blend in eggs and vanilla extract. Mix in flour, cocoa powder, baking soda, and salt together until dough is uniform.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 1/2 cup sugar onto a shallow plate. Shape dough into 2-inch balls and roll in the sugar until coated. Arrange on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are set, about 10 minutes. Immediately press a dark chocolate wafer into each cookie. Let cool on a wire rack.\n"
            }
        ],
