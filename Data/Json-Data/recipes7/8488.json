        "recipeIngredient": [
            "4 cups fresh raspberries",
            "2 eggs",
            "1\u2009\u2153 cups white sugar",
            "1\u2009\u00bd cups half-and-half",
            "1 cup heavy whipping cream",
            "\u00bc cup light corn syrup",
            "1 tablespoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree raspberries in a blender or food processor; pour mixture through a strainer to remove seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and sugar together in a bowl until smooth. Stir raspberry puree, half-and-half, cream, corn syrup, and lemon juice into the egg-sugar mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer raspberry cream mixture to the ice cream maker. Freeze according to the manufacturer's instructions.\n"
            }
        ],
