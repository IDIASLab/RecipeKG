    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/93476/autumn-apple-squash-crisp/",
        "name": "Autumn Apple-Squash Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Autumn Apple-Squash Crisp"
        },
        "datePublished": "2020-06-19T03:13:09.000Z",
        "description": "This unique way to use fall squash is the most popular seasonal dessert at our house. It's also a nice alternative to pumpkin pie.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4\u2009\u00bd cups butternut squash - peeled, seeded, and cut into 3/4-inch chunks",
            "\u2154 cup packed brown sugar",
            "\u00bd cup all-purpose flour",
            "2 eggs",
            "2 teaspoons milk",
            "2 teaspoons vanilla extract",
            "2 teaspoons ground cinnamon",
            "2 teaspoons ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "4 large Granny Smith apple - peeled, cored and chopped",
            "2 large carrots, peeled and shredded",
            "1 cup raisins",
            "2 cups rolled oats",
            "1 cup wheat bran",
            "1 cup packed brown sugar",
            "\u00bd cup whole wheat flour",
            "1 tablespoon ground cinnamon",
            "\u00bd cup melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour about 1 inch of water into the bottom of a pan. Place the squash into a steamer basket fitted into the pan. Bring to a boil, then reduce heat to medium, cover, and steam the squash until tender and easily pierced with a fork, about 15  minutes. Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the squash, 2/3 cup brown sugar, 1/2 cup all-purpose flour, eggs, milk, vanilla, 2 teaspoons cinnamon, nutmeg, and cloves in the bowl of a food processor. Pulse until the mixture is smooth. Pour into a large mixing bowl. Stir in the apples, carrots, and raisins until evenly blended. Spread the mixture over the bottom of the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together the rolled oats, wheat bran, 1 cup brown sugar, 1/2 cup whole wheat flour, and 1 tablespoon cinnamon with the melted butter in a bowl until crumbly. Spoon the topping over the apple-squash mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until top is golden brown and the apples are tender, 30 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash",
            "Winter Squash",
            "Butternut Squash"
        ],
