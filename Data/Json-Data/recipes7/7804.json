    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228809/slow-cooker-cranberry-pork-roast/",
        "name": "Slow Cooker Cranberry Pork Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3920621.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2020-06-19T03:51:07.000Z",
        "description": "This is a slow cooker recipe that my sweet mother-in-law gave to us and is absolutely wonderful!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT6H5M",
        "totalTime": "P0DT6H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (1 1/2-pound) boneless pork loin roast",
            "1 (16 ounce) can jellied cranberry sauce",
            "\u00bc cup white sugar",
            "\u00bd cup cranberry juice",
            "1 teaspoon dry mustard",
            "\u00bc teaspoon ground cloves",
            "salt to taste",
            "2 tablespoons cold water",
            "2 tablespoons cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place pork roast in a slow cooker. Mash cranberry sauce in a bowl; stir in sugar, cranberry juice, mustard, cloves, and salt. Pour cranberry mixture over pork roast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 6 to 8 hours. Transfer roast to a plate; cover and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Skim fat from cranberry mixture in slow cooker. Measure 2 cups of cranberry mixture, adding water if necessary. Pour into saucepan and bring to a boil. Whisk cold water with cornstarch until pasty; pour into cranberry mixture. Cook and stir until thickened, 5 to 10 minutes. Serve alongside pork.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Roast Recipes"
        ],
