        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222033/buryls-ice-box-pickles/",
        "name": "Buryl's Ice Box Pickles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2054006.jpg",
            "width": 7272,
            "height": 7272
        },
        "datePublished": "2020-06-19T03:48:54.000Z",
        "description": "This recipe was given to me by a good friend and neighbor 30 years ago. Buryl has been gone for 25 years, but this recipe lives on.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "2 quarts pickles",
        "recipeIngredient": [
            "16 cups sliced cucumbers",
            "1 onion, sliced",
            "2 tablespoons salt",
            "1\u2009\u00bd cups white sugar",
            "\u00bd cup apple cider vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Toss cucumber slices, onion slices, and salt together in a bowl; let stand 2 hours. Drain as much moisture from the mixture as possible.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar and vinegar together in a saucepan over medium-low heat; cook and stir until the sugar dissolves completely, 5 to 10 minutes. Set aside to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the cooled vinegar mixture over the cucumbers and onion; toss to coat. Transfer to large freezer bags.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Store in freezer.\n"
            }
        ],
