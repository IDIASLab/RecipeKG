        "name": "Cinnamon Cookie Dough Superfood Shake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4348065.jpg",
            "width": 3024,
            "height": 4032
