    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240749/sour-cream-fudge/",
        "name": "Sour Cream Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sour Cream Fudge"
        },
        "datePublished": "2020-06-19T04:01:59.000Z",
        "description": "Sour cream fudge is tangy and sweet, with a smooth texture enlivened by walnuts. This is an old family-favorite tradition that I remember my Uncle Carl making for the family at Christmas-time.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 8x8-inch pan",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup sour cream",
            "2 tablespoons light corn syrup",
            "\u00bd teaspoon salt",
            "2 tablespoons butter",
            "1 teaspoon vanilla extract",
            "\u00bd cup chopped candied cherries",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter an 8-inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter the sides of a heavy, 2-quart saucepan; add sugar, sour cream, corn syrup, and salt. Cook, stirring constantly, over low heat until sugar is dissolved, about 5 minutes. Increase heat to medium and cook until mixture comes to a boil. Cook, stirring occasionally, until mixture reaches 239 degrees F (115 degrees C) or until a small amount of sugar mixture dropped into cold water forms a soft ball that flattens when removed from the water and placed on a flat surface. Remove saucepan from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir butter and vanilla extract into sugar mixture and cool to lukewarm, about 110 degrees F (43 degrees C). Beat mixture vigorously until fudge is creamy and no longer glossy. Fold cherries and walnuts into fudge; pour into the prepared 8-inch pan. Allow to cool until hardened, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Fudge Recipes"
        ],
