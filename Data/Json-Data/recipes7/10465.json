        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235547/kosher-pineapple-cranberry-salsa-pareve/",
        "name": "Kosher Pineapple Cranberry Salsa (Pareve)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Kosher Pineapple Cranberry Salsa (Pareve)"
        },
        "datePublished": "2020-06-19T03:57:21.000Z",
        "description": "Fresh fruit, jalapeno, and onion combine to produce a sweet and spicy salsa perfect for turkey, chicken, or pork.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (12 ounce) bag fresh cranberries",
            "1 cup white sugar",
            "\u00bd cup water",
            "1 fresh pineapple - peeled, cored, and cut into small chunks",
            "1 mango - peeled, seeded, and diced",
            "1 bunch cilantro, chopped",
            "3 Satsuma mandarin oranges, peeled and segmented",
            "1 sweet onion, diced",
            "1 jalapeno pepper, diced",
            "\u00bc cup orange juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and gently stir cranberries, sugar, and water in a large skillet over medium heat until the mixture begins to bubble. Reduce heat to low and cook until cranberries begin to soften and break down, about 5 minutes. Remove from heat and cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pineapple, mango, cilantro, mandarin oranges, onion, and jalapeno together in a bowl; drizzle with orange juice. Stir cooled cranberries and pineapple mixture together. Refrigerate until chilled, at least 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Salsa Recipes",
            "Mango Salsa Recipes"
        ],
