        "recipeIngredient": [
            "2 (16 ounce) cans diced tomatoes",
            "2 (15 ounce) cans tomato sauce",
            "1 tablespoon garlic powder",
            "2 teaspoons white sugar",
            "2 teaspoons dried parsley",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon dried oregano",
            "\u00bc teaspoon dried basil",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine diced tomatoes, tomato sauce, garlic powder, sugar, parsley, salt, oregano, basil, and pepper in a saucepan; bring to a boil. Lower heat to medium-low, cover saucepan, and simmer until flavors blend, about 30 minutes.\n"
            }
