        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236006/chicken-salad-avocado/",
        "name": "Chicken Salad Avocado",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1109929.jpg",
