        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256009/spicy-chinese-mustard-green-beans/",
        "name": "Spicy Chinese Mustard Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6501239.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "2020-06-19T04:18:38.000Z",
        "description": "A zesty, spicy addition to any meal. Great for traveling and picnics.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H48M",
        "recipeYield": "1 pound",
        "recipeIngredient": [
            "1 pound fresh green beans, trimmed",
            "2 teaspoons dry mustard powder (such as Coleman's\u00ae)",
            "1\u2009\u00bd teaspoons cold water",
            "1\u2009\u00bd tablespoons white vinegar",
            "1 tablespoon soy sauce",
            "1 tablespoon sesame oil",
            "1 tablespoon sesame seeds",
            "1 teaspoon white sugar",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add green beans and cook uncovered until bright green, about 3 minutes. Drain in a colander and immediately immerse in ice water for several minutes to stop the cooking process. Drain and transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mustard powder and cold water together in a small bowl to make a paste. Stir in vinegar, soy sauce, sesame oil, sesame seeds, sugar, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mustard paste mixture over green beans; toss to coat. Chill for at least 30 minutes before serving.\n"
            }
        ],
