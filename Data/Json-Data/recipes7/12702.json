        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241715/no-bake-peanut-butter-oatmeal-cookies/",
        "name": "No-Bake Peanut Butter Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2172532.jpg",
