        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255086/simple-mango-pudding/",
        "name": "Simple Mango Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4500407.jpg",
            "width": 2468,
