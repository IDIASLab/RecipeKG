        "recipeIngredient": [
            "1 (750 milliliter) bottle dry white wine",
            "\u00bc cup brandy",
            "\u00bc cup white sugar",
            "ice",
            "4 slices  lime",
            "4 slices  lemon",
            "4 slices green apples",
            "1 bunch green grapes",
            "1 cup club soda"
