        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245428/breakfast-muesli/",
        "name": "Breakfast Muesli",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Breakfast Muesli"
        },
        "datePublished": "2020-06-19T04:03:57.000Z",
        "description": "A hearty alternative to store-purchased granola with a smaller amount of sugar. Consider using organic ingredients when you can.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "3 cups rolled oats",
            "1 cup peanuts",
            "\u00bd cup almonds",
            "\u00bd cup coconut flakes",
            "\u00bd cup bran",
            "\u00bd cup pumpkin seeds",
            "\u00bd cup chia seeds",
            "\u00bd cup sesame seeds",
            "\u00bd cup sunflower seeds",
            "\u00bc cup canola oil",
            "\u00bc cup honey",
            "\u00bc cup white sugar",
            "1 cup golden raisins (sultanas)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, peanuts, almonds, coconut flakes, bran, pumpkin, chia, sesame, and sunflower seeds in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place oil, honey, and sugar in a small saucepan over medium heat; heat until boiling. Pour over oat mixture and stir well to combine. Spread muesli out onto baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake muesli in the preheated oven, stirring every 10 minutes, until golden, about 40 minutes total. Remove from oven and cool, about 30 minutes. Stir in golden raisins and store in a sealed container.\n"
            }
        ],
