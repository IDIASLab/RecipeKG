    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258070/spargel-antipasti-mit-lachs-white-asparagus-and-smoked-salmon/",
        "name": "Spargel-Antipasti mit Lachs (White Asparagus and Smoked Salmon)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4538986.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:21:21.000Z",
        "description": "A special starter for a dinner for two: cooked white asparagus with smoked salmon, topped with fresh herbs and Parmesan cheese.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "10 ounces white asparagus",
            "1 tablespoon butter",
            "1 pinch white sugar",
            "salt to taste",
            "4 ounces smoked salmon",
            "3 tablespoons corn oil",
            "1 tablespoon chopped fresh parsley",
            "1 ounce Parmesan cheese",
            "coarsely ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel asparagus spears from top to bottom with a vegetable peeler, starting below the tips. Cut off woody ends with a knife.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place asparagus spears in a large, wide saucepan and pour in enough lightly salted water to just cover. Bring water to a boil; add butter, sugar, and salt. Reduce heat; simmer until spears are tender and easily pierced with a knife, 15 to 25 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide smoked salmon between 2 serving plates. Place asparagus spears on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm oil and parsley in a small saucepan over low heat until fragrant, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle oil over asparagus. Grate Parmesan cheese on top using a cheese grater or shave into strips using a vegetable peeler. Season with pepper.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood"
        ],
