        "recipeIngredient": [
            "1 tablespoon gin",
            "1 tablespoon light rum",
            "1 tablespoon vodka",
            "1 tablespoon tequila",
            "2 tablespoons creme de menthe liqueur",
            "2 tablespoons Galliano",
            "1 cup ice cubes",
            "1 slice lemon"
