    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235765/sour-cream-sugar-cookies/",
        "name": "Sour Cream Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4577911.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-06-19T03:57:32.000Z",
        "description": "These are my husband's favorite Christmas cookies. The recipe was passed down to me from his mother. The frosting is absolutely delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "3 dozen cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup butter, softened",
            "3 eggs",
            "1 cup sour cream",
            "2 teaspoons vanilla extract",
            "3\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "\u2153 cup butter, softened",
            "2 cups confectioners' sugar",
            "2 tablespoons milk, or more as needed",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat white sugar and 1 cup butter together with an electric mixer in a large bowl until creamy. Add eggs 1 at a time, beating well after each addition. Beat sour cream and 2 teaspoons vanilla extract into butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, baking powder, and baking soda together in a bowl. Add flour mixture to sour cream mixture and stir well. Cover bowl with plastic wrap and refrigerate for at least 2 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll cookie dough out on a floured surface to 1/4-inch thick. Cut into 3-inch round cookies and place onto prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies spring back when lightly touched in the center, 10 to 12 minutes; transfer to wire racks to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1/3 cup butter, confectioners' sugar, milk, 1 1/2 teaspoons vanilla extract, and salt together in a bowl until smooth. Spread frosting over cooled cookies.\n"
            }
        ],
