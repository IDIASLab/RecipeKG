    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266098/no-egg-mango-cupcakes/",
        "name": "No-Egg Mango Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5363360.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T04:30:35.000Z",
        "description": "Eggless mango cupcakes, very soft, delicious, spongy, and rich in taste as well as flavor. The best evening dessert or snack best accompanied with tea. Try at your home today...",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 cupcakes",
        "recipeIngredient": [
            "\u00bd cup maida flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "1 pinch salt",
            "\u00bc cup semolina flour",
            "\u00bd cup mango puree",
            "1 tablespoon mango puree",
            "\u00bc cup plain yogurt",
            "\u00bc cup white sugar",
            "1 tablespoon olive oil",
            "\u00bc teaspoon ground cardamom",
            "\u00bd cup chilled mango puree",
            "\u00bc cup sliced almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 12-cup muffin tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift maida flour, baking powder, baking soda, and salt into a bowl. Sift a second time. Add semolina flour and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup plus 1 tablespoon mango puree, yogurt, sugar, olive oil, and cardamom in a large bowl; beat well. Add flour mixture and mix well. Transfer batter to the prepared muffin tin, filling cups 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, 25 to 30 minutes. Remove cupcakes from tin and allow to cool completely, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost cupcakes with chilled mango puree, sprinkle almonds on top, and serve.\n"
            }
        ],
