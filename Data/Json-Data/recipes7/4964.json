        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215001/kohlrabi-slaw/",
        "name": "Kohlrabi Slaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F680246.jpg",
