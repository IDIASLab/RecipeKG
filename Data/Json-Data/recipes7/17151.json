    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261565/beef-and-eggplant-sauce-for-pasta/",
        "name": "Beef and Eggplant Sauce for Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Beef and Eggplant Sauce for Pasta"
        },
        "datePublished": "2020-06-19T04:25:38.000Z",
        "description": "This eggplant sauce is my family's favorite. I had a bumper crop of eggplant so have made up my own recipes using eggplant. Serve over your favorite pasta.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H29M",
        "totalTime": "P0DT0H44M",
        "recipeYield": "8 to 10 cups sauce",
        "recipeIngredient": [
            "olive oil, divided",
            "2 small eggplants, diced with skin",
            "1 large red onion, chopped",
            "1 (5 ounce) can sliced mushrooms, drained, or to taste",
            "4 cloves garlic, minced, or more to taste",
            "6 Roma tomatoes, sliced",
            "2 pounds ground beef",
            "2 cups pasta sauce (such as Hunt's\u00ae Traditional)",
            "1 (15 ounce) can tomato sauce",
            "1 (14 ounce) can pizza sauce",
            "\u00bc cup white sugar, or more to taste",
            "1 (1.5 ounce) envelope dry spaghetti sauce mix",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a large pot over medium-high heat. Saute eggplants, red onion, mushrooms, and garlic until softened, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining 1 tablespoon olive oil in a skillet over medium-high heat. Saute tomatoes until juicy and tender, about 6 minutes. Transfer tomatoes to the pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir ground beef in the same skillet until browned and crumbly, 5 to 7 minutes. Transfer to the pot; add pasta sauce, tomato sauce, pizza sauce, sugar, spaghetti sauce mix, salt, and pepper. Stir and simmer until flavors are well blended, about 10 minutes.\n"
            }
        ],
