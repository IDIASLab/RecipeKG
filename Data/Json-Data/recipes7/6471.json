    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220798/justins-frosted-chocolate-cone-cakes/",
        "name": "Justin's Frosted Chocolate Cone Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5272905.jpg",
            "width": 1576,
            "height": 1575
        },
        "datePublished": "2020-06-19T03:48:02.000Z",
        "description": "An easy-to-do chocolate cake in ice cream cones with yummy cheese frosting. A sure hit with kids and adults!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "10 cupcakes",
        "recipeIngredient": [
            "10 flat-bottomed wafer ice cream cones",
            "1 cup all-purpose flour",
            "1 cup white sugar",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bd cup butter",
            "\u00bd cup water",
            "1 tablespoon unsweetened cocoa powder",
            "2 eggs",
            "\u00bc cup buttermilk",
            "1 teaspoon vanilla extract",
            "\u2153 cup cream cheese, softened",
            "\u00bd cup confectioners' sugar",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set ice cream cones upright in 10 muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, white sugar, baking soda, and salt together in a mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over low heat; stir in water and cocoa powder until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir melted butter mixture into flour mixture. Stir in eggs, buttermilk, and 1 teaspoon vanilla extract until the batter is moistened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat batter with an electric mixer on medium speed until thoroughly combined, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chocolate cake batter into ice cream cones, leaving about 1 inch of space at the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake immediately in the preheated oven (to avoid cones becoming soggy) until a toothpick inserted into the middle of a cake center comes out clean, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cupcakes cool thoroughly, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, confectioners' sugar, and 1/2 teaspoon vanilla extract together in a bowl until smooth and creamy. Frost cupcakes with the cream cheese frosting.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frostings and Icings",
            "Chocolate"
        ],
