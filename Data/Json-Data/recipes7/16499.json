    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259756/grandmas-sweet-potato-pie/",
        "name": "Grandma's Sweet Potato Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grandma's Sweet Potato Pie"
        },
        "datePublished": "2020-06-19T04:24:02.000Z",
        "description": "Delicious, spicy, and sweet. This is Grandma's famous Southern sweet potato pie recipe that has always kept our family going down South for each and every holiday! Enjoy with ice cream of whipped topping.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "3 large sweet potatoes, peeled and diced",
            "\u00bd orange, peel cut into thin strips",
            "1 pastry for a 9-inch pie crust",
            "2 eggs",
            "1 tablespoon pineapple juice",
            "1 tablespoon salted butter, melted",
            "1 teaspoon vanilla extract",
            "1 tablespoon all-purpose flour",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground ginger",
            "1 pinch ground cloves",
            "1 pinch salt",
            "1 cup white sugar",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place sweet potatoes and orange peel into a large pot and cover with water; bring to a boil. Reduce heat to medium-low and simmer until soft, about 10 minutes. Drain well and let cool. Discard orange peel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Line a 9-inch pie plate with pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash cooled sweet potatoes with a potato masher or fork. Mix in eggs until well blended. Add pineapple juice, butter, and vanilla extract; mix until combined. Mix in flour, cinnamon, nutmeg, ginger, cloves, and salt. Mix in sugar until dissolved. Blend in heavy cream using a whisk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sweet potato mixture into the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 40 to 50 minutes. Place on a cloth on a table or window sill to cool down.\n"
            }
        ],
