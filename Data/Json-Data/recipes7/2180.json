    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/141902/strawberry-cupcakes/",
        "name": "Strawberry Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F787048.jpg",
            "width": 3246,
            "height": 3246
        },
        "datePublished": "2020-06-19T03:30:25.000Z",
        "description": "Strawberry cupcakes with real fresh strawberry bits!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 dozen cupcakes",
        "recipeIngredient": [
            "10 tablespoons butter, room temperature",
            "\u00be cup white sugar",
            "3 eggs",
            "1 teaspoon strawberry extract",
            "1\u2009\u00be cups self-rising flour",
            "\u00bc teaspoon salt",
            "\u00bc cup finely chopped fresh strawberries, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease 12 cupcake pan cups or line with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and sugar until light and fluffy. Beat in the eggs one at a time, then stir in the strawberry extract. Combine the self-rising flour and salt; stir into the batter just until blended. Fold in strawberries last. Spoon the batter into the prepared cups, dividing evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the tops spring back when lightly pressed, 20 to 25 minutes. Cool in the pan set over a wire rack. When cool, arrange the cupcakes on a serving platter. Frost with desired frosting.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Strawberry Dessert Recipes"
        ],
