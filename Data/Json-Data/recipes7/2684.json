        "mainEntityOfPage": "https://www.allrecipes.com/recipe/150591/eggplant-appetizer/",
        "name": "Eggplant Appetizer",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5620871.jpg",
