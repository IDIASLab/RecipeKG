    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259691/pumpkin-caramel-bourbon-poke-cake/",
        "name": "Pumpkin Caramel Bourbon Poke Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4547233.jpg",
            "width": 2000,
            "height": 1217
        },
        "datePublished": "2020-06-19T04:23:56.000Z",
        "description": "A rich, creamy pumpkin poke cake with caramel and bourbon. One slice is never enough!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H36M",
        "totalTime": "P0DT0H56M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup pumpkin puree",
            "\u00be cup white sugar",
            "\u00bd cup canola oil",
            "2 large eggs",
            "2 teaspoons pumpkin pie spice",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "\u00bd cup caramel sauce",
            "1 tablespoon bourbon, divided",
            "1 cup heavy whipping cream",
            "\u00bd cup chopped toasted pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line an 8-inch square cake pan with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat pumpkin puree, sugar, oil, eggs, pumpkin pie spice, and vanilla extract together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, baking powder, and salt into a bowl. Add to the pumpkin mixture; stir until just combined. Pour into prepared pan; smooth top with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the middle comes out clean, 35 to 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine caramel sauce and 1 1/2 teaspoons bourbon in a saucepan over low heat; warm through, about 2 minutes. Poke holes evenly over the surface of the cake using a chopstick. Reserve 1 teaspoon of the caramel-bourbon mixture; pour remaining mixture over the cake. Place on cooling rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream and remaining 1 1/2 teaspoons bourbon in a bowl using an electric mixer until soft peaks form. Spoon whipped cream on cake; scatter pecans and drizzle reserved caramel sauce on top.\n"
            }
        ],
