    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237304/peanut-butter-malted-cookies/",
        "name": "Peanut Butter Malted Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1126157.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:58:54.000Z",
        "description": "Makes great cookies! Using barley malt sweetener instead of brown sugar yields a light, not-quite-as-sweet cookie.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "1 cup unsalted butter, softened",
            "1 cup crunchy peanut butter",
            "1 cup barley malt syrup",
            "1 cup white sugar",
            "2 eggs",
            "2\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking soda",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat butter, peanut butter, barley malt syrup, and white sugar together in a bowl using an electric mixer until smooth and creamy. Beat eggs into creamed butter mixture until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, baking powder, and salt together in a bowl; stir into creamed butter mixture until dough is just mixed. Refrigerate dough for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1-inch balls and arrange on baking sheets. Flatten each ball using a fork, making a crisscross pattern.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated until edges of cookies are browned, about 10 minutes.\n"
            }
        ],
