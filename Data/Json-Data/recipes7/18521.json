    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267979/smores-cookie/",
        "name": "S'mores Cookie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5769538.jpg",
            "width": 2340,
            "height": 2925
        },
        "datePublished": "2020-06-19T04:32:14.000Z",
        "description": "A unique variation of the epic s'mores using graham cracker cereal and instant pudding. Toasting the marshmallows gives an authentic touch.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H16M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "20 large cookies",
        "recipeIngredient": [
            "2\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "\u00bd pound butter, at room temperature",
            "\u00be cup packed brown sugar",
            "\u00bc cup white sugar",
            "1 (3.4 ounce) package instant vanilla pudding mix (such as Jell-O\u00ae)",
            "2 large eggs",
            "1 teaspoon vanilla extract, or more to taste",
            "1 cup semisweet chocolate chunks",
            "1 cup honey graham cereal (such as Golden Grahams\u00ae)",
            "\u00bd (10.5 ounce) package miniature marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, brown sugar, and white sugar together in a large bowl using an electric mixer until light and fluffy. Add instant pudding and mix well. Add eggs and vanilla extract, blending thoroughly and scraping down sides of the bowl as necessary. Slowly add flour and mix well. Stir in chocolate chunks and cereal until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop cookie dough onto an ungreased baking sheet using a 2-ounce ice cream scoop. Use rounded tablespoons if you prefer smaller cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, about 15 minutes. Place 5 or 6 marshmallows on top of each cookie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler. Place cookies under broiler, leaving the door open. Bake until marshmallows are slightly toasted, 15 to 30 seconds, watching carefully. Remove cookies from oven. Let rest for 1 minute before cooling on a wire rack.\n"
            }
        ],
