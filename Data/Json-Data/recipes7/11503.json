        "recipeYield": "14 servings",
        "recipeIngredient": [
            "\u00bd gallon whole milk",
            "1\u2009\u2153 cups white sugar",
            "1 cup small pearl tapioca",
            "4 eggs, lightly beaten",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir milk, sugar, tapioca, and eggs together in a slow cooker crock.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low, stirring once per hour, for 6 hours. Stir vanilla into the pudding.\n"
            }
        ],
