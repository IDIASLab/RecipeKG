        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268993/spaghetti-and-meatballs-in-the-instant-pot/",
        "name": "Spaghetti and Meatballs in the Instant Pot&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5935014.jpg",
            "width": 3024,
