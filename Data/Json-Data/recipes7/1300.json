        "recipeIngredient": [
            "4 cups ice cubes",
            "1 cup pisco",
            "\u2153 cup lemon juice",
            "\u2153 cup white sugar",
            "1 egg white",
            "aromatic bitters"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place ice cubes, pisco, lemon juice, sugar, egg white, and bitters in the bowl of a blender.  Blend on high speed until finely pureed.  Pour into two glasses and garnish with an additional dash of bitters.\n"
            }
