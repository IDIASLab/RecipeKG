        "totalTime": "P0DT4H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 pounds broccoli florets",
            "10 slices crisply cooked bacon, crumbled",
            "\u00bd cup chopped red onion",
            "\u00bd cup golden raisins",
            "\u00bd cup cashews",
            "\u00bd cup sliced water chestnuts",
            "1 cup mayonnaise",
            "\u00bd cup white sugar",
            "2 tablespoons vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Toss broccoli, bacon, red onion, raisins, cashews, and water chestnuts together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk mayonnaise, sugar, and vinegar together until smooth; pour over salad and toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover bowl with plastic wrap and refrigerate until the dressing sets, at least 4 hours.\n"
            }
        ],
