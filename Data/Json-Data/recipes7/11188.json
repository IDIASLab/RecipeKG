        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237602/spicy-jalapeno-coleslaw/",
        "name": "Spicy Jalapeno Coleslaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1133333.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:59:03.000Z",
        "description": "A spicy coleslaw my family enjoys that is great on sandwiches or with BBQ.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT2H30M",
        "recipeYield": "25 servings",
        "recipeIngredient": [
            "1 head green cabbage, shredded",
            "1 onion, chopped",
            "2 carrots, shredded",
            "2 jalapeno chile peppers, chopped",
            "1 egg",
            "1 small lemon, juiced",
            "1 tablespoon white vinegar",
            "\u00bc teaspoon salt",
            "1 cup vegetable oil",
            "2 tablespoons prepared mustard",
            "2 tablespoons white sugar",
            "1 tablespoon cider vinegar",
            "\u00bd teaspoon ground chipotle chile pepper",
            "\u00bd teaspoon celery salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cabbage, onion, carrots, and jalapeno peppers together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend egg, lemon juice, white vinegar, and salt in a food processor until smooth. Slowly drizzle oil into egg mixture through the feeding tube while the food processor is running until mayonnaise is smooth and thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mustard, sugar, cider vinegar, chipotle chile pepper, and celery salt into mayonnaise until dressing is smooth and evenly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir dressing into cabbage mixture until evenly coated. Cover bowl with plastic wrap and refrigerate until flavors blend, at least 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Coleslaw Recipes",
            "No Mayo"
        ],
