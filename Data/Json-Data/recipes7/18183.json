    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266039/bat-wings-halloween-sweet-wing-appetizer/",
        "name": "Bat Wings (Halloween Sweet Wing Appetizer)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F728180.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T04:30:33.000Z",
        "description": "Your favorite wing recipe plus a bit of black food coloring paste equals one yummy bat wing dish! This is a basic marinade to get you started, but feel free to experiment.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "4 pounds wings",
        "recipeIngredient": [
            "4 cups reduced-sodium soy sauce",
            "3 cups white sugar",
            "1 teaspoon black food coloring paste",
            "2 pinches onion powder, or to taste",
            "2 pinches garlic powder, or to taste",
            "1 pinch Cajun seasoning",
            "1 (4 pound) package chicken wings"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix soy sauce, sugar, food coloring, onion powder, garlic powder, and Cajun seasoning together in a bowl until paste dissolves completely. Carefully open the bag of chicken wings, keeping the seal intact. Pour in the soy sauce mixture, seal bag, and lay it flat in the refrigerator. Marinate, turning occasionally to coat and dye every piece, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour wings and marinade into a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Turn wings and bake until no longer pink at the bone and the juices run clear, 15 to 20 minutes more. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove wings from the sauce and place in a bowl or platter labeled 'Bat Wings.'\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Chicken"
        ],
