        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235310/microwave-pecan-brittle/",
        "name": "Microwave Pecan Brittle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4164351.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2020-06-19T03:57:06.000Z",
        "description": "The pecans give this brittle a whole new flavor. I am begged for this recipe all of the time. I hope all enjoy.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H9M",
        "totalTime": "P0DT0H49M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 cup chopped pecans",
            "1 cup white sugar",
            "\u00bd cup light corn syrup",
            "\u215b teaspoon salt",
            "1 tablespoon butter",
            "1 teaspoon baking soda",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pecans, sugar, corn syrup, and salt in a glass 4-cup measuring cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat in microwave for 3 1/2 minutes; stir and heat for 3 1/2 more minutes. Stir butter into pecan mixture and heat in microwave for 2 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir baking soda and vanilla extract into pecan mixture until foamy; pour onto the prepared baking sheet. Allow to cool completely before breaking brittle into pieces. Store in an airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
