        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237116/waldorfs-sour-cream-cheesecake/",
        "name": "Waldorf's Sour Cream Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1401385.jpg",
            "width": 5184,
            "height": 3888
        },
        "datePublished": "2020-06-19T03:58:32.000Z",
        "description": "A wonderful and easy cheesecake recipe my mother-in-law made for the holidays and special occasions.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "7 ounces vanilla wafers, crushed",
            "\u00bd cup margarine, melted",
            "2 tablespoons white sugar",
            "1 pinch ground cinnamon",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (3 ounce) package cream cheese, softened",
            "1 cup white sugar",
            "2 tablespoons lemon juice",
            "4 eggs",
            "1 pint sour cream",
            "2 tablespoons white sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vanilla wafers, margarine, 2 tablespoons sugar, and cinnamon together in a bowl; press into the bottom of a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the two 8-ounce packages and 3-ounce package cream cheese with 1 cup sugar and lemon juice until smooth. Beat eggs, 1 at a time, into cream cheese mixture, stirring well after each addition; spread over crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until middle of cheesecake is set, 25 to 30 minutes; cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase oven temperature to 475 degrees F (245 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sour cream, 2 tablespoons white sugar, and vanilla extract together in a bowl until creamy; pour over cheesecake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until topping is set, about 5 minutes. Cool cheesecake to room temperature and refrigerate until serving.\n"
            }
        ],
