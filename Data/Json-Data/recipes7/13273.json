        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put cherries in a clean, 1-quart glass jar; add sugar. Fill the rest of the jar with vodka. Place lid on jar and shake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Store jar in a cool, dark area for 1 to 4 months, shaking jar occasionally.\n"
