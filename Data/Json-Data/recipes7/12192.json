    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240234/cranberry-nut-bread/",
        "name": "Cranberry-Nut Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3236703.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:01:30.000Z",
        "description": "This recipe delivers a Christmas classic from a recipe my Mom would make every year. The bread freezes well.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup white sugar",
            "\u00bd cup brown sugar",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc cup butter",
            "\u00be cup orange juice",
            "1 egg, well beaten",
            "1 teaspoon vanilla extract",
            "1 tablespoon grated orange zest",
            "1\u2009\u00bd cups cranberries",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5-inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, white sugar, brown sugar, baking powder, baking soda, and salt together in a large mixing bowl. Mash butter into the flour mixture with a pastry cutter until the mixture resembles coarse cornmeal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir orange juice, egg, vanilla extract, and orange zest together in a small bowl; add to flour mixture and stir just until the dry ingredients are moistened and batter-like. Fold cranberries and walnuts through the batter; spoon into the prepared loaf pan, spreading so the corners and sides are slightly higher than the center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour. Cool in the pans for 10 minutes before removing to cool completely on a wire rack.\n"
            }
        ],
