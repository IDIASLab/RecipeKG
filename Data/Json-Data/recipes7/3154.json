        "mainEntityOfPage": "https://www.allrecipes.com/recipe/162397/classic-old-fashioned/",
        "name": "Classic Old Fashioned",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2015%2F05%2F1099974.jpg",
            "width": 600,
