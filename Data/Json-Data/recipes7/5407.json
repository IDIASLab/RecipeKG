    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216789/gluten-free-danish/",
        "name": "Gluten Free Danish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F646415.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:44:06.000Z",
        "description": "My husband says these are the closest thing to 'real' Danish he has had since going gluten-free five years ago. We like raspberry jam, but you could also try pie filling in the middles.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 Danish",
        "recipeIngredient": [
            "2 cups gluten-free pancake mix",
            "\u00bc cup butter, room temperature",
            "2 tablespoons white sugar",
            "\u2154 cup milk",
            "\u00bd teaspoon almond extract",
            "\u00bc cup raspberry jam",
            "\u2154 cup confectioners' sugar",
            "2 tablespoons milk, or more as needed",
            "\u00bd teaspoon vanilla extract",
            "\u00bc teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 400 degrees F (200 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the pancake mix, butter, and sugar; mix until crumbly. Stir in 2/3 cup milk and 1/2 teaspoon almond extract and mix until fully incorporated. The dough will be sticky.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop the dough by rounded tablespoons onto the baking sheet, spacing the dough about 2 inches apart. With the back of a teaspoon, make a shallow indentation in the center of each dough ball. Fill the centers with a teaspoon of jam.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 10 to 12 minutes. Allow the pastries to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the confectioners' sugar, 3 tablespoons of milk, vanilla extract, and 1/4 teaspoon almond extract and mix until smooth. Drizzle the glaze over the Danish.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
