    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244227/biscones/",
        "name": "Biscones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2313911.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "2020-06-19T04:03:16.000Z",
        "description": "This is part biscuit, part scone, and all delicious! Brush with milk before baking, if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 biscones",
        "recipeIngredient": [
            "2 cups unbleached all-purpose flour",
            "4 teaspoons baking powder",
            "1 tablespoon white sugar, or more to taste",
            "\u00bd teaspoon salt",
            "6 tablespoons butter",
            "\u00be cup buttermilk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, sugar, and salt together in a bowl. Cut butter into flour mixture using a fork, your hands, or a pastry blender until mixture resembles coarse crumbs. Mix buttermilk into flour mixture until dough starts to stick together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough onto a floured work surface and shape into a 1/2-inch-thick circle. Cut dough into 4 wedges and then into 8 wedges using a sharp knife. Arrange wedges on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until biscones are lightly browned, about 20 minutes.\n"
            }
        ],
