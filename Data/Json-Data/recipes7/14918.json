    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255563/butternut-squash-cookies/",
        "name": "Butternut Squash Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7595197.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:18:23.000Z",
        "description": "This recipe combines two fall favorites - oatmeal cookies and pumpkin - to create a cookie perfect for the chill autumn brings.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1 butternut squash, halved and seeded",
            "1\u2009\u00bd cups self-rising flour",
            "1\u2009\u00bd cups quick-cooking oats",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger",
            "\u00bd teaspoon ground nutmeg",
            "\u00bc cup butter",
            "\u00bc cup shortening",
            "\u00bd cup white sugar",
            "\u2153 cup packed brown sugar",
            "1 egg",
            "2 tablespoons milk",
            "\u00bc teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Place butternut squash halves cut-side down on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until tender when pierced with a fork, 45 to 60 minutes. Let cool, about 15 minutes. Scoop flesh into a large bowl and mash until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, oats, cinnamon, ginger, and nutmeg together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and shortening in a bowl until blended, about 30 seconds. Add white sugar and brown sugar; beat until fluffy. Add mashed squash, egg, milk, and vanilla extract; beat thoroughly until dough is combined. Fold in flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill dough until firm, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop 1-inch scoops of dough onto the baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden brown, 10 to 12 minutes. Transfer to a wire rack to cool before serving.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash",
            "Winter Squash",
            "Butternut Squash"
        ],
