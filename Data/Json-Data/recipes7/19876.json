    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273499/braided-italian-herb-bread/",
        "name": "Braided Italian Herb Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7655212.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:37:05.000Z",
        "description": "A beautifully braided bread with a delicious blend of basil, oregano, rosemary, and garlic powder that is sure to please!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "4\u2009\u00bd cups all-purpose flour, divided",
            "3 tablespoons white sugar",
            "2 (.25 ounce) packages active dry yeast",
            "2 teaspoons dried basil",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon dried oregano",
            "\u00be teaspoon garlic powder",
            "\u00be teaspoon dried rosemary",
            "\u00be cup milk",
            "\u00bd cup water",
            "4 tablespoons butter",
            "1 egg",
            "1 tablespoon olive oil",
            "1 teaspoon dried basil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 1 1/2 cups flour, sugar, yeast, basil, salt, oregano, garlic powder, and rosemary in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk, water, and butter in a small microwave-safe bowl. Heat in a microwave until butter melts, about 30 seconds. Pour over the flour mixture; beat until dough is just moistened. Add egg; beat until smooth. Stir in remaining 3 cups flour to form a soft dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough onto a floured surface such as a large cutting board or countertop. Knead until smooth and elastic, about 4 minutes. Cover and let rest for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into thirds. Shape each piece into a 15-inch rope. Transfer ropes to a greased baking sheet. Braid together. Pinch ends to seal; tuck under the braid. Cover and let rise until doubled, about 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Brush top of the braid with olive oil and sprinkle with basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 20 to 25 minutes. Cool on the baking sheet, or remove to a wire rack.\n"
            }
        ],
