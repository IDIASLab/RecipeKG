    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255621/applesauce-pumpkin-spice-bread/",
        "name": "Applesauce Pumpkin Spice Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4108405.jpg",
            "width": 4824,
            "height": 4824
        },
        "datePublished": "2020-06-19T04:18:23.000Z",
        "description": "This is my favorite holiday food! It keeps well and is a great food to bring to holiday gatherings.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "cooking spray",
            "3\u2009\u00bd cups unbleached all-purpose flour",
            "3 tablespoons ground cinnamon",
            "1\u2009\u00bd tablespoons ground nutmeg",
            "2 teaspoons ground cloves",
            "1 teaspoon ground ginger",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "\u00bd teaspoon baking powder",
            "2 cups dark brown sugar",
            "1 cup white sugar",
            "1 cup unsweetened applesauce",
            "4 eggs, lightly beaten",
            "1 (15 ounce) can solid-pack pumpkin",
            "\u00bd cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 2 loaf pans with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, cinnamon, nutmeg, cloves, ginger, baking soda, salt, and baking powder together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, white sugar, applesauce, and eggs in a large bowl. Add pumpkin and mix well. Add flour mixture alternately with water, mixing well between additions. Divide between the prepared loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake loaves in the preheated oven until a toothpick inserted into the center comes out clean, 60 to 65 minutes. Cool in pans for 10 minutes. Invert onto a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
