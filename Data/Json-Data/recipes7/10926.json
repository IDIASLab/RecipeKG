    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236874/quadruple-chocolate-chip-cookies/",
        "name": "Quadruple Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1120344.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:58:21.000Z",
        "description": "White chocolate, milk chocolate, semi-sweet chocolate, and Reese's&reg; peanut butter chocolate, all in one cookie!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "18 cookies",
        "recipeIngredient": [
            "1\u2009\u00bc cups unsalted butter, melted and cooled",
            "\u00be cup white sugar",
            "\u00be cup packed brown sugar",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "1\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 cup white chocolate chips",
            "1 cup milk chocolate chips",
            "1 cup semisweet chocolate chips",
            "1 cup peanut butter chips (such as Reese's\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat butter, white sugar, and brown sugar together in a bowl with an electric mixer until creamy. Beat eggs and vanilla into butter mixture until eggs are completely incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking soda, and salt together in a bowl. Add flour mixture to butter mixture and stir to combine. Fold white chocolate chips, milk chocolate chips, semisweet chocolate chips, and peanut butter chips into dough. Cover bowl with plastic wrap and refrigerate until dough is firm, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of the cookie dough 2 inches apart onto baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned and puffed up, 10 to 12 minutes. Cool on the baking sheet for 5 minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
