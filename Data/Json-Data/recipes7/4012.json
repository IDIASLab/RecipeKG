        "recipeYield": "2 cocktails",
        "recipeIngredient": [
            "\u00bc cup raspberry rum",
            "\u00bc cup raspberry schnapps",
            "\u00bd cup frozen strawberry juice concentrate",
            "12 frozen strawberries",
            "12 ice cubes",
            "\u00bc cup cold water",
            "1 tablespoon white sugar",
            "\u00bd cup heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place raspberry rum, raspberry schnapps, strawberry juice concentrate, strawberries, ice cubes, water, and sugar in a blender. Blend until smooth then add cream. Blend until just incorporated. Serve in tall glasses.\n"
            }
