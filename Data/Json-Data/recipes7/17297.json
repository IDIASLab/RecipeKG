    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261877/vegan-banana-cake/",
        "name": "Vegan Banana Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4684200.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-06-19T04:26:11.000Z",
        "description": "Our whole family loves banana cake and this is a vegan version without eggs or dairy. If you like, you can add raisins and nuts. If using raisins, reduce the amount of sugar.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 tablespoon flaxseed meal",
            "2 tablespoons lukewarm water",
            "2 cups all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "1 tablespoon ground cinnamon",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon baking soda",
            "1 pinch salt",
            "4 overripe bananas, mashed",
            "\u00bd cup vegetable oil",
            "\u2153 cup unsweetened soy milk",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bc cup chopped walnuts",
            "\u00bc cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flaxseed meal and water in a cup and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a fluted tube pan (such as Bundt&reg;) and dust with flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, cinnamon, baking powder, baking soda, and salt in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bananas, oil, soy milk, and vanilla extract in a second bowl. Add flaxseed mixture and stir to combine. Fold in flour mixture in batches, but do not overmix. Fold in nuts and raisins. Fill batter into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick comes out clean, 40 to 50 minutes.\n"
            }
        ],
