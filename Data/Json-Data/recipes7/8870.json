    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231487/easy-strawberry-pie/",
        "name": "Easy Strawberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Strawberry Pie"
        },
        "datePublished": "2020-06-19T03:53:27.000Z",
        "description": "I used this for Easter Sunday dessert and thought it was the best thing for a light, sweet ending to the day!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1\u2009\u00bc cups crushed buttery round crackers",
            "\u00bc cup butter, melted",
            "1 (8 ounce) package cream cheese",
            "2 tablespoons milk",
            "2 tablespoons white sugar",
            "1 cup strawberries, halved",
            "\u00bd cup heavy whipping cream",
            "1 tablespoon heavy whipping cream",
            "1 (3.5 ounce) package instant French vanilla pudding mix",
            "1\u2009\u00bd cups milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cracker crumbs with melted butter in a bowl and press mixture into a 9-inch pie dish to make a crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until lightly browned, about 8 minutes; let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese with 2 tablespoons milk and white sugar in a bowl, using an electric mixer, until creamy and smooth. Spread cream cheese mixture into pie crust. Arrange strawberry halves over cream cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip both amounts of cream with an electric mixer in a bowl until the cream is fluffy and holds firm peaks, about 3 minutes. Whisk French vanilla pudding mix with 1 1/2 cups milk in a separate bowl until pudding begins to thicken, 2 minutes. Let pudding stand 3 more minutes to set. Gently fold whipped cream into pudding mix and spread pudding mixture atop strawberries. Let pie chill for 2 hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes",
            "Pudding Pie Recipes"
        ],
