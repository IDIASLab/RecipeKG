    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268952/gluten-free-gingersnaps/",
        "name": "Gluten-Free Gingersnaps",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6302874.jpg",
            "width": 4962,
            "height": 3837
        },
        "datePublished": "2020-06-19T04:33:26.000Z",
        "description": "These are flat and crunchy, crispy gingersnap cookies made with oat flour.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar, divided",
            "\u00be cup margarine",
            "1 egg",
            "\u00bc cup molasses",
            "2 cups oat flour",
            "1 tablespoon ground ginger",
            "3 teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cardamom"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1 cup sugar and margarine together in a medium bowl until creamy. Beat in egg and molasses until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oat flour, ginger, baking powder, cinnamon, salt, and cardamom in another bowl; stir into the molasses mixture to form a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1-inch balls and rolls in the remaining 1/2 cup sugar. Place cookies 2 inches apart on the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops are slightly cracked, 15 to 18 minutes. Let cookies cool for 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
