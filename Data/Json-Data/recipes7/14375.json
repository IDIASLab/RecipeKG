        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1 cup water",
            "2 tablespoons water",
            "1 cup white sugar",
            "1 cup unsweetened cocoa powder",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk 1 cup plus 2 tablespoons water, sugar, cocoa powder, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate mixture until chilled, 30 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into an ice cream maker. Freeze according to manufacturer's instructions, about 15 minutes.\n"
            }
        ],
