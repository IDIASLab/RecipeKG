    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255930/butternut-squash-soup-with-baked-cinnamon-apples/",
        "name": "Butternut Squash Soup with Baked Cinnamon Apples",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Butternut Squash Soup with Baked Cinnamon Apples"
        },
        "datePublished": "2020-06-19T04:18:35.000Z",
        "description": "This is one of my favorite recipes handed down from a private chef in New York.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H59M",
        "totalTime": "P0DT1H39M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 pounds butternut squash - peeled, seeded, and chopped",
            "2 small onions, chopped",
            "2 small carrots, peeled and chopped",
            "2 stalks celery, chopped",
            "5 cups chicken broth",
            "2\u2009\u00bd teaspoons ground cinnamon, divided",
            "\u00bd teaspoon ground nutmeg",
            "\u00bc cup maple syrup",
            "salt and ground black pepper to taste",
            "1 cup heavy whipping cream",
            "2 cups peeled and diced Granny Smith apples",
            "\u00bc cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a heavy-bottomed pot over medium heat. Add butternut squash, onions, carrots, and celery; cook, stirring often, until onions are tender, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour broth into the pot; stir in 2 teaspoons cinnamon and nutmeg. Cover and simmer until carrots are soft, about 40 minutes. Stir maple syrup, salt, and pepper into the soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool soup slightly, about 10 minutes. Puree in a food processor or blender in small batches until smooth. Transfer to another pot; stir in heavy cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apples, white sugar, and 1/2 teaspoon cinnamon together and spread on a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake apple mixture in the preheated oven until softened, 7 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve soup garnished with baked apples.\n"
            }
        ],
