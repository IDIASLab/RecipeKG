        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240291/homemade-taco-sauce/",
        "name": "Homemade Taco Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1807150.jpg",
