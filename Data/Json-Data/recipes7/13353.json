    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244991/easy-cranberry-cheesecake/",
        "name": "Easy Cranberry Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7523400.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:03:42.000Z",
        "description": "This is a family favorite that we have every year for Christmas. Kids and adults love it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 9-inch springform pan",
        "recipeIngredient": [
            "\u00be cup graham cracker crumbs",
            "\u00bd cup chopped macadamia nuts",
            "\u00bc cup melted margarine",
            "2 tablespoons white sugar",
            "\u00bc cup cold water",
            "1 (.25 ounce) envelope unflavored gelatin",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (7 ounce) jar marshmallow creme",
            "1 (16 ounce) can whole berry cranberry sauce",
            "1 cup frozen whipped topping (such as Cool Whip\u00ae), thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs, macadamia nuts, margarine, and sugar together in a bowl; press into the bottom of a 9-inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is lightly browned, about 10 minutes. Cool crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water and gelatin together in a saucepan over low heat until dissolved, 3 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and marshmallow cream together in a bowl using an electric mixer on medium speed until well mixed. Gradually stir gelatin mixture and cranberry sauce into cream cheese mixture; fold in whipped topping. Spoon mixture over crust. Chill cheesecake in the refrigerator until firm, at least 1 hour.\n"
            }
        ],
