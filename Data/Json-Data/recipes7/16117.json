        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258755/simple-custard/",
        "name": "Simple Custard",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7790957.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:22:46.000Z",
        "description": "This is my own alteration of a few different recipes. Not too rich or too sweet as many egg custards can be. Can be served warm immediately or cold.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "4 cups whole milk",
            "1 tablespoon vanilla extract",
            "1 teaspoon butter",
            "4 eggs",
            "\u00bd cup white sugar",
            "3 tablespoons cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir milk, vanilla extract, and butter in a saucepan over medium heat until simmering. Remove mixture from heat before it comes to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs, sugar, and cornstarch together in a bowl until sugar dissolves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set saucepan back over low heat. Pour in egg mixture slowly, whisking constantly, until custard thickens enough to coat the bottom of a spoon, 5 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Custards and Puddings"
        ],
