    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216566/mothers-date-dips-from-1940s/",
        "name": "Mother's Date Dips from 1940's",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mother's Date Dips from 1940's"
        },
        "datePublished": "2020-06-19T03:43:53.000Z",
        "description": "My mother used to bake these cookies for  a special occasion, Christmas or for weddings. This recipe has been in the family for 100 years and was passed down from my grandmother to my mother. Hope you enjoy it.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "48 date cookies",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "6 tablespoons butter",
            "\u2153 cup white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00bc cup heavy cream",
            "3 cups whole pitted dates"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease 2 baking sheets. In a bowl, sift together the flour, baking powder, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar in a bowl until light and fluffy with an electric mixer set on medium speed. Add egg and vanilla extract to mixture and beat in. Mix in the flour mixture in 3 additions, alternating with cream. Mixture should form a thick batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "One at a time, dip each date in the batter and place onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake one sheet at a time in the center of the preheated oven until the batter is lightly browned, about 10 minutes.\n"
            }
        ],
