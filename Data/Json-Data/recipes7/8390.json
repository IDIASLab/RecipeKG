        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cucumbers, thinly sliced",
            "\u00bd cup white vinegar",
            "3 tablespoons white sugar",
            "2 tablespoons water",
            "\u00bc teaspoon salt",
            "2 tablespoons minced parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put sliced cucumbers into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir vinegar, sugar, water, and salt together in a separate bowl to dissolve the sugar and salt; pour over the cucumbers. Sprinkle parsley over the cucumbers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate cucumbers 3 hours to overnight.\n"
            }
        ],
