    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/83148/cheesy-pork-chops-with-spicy-apples/",
        "name": "Cheesy Pork Chops with Spicy Apples",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F675588.jpg",
            "width": null,
            "height": null,
            "caption": "Cheesy Pork Chops with Spicy Apples"
        },
        "datePublished": "2005-02-22T10:59:23.000Z",
        "description": "Succulent pork chops topped with a spicy apple and onion topping and finished with cheese. Perfect with braised cabbage.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon butter",
            "1 onion, sliced",
            "1 pinch red pepper flakes",
            "1 apple, cored and sliced",
            "2 teaspoons white sugar",
            "2 tablespoons balsamic vinegar",
            "4 pork chops",
            "salt and pepper to taste",
            "4 slices extra sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare a grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the grill heats, melt the butter in a skillet over medium heat. Add the onion, and cook until soft. Season with red pepper flakes then add the sliced apple. Stir in the sugar and balsamic vinegar, and simmer for 5 minutes, or until apples are soft and golden.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season the pork chops with salt and pepper. Grill for 3 to 5 minutes per side, depending on thickness. Spoon the onions and apples on top of the chops, and top with a slice of Cheddar cheese. Cover the grill, and cook for about 3 minutes until cheese is melted and bubbling.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Borden Cheese"
        ],
