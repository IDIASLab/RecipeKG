    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220136/brownie-cheesecake-hearts/",
        "name": "Brownie Cheesecake Hearts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F881591.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:47:28.000Z",
        "description": "Chocolate chunk brownies swirled with rich cheesecake are topped with a creamy chocolate ganache. It makes a perfect Valentine's dessert!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "15 hearts",
        "recipeIngredient": [
            "15 small heart-shaped baking molds",
            "2 teaspoons butter",
            "1 (19.8 ounce) package chocolate chunk brownie mix",
            "\u2153 cup vegetable oil",
            "3 tablespoons water",
            "1 egg",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract",
            "1 egg",
            "\u00bc cup heavy cream",
            "\u00be cup chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter the heart-shaped molds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brownie mix, vegetable oil, water, and 1 egg together in a bowl until smooth and well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the batter into the prepared heart molds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, sugar, and vanilla extract together with an electric mixer in a large bowl until light and fluffy. The mixture should be noticeably lighter in color. Beat in 1 egg until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon a dollop of the cream cheese mixture into each heart mold and mix slightly with a knife to make decorative swirls of cream cheese filling in the brownie batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While brownies are baking, place chocolate chips into a microwave-safe bowl and microwave for 30-second intervals, stirring after each time, until the chocolate is warm, smooth, and melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir heavy cream into the chocolate. Mixture will be clumpy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove hearts from oven, and immediately spread about 1 tablespoon of chocolate mixture over each heart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the hearts to the oven and bake until the chocolate topping spreads out, about 5 minutes. Cool before serving.\n"
            }
        ],
