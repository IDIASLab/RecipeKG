    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222076/healthier-monkey-bread-i/",
        "name": "Healthier Monkey Bread I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2065132.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T03:48:55.000Z",
        "description": "Less sugar, butter instead of margarine, and refrigerated pizza dough instead of biscuits - but it still adds up to a delicious cinnamon-y treat! You'll love this healthier monkey bread.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 10-inch tube pan",
        "recipeIngredient": [
            "\u2153 cup white sugar",
            "2 teaspoons ground cinnamon",
            "36 ounces refrigerated whole wheat pizza dough",
            "\u00bd cup chopped walnuts",
            "\u00bd cup raisins",
            "6 tablespoons butter",
            "1 cup packed brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 10-inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix white sugar and cinnamon in a plastic bag. Cut pizza dough into 1 1/2-inch pieces. Shake 6 to 8 pieces of dough in the sugar-cinnamon mix. Arrange pieces in bottom of prepared pan. Continue until all dough pieces are coated and placed in pan. Arrange nuts and raisins in and among dough pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter and brown sugar in a small saucepan over medium heat. Boil for 1 minute. Pour over dough; let rise for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until bread is golden brown, about 35 minutes. Let bread cool in pan for 10 minutes, then turn out onto a plate. Pull apart pieces of bread to eat.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
