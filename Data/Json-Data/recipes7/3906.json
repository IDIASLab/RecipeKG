    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/192623/asian-cucumber-salad/",
        "name": "Asian Cucumber Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3818619.jpg",
            "width": 5184,
            "height": 5184
        },
        "datePublished": "2020-06-19T03:39:14.000Z",
        "description": "This is a really delicious, light salad.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT1H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cucumbers - halved lengthwise, seeded, and sliced",
            "2 teaspoons salt",
            "\u00bd cup rice vinegar",
            "\u00bc cup white sugar",
            "2 tablespoons sesame oil",
            "1 tablespoon minced garlic",
            "1 tablespoon minced fresh ginger root",
            "1 tablespoon sesame seeds",
            "4 fresh red chile peppers, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Put the cucumber slices in a colander and sprinkle with salt; set aside to drain for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk the vinegar and sugar together until the sugar is dissolved; add the sesame oil, garlic, ginger, and sesame seeds; stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse salt off the cucumber slices by running under cold water; place in a large bowl with the sliced red chile peppers. Drizzle the dressing over the vegetables and toss to coat. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Cucumber Salad Recipes"
        ],
