        "recipeYield": "1 quart of yogurt",
        "recipeIngredient": [
            "1 quart whole milk",
            "2 tablespoons white sugar",
            "2 tablespoons vanilla extract",
            "2 tablespoons plain yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat up the oven by turning on for 3 to 4 minutes; turn the oven off.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring milk to a boil over medium-high heat in a large saucepan. Stir in sugar and vanilla extract until blended. Turn off heat and let sit until warm but not cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place yogurt in a large container; pour in milk mixture. Cover and place in the oven until yogurt has set, 8 to 16 hours. Store in the refrigerator.\n"
            }
        ],
