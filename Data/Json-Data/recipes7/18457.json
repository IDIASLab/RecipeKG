    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267843/cinnamon-apple-pecan-bread/",
        "name": "Cinnamon-Apple-Pecan Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cinnamon-Apple-Pecan Bread"
        },
        "datePublished": "2020-06-19T04:32:02.000Z",
        "description": "A delicious buttery loaf. This recipe is basically an apple-cinnamon version of classic banana bread, plus pecans.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "3 large Gala apples, peeled and cubed",
            "12 tablespoons butter, divided",
            "4 tablespoons honey",
            "2 tablespoons ground cinnamon,  divided",
            "1 cinnamon stick",
            "1\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon fine salt",
            "1 cup white sugar",
            "2 large eggs",
            "\u00bd teaspoon vanilla extract",
            "1 cup chopped toasted pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x5x3-inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples, 4 tablespoons butter, honey, 1 tablespoon cinnamon, and cinnamon stick in a saucepan over medium heat. Cover with a lid and cook, stirring frequently, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While apples are simmering, sift flour, baking powder, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 8 tablespoons butter, remaining 1 tablespoon cinnamon, and sugar in the bowl of a stand mixer. Beat together until light and fluffy. Add eggs one at a time; beat until fully incorporated. Add vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Discard cinnamon stick and mash cooked apples. Add mashed apples and pecans to the batter. Use a rubber spatula to mix in flour mixture until just incorporated. Transfer batter to the prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, about 1 hour. Cool on a wire rack.\n"
            }
        ],
