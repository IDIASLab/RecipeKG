        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229569/josess-zucchini-salad/",
        "name": "Joses's Zucchini Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1943913.jpg",
            "width": 2802,
