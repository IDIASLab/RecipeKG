    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220980/chocolate-passover-sponge-cake/",
        "name": "Chocolate Passover Sponge Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2201238.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2020-06-19T03:48:11.000Z",
        "description": "A true chocolate sponge cake! Use brewed coffee or espresso powder mixed with water.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "1 10-inch tube cake",
        "recipeIngredient": [
            "cooking spray",
            "\u00bc cup matzo cake meal, sifted",
            "\u00be cup potato starch, sifted",
            "10 eggs, separated",
            "1 cup white sugar",
            "\u00bd cup unsweetened cocoa powder",
            "\u00bd cup white sugar",
            "3 tablespoons cold strong coffee",
            "\u00bc cup chopped walnuts, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a 10-inch tube pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift matzo cake meal with potato starch into a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place egg yolks into a large mixing bowl and beat with an electric mixer on medium speed until light, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly beat 1 cup sugar into the egg yolks; continue beating until mixture is thick and pale, about 5 more minutes. Beat cocoa powder into the egg yolk mixture until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites with an electric mixer fitted with clean beaters in a separate bowl until foamy. Gradually beat in 1/2 cup sugar and continue to beat until egg whites hold stiff peaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly stir matzo cake meal mixture and coffee into egg yolk-cocoa mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently fold 1/3 of the egg whites into the batter, retaining as much volume as possible. Fold in remaining egg whites, mixing just until no white streaks remain. Gently mix in chopped walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the batter evenly into the prepared tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top of cake shows small cracks and springs back when lightly pressed, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Run a knife around the sides of cake to loosen from the pan. Invert cake onto a wire rack to cool.\n"
            }
        ],
