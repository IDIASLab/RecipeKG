        "recipeYield": "10 sandwiches",
        "recipeIngredient": [
            "1 cup milk",
            "1 banana",
            "3 cubes  ice",
            "1 tablespoon white sugar",
            "20 graham crackers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine milk, banana, ice cubes, and sugar in a blender; blend until smooth. Pour into a freezer-safe container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze banana ice cream until firm, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread ice cream onto 10 graham crackers; top each with another graham cracker to create a sandwich.\n"
            }
