    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257200/spicy-center-cut-pork-chops/",
        "name": "Spicy Center-Cut Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Center-Cut Pork Chops"
        },
        "datePublished": "2020-06-19T04:20:00.000Z",
        "description": "A spicy and different pork chop recipe the guys in my house beg for.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 chops",
        "recipeIngredient": [
            "cooking spray",
            "2 (15.25 ounce) cans mixed fruit with pineapple in light syrup",
            "1 small onion, chopped",
            "2 tablespoons Szechuan sauce, or more to taste",
            "2 tablespoons white sugar",
            "2 cups bread crumbs",
            "\u00bc cup Szechuan seasoning",
            "4 (1-inch thick) boneless center-cut pork chops"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a large baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine canned fruit and onion in a food processor; blend until almost pureed. Mix in Szechuan sauce and sugar. Pour into a shallow bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bread crumbs and Szechuan seasoning together in a shallow dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dredge pork chops in fruit mixture; coat with bread crumbs. Transfer to the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 35 minutes. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C).\n"
            }
        ],
