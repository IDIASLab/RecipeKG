        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241378/classic-barbecued-ribs/",
        "name": "Classic Barbecued Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8240923.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:02:20.000Z",
        "description": "These are some classic ribs that your mother used to make.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (4 pound) package pork spareribs",
            "1 pinch garlic salt, or to taste",
            "1 cup ketchup",
            "\u00bd cup white vinegar",
            "\u00bc cup white sugar",
            "4 teaspoons celery salt",
            "1\u2009\u00bd teaspoons chili powder",
            "1 teaspoon salt",
            "1 teaspoon dry mustard",
            "1 teaspoon paprika",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle ribs with garlic salt and place into a roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir ketchup, vinegar, sugar, celery salt, chili powder, salt, dry mustard, paprika, and black pepper together in a bowl; pour over the ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook, basting with the pan drippings every half-hour, until the meat pulls away easily from the bone, about 90 minutes.\n"
            }
        ],
