    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270368/grasshopper-cheesecake-bars/",
        "name": "Grasshopper Cheesecake Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F488628.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T04:34:43.000Z",
        "description": "Delicious mint cheesecake sandwiched between two layers of crispy chocolate. Excellent for Christmas cookies!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "25 bars",
        "recipeIngredient": [
            "\u00be cup all-purpose flour",
            "\u2153 cup white sugar",
            "\u2153 cup unsweetened cocoa powder",
            "6 tablespoons cold butter",
            "1 (8 ounce) package cream cheese, at room temperature  ",
            "\u00bc cup white sugar",
            "1 egg",
            "1 teaspoon peppermint extract",
            "5 drops green food coloring",
            "\u00bc cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, and cocoa powder in a large bowl. Cut in butter until mixture resembles fine crumbs. Set aside 1 cup of the crumbs to be used as the topping. Press remaining crumb mixture into an ungreased 8x8-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until set, about 15 minutes. Remove and let cool. Keep oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While crust is cooling, beat cream cheese and sugar with an electric mixer until fluffy. Add egg, peppermint extract, and green food coloring; beat well. Stir in milk until batter is smooth. Pour batter over the cooled crust. Sprinkle reserved crumb mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until filling is set, 20 to 25 minutes. Cool before cutting into 25 bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Holiday Cake Recipes"
