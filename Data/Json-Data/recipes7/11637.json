    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238639/chai-snickerdoodle-cookies/",
        "name": "Chai Snickerdoodle Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6270506.jpg",
            "width": 1283,
            "height": 960
        },
        "datePublished": "2020-06-19T03:59:59.000Z",
        "description": "It's sunny out and the family is home; why not whip out this recipe and make the family happy? Fast, easy, tasty, and sweet with a kick of spice.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1\u2009\u00be cups white sugar",
            "1 cup butter, softened",
            "2 eggs",
            "2 tablespoons chai concentrate",
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bc teaspoon salt",
            "3 tablespoons white sugar",
            "4 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1 3/4 cups sugar, butter, eggs, and chai concentrate together in a bowl until light and fluffy. Whisk flour, baking soda, and salt together in another bowl. Stir flour mixture into butter mixture until dough is combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 3 tablespoons sugar and cinnamon together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into 1-inch balls and roll each ball in cinnamon sugar to coat; place 2-inches apart on baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies in the preheated oven until golden, about 10 minutes. Cool on wire racks.\n"
            }
        ],
