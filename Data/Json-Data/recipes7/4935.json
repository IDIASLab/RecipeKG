    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/214905/key-lime-ice-cream/",
        "name": "Key Lime Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F882703.jpg",
            "width": 4288,
            "height": 2848
        },
        "datePublished": "2020-06-19T03:42:44.000Z",
        "description": "Best. Ice cream. Ever. A super refreshing summer treat!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT3H15M",
        "recipeYield": "1 quart",
        "recipeIngredient": [
            "2 large eggs",
            "1\u2009\u00bc cups white sugar",
            "4 egg yolks",
            "1 tablespoon lemon zest",
            "2\u2009\u00bc cups half-and-half cream",
            "\u00be cup lime juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the eggs, egg yolks, sugar, lime juice, and lemon zest in a saucepan over medium heat until well-blended. Continuously stir the egg mixture with a wooden spoon until thickened, 7 to 8 minutes. The mixture should be thick enough to coat the back of the spoon. Remove from heat, and stir in the half and half until smooth. Strain the mixture through a fine sieve set over a clean bowl. Cover and chill the mixture in the refrigerator, stirring occasionally, until cool, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the chilled mixture into an ice cream maker and freeze according to manufacturer's directions until it reaches \"soft-serve\" consistency. Transfer ice cream to a one- or two-quart lidded plastic container; cover surface with plastic wrap and seal. For best results, ice cream should ripen in the freezer for at least 2 hours or overnight.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frozen Dessert Recipes",
            "Ice Cream"
        ],
