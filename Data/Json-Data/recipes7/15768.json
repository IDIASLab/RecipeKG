    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257916/taro-rolls/",
        "name": "Taro Rolls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5726715.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-06-19T04:21:04.000Z",
        "description": "Hawaiian taro (poi) flour and purple food coloring are used to create these unique rolls.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "18 rolls",
        "recipeIngredient": [
            "1\u2009\u2153 cups warm water (90 degrees F (32 degrees C))",
            "1 cup poi (taro root powder)",
            "\u00bd cup butter, softened",
            "1 egg",
            "2\u2009\u00bd teaspoons active dry yeast",
            "1 teaspoon purple food coloring",
            "1 cup white sugar",
            "\u00bc teaspoon salt",
            "4 cups all-purpose flour, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water, poi, butter, egg, yeast, and purple food coloring in the bowl of a stand mixer fitted with the dough hook. Let stand until yeast softens and forms a creamy foam, about 5 minutes. Add sugar and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix on low speed, adding flour gradually, until dough comes together and texture is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out onto a floured work surface. Divide into 18 pieces; shape into rolls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Line a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place rolls on the prepared baking sheet. Cover and let rise until doubled, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bottoms start to brown, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes",
            "Rolls and Buns"
