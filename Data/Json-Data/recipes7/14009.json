    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246607/funfetti-donut-holes/",
        "name": "Funfetti&reg; Donut Holes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3356160.jpg",
            "width": 3000,
            "height": 2000
        },
        "datePublished": "2020-06-19T04:06:10.000Z",
        "description": "A favorite, classic cake flavor turned into a donut. Serve while warm.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT0H27M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups confectioners' sugar",
            "\u00bc cup multicolored candy sprinkles",
            "3 tablespoons whole milk",
            "1 tablespoon lemon juice",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "2 cups all-purpose flour",
            "2 tablespoons brown sugar, or more to taste",
            "1\u2009\u00bd tablespoons baking powder",
            "\u00be teaspoon kosher salt",
            "1 cup milk",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00bc cup unsalted butter, melted",
            "\u00bc cup multicolored candy sprinkles",
            "vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk confectioners' sugar, 1/4 cup sprinkles, 3 tablespoons milk, lemon juice, 1 teaspoon vanilla extract, and 1 pinch salt together in a bowl to make a smooth glaze.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, brown sugar, baking powder, and 3/4 teaspoon salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup milk, egg, and 1 teaspoon vanilla extract in a small bowl. Stir into the flour mixture. Add melted butter and 1/4 cup sprinkles; mix until a soft dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat at least 2 inches of oil in a large pot to 350 degrees F (175 degrees C). Line a baking sheet or wire rack with paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop a few tablespoons of dough into the hot oil using a small ice cream scoop. Fry donut holes until golden brown, about 1 minute per side. Transfer donut holes to the paper towels using a slotted spoon; let drain and cool slightly. Repeat with remaining dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a wire rack on a baking sheet. Dip one donut hole at a time into the glaze and place on the wire rack; allow excess glaze to drip off before serving.\n"
            }
        ],
