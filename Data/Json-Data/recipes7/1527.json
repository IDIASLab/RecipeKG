    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/107886/cherry-dots/",
        "name": "Cherry Dots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F530510.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:13:44.000Z",
        "description": "A cookie tradition in my family, Christmas is not complete without our Cherry Dots.",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT4H55M",
        "recipeYield": "8 dozen cookies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup white sugar",
            "2 tablespoons milk",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon salt",
            "34 candied cherries",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat the butter and sugar with an electric mixer in a large bowl until smooth. Stir in milk and vanilla extract. Mix in the flour and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the dough into two equal pieces, and shape each half into a 12 inch roll on a sheet of waxed paper. Place 17 candied cherries side by side on top of each roll, and press into the roll until the cherries are centered. Mold dough around cherries. Sprinkle the outside evenly with chopped walnuts. Wrap the dough in the waxed paper, and refrigerate 4 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each roll into 1/4 inch slices, and place the slices onto ungreased baking sheets. Bake in the preheated oven until golden, 12 to 15 minutes.\n"
            }
        ],
