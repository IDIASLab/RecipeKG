        "mainEntityOfPage": "https://www.allrecipes.com/recipe/74722/grandmothers-peach-fuzz/",
        "name": "Grandmother's Peach Fuzz",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1013399.jpg",
            "width": 958,
