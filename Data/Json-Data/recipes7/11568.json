    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238465/amaranth-ginger-cookies/",
        "name": "Amaranth Ginger Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1323199.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:59:51.000Z",
        "description": "A very crisp cookie with the goodness of amaranth grains and oatmeal for extra fiber for your midday snack.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "30 servings",
        "recipeIngredient": [
            "1 cup boiling water",
            "\u00bc cup amaranth",
            "1\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup white sugar",
            "\u00bd cup butter, softened",
            "2 eggs",
            "1 cup quick-cooking oats",
            "1 cup chopped crystallized ginger",
            "\u00bd cup finely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and amaranth to a boil in a small saucepan. Reduce heat to low, cover, and simmer until amaranth is tender and water has been absorbed, about 10 minutes. Remove saucepan from heat and cool amaranth to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder, baking soda, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar and butter together in a bowl using an electric mixer until smooth and creamy. Beat 1 egg into creamed butter mixture until smoothly incorporated; repeat with second egg. Add amaranth, oats, ginger, and walnuts to creamed butter mixture and beat until mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour mixture into creamed butter mixture until dough is just combined. Drop dough by the teaspoonful onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned on top, about 10 minutes. Remove baking sheet from oven and allow cookies to cool on baking sheet for 5 minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
