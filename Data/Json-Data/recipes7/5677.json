    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218006/traditional-blackberry-cobbler/",
        "name": "Traditional Blackberry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F772535.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2020-06-19T03:44:54.000Z",
        "description": "This cobbler has a traditional drop-biscuit topping.  You can substitute raspberries for half of the blackberries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "1 9x9-inch baking dish",
        "recipeIngredient": [
            "2 pounds frozen blackberries, thawed",
            "1\u2009\u00bc cups white sugar",
            "1\u2009\u00bd tablespoons all-purpose flour",
            "1\u2009\u00bd tablespoons butter",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "2 tablespoons white sugar",
            "\u00bc cup softened butter",
            "\u2154 cup milk",
            "1 large egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss the blackberries with 1 1/4 cups sugar and 1 1/2 tablespoons flour in a mixing bowl. Spread into a 9x9-inch baking dish, and dot with 1 1/2 tablespoons of butter. In a separate bowl, whisk 1 1/2 cups flour together with baking powder, salt, and 2 tablespoons sugar. Cut in the softened butter until the mixture resembles cornmeal. Stir in the milk and egg until combined. Drop the batter over the berries by the spoonful.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the fruit is tender and the topping is golden brown, 20 to 30 minutes.\n"
            }
        ],
