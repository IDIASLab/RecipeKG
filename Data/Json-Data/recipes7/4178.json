        "mainEntityOfPage": "https://www.allrecipes.com/recipe/206433/cinnamon-and-sugar-french-waffle-toast/",
        "name": "Cinnamon and Sugar French Waffle Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6472870.jpg",
