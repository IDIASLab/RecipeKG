    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235940/orange-almond-pound-cake/",
        "name": "Orange Almond Pound Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1247218.jpg",
            "width": 5184,
            "height": 3456
        },
        "datePublished": "2020-06-19T03:57:51.000Z",
        "description": "This recipe is very versatile. I love using it for holiday parties.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "1 pound cake",
        "recipeIngredient": [
            "3 cups all-purpose flour, sifted",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "1 pound butter, softened",
            "3 cups white sugar",
            "3 ounces marzipan",
            "6 eggs",
            "\u2153 cup half-and-half",
            "\u2153 cup milk",
            "\u2153 cup orange juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 10-inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder, and baking soda together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar together in a bowl until creamy. Add marzipan. Add eggs one at a time, beating well after each addition. Beat half-and-half into butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 1 cup of flour mixture into the butter mixture. Stir in milk. Add another 1 cup flour mixture to the butter mixture and stir milk into mixture. Add remaining flour mixture and stir in orange juice. Scrape down the sides of the bowl with a spatula and give batter a final mix. Pour batter into prepared tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the cake comes out clean, about 90 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool cake in the pan for 15 to 20 minutes before turning out onto a wire rack to cool completely.\n"
            }
        ],
