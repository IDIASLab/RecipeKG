    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239551/sambal-shrimp/",
        "name": "Sambal Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4535184.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2014-09-26T21:14:39.000Z",
        "description": "Shrimp are marinated in a spicy chili pepper sauce then quickly grilled in this easy preparation.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup Asian chili paste (sambal)",
            "2 tablespoons sugar",
            "Juice of 1 lime",
            "2 tablespoons olive oil",
            "3 cloves garlic, minced",
            "1 tablespoon minced fresh ginger",
            "2 tablespoons mirin (Japanese sweet wine)",
            "1 tablespoon fish sauce",
            "1 tablespoon sesame oil",
            "1 pound large shrimp - peeled, deveined, and tails left on",
            "1 tablespoon chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sambal paste, sugar, lime juice, olive oil, garlic, ginger, mirin, fish sauce, and sesame oil in a large bowl; mix well. Reserve half of the marinade for basting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir shrimp into the remaining half of the marinade, coating evenly. Cover and refrigerate for about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate. Remove shrimp from marinade; discard marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill shrimp about 2 minutes on each side. Brush with the reserved marinade. Remove shrimp from grill to serving platter. Garnish with cilantro.\n"
            }
        ],
