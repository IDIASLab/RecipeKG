    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258259/pollo-alla-cacciatore/",
        "name": "Pollo alla Cacciatore",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5127937.jpg",
            "width": 2193,
            "height": 1641
        },
        "datePublished": "2020-06-19T04:21:31.000Z",
        "description": "Chicken cacciatore is typical Tuscan dish, traditionally cooked in a terracotta pot with caramelized onions, capers, olives, tomatoes, and herbs. As a final touch, fried potatoes are added to the pot, too.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H54M",
        "totalTime": "P0DT2H24M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "12 green olives, pitted",
            "1 stalk celery, chopped",
            "2 onions, sliced",
            "\u00bd cup water",
            "2 teaspoons white sugar",
            "1 tablespoon extra-virgin olive oil",
            "1 (2 pound) whole chicken, cut into pieces",
            "4 ripe tomatoes, diced",
            "1 bunch fresh basil, chopped",
            "1 lemon, juiced",
            "2 tablespoons capers",
            "salt and ground black pepper to taste",
            "\u00bc cup extra-virgin olive oil, or to taste",
            "5 potatoes, peeled and cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a small pot of water to a boil. Add olives and celery; simmer until celery is bright green, about 30 seconds. Drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine onions, water, and sugar in a saucepan over medium heat. Cover and cook, stirring occasionally, until onions are soft and caramelized, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a large skillet over medium heat. Cook chicken pieces until browned, about 4 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer chicken pieces to a large saucepan; add olives, celery, caramelized onions, tomatoes, basil, lemon juice, capers, salt, and pepper. Cover and cook over medium heat until chicken is no longer pink in the center, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1/4 cup olive oil in a deep skillet. Add potatoes; cook and stir until crisp and golden brown, about 10 minutes. Stir potatoes into the saucepan shortly before removing from the heat.\n"
            }
        ],
