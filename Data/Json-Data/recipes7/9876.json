    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234096/hot-and-tangy-german-potato-salad/",
        "name": "Hot and Tangy German Potato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1038806.jpg",
            "width": 2816,
            "height": 2112
        },
        "datePublished": "2020-06-19T03:55:56.000Z",
        "description": "Delicious, tangy German potato salad is a great side dish for bratwurst.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT3H40M",
        "totalTime": "P0DT3H55M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "5 pounds red potatoes, diced",
            "1 pound bacon",
            "2 sweet onions, diced",
            "4 cloves garlic, minced",
            "1 cup white vinegar",
            "\u00bd cup water",
            "2 tablespoons white sugar",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with water; season water with salt and bring to a boil. Reduce heat to medium-low and simmer until potatoes are tender, about 20 minutes; drain. Put potatoes in a slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Remove the bacon slices to a plate lined with paper towels. Crumble bacon and add to potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return skillet to medium heat and reheat bacon drippings. Cook and stir sweet onions in the drippings until translucent, 5 to 7 minutes; add garlic and continue cooking until fragrant, 1 to 2 minutes more. Pour onion mixture over the potatoes and bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir vinegar, water, sugar, and salt together in a bowl; pour over the potato mixture. Stir potatoes gently to mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low, stirring occasionally, 3 to 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Potato Salad Recipes",
            "Red Potato Salad Recipes"
        ],
