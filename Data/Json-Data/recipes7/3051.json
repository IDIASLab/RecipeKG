    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/160157/sambal-sauce/",
        "name": "Sambal Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2414338.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:34:22.000Z",
        "description": "This is a Malaysian recipe from our Singaporean cook for a rather hot sauce to be used with shrimp or chicken.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 cup chopped serrano chiles, with seeds",
            "2 tablespoons white sugar",
            "2 tablespoons salt",
            "1 tablespoon belacan shrimp paste",
            "1 tomato, chopped",
            "\u00bd onion, chopped",
            "1 bulb garlic, peeled and crushed",
            "2 tablespoons fresh lime juice",
            "2 tablespoons vegetable oil",
            "2 lemongrass, bruised",
            "2 fresh curry leaves",
            "1 (1/2 inch) piece galangal, thinly sliced",
            "2 tablespoons tamarind juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place serrano peppers, sugar, salt, shrimp paste, tomato, onion, garlic, and lime juice into a blender, and blend until smooth. Heat vegetable oil in a saucepan over medium-high heat. Stir in the chile puree along with the lemongrass, curry leaves, and galangal. Cook and stir until the mixture changes color and becomes very fragrant, about 15 minutes. Stir in the tamarind juice, and cook for 1 minute more. Strain before serving.\n"
            }
        ],
