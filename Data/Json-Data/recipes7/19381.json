    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/271004/easter-mm-cookies/",
        "name": "Easter M&M&reg; Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6450784.jpg",
            "width": 1032,
            "height": 1547
        },
        "datePublished": "2020-06-19T04:35:14.000Z",
        "description": "These cookies are so festive for Easter and taste delicious! Very simple.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup packed brown sugar",
            "\u00be cup white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "\u00be teaspoon salt",
            "\u00be teaspoon baking soda",
            "2 cups Easter-colored candy-coated chocolate pieces (such as M&M's\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugars together in a large bowl on medium speed until fluffy, about 5 minutes. Beat in eggs and vanilla extract. Add flour, salt, and baking soda and mix on low speed. Stir in milk chocolate candies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate dough for 1 hour for best results.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop tablespoons of dough onto an ungreased cookie sheet. Flatten slightly with your palm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are flattened and light brown, 8 to 14 minutes. Cool on the cookie sheet for a few minutes, then remove and place on a cooling rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
