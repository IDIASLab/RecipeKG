    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247083/chicken-pepperoni/",
        "name": "Chicken Pepperoni",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3731279.jpg",
            "width": 2125,
            "height": 2125
        },
        "datePublished": "2020-06-19T04:06:41.000Z",
        "description": "Wonderful breaded chicken topped with pepperoni, and tomato sauce, and mozzarella cheese. Serve plain, or over your favorite pasta.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H2M",
        "totalTime": "P0DT1H22M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "8 skinless, boneless chicken breasts",
            "1 cup bread crumbs",
            "1 teaspoon dried oregano",
            "\u00bd teaspoon garlic salt",
            "salt and ground black pepper to taste",
            "6 tablespoons olive oil, divided",
            "2 (14 ounce) cans diced tomatoes",
            "2 teaspoons white sugar",
            "1 teaspoon garlic salt",
            "1 teaspoon dried basil",
            "\u00bd teaspoon dried oregano",
            "salt and ground black pepper to taste",
            "6 ounces sliced pepperoni",
            "3 cups shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts between 2 sheets of heavy plastic or waxed paper on a solid, level surface. Firmly pound with the smooth side of a meat mallet to a 1/2-inch thickness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bread crumbs, 1 teaspoon oregano, 1/2 teaspoon garlic salt, salt, and black pepper in a shallow bowl. Dip both sides of chicken in bread crumb mixture until evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 1/2 tablespoon olive oil in a large skillet over medium heat. Cook 2 chicken breasts until browned and no longer pink in the center, about 4 minutes per side. Repeat with remaining olive oil and chicken breasts. Arrange chicken breasts in a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, sugar, 1 teaspoon garlic salt, basil, 1/2 teaspoon oregano, salt and black pepper in a saucepan. Cook over medium-low heat until heated through, 10 to 15 minutes. Pour sauce evenly over chicken breasts in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover tomato sauce with pepperoni and mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted, 20 to 30 minutes.\n"
            }
        ],
