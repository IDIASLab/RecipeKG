    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267934/german-apple-sheet-cake/",
        "name": "German Apple Sheet Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5758071.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T04:32:06.000Z",
        "description": "This is my grandma's recipe for a super-easy apple cake. We had an apple tree, and this was a great way to use them.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4\u2009\u00bd pounds apples - peeled, cored, and sliced",
            "1 lemon, juiced",
            "1\u2009\u215b cups unsalted butter",
            "1\u2009\u00bc cups white sugar",
            "1 tablespoon vanilla sugar",
            "5 eggs",
            "2\u2009\u00be cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Grease a deep baking pan with butter or line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss apple slices with lemon juice in a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, sugar, and vanilla sugar in a large bowl and beat using an electric blender until pale and fluffy. Add eggs, one at a time, and mix well after each addition. Combine flour, baking powder, and salt together in a separate bowl and mix on low speed. Fold apple slices into the batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into the prepared baking pan and smooth the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 30 minutes.\n"
            }
        ],
