    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261005/butternut-squash-mac-and-cheese-from-almond-breeze/",
        "name": "Butternut Squash Mac and Cheese from Almond Breeze",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4573381.jpg",
            "width": 1130,
            "height": 960
        },
        "datePublished": "2020-06-19T04:25:06.000Z",
        "description": "Pairs well with a crisp autumn day and good company. For extra creaminess use any Almond Breeze Almond-Cashew Blend instead of Almond Breeze Unsweetened Original.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 cups diced butternut squash",
            "2\u2009\u00bd cups Almond Breeze Unsweetened Original OR any Almond Breeze Almond-Cashew Blend",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon smoked paprika",
            "1 pinch ground nutmeg",
            "1 (15 ounce) can white beans, drained and rinsed",
            "8 ounces white Cheddar cheese, shredded",
            "3 cups packed chopped kale",
            "1 pound whole wheat (or alternative grain) pasta",
            "salt and ground black pepper to taste",
            "Additional shredded Cheddar cheese for topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the butternut squash, Almond Breeze, garlic powder, smoked paprika, and nutmeg in a pot. Bring mixture to a boil over high heat, then reduce heat to low, cover, and cook for 10-15 minutes, until the squash is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the butternut squash mixture to a blender with the white beans and blend until completely smooth. A stick blender should also work here, if you prefer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer butternut squash/bean mixture back to the pot (no heat yet). Add the cheese a little at a time, stirring until melted and incorporated. Then, add the kale.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the pot to heat and cook over low heat until kale is wilted. Season sauce to taste with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the pasta according to package directions, drain, then mix with the sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve mac &amp; cheese immediately or pour into a baking dish, top with additional shredded cheese, and broil until golden brown and bubbling.\n"
            }
        ],
