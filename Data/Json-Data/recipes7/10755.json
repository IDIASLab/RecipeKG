    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236271/spicy-tea-bread/",
        "name": "Spicy Tea Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Tea Bread"
        },
        "datePublished": "2020-06-19T03:58:06.000Z",
        "description": "This was my mother's recipe which has been modified from her great grandmother's. My mother's family is from Great Britain and Germany so they brought tea time and a love of spicy sweets with them and this is the result.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (8 ounce) package pitted dates",
            "1 cup white sugar",
            "2 tablespoons molasses",
            "2 tablespoons butter, each cut into quarters",
            "1\u2009\u00bc cups strong hot black tea",
            "1\u2009\u00bd cups all-purpose flour",
            "1 cup whole wheat flour",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon grated nutmeg",
            "\u00bc teaspoon ground cardamom",
            "\u00bc teaspoon white pepper",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00be cup walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and dust a 9x5-inch loaf pan with white sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine dates, sugar, molasses, and butter in a large bowl. Pour tea over date mixture and mix well; set aside to cool, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk all-purpose flour, whole wheat flour, baking powder, cinnamon, ginger, cloves, salt, nutmeg, cardamom, and white pepper together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir egg and vanilla extract into cooled date mixture. Add 1/3 the flour mixture to the date mixture; mix well. Repeat 2 more times with remaining flour mixture until batter is just combined; fold in walnuts. Pour batter into the prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a knife inserted in the middle of the bread comes out clean, about 1 1/4 hours. Cool in pan for 15 minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
