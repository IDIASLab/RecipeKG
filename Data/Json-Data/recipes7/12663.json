        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 cup cooked white beans, drained",
            "2 cups milk",
            "\u2153 cup white sugar",
            "1 egg, beaten",
            "1\u2009\u00bd tablespoons orange zest",
            "1\u2009\u00bd tablespoons ground ginger",
            "1\u2009\u00bd teaspoons cornstarch",
            "2 tablespoons butter",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Process beans and milk together in a blender until smooth; pour into a small saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sugar, egg, orange zest, ginger, and cornstarch into the bean mixture until smooth; place over medium heat and cook, stirring constantly, until thickened, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir butter and vanilla extract into the thickened mixture until the butter is melted; pour into 4 dessert cups and refrigerate until chilled, at least 15 minutes.\n"
            }
        ],
