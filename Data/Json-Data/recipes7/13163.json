        "name": "Homemade Cinnamon-Vanilla Salvadorian Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5317775.jpg",
            "width": 3264,
            "height": 1836
