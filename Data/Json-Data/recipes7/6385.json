        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220480/adams-apple-cake/",
        "name": "Adam's Apple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4584616.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-06-19T03:47:50.000Z",
        "description": "This simple, delicious apple cake recipe has been in our family for generations.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "1 9x13-inch baking pan",
        "recipeIngredient": [
            "cooking spray",
            "3 cups all-purpose flour",
            "2 cups white sugar",
            "1\u2009\u00bc cups vegetable oil",
            "3 eggs",
            "1 teaspoon salt",
            "\u00bd teaspoon baking soda",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground cinnamon",
            "4 apples - peeled, cored and chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a 9x13-inch baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, sugar, vegetable oil, eggs, salt, baking soda, vanilla extract, and cinnamon in a large bowl; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the chopped apples until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
