    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258414/kits-alcohol-slush/",
        "name": "Kit's Alcohol Slush",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Kit's Alcohol Slush"
        },
        "datePublished": "2020-06-19T04:21:37.000Z",
        "description": "Tastes great on a warm summer day!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "32 drinks",
        "recipeIngredient": [
            "9 cups water",
            "2 cups white sugar",
            "4 green tea bags",
            "1 (12 fluid ounce) can frozen lemonade concentrate",
            "1 (12 fluid ounce) can frozen orange juice concentrate",
            "12 fluid ounces vodka",
            "12 fluid ounces gin",
            "1 (2 liter) bottle lemon-lime soda (such as 7-Up\u00ae), or to taste",
            "32 maraschino cherries, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water, sugar, and green tea bags in a large pot. Bring to a boil and dunk green tea bags several times in the boiling water. Discard tea bags. Stir in lemonade and orange juice concentrates until dissolved. Remove pot from heat and allow mixture to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir vodka and gin into the cooled tea-juice mixture. Pour mixture into a freezer-safe container and freeze until slushy, 3 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir slush to evenly distribute the alcohol. Fill small glasses with about 1/4 cup of slush; top off with lemon-lime soda. Garnish each drink with a maraschino cherry.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Slushie Recipes"
        ],
