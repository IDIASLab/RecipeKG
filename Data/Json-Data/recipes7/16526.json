    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259810/spicy-new-mexico-fish/",
        "name": "Spicy New Mexico Fish",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy New Mexico Fish"
        },
        "datePublished": "2020-06-19T04:24:05.000Z",
        "description": "Green chiles from Hatch, New Mexico are a staple cooking ingredient in the state. I order my chiles online from Bueno Foods. Serve with hot rice!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 (14.5 ounce) can crushed tomatoes",
            "\u00bd cup diced hot Hatch green chiles",
            "1 teaspoon white sugar",
            "1 clove garlic, minced",
            "\u00bd jalapeno pepper, minced, or to taste",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2 halibut steaks",
            "2 cups sliced zucchini"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, green chiles, sugar, garlic, jalapeno, salt, and black pepper in a saucepan over medium heat; bring to a boil. Reduce heat to medium-low; add zucchini. Simmer until sauce is combined, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place halibut steaks in a shallow casserole dish; pour sauce on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven until fish flakes easily with a fork, about 30 minutes.\n"
            }
        ],
