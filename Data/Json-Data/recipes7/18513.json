    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267966/french-pear-tart-with-nuts/",
        "name": "French Pear Tart with Nuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5738796.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2020-06-19T04:32:08.000Z",
        "description": "A very easy and quick pear tart with a puff pastry base and a topping of creme fraiche and mixed nuts.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (17.25 ounce) package frozen puff pastry, thawed",
            "1 (8 ounce) container creme fraiche",
            "1 egg",
            "\u00bc cup white sugar",
            "\u00be cup mixed nuts",
            "3 pears - peeled, cored and sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unroll the puff pastry sheet and line a tart pan with it. Cut off any overhanging pastry. Prick pastry several times with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together creme fraiche, egg, and sugar in a bowl until smooth. Fold in nut mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange pear slices on puff pastry and cover with creme fraiche mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake tart in the preheated oven until the filling is lightly colored and pastry has puffed up and browned, about 25 minutes.\n"
            }
        ],
