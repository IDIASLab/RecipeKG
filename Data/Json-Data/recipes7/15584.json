        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257463/sweet-coconut-bread/",
        "name": "Sweet Coconut Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sweet Coconut Bread"
        },
        "datePublished": "2020-06-19T04:20:33.000Z",
        "description": "A Bahamas recipe.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H1M",
        "totalTime": "P0DT1H36M",
        "recipeYield": "5 loaves",
        "recipeIngredient": [
            "3\u2009\u00bd cups brown sugar",
            "2 (8 ounce) packages grated coconut",
            "1 cup all-purpose flour",
            "\u00be cup seedless raisins ",
            "4 ounces candied mixed fruit peel",
            "4 ounces fresh cherries, pitted and cut into small pieces",
            "\u00bc cup baking powder",
            "1 tablespoon mixed essence",
            "\u00bd teaspoon ground nutmeg",
            "1 cup margarine",
            "\u00bd cup shortening",
            "\u00bd cup milk",
            "\u00bd teaspoon salt",
            "1 tablespoon water, or more if needed",
            "5 teaspoons white sugar, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Grease five 8x4-inch baking pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, coconut, flour, raisins, fruit peel, cherries, baking powder, mixed essence, and nutmeg in a large bowl using a large wooden spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine margarine and shortening in a microwave-safe bowl; heat in microwave until melted, 30 seconds to 1 minute. Pour melted mixture into flour mixture, alternating with milk, until batter is just mixed. Add water, a teaspoon at a time, if more moisture is needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared baking pans, smoothing the tops with the back of a wet spoon. Sprinkle white sugar over each loaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick or skewer inserted in the middle comes out clean, about 1 hour. Cool in the pans for 5 minutes. Invert onto cooking racks.\n"
            }
        ],
