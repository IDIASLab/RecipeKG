    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/206174/plantain-corn-muffins/",
        "name": "Plantain Corn Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1938030.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:40:08.000Z",
        "description": "This is a recipe I came up with when I needed to use the last two over-ripe plantains in my fruit bowl.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "16 muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u2154 cup sugar",
            "\u00bd cup cornmeal",
            "1 tablespoon baking powder",
            "\u00bd teaspoon salt",
            "1 cup milk",
            "1 cup mashed, ripe plantains",
            "2 large eggs",
            "\u2153 cup corn oil",
            "3 tablespoons melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). Grease 16 muffin cups or line with paper muffin liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour, sugar, cornmeal, baking powder, and salt together in a bowl. Whisk the milk, plantains, eggs, oil, and butter together in a separate bowl. Mix the flour mixture into the milk mixture until just combined; do not over mix. Allow the mixture sit at room temperature for 10 to 15 minutes. Pour into prepared muffin cups to about 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and the tops spring back when lightly pressed, 15 to 18 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Muffin Recipes",
            "Corn Muffin Recipes"
        ],
