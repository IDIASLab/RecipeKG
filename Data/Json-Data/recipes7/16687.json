    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260309/chocolate-butter-mochi/",
        "name": "Chocolate Butter Mochi",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Butter Mochi"
        },
        "datePublished": "2020-06-19T04:24:35.000Z",
        "description": "A chocolatey twist on an old Hawaiian favorite! So irresistible and buttery smooth melts in your mouth! A great mochi alternative to traditional brownies. Serve with whipped cream on top if you dare. It's quite addicting.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H56M",
        "totalTime": "P0DT2H11M",
        "recipeYield": "1 9x13-inch baking pan",
        "recipeIngredient": [
            "2 cups sweet rice flour (mochiko)",
            "1\u2009\u00be cups white sugar",
            "3 tablespoons cocoa powder ",
            "1 tablespoon baking soda",
            "\u00bd cup butter",
            "1 cup chocolate chips",
            "1 (13 ounce) can coconut milk",
            "1 (12 fluid ounce) can evaporated milk",
            "2 eggs",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift mochiko, sugar, cocoa powder, and baking soda together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place butter in a microwave-safe bowl. Heat in the microwave until melted, about 1 minute. Stir in chocolate chips until smooth and creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat coconut milk, evaporated milk, and eggs together in a bowl. Add vanilla extract. Add chocolate-butter mixture and stir until incorporated. Add to the flour mixture slowly, whisking by hand until well incorporated and no lumps remain. Pour into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 55 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool completely before cutting, about 1 hour. Cut with a plastic knife; mochi sticks to metal.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes"
        ],
