    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261872/easy-sugar-cookie-bars/",
        "name": "Easy Sugar Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6633587.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-06-19T04:26:09.000Z",
        "description": "I love sugar cookies, but hate how much time they take to make. These bars are quick, easy, and best of all, yummy!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "48 cookie bars",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup butter, at room temperature",
            "1 (8 ounce) package cream cheese, softened",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon almond extract",
            "2\u2009\u00bd cups flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd cup butter, softened",
            "\u00bd (8 ounce) package cream cheese, softened",
            "3\u2009\u00bd cups confectioners' sugar",
            "1 teaspoon vanilla extract",
            "3 tablespoons milk, or more as needed",
            "3 drops green food coloring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 13x18-inch rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine white sugar, 1 cup butter, 8 ounces cream cheese, and egg in a large bowl; beat with an electric mixer until well mixed, about 4 minutes. Beat in 1 teaspoon vanilla extract and almond extract. Mix in flour, baking powder, and baking soda until dough comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press dough evenly over the greased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly golden, about 20 minutes. Let cool completely, 10 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1/2 cup butter and 4 ounces cream cheese together in a bowl until smooth. Beat in confectioners' sugar. Thin icing with milk and vanilla extract. Split icing between 2 bowls. Mix green food coloring into 1 bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread green icing over cooled cookies. Cut into 48 squares. Pour white icing into a resealable plastic bag. Cut off corner tip. Drizzle white icing over cookies.\n"
            }
        ],
