        "name": "Jan's Crumb Topping or Streusel",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4787763.jpg",
            "width": 3334,
            "height": 3336
