        "recipeIngredient": [
            "8 cups water",
            "4 family-sized tea bags",
            "2 cups white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a pot. Dissolve sugar in boiling water; remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Steep tea bags in water to desired strength of tea, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate tea until cold, at least 1 hour.\n"
            }
        ],
