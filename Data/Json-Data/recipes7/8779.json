        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231283/creamy-cauliflower-egg-salad/",
        "name": "Creamy Cauliflower Egg Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1079616.jpg",
            "width": 2963,
