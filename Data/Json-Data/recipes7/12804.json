        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241946/vegetable-salad/",
        "name": "Vegetable Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2213941.jpg",
