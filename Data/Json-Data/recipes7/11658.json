        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238703/chocolate-duet-cookies/",
        "name": "Chocolate Duet Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2046918.jpg",
            "width": 2726,
            "height": 2206
        },
        "datePublished": "2020-06-19T04:00:01.000Z",
        "description": "These taste just like the Panera Bread&reg; chocolate duet cookies! They're even better the second day. Enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "\u00be cup white sugar",
            "\u00be cup packed brown sugar",
            "2 eggs, divided",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "\u00bd cup dark cocoa powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup chopped walnuts",
            "1 cup white chocolate chips",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, white sugar, and brown sugar together with an electric mixer in a large bowl until smooth and creamy. Beat 1 egg into butter mixture; beat remaining egg and vanilla extract into butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, cocoa powder, baking soda, and salt together in a bowl; add to creamed butter mixture and beat until the dry ingredients are integrated into a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold walnuts, white chocolate chips, and semisweet chocolate chips into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by teaspoonful onto baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until browning along the edges, 9 to 12 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
