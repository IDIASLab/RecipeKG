    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255004/scrumptious-strawberry-pie-bars/",
        "name": "Scrumptious Strawberry Pie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9268546.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:17:55.000Z",
        "description": "Delicious whipped cream and ripe strawberries on a golden graham cracker crust. YUM! My mom can't get enough. Eat within 4 days.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT1H47M",
        "recipeYield": "1 8-inch pan",
        "recipeIngredient": [
            "cooking spray",
            "9 graham crackers, crushed",
            "\u2153 cup butter, melted",
            "\u2153 cup white sugar",
            "2 teaspoons shortening, softened",
            "\u00bd teaspoon ground cinnamon",
            "1\u2009\u00bd cups heavy whipping cream",
            "6 tablespoons confectioners' sugar",
            "1 tablespoon vanilla extract",
            "1 pound fresh strawberries, sliced",
            "2 tablespoons strawberry syrup, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a large metal bowl and beaters from an electric mixer in the freezer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease an 8-inch square pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix crushed graham crackers, butter, white sugar, shortening, and cinnamon in a bowl until well blended. Press into the prepared pan to make crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until set, about 7 minutes. Let cool, about 5 minutes. Refrigerate for 10 minutes. Transfer to the freezer and chill until cool to the touch.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat heavy cream in chilled bowl with an electric mixer on high speed until soft peaks form. Beat in confectioners' sugar 1 tablespoon at a time. Add vanilla extract; continue to beat until stiff peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange 1 layer of strawberries over chilled crust. Add 1 layer of whipped cream. Repeat with a second layer of strawberries and whipped cream. Top with remaining strawberries. Drizzle strawberry syrup on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate pie until set, about 1 hour. Cut into 9 pieces before serving.\n"
            }
        ],
