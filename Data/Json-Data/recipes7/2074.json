    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/140090/date-and-chocolate-chip-whole-wheat-scones/",
        "name": "Date and Chocolate Chip Whole Wheat Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F974015.jpg",
            "width": 2666,
            "height": 2667
        },
        "datePublished": "2020-06-19T03:29:50.000Z",
        "description": "Chopped dates and chocolate chips add a bright, sweet note to these quick whole wheat scones.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "18 scones",
        "recipeIngredient": [
            "2 cups whole wheat flour",
            "1 cup all purpose flour",
            "3 tablespoons baking powder",
            "\u00bc teaspoon salt",
            "1 tablespoon sugar",
            "1 teaspoon cinnamon",
            "\u2153 cup cold butter",
            "\u00be cup chopped dates",
            "\u00bd cup chocolate chips",
            "1\u2009\u00bd cups milk",
            "milk for brushing",
            "extra sugar and cinnamon to sprinkle"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together wheat and all-purpose flours, baking powder, salt, sugar and cinnamon into a large bowl. Rub butter into dry ingredients until it resembles fine breadcrumbs. Stir in dates and chocolate chips. Mix in the milk, just until blended and form dough into a ball.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn the dough out onto a floured surface. Roll into a circle about 1/2 inch thick. Dip a round cookie cutter into flour (to prevent sticking) then cut out round scones. Place scones on a greased baking sheet. Brush with milk and sprinkle with cinnamon and sugar mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until risen and golden brown, 10 to 12 minutes.\n"
            }
        ],
