        "mainEntityOfPage": "https://www.allrecipes.com/recipe/106187/tangy-shrimp-dip/",
        "name": "Tangy Shrimp Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1107988.jpg",
            "width": 2048,
