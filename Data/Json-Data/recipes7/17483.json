        "cookTime": null,
        "totalTime": "P0DT0H5M",
        "recipeYield": "3 /4 cup",
        "recipeIngredient": [
            "\u00bc cup balsamic vinegar",
            "1 teaspoon Dijon mustard",
            "1 teaspoon white sugar",
            "1 clove garlic, minced",
            "\u00bd teaspoon salt",
            "freshly ground black pepper to taste",
            "\u00bd cup extra-virgin olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk vinegar, mustard, sugar, garlic, salt, and black pepper together in a small bowl until sugar and salt are dissolved. Add oil in a thin stream, whisking continually until emulsified. Season with additional salt and pepper to taste.\n"
            }
