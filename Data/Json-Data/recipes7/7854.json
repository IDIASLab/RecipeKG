    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228937/quick-peach-cobbler/",
        "name": "Quick Peach Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F965158.jpg",
            "width": 3258,
            "height": 3269
        },
        "datePublished": "2020-06-19T03:51:17.000Z",
        "description": "This is a quick dessert that is pretty good. It has been around for a while, but I added a new twist. Serve warm with ice cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd cup butter, melted",
            "1 cup self-rising flour",
            "1 cup white sugar",
            "1 cup milk",
            "1 (28 ounce) can sliced peaches",
            "1 teaspoon almond extract",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place butter in a 9x9-inch baking pan; melt in the preheating oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, and milk together in bowl until smooth. Open peaches and add almond extract to the peaches and juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peaches over melted butter in the 9x9-inch pan; spoon batter atop peaches without mixing. Sprinkle with cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling and top is lightly browned, about 45 to 60 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Peach Dessert Recipes"
        ],
