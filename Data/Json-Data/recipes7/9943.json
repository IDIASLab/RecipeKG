    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234300/sweet-dark-cherry-cobbler/",
        "name": "Sweet Dark Cherry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1409023.jpg",
            "width": 1500,
            "height": 1213
        },
        "datePublished": "2020-06-19T03:56:06.000Z",
        "description": "You can try several variations of this, like using blueberries instead of cherries. Serve warm with ice cream.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup white sugar",
            "2 tablespoons cornstarch",
            "\u00bd teaspoon ground cinnamon",
            "\u00be cup water",
            "1 (12 ounce) bag frozen pitted dark sweet cherries, thawed",
            "1 tablespoon butter",
            "1 cup all-purpose flour",
            "\u00bc cup white sugar, divided",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bd cup milk",
            "3 tablespoons shortening"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease an 8-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup sugar, cornstarch, and cinnamon in a large saucepan; stir in water until mixture is smooth. Add cherries and butter to sugar mixture and bring to a boil; cook and stir until cherry mixture is thickened, about 2 minutes. Pour cherry filling into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, 2 tablespoons sugar, baking powder, and salt in a bowl; cut in shortening using a pastry blender until mixture resembles coarse crumbs. Stir milk into flour mixture until topping is just moistened. Drop topping by spoonfuls over cherry filling; sprinkle remaining 2 tablespoons sugar over topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until topping is golden brown, 30 to 35 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cherry Dessert Recipes"
        ],
