    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229209/liams-famous-apple-cobbler/",
        "name": "Liam's Famous Apple Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2166954.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:51:32.000Z",
        "description": "My son came up with this recipe. It is good!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "12 slices apple",
            "1 cup applesauce",
            "\u00bd cup butter, melted",
            "2 eggs",
            "\u00be cup brown sugar",
            "1 cup bread crumbs, or as needed",
            "\u00bd cup butter, cut into small chunks",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease an 8x8-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apple slices and applesauce together in a bowl. Whisk 1/2 cup melted butter and eggs together in a separate bowl; pour into apple mixture. Stir in brown sugar until well incorporated. Pour apple mixture into prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bread crumbs, 1/2 cup butter, white sugar, and vanilla extract together in a bowl; sprinkle over apple mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until apples are tender and topping is lightly browned, about 30 minutes.\n"
            }
        ],
