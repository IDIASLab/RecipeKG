    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229999/custard-bread-pudding/",
        "name": "Custard Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1102579.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2020-06-19T03:52:11.000Z",
        "description": "I received this recipe from my husband's aunt who has been serving this at her restaurant for years. Great for people who don't like bready things, because it has more custard than bread.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 9x13-inch baking dish",
        "recipeIngredient": [
            "1 quart half-and-half",
            "4 large eggs",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon salt",
            "4 slices bread",
            "\u00bc cup raisins, or to taste",
            "1 pinch ground cinnamon, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend half-and-half, eggs, sugar, vanilla extract, and salt in a blender until smooth. Arrange bread in a 9x13-inch baking dish; sprinkle with raisins and cinnamon. Pour egg mixture over bread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place baking dish in a large roasting pan; fill roasting pan with boiling water halfway up the sides of the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until set, about 1 1/2 hours. Serve warm or cold.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Bread Pudding Recipes"
        ],
