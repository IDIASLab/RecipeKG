        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213260/whipped-hot-chocolate/",
        "name": "Whipped Hot Chocolate",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F517254.jpg",
