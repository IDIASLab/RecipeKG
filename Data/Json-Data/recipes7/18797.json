    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268802/roasted-butternut-squash-chutney/",
        "name": "Roasted Butternut Squash Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Butternut Squash Chutney"
        },
        "datePublished": "2020-06-19T04:33:19.000Z",
        "description": "For a unique holiday condiment, try this seasonal chutney made with roasted butternut squash combined with sweet stewed apples seasoned with garlic, ginger, cinnamon, and turmeric.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "1 pound butternut squash, peeled and diced",
            "3 apples, diced",
            "1\u2009\u2153 cups brown sugar",
            "1 red onion, diced",
            "4 tablespoons cider vinegar",
            "1 tablespoon ground turmeric",
            "\u00bd teaspoon chile powder",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon cumin",
            "\u00bd teaspoon ground ginger",
            "\u00bc teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place butternut squash on the prepared baking sheet in a single layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fork-tender, about 20 minutes. Transfer butternut squash to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine apples, brown sugar, onion, vinegar, turmeric, chile powder, cinnamon, cumin, ginger, and garlic powder in a saucepan over low heat. Cover and cook, stirring occasionally, until apples and onion are soft, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add apple mixture to butternut squash in the bowl and stir gently to combine. Store in a covered container in the refrigerator.\n"
            }
        ],
