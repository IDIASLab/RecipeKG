    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/84517/passover-lemon-bars/",
        "name": "Passover Lemon Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Passover Lemon Bars"
        },
        "datePublished": "2020-06-19T03:12:00.000Z",
        "description": "This Pesadik variation of a classic recipe is actually yummy, not just 'pretty good for Passover'.  Also great for anyone allergic to wheat or corn.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "25 bars",
        "recipeIngredient": [
            "6 ounces ground almonds",
            "\u00be cup white sugar",
            "1 pinch salt",
            "1 egg",
            "2 egg whites",
            "6 eggs",
            "3 cups white sugar",
            "3 tablespoons potato starch",
            "2 large lemons, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). In a medium bowl, mix together the almonds, 3/4 cup sugar, 1 egg and egg whites. Spread evenly in the bottom of a 9x11 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 minutes in the preheated oven, or until the top is dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, whisk together 6 eggs, 3 cups sugar, potato starch and lemon juice until well blended and smooth. Pour over the crust, and return to the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 minutes, or until the center of the bars no longer appears liquid. Cool over a wire rack, and cut into bars while still warm. The lemon topping will now be on the bottom, so invert the pan onto a plate to remove bars, and serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
