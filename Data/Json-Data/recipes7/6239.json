    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219970/bethanys-favorite-valentine-cut-out-sugar-cookies/",
        "name": "Bethany's Favorite Valentine Cut Out Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F789167.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2020-06-19T03:47:18.000Z",
        "description": "Soft and moist with a light vanilla and almond flavor. Got these from a friend's wedding.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT3H25M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup white sugar",
            "1 egg, beaten",
            "2 tablespoons milk",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon almond extract",
            "2\u2009\u00bd cups flour",
            "1 teaspoon baking powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together the butter, sugar, egg, milk, vanilla extract, and almond extract in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the flour and baking powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and refrigerate until firm, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the dough into thirds. On a lightly floured surface, roll out each portion to 1/4-inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut cookies out using a floured cookie cutter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange cookies on an ungreased baking sheet about 1-inch apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until cookies are lightly golden, 6 to 9 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cookies to a wire rack to cool completely.\n"
            }
        ],
