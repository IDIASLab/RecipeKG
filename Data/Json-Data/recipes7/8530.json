    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230521/almond-butter-blossoms/",
        "name": "Almond Butter Blossoms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1902638.jpg",
            "width": 2011,
            "height": 2011
        },
        "datePublished": "2020-06-19T03:52:40.000Z",
        "description": "This is a New Year's Eve tradition for my husband and I. It's an absolutely delicious new twist to the traditional peanut butter cookies, though they look almost exactly the same.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "4 dozen cookies",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "\u00bd cup almond butter",
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "\u00be teaspoon baking soda",
            "\u00bd teaspoon cream of tartar",
            "\u00bc teaspoon salt",
            "1 egg",
            "2 tablespoons milk",
            "\u00bd teaspoon vanilla extract",
            "1 cup whole wheat flour",
            "1 cup all-purpose flour",
            "3 tablespoons white sugar",
            "2 tablespoons ground almonds",
            "48 milk chocolate candy kisses with almond, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and almond butter in a large mixing bowl with an electric mixer on medium-high speed until creamy, about 30 seconds. Beat in 1/2 cup sugar, brown sugar, baking soda, cream of tartar, and salt until thoroughly combined. Beat egg, milk, and vanilla extract into moist ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat whole wheat flour and as much of the all-purpose flour as the mixer can handle; stir remaining flour into dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 3 tablespoons sugar with ground almonds in a shallow bowl. Pinch off dough and roll into 1-inch balls; roll balls in almond-sugar mixture. Place cookies 2 inches apart on ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are firm and tops are cracked, 8 to 10 minutes. Immediately press a chocolate almond kiss into middle of each cookie; let cool.\n"
            }
        ],
