    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244859/hawaiian-pork-chops/",
        "name": "Hawaiian Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8272927.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T04:03:37.000Z",
        "description": "Pork chops with a sweet pineapple gravy are great served with mashed potatoes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup all-purpose flour",
            "salt and ground black pepper to taste",
            "4 pork chops",
            "\u00bd cup shortening (such as Crisco\u00ae)",
            "\u00be cup water",
            "\u00bd (12 fluid ounce) can frozen pineapple juice concentrate",
            "1 teaspoon white sugar",
            "1 teaspoon lemon juice",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "1 (8 ounce) can pineapple rings",
            "1 tablespoon water",
            "1 tablespoon cornstarch"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, salt, and pepper together in a shallow bowl. Press pork chops into flour mixture until coated on both sides; shake excess flour back into bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat shortening in a large skillet over medium heat; cook pork chops until browned, about 3 minutes per side. Drain excess fat from skillet and return pork chops to skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3/4 cup water, pineapple juice, sugar, lemon juice, cinnamon, and nutmeg in skillet with pork chops. Cover skillet and simmer until cooked through and sauce is somewhat reduced, about 30 minutes. Add pineapple rings and cook 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pork chops and pineapple from skillet. Combine 1 tablespoon water and cornstarch in a bowl; stir into sauce in the skillet until thickened. Serve sauce with pork chops and pineapple.\n"
            }
        ],
