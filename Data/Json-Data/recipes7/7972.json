    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229314/vegetarian-pumpkin-chili/",
        "name": "Vegetarian Pumpkin Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F923660.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:51:34.000Z",
        "description": "Wonderful twist for Halloween season or entertaining. The mix of sweet and spicy will amaze your taste buds!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT9H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound dry black beans",
            "cold water, or as needed",
            "\u00bd cup lentils",
            "2 tablespoons dry roux mix",
            "1 green bell pepper, diced",
            "1 (10.75 ounce) can  condensed tomato soup",
            "1 cup pumpkin puree",
            "1 (1.25 ounce) package chili seasoning mix",
            "2 tablespoons white sugar",
            "1 tablespoon ground cinnamon",
            "1 teaspoon nutmeg",
            "1 teaspoon ground cloves",
            "\u00bd teaspoon onion powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place black beans into a large bowl and cover with several inches cool water; let soak 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain and rinse beans thoroughly; place in a large pot. Cover with 1 inch of water; add lentils. Bring mixture to a boil; stir in roux mix until fully incorporated. Cover, reduce heat, and simmer until beans are tender, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place diced bell pepper in a microwave-safe bowl; cook in the microwave until slightly tender, 30 to 60 seconds. Stir bell pepper, tomato soup, pumpkin puree, chili seasoning mix, sugar, cinnamon, nutmeg, cloves, and onion powder into bean mixture. Cover and simmer until flavors have blended, about 15 minutes, stirring occasionally.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Squash Soup Recipes",
            "Pumpkin Soup Recipes"
        ],
