    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270664/turkey-bacon-flatbread-pizza/",
        "name": "Turkey Bacon Flatbread Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6415715.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-02-13T04:42:43.000Z",
        "description": "Take your pizza game to a whole new level with this delicious recipe idea featuring Pure Farmland&reg; Turkey Bacon.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 slices Pure Farmland\u00ae Uncured Turkey Bacon, cut into 1-inch pieces",
            "2 teaspoons extra-virgin olive oil",
            "1 teaspoon minced garlic",
            "1 (14 ounce) package par-baked flatbread pizza crust",
            "1 cup shredded mozzarella cheese",
            "8 grape tomatoes, cut in half",
            "\u00bc cup fresh basil leaves",
            "2 teaspoons balsamic glaze"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook bacon pieces in a medium nonstick skillet over medium heat until cooked through and browned, 8 to 10 minutes. Transfer to a plate lined with a paper towel and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine olive oil and garlic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place flatbread on a baking sheet and brush with garlic oil. Sprinkle cheese evenly over flatbread. Top with cooked bacon and tomato halves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake pizza for 10 minutes or until cheese is melted. Remove from oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place basil leaves on pizza. Drizzle balsamic glaze on top. Cut pizza into slices and serve.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pizza Recipes"
        ],
