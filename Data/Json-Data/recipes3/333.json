        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6855/easy-biscuits/",
        "name": "Easy Biscuits",
        "image": {
            "@type": "ImageObject",
