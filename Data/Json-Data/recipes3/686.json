    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9965/mexican-wedding-cakes-ii/",
        "name": "Mexican Wedding Cakes II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2250046.jpg",
            "width": 1900,
            "height": 1900
        },
        "datePublished": "1997-09-07T03:09:24.000Z",
        "description": "Light, crunchy, round, buttery balls, with chopped nuts in them, coated with confectioner's sugar.   These look nice when placed on a tray in the mini paper cupcake liners.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "42 cookies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "8 tablespoons confectioners' sugar",
            "2 cups all-purpose flour",
            "2 cups chopped walnuts",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix all the ingredients together with a mixer until well blended. Roll dough into round small balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10-12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool completely then roll in additional confectionary sugar.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Latin American",
            "Mexican"
        ],
        "recipeCuisine": [
            "Latin American",
            "Mexican"
        ],
