    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257834/sugar-free-sugar-cookies-with-sugar-free-sugar-icing/",
        "name": "Sugar-Free \"Sugar\" Cookies with Sugar-Free \"Sugar\" Icing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4528416.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-04-13T01:53:20.000Z",
        "description": "These cookies don't have to be a guilty pleasure!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00bd cup unsalted butter, softened",
            "\u00bc cup Pyure Organic All Purpose Stevia Blend",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "\u00bd cup cornstarch",
            "3 tablespoons Pyure Organic All Purpose Stevia Blend",
            "2 tablespoons unsalted butter, softened",
            "\u00bc cup milk, or as needed",
            "1 teaspoon vanilla extract",
            "1 drop Food coloring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat butter with Pyure Organic All Purpose Stevia Blend until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in egg until well blended. Beat in vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder and salt together. Stir into butter mixture in two additions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap dough in plastic wrap and refrigerate for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll on lightly floured surface or between sheets of waxed paper to 1/4-inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using desired cookie cutter, cut out desired shapes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Place cookies 2-inches apart on parchment paper lined baking sheets. Bake for 10 to 12 minutes or until golden. Let cool on racks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Glaze cookies with Pyure Sugar Free Cookie Icing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Icing Directions: Beat cornstarch, Pyure Organic All Purpose Stevia Blend and butter with a mixer on medium-high speed until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually add milk and vanilla until smooth. Add more milk if icing mixture is too dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "If adding food coloring, add a drop at a time until desired color is reached.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate icing for 10 minutes until set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread or pipe over cooled Sugar Free \"Sugar\" Cookies. Use day of.\n"
            }
        ],
        "recipeCategory": [
