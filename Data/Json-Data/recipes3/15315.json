    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282923/baked-lobster-tails/",
        "name": "Baked Lobster Tails",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Baked Lobster Tails"
        },
        "datePublished": "2018-07-19T01:17:08.000Z",
        "description": "Tender, juicy small lobster tails with lemon and garlic zest. Serve with more melted butter.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 lobster tails",
        "recipeIngredient": [
            "8 small lobster tails",
            "\u00bd cup salted butter, melted",
            "\u00bd tablespoon garlic powder",
            "1 teaspoon dried parsley",
            "1 pinch seafood seasoning (such as Old Bay\u00ae), or to taste",
            "\u00bd lemon, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the lobster tails straight down the middle using kitchen shears. Butterfly the lobster meat by pulling it through the opening and laying the meat on top of the shell. Rinse tails under water; place in an oven-safe baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine melted butter, garlic powder, and parsley in a bowl. Mix together with a spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season lobster tails with seafood seasoning and pour melted butter mixture on top until each tail is completely covered. Squeeze lemon juice over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until shells are bright red and meat is opaque, 12 to 15 minutes. An instant-read thermometer inserted into the centers should read 140 to 145 degrees F (60 to 63 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Lobster"
        ],
