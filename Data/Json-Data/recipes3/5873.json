        "mainEntityOfPage": "https://www.allrecipes.com/recipe/87319/heart-attack-eggs/",
        "name": "Heart Attack Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4517729.jpg",
