    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267322/no-churn-rainbow-mermaid-ice-cream/",
        "name": "No-Churn Rainbow Mermaid Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5770015.jpg",
            "width": 3017,
            "height": 3018
        },
        "datePublished": "2018-08-31T08:53:44.000Z",
        "description": "This recipe makes 1 quart of rainbow-colored ice cream! Use the colors of your choice.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT5H15M",
        "recipeYield": "1 quart",
        "recipeIngredient": [
            "2 cups heavy cream",
            "2 teaspoons vanilla extract",
            "1 (14 ounce) can sweetened condensed milk",
            "3 drops red food coloring",
            "3 drops purple food coloring",
            "3 drops blue food coloring",
            "3 drops green food coloring",
            "3 tablespoons multicolored candy sprinkles, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a freezer-safe glass or metal loaf pan in the freezer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine heavy cream and vanilla extract in the bowl of a stand mixer. Beat on high speed until stiff peaks form. Gently fold in condensed milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture among 4 bowls. Add one shade of food coloring to each bowl. Stir until food coloring is thoroughly mixed in.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the loaf pan from the freezer. Add the colored mixtures a few scoops at a time, alternating colors and layering them over each other, until loaf pan is full. Drag a butter knife through the entire mixture 2 to 3 times to swirl colors together. Top with sprinkles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the loaf pan with plastic wrap and freeze until ice cream is firm, 5 to 6 hours.\n"
            }
        ],
