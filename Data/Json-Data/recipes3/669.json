    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9606/german-chocolate-bars/",
        "name": "German Chocolate Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1814687.jpg",
            "width": 2940,
            "height": 2940
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These are just like German chocolate cake, but you can eat them with your hands!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "20 bars",
        "recipeIngredient": [
            "1 (14 ounce) package individually wrapped caramels, unwrapped",
            "\u2153 cup sweetened condensed milk",
            "1 (18.25 ounce) package German chocolate cake mix",
            "\u00be cup butter, melted",
            "\u00bd cup sweetened condensed milk",
            "1 cup semisweet chocolate chips",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Grease and flour 13 x 9 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt caramels with 1/3 cup condensed milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, combine cake mix with melted butter and 1/2 cup of condensed milk.  Stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1/2  of the cake mix mixture in pan and bake for 6 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and cover with caramel, chips and nuts.  Put remaining cake mix on top.  Bake for another 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
