        "name": "Hidden Valley&reg; Easy Slow Cooker White Chicken Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5433937.jpg",
