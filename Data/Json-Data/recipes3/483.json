        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8297/punch-bowl-cake-ii/",
        "name": "Punch Bowl Cake II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2110217.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Can use cherry or strawberry pie filling or fresh, sliced, sweetened strawberries.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 large punch bowl",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "2 (5 ounce) packages instant vanilla pudding mix",
            "5 cups milk",
            "6 bananas",
            "2 (21 ounce) cans cherry pie filling",
            "2 (20 ounce) cans crushed pineapple, drained",
            "6 ounces flaked coconut",
            "1 (16 ounce) package frozen whipped topping, thawed",
            "\u00bc cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bake cake mix according to package directions for 2 layers, let cakes cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare the pudding according to package with the 5 c. milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large punch bowl, start with one layer of the cake, broken up into pieces. Spread with half of the pudding, then slice three bananas over the pudding. Then layer 1 can drained pineapple, one can pie filling and half of the coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Repeat in the same order with the remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with whipped topping, then sprinkle with chopped walnuts. Chill and serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cherry Dessert Recipes"
        ],
