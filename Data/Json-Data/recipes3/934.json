        "recipeIngredient": [
            "1 (3.9 ounce) package instant chocolate pudding mix",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "1 cup crushed chocolate sandwich cookies",
            "1 (9 inch) prepared chocolate cookie crumb crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare pudding as directed for pie filling; let set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When pudding is set, fold in whipped topping and crushed cookies.  Pour pudding mixture into crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze overnight.  Serve.\n"
            }
