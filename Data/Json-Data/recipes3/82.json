        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14842/stuffed-cherry-peppers/",
        "name": "Stuffed Cherry Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5828277.jpg",
            "width": 1074,
