        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/82803/sweet-sour-brussels-sprouts/",
        "name": "Sweet & Sour Brussels Sprouts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5238984.jpg",
