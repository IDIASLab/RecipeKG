    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8502/cranberry-chicken-i/",
        "name": "Cranberry Chicken I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4477153.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-04-19T19:03:07.000Z",
        "description": "This is an incredibly easy recipe - and delicious!  Just mix it up before going to work, then pop it in the oven when you get home in the evening.  The sauce that forms while baking tastes great on rice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT9H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 skinless, boneless chicken breasts",
            "1\u2009\u00bd (1 ounce) packages dry onion soup mix",
            "1 (16 ounce) can jellied cranberry sauce",
            "1 cup French dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the chicken breasts in a glass or non-reactive baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the onion soup mix, cranberry sauce, and French dressing together in a bowl until well blended. Pour over the chicken breasts. Cover the baking dish with plastic wrap, and refrigerate at least 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove plastic wrap from the baking dish, and cover loosely with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken in preheated oven until top is bubbly and slightly browned, about 1 hour  and 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
