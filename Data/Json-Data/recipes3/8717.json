    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221196/overnight-ham-and-cheese-omelet/",
        "name": "Overnight Ham and Cheese Omelet",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F876679.jpg",
            "width": 4320,
            "height": 3240
        },
        "datePublished": "2018-06-08T14:01:19.000Z",
        "description": "We enjoy this omelet every Christmas morning! Throw the ingredients together the night before, put it in the refrigerator, and pop it in the oven the next morning. So simple and delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT9H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 teaspoon butter for greasing, or as needed",
            "12 slices firm bread, discard crusts and cube bread",
            "1 cup cubed Cheddar cheese",
            "1 cup cubed ham",
            "3 cups milk",
            "6 large eggs",
            "\u00bd teaspoon dried thyme",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter bottom and sides of a shallow roasting dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer bread, Cheddar cheese, and ham in prepared roasting dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat milk, eggs, thyme, salt, and black pepper together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour egg mixture over bread mixture; cover and refrigerate 8 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered egg mixture in the preheated oven until center is set, about 1 hour. Allow to sit and cool for 15 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Omelet Recipes"
