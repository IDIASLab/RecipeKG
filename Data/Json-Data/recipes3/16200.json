    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267967/low-carb-stuffed-peppers/",
        "name": "Low-Carb Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5783711.jpg",
            "width": 3188,
            "height": 2126
        },
        "datePublished": "2018-09-18T23:53:19.000Z",
        "description": "Enjoy these traditional savory stuffed peppers without all the extra carbohydrates! My family loves these! You can use any kind of ground sausage, hot or mild, your choice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 stuffed peppers",
        "recipeIngredient": [
            "cooking spray (such as Pam\u00ae)",
            "1\u2009\u00bd pounds ground sirloin",
            "\u00bd pound bulk pork sausage",
            "6 Roma tomatoes, chopped, divided",
            "1 small white onion, finely diced",
            "1 tablespoon butter",
            "2 teaspoons crushed garlic",
            "1 teaspoon dried oregano, or to taste",
            "\u00bd teaspoon fennel seed, or to taste",
            "seasoned pepper to taste",
            "4 large green bell peppers, tops and seeds removed",
            "6 ounces crumbled feta cheese",
            "\u00bd cup grated Parmesan cheese",
            "\u00bd cup shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Coat a casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook ground sirloin and sausage in a skillet over medium-low heat until browned and crumbly, stirring often, 5 to 10 minutes. Drain thoroughly. Mix in 3/4 the tomatoes, onion, butter, garlic, oregano, fennel seed, and seasoned pepper. Simmer over low heat until meat mixture comes together, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill bell peppers with meat mixture, alternating with layers of feta cheese and Parmesan cheese. Place stuffed bell peppers side-by-side in the prepared casserole dish; add remaining tomatoes around and under the peppers so they cook up and into the peppers. Sprinkle with mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned and bubbly, about 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Sausage"
        ],
