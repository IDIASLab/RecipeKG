    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234038/canadian-tea-biscuits/",
        "name": "Canadian Tea Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060918.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2018-06-08T14:09:55.000Z",
        "description": "A delicious recipe with a mingling of melt-in-your-mouth flavor!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 biscuits",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon salt",
            "\u2153 cup shortening",
            "\u00bd cup shredded Cheddar cheese",
            "2 tablespoons chopped fresh chives",
            "1 cup milk, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together flour, baking powder, and salt in a bowl. Cut in shortening with a knife or pastry blender until the mixture resembles coarse crumbs. Stir Cheddar cheese and chives into flour mixture; gradually stir in milk to form a soft dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out onto a lightly floured surface and with floured hands, knead until smooth, about 15 times. Roll dough out to a 1-inch thickness with a floured rolling pin. Cut biscuits with a 2 1/2-inch round cookie cutter; place onto an ungreased baking sheet. Press dough trimmings together, roll out again, and cut remaining dough into rounds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Biscuits"
        ],
