        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71600/easy-arugula-salad/",
        "name": "Easy Arugula Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1131650.jpg",
