    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264182/potato-bake-with-herbs/",
        "name": "Potato Bake with Herbs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5343694.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2018-07-25T13:03:08.000Z",
        "description": "A delicious potato side dish with fresh herbs and cheese. You can use any type of herbs you like. I prefer a mild cheese like Gruyere, but you can use Gouda or Emmenthal as well.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 pounds small potatoes, scrubbed",
            "1 teaspoon butter",
            "3 tablespoons butter, melted",
            "1 teaspoon coarse salt, or to taste",
            "5 tablespoons finely chopped fresh chervil",
            "2 bunches fresh chives, finely chopped",
            "1\u2009\u00bc cups shredded Gruyere cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease a baking dish with 1 teaspoon butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel potatoes and cut in half lengthwise. Place in the prepared baking dish. Drizzle with melted butter and season with coarse salt. Sprinkle with chervil and chives. Cover with Gruyere cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and lightly browned, about 30 minutes.\n"
            }
        ],
