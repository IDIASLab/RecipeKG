        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 tablespoon plain yogurt",
            "2 teaspoons tahini",
            "2 teaspoons za'atar",
            "salt and ground black pepper to taste",
            "1 tablespoon chopped cornichon pickle",
            "4 hard-boiled eggs, peeled and coarsely chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix yogurt, tahini, za'atar, salt, and pepper together in a bowl; fold in pickle. Gently stir eggs into tahini mixture.\n"
            }
