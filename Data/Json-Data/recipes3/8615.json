    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220636/quick-and-easy-cheesy-red-scalloped-potatoes/",
        "name": "Quick and Easy Cheesy Red Scalloped Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F874072.jpg",
            "width": 1860,
            "height": 1514
        },
        "datePublished": "2018-06-08T14:00:42.000Z",
        "description": "Boiling these potatoes first cuts down on cooking time, making them ready to eat even quicker.  My friends can't get over how delicious these potatoes taste! Using three different cheeses gives it awesome flavor!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "12 small red potatoes, sliced",
            "1 cube chicken bouillon",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 cup milk",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon onion powder",
            "\u00bc teaspoon salt",
            "\u215b teaspoon ground black pepper",
            "3 slices provolone cheese, torn into small pieces",
            "\u00bc cup grated Parmesan cheese",
            "\u00bd cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a 2-quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes and bouillon cube in a large pot and cover with water. Bring to a boil, then reduce heat to medium-low and simmer until tender, 10 to 15 minutes. Drain and return potatoes to empty pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together cream of chicken soup, milk, garlic powder, onion powder, salt, and black pepper in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup mixture over potatoes and mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour potato mixture into prepared casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir provolone cheese and Parmesan cheese into potato mixture. Top with a layer of Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until bubbling and cheese topping is lightly browned, about 25 minutes. Let stand for 5 minutes before serving.\n"
            }
        ],
