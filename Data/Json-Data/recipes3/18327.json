    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270281/keto-broccoli-salad/",
        "name": "Keto Broccoli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6416606.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2019-01-26T10:13:35.000Z",
        "description": "This keto broccoli salad can be eaten as a main dish.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound bacon",
            "2 cups broccoli florets, chopped",
            "2 cups cauliflower florets, chopped",
            "1 carrot, chopped",
            "\u00bc cup chopped green onions",
            "\u00bd cup mayonnaise",
            "\u00bc cup distilled white vinegar",
            "1 tablespoon sour cream",
            "1 pinch garlic powder",
            "1 pinch dried basil",
            "\u00bc cup shredded Cheddar cheese",
            "1 tablespoon chopped fresh cilantro, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Chop and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine broccoli, cauliflower, carrot, and green onions in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mayonnaise, vinegar, and sour cream in a separate bowl. Beat until smooth. Season with garlic powder and basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour dressing over vegetables and toss to coat evenly. Chill until flavors meld, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve salad garnished with Cheddar cheese and cilantro.\n"
            }
        ],
