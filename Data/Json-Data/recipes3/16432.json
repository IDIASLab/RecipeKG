        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13674/easy-spinach-casserole/",
        "name": "Easy Spinach Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F697096.jpg",
