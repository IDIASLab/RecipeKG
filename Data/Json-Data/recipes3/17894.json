        "recipeYield": "12 squares",
        "recipeIngredient": [
            "1 cup semisweet chocolate chips",
            "1 cup miniature marshmallows",
            "1 cup granola",
            "\u00bd cup peanuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a small glass container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in a microwave-safe glass bowl in 15-second intervals, stirring after each interval, 1 to 3 minutes. Add marshmallows, granola, and peanuts. Spread mixture into the prepared container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze or refrigerate until hard, about 1 hour. Cut into squares and store in the refrigerator.\n"
            }
        ],
