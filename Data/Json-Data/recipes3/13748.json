        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261129/parsley-mushroom-gravy/",
        "name": "Parsley-Mushroom Gravy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5147993.jpg",
