        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23982/easy-vegan-pasta-sauce/",
        "name": "Easy Vegan Pasta Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4530188.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2018-06-08T13:15:14.000Z",
        "description": "This is the most beautiful tasting low-fat pasta sauce you'll ever taste! Don't wait to try it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "1 teaspoon vegetable oil",
            "\u00bd small yellow onion, diced",
            "2 cloves garlic, minced",
            "5 large tomatoes, cubed",
            "1 small green bell pepper, diced",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon black pepper",
            "1 teaspoon dried basil leaves",
            "\u00bd teaspoon dried oregano"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a skillet over medium-low heat, saute onion and garlic in the vegetable oil.  Place tomatoes into onion and garlic mixture. Stir in diced bell pepper, salt, pepper, basil and oregano.  Let simmer for 20 minutes, stirring occasionally.  Turn down heat if it starts to stick.\n"
            }
        ],
