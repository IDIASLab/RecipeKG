    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232575/moist-spicy-turkey-burgers/",
        "name": "Moist Spicy Turkey Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1023667.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-08T14:08:36.000Z",
        "description": "Since I love food with lots of flavor and spice, this recipe is definitely a winner! Very moist and full of flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "4 burgers",
        "recipeIngredient": [
            "1 pound ground turkey",
            "\u00bc cup dry bread crumbs",
            "1 onion, finely chopped",
            "1 green onion, chopped",
            "1 egg",
            "2 cloves garlic, minced",
            "2 tablespoons parsley paste",
            "1\u2009\u00bd tablespoons Worcestershire sauce",
            "1 tablespoon hot pepper sauce (such as Tabasco\u00ae)",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix ground turkey, bread crumbs, onion, green onion, egg, garlic, parsley paste, Worcestershire sauce, hot pepper sauce, and salt together in a bowl. Cover bowl with plastic wrap. Refrigerate at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form turkey mixture into 4 patties; arrange in a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake turkey burgers in the oven until no longer pink in the center and the juices run clear, about 30 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
