    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220425/halibut-baked-with-sauerkraut/",
        "name": "Halibut Baked with Sauerkraut",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Halibut Baked with Sauerkraut"
        },
        "datePublished": "2018-06-08T14:00:30.000Z",
        "description": "Who knew they would make great companions AND create their own entree and side dish? This dish is quick, easy, and fresh!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H22M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "4 (4 ounce) portions of halibut fillet",
            "salt and ground black pepper to taste",
            "4 cups sauerkraut, drained (such as Claussen\u00ae)",
            "1 cup diced tomatoes",
            "1 teaspoon malt vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a 9x12-inch glass baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pat dry halibut fillets with paper towels. Season each side with salt and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place fillets in prepared baking dish; top each fillet with sauerkraut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread tomatoes over fillets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the dish loosely with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the fish flakes easily with a fork, 12 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with malt vinegar before serving, if desired.\n"
            }
        ],
