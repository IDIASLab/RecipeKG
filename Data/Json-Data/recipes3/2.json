        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14465/orange-gelatin-salad/",
        "name": "Orange Gelatin Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4539186.jpg",
            "width": 1280,
            "height": 960
