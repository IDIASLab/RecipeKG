    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263897/fig-scones/",
        "name": "Fig Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2390402.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "2018-04-27T03:34:15.000Z",
        "description": "Gluten-free version of fig scones that uses fresh figs! This is great during fresh fig season!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 scones",
        "recipeIngredient": [
            "1 cup rice flour",
            "\u00bd cup rolled oats",
            "2 teaspoons baking powder",
            "\u00bc cup honey",
            "\u00bc cup oil",
            "1 egg",
            "\u00bd cup chopped fresh figs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine rice flour, oats, and baking powder in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix honey, oil, and egg together in a small bowl. Combine with the flour mixture. Stir figs into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread dough into a 7-inch circle onto the parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 10 minutes. Score dough into 8 wedges. Continue baking scones until golden brown, about 15 minutes more.\n"
            }
        ],
