    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256589/creamy-sun-dried-tomato-couscous/",
        "name": "Creamy Sun-Dried Tomato Couscous",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4614850.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "2018-06-08T14:41:58.000Z",
        "description": "This savory and satisfying one-pot meal is quick to make and devoured even faster! Great as a vegetarian entree or pairs well with any protein. Wonderful creamy texture without adding butter or cream. This will find its way into your weekly rotation!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H19M",
        "totalTime": "P0DT0H39M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons extra-virgin olive oil",
            "\u00bd small sweet onion, chopped",
            "1 small Chinese eggplant, chopped",
            "\u00bd zucchini, chopped",
            "2 ounces baby bella mushrooms, chopped",
            "salt and ground black pepper to taste",
            "2 ounces sun-dried tomatoes packed in oil, drained",
            "2 cloves garlic, chopped",
            "2 teaspoons dried basil",
            "1\u2009\u00bd cups vegetable stock",
            "1 cup tri-colored pearl couscous",
            "2 tablespoons sun-dried tomato oil",
            "1 tablespoon shaved Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat; cook and stir onion, eggplant, zucchini, mushrooms, salt, and pepper until onion is translucent, 5 to 7 minutes. Add tomatoes, garlic, and basil; cook and stir for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable stock into the skillet; bring to a boil. Add couscous, stir well, and cover skillet; simmer until couscous is tender yet firm to the bite, 7 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir tomato oil into couscous mixture and top with Parmesan cheese.\n"
            }
        ],
