        "recipeIngredient": [
            "1 pound ripe strawberries, hulled",
            "1 (6 ounce) can frozen limeade concentrate",
            "4 cups water",
            "\u00bc teaspoon kosher salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree strawberries and frozen limeade in a blender with 2 cups water. Strain into a pitcher and stir in remaining 2 cups water and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place in the refrigerator until fully chilled, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve over ice. Refrigerate leftovers.\n"
            }
