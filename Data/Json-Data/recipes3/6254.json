        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/105039/whole-wheat-drop-biscuits/",
        "name": "Whole Wheat Drop Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7804697.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-08T13:29:40.000Z",
        "description": "These are quick, easy, vegan biscuits.  I especially like them with soup or vegetarian chili.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "16 biscuits",
        "recipeIngredient": [
            "1 cup whole wheat flour",
            "1 cup all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon salt",
            "\u00bc cup canola oil",
            "\u00be cup unsweetened soy milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the whole wheat flour, all-purpose flour, baking powder, and salt.  Combine the oil and soy milk in a measuring cup. Pour into the dry ingredients all at once, and stir just until the dough pulls away from the sides of the bowl. Drop by heaping spoonfuls onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to10 minutes in the preheated oven, until the biscuits are browned on the top and bottom.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegan",
            "Breakfast and Brunch"
        ],
