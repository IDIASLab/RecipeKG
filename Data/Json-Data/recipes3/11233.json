    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240667/creamy-potato-and-leek-soup/",
        "name": "Creamy Potato and Leek Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1926495.jpg",
            "width": 1536,
            "height": 2048
        },
        "datePublished": "2018-06-08T14:18:44.000Z",
        "description": "Not only do my somewhat picky kids love to eat this soup, they love to help me make it too...delicious and simple!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "3 leeks (white and pale green parts only), sliced",
            "1 tablespoon chopped garlic",
            "salt and ground black pepper to taste",
            "1 quart chicken broth",
            "1 tablespoon cornstarch",
            "5\u2009\u00bd cups peeled and diced potatoes, or more to taste",
            "1 tablespoon onion powder",
            "2 teaspoons garlic powder",
            "2 cups half-and-half"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large pot over medium heat. Cook and stir leeks, garlic, salt, and pepper in the hot butter until leeks are tender and lightly browned, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chicken broth and cornstarch together in a bowl until cornstarch is dissolved; pour over the leek mixture. Add potatoes to the broth mixture and bring to a boil; season with onion powder and garlic powder. Reduce heat, add half-and-half to broth mixture, and simmer until potatoes are tender, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pot from heat and blend soup using a hand blender until smooth; season with salt and pepper.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Potato Soup Recipes"
        ],
