    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268250/baked-honey-sriracha-salmon/",
        "name": "Baked Honey Sriracha Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8162273.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-09-22T07:13:49.000Z",
        "description": "Sweet and spicy, honey sriracha salmon baked in foil is tender and flaky and has the most incredible flavors. A nutritious and easy 30-minute meal for salmon lovers!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 (4 ounce) fillets salmon fillets",
            "1 teaspoon salt, or to taste",
            "\u00bc teaspoon ground black pepper, or to taste",
            "1 lime, sliced",
            "2 tablespoons butter",
            "\u2153 cup honey",
            "\u2153 cup sriracha sauce",
            "1 lime, juiced",
            "2 tablespoons low-sodium soy sauce",
            "1 teaspoon minced garlic",
            "1 tablespoon chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with foil, grease lightly, and lay salmon on top. Season salmon with salt and pepper. Slide lime slices under the edges of the salmon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a medium saucepan over medium-high heat. Stir in honey, sriracha sauce, lime juice, soy sauce, and garlic. Bring to a boil. Reduce heat to medium and cook for 3 to 4 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 2/3 of the sauce over the salmon. Use a spoon or spatula to make sure the sauce covers the salmon, leaving no dry spots. Fold up edges of the foil around the salmon to keep the sauce from spilling out.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Broil until the edges of the salmon start to char slightly, 4 to 5 minutes. Watch carefully so the salmon doesn't burn.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top salmon with remaining sauce and garnish with cilantro.\n"
            }
        ],
