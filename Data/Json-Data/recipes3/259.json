    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15793/ice-cream-snowball-pie/",
        "name": "Ice Cream Snowball Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Ice Cream Snowball Pie"
        },
        "datePublished": "1999-06-17T00:57:18.000Z",
        "description": "This show-stopping dessert takes less than 30 minutes to prepare!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "2 (4 ounce) bars  German sweet chocolate",
            "3 tablespoons butter, melted",
            "2 cups crisp rice cereal",
            "2 tablespoons butter",
            "1 pint vanilla ice cream",
            "1 pint mint chocolate chip ice cream",
            "1 pint strawberry ice cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Lightly grease a 9-inch pie pan. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coarsely chop chocolate. Set aside 1/4 of it.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Crust: Place remaining 3/4 of chocolate in a small saucepan with 3 tablespoons butter or margarine. Melt over low heat, stirring frequently, until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place crisp rice cereal in a medium mixing bowl. Pour chocolate mixture over cereal, and stir gently to coat. Using the back of a spoon, press mixture onto bottom and sides of greased pie pan. Place crust in freezer just until firm, about 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Chocolate Topping: Place remaining 1/4 of chopped chocolate and 2 tablespoons of butter or margarine in a small saucepan. Melt over low heat, stirring frequently, until smooth. Cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from freezer and arrange scoops of vanilla, mint chocolate chip, and strawberry ice cream in it. Drizzle chocolate mixture over top and serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frozen Dessert Recipes",
