        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13972/lite-brocamole/",
        "name": "Lite Brocamole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F24371.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1999-07-25T13:30:00.000Z",
        "description": "This lite version of guacamole can be used as a delicious appetizer with tortilla chips, or add as a topping to your favorite Tex-Mex recipes. Use more lime juice to make this dip extra tangy.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 to 3 cups",
        "recipeIngredient": [
            "1 avocado, peeled and pitted",
            "1 large head fresh broccoli",
            "2 tablespoons red onion, minced",
            "4 cloves garlic, minced",
            "2 tablespoons lime juice",
            "1 tablespoon hot pepper sauce",
            "2 tablespoons chopped fresh cilantro",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Boil or steam broccoli until very soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mash broccoli with avocado.  Mix in onions, garlic, lime juice, hot pepper sauce, and cilantro until mixture resembles the consistency of mashed potatoes.  Season to taste with salt.\n"
            }
        ],
