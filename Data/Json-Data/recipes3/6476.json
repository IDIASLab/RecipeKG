    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/131751/swiss-cheese-meatloaf/",
        "name": "Swiss Cheese Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F281830.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:44:12.000Z",
        "description": "This is a tasty twist to the traditional meatloaf.  It is one of my families favorites!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 - 3x7 inch meatloaf",
        "recipeIngredient": [
            "2 eggs",
            "16 buttery crackers, crushed",
            "1 small onion, chopped",
            "2 teaspoons minced garlic",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bd teaspoon rubbed sage",
            "1 cup shredded Swiss cheese, divided",
            "1 pound lean ground beef"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 375 degrees F (190 degrees C). Grease a 3x7 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the eggs in a bowl to mix. Whisk in the cracker crumbs, onion, garlic, salt, pepper, and sage. Set aside 3 tablespoons of the Swiss cheese to use as a topping, and stir the rest into the egg mixture along with the ground beef. Pack the mixture into the prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until no longer pink in the center, about 1 hour. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C). About 2 minutes before the meatloaf is ready, sprinkle with the reserved Swiss cheese, and return to the oven until melted.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Beef Meatloaf Recipes"
        ],
