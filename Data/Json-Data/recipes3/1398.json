        "name": "Ha Ha Juice (a.k.a. Party Punch)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F617069.jpg",
            "width": 250,
