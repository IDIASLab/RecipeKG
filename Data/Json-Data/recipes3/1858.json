    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8026/sugarless-fruitcake/",
        "name": "Sugarless Fruitcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F288029.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-09-02T03:09:06.000Z",
        "description": "Pineapple, musts, cranberries, and coconut combine in this moist fruitcake using artificial sweetener in place of sugar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch loaf cake",
        "recipeIngredient": [
            "1 cup chopped golden raisins",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups chopped walnuts",
            "1 cup chopped cranberries",
            "1 cup unsweetened crushed pineapple",
            "\u00bc cup grated lemon peel",
            "1 cup flaked coconut",
            "\u2153 cup orange juice",
            "8 packets artificial sweetener",
            "1 teaspoon ground mace",
            "1 teaspoon ground allspice",
            "1 teaspoon baking soda",
            "1\u2009\u00bd cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar substitute with orange juice.  Pour over chopped cranberries.  Soak 1 hour, mixing often.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix raisins, nuts, coconut, lemon rind in flour until well coated.  Add cranberries and orange juice mixture.  Sprinkle baking soda over mixture, and mix.  Mix in spices.  Fold in crushed pineapple.  Pour batter into a greased and floured 9 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 40 minutes.  Cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cranberry Dessert Recipes"
        ],
