        "mainEntityOfPage": "https://www.allrecipes.com/recipe/83437/vodka-cocktail-sauce/",
        "name": "Vodka Cocktail Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F871386.jpg",
            "width": 1600,
