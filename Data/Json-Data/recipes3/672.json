    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9739/icebox-cookies-i/",
        "name": "Icebox Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F667375.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "You can make the dough in advance, refrigerate, and make the cookies as you need them....for up to a week.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup unsalted butter",
            "\u215c cup superfine sugar",
            "1 egg yolk",
            "\u00bd teaspoon almond extract",
            "1\u2009\u00be cups sifted all-purpose flour",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, cream together the butter and sugar until light and fluffy. Mix in the egg yolk, vanilla extract and almond extract until well blended. Stir in the flour. Divide the dough in half.  Roll each half into a log shape about 3 inches in diameter.  Wrap in plastic wrap and refrigerate for at least three hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Remove dough from refrigerator and cut 1/8-inch thick slices and place on cookie sheets about 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for about 10 minutes, or until cookies get a tinge of color.  Let cool on wire rack.\n"
            }
        ],
