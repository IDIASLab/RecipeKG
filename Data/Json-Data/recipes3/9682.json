        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231591/easter-spicy-deviled-eggs/",
        "name": "Easter Spicy Deviled Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F996279.jpg",
