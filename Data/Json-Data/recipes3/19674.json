        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14525/beer-steak/",
        "name": "Beer Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F697567.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "1999-09-01T01:38:08.000Z",
        "description": "Grilled steak with a simple beer marinade!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 (1/2 pound) rib-eye steaks, or steak of choice",
            "2 tablespoons sea salt",
            "2 tablespoons lemon pepper",
            "2 (12 fluid ounce) cans or bottles beer of choice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the steaks in a large, shallow container with a lid. Season each side of the steaks with the salt and lemon pepper. Gently pour the beer over the steaks (making sure the seasoning doesn't wash off). Cover, and refrigerate for 1 to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly oil grill grate. Place steaks on grill, and discard beer marinade. Cook for 5 minutes per side, or to desired doneness.\n"
            }
        ],
