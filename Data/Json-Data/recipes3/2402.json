        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14377/red-potato-salad-with-sour-cream-and-chives/",
        "name": "Red Potato Salad with Sour Cream and Chives",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9415645.jpg",
            "width": 0,
            "height": 0
