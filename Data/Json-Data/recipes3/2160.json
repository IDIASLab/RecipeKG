    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12777/beer-batter-for-fish/",
        "name": "Beer Batter for Fish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F972516.jpg",
            "width": 1933,
            "height": 1933
        },
        "datePublished": "1999-07-02T03:22:26.000Z",
        "description": "Basic beer batter, good for almost any white fleshed fish.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "2 to 4 servings",
        "recipeIngredient": [
            "3 eggs",
            "\u00be cup beer",
            "1\u2009\u00bd cups milk",
            "4 cups pastry flour",
            "1 tablespoon baking powder",
            "\u00bd teaspoon baking soda",
            "2 tablespoons cornstarch",
            "salt to taste",
            "ground black pepper to taste",
            ".063 teaspoon garlic powder",
            "1\u2009\u00bd pounds cod fillets",
            "2 quarts vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together flour, baking powder, baking soda, and cornstarch.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat together eggs and milk. Mix in beer.  Stir in flour mixture. Season with salt, black pepper, and garlic powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In an electric deep fryer or a heavy saucepan, heat oil to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat fish in batter, and submerge in hot oil.  Fry until golden brown, about 4 to 5 minutes. Serve.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish"
        ],
        "recipeCuisine": [],
