        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "2 1/2 cups",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 tablespoon salt",
            "1 teaspoon celery salt",
            "1 tablespoon black pepper",
            "2 tablespoons dry mustard",
            "4 teaspoons paprika",
            "1 tablespoon garlic powder",
            "5 teaspoons dried thyme",
            "1 teaspoon dried savory",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon dried basil",
            "\u00bd teaspoon dried oregano"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the flour, salt, celery salt, pepper, dry mustard, paprika, garlic powder, thyme, savory, ginger, basil and oregano. Keep mixture in an airtight container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To use: Steam chicken until tender, dip pieces in beaten egg then roll in the spice mixture. Deep fry for 3 minutes on each side.\n"
            }
        ],
