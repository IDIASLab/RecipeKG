    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233543/incredible-edible-fried-pickles/",
        "name": "Incredible Edible Fried Pickles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3144973.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-08T14:09:26.000Z",
        "description": "Super simple and delicious fried pickles ready in no time!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cups dried plain bread crumbs",
            "1 cup all-purpose flour",
            "2 tablespoons seafood seasoning (such as Old Bay\u00ae)",
            "1 tablespoon garlic powder",
            "1 tablespoon salt",
            "1 tablespoon ground black pepper",
            "\u00bc cup half and half",
            "2 eggs",
            "1 cup dill pickle slices, drained",
            "3 cups peanut oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir bread crumbs, flour, seafood seasoning, garlic powder, salt, and ground black pepper together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat half-and-half and eggs together in a bowl until completely blended. Stir in 1/2 cup pickles and toss to coat completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place coated pickle slices into the bread crumb mixture and toss to coat. Return breaded slices to egg mixture and repeat the entire breading process. Remove slices to a sheet of wax/parchment paper and repeat the entire process for the remaining 1/2 cup of pickle slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat. Place 10 to 15 pickles slices in skillet. Cook until golden brown, 3 to 5 minutes. Repeat with remaining slices.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Pickled"
        ],
