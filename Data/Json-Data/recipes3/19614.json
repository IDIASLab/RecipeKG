        "name": "Spiced Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2017%2F10%2F456923920Spiced20Pumpkin20Seeds20Photo20by20pjudy20650x465.jpg",
