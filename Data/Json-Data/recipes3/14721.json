    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/159254/roasted-creole-potatoes/",
        "name": "Roasted Creole Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F65276.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-13T13:02:11.000Z",
        "description": "My family loves this very versatile recipe.  It can be eaten as a main dish, a side dish, or mixed with eggs for a great breakfast dish.  Delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds cubed white potatoes",
            "12 ounces andouille sausage, sliced",
            "\u00bd cup chopped green bell pepper",
            "\u00bd cup chopped onion",
            "2 tablespoons olive oil",
            "1 tablespoon paprika",
            "1 tablespoon Creole seasoning",
            "1 tablespoon garlic powder",
            "1 teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the potatoes, sausage, green pepper, and onion into a high-sided roasting pan. Drizzle with olive oil, and toss to coat. Sprinkle with paprika, Creole seasoning, garlic powder, and black pepper; toss again to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in preheated oven until the potatoes are tender and beginning to turn golden brown, 45 minutes to 1 hour. Stir occasionally during roasting.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Roasted Potato Recipes"
        ],
