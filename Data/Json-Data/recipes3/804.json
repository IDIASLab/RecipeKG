        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11428/egg-free-bonbon-cookies/",
        "name": "Egg-Free Bonbon Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F153268.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-06-14T18:28:49.000Z",
        "description": "Easy recipe with a surprise inside!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups butter, softened",
            "1\u2009\u00bd cups confectioners' sugar",
            "2 tablespoons vanilla extract",
            "3 cups all-purpose flour",
            "1 (10 ounce) jar maraschino cherries, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter or margarine, sugar and vanilla until mixture is light and fluffy. Slowly blend in the flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For each cookie, shape one level tablespoon of dough around a cherry. Form each cookie into a ball and place them on an ungreased cookie sheet 1 inch apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cherry Dessert Recipes"
        ],
