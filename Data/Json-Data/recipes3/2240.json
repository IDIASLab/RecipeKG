    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13203/creamy-broccoli-soup/",
        "name": "Creamy Broccoli Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1072523.jpg",
            "width": 2000,
            "height": 2000
        },
        "datePublished": "1999-02-13T14:29:40.000Z",
        "description": "A quick, creamy, hearty soup.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2 cups chicken broth",
            "2\u2009\u00bd cups fresh broccoli",
            "\u00bc cup chopped onion",
            "1 cup milk",
            "2 tablespoons all-purpose flour",
            "1 cup shredded Cheddar cheese",
            "\u00bd teaspoon dried oregano",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring broth to a boil. Add broccoli and onion. Cook for five minutes, or until broccoli is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, slowly add milk to flour, and mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour mixture into broth mixture.  Cook, stirring constantly, until soup is thick and bubbly.  Add cheese if desired; stir until melted.  Add seasonings and serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
