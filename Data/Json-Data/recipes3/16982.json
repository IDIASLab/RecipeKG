        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268658/cuban-mini-melts/",
        "name": "Cuban Mini Melts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5857766.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-10-17T04:17:45.000Z",
        "description": "Delight your game day guests with these miniature Cuban sandwiches.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "20 mini sandwiches",
        "recipeIngredient": [
            "10 slices white sandwich bread, quartered",
            "12 slices honey ham, chopped",
            "1\u2009\u00bd cups shredded Swiss cheese",
            "2 tablespoons spicy brown mustard",
            "2 tablespoons mayonnaise",
            "20 dill pickle slices",
            "20 toothpicks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Divide quartered bread slices evenly between 2 large baking sheets .\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ham, Swiss cheese, mustard, and mayonnaise in a large bowl and mix well. Place 1 tablespoon of mixture on top of each piece of bread on 1 of the baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake topped bread in the preheated oven for 4 minutes. Add baking sheet with plain bread to the oven and continue baking 3 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove both baking sheets from the oven. Place a dill pickle slice on each topped bread slice; cover with a piece of plain toasted bread. Secure each mini sandwich with a toothpick.\n"
            }
        ],
