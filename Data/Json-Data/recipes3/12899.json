    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257327/easy-chicken-shawarma/",
        "name": "Easy Chicken Shawarma",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4482103.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-08T14:42:36.000Z",
        "description": "This is the authentic shawarma taste that I have always loved during my stay in the Middle East. Light and filling! It's totally divine!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H27M",
        "totalTime": "P0DT1H17M",
        "recipeYield": "8 sandwiches",
        "recipeIngredient": [
            "2 tablespoons white vinegar",
            "2 tablespoons soy sauce",
            "2 teaspoons garlic paste, divided",
            "1 teaspoon ginger paste",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 pound skinless, boneless chicken breasts",
            "3 potatoes, cut into fries",
            "oil for frying",
            "1 tablespoon vegetable oil",
            "1 cup mayonnaise",
            "8 pita breads, split",
            "1 cup pickled cucumbers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix vinegar, soy sauce, 1 teaspoon garlic paste, ginger paste, salt, and pepper together in a shallow dish. Add chicken breasts; turn to coat. Marinate in the refrigerator for at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 inches oil in a heavy pot to 325 degrees F (165 degrees C). Fry potatoes in batches until golden brown, about 5 minutes. Transfer to paper towels to drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon vegetable oil in a large skillet over medium heat. Remove chicken breasts from the marinade and cook until browned and no longer pink in the center, 6 to 8 minutes per side. Transfer to a cutting board; slice into thin strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining 1 teaspoon garlic paste with mayonnaise in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread garlic mayonnaise inside pita breads. Fill each pita with French fries, chicken slices, and pickles.\n"
            }
        ],
