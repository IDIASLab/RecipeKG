    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263662/worlds-best-vegan-tomato-soup/",
        "name": "World's Best Vegan Tomato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "World's Best Vegan Tomato Soup"
        },
        "datePublished": "2018-06-08T14:48:23.000Z",
        "description": "Absolutely delicious, light, and easy vegan tomato soup to make! A great way to get vegetables into picky eaters.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 gallon",
        "recipeIngredient": [
            "\u2153 cup extra-virgin olive oil, divided",
            "2 pounds Roma tomatoes, chopped",
            "1 red bell pepper, chopped",
            "1 large carrot, chopped",
            "1 head garlic, cloves peeled",
            "\u00bd cup raw sugar",
            "1 cup fresh basil",
            "1 quart vegetable stock",
            "2 tablespoons salt",
            "1 pinch freshly cracked black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons olive oil in a large skillet over medium to medium-high heat. Add tomatoes, bell pepper, carrot, and garlic cloves. Cook and stir until softened, about 15 minutes. Stir in sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill blender or food processor halfway with the tomato mixture. Cover and hold lid down with a potholder. Blend until smooth, adding 1/2 the basil and pouring in 1/2 the vegetable stock slowly. Transfer soup mixture to a large pot. Repeat with remaining tomato mixture, basil, and vegetable stock.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring soup to a simmer over medium heat. Continue simmering until flavors combine, about 20 minutes. Add salt, pepper, and remaining olive oil.\n"
            }
        ],
