    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284565/mad-scientist-chili/",
        "name": "Mad Scientist Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mad Scientist Chili"
        },
        "datePublished": "2018-05-26T14:51:33.000Z",
        "description": "Whipped this up from scratch with ingredients around my home at 2am. Was deemed a mad scientist by my girlfriend who saw me meticulously working away in the kitchen in the middle of the night. Hence the name. Warning: do not eat if you can't take extreme heat.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT2H5M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 pound skinless, boneless chicken breast, diced",
            "1 (29 ounce) can tomato sauce",
            "1 (29 ounce) can crushed tomatoes",
            "2 cups water",
            "8 fluid ounces IPA beer",
            "5 peppers habanero peppers, minced",
            "3 peppers serrano chile peppers, chopped",
            "2 peppers Anaheim chile peppers, chopped",
            "1 Hungarian hot pepper, finely chopped",
            "1 Carolina reaper pepper, minced",
            "1 medium yellow onion, chopped",
            "1 medium bell pepper, chopped",
            "3 stalks green onions, chopped",
            "2 cloves garlic, minced",
            "1 tablespoon red pepper flakes",
            "4 tablespoons chili powder",
            "1 tablespoon cayenne pepper",
            "1 tablespoon black garlic powder",
            "1 drop pure capsaicin extract",
            "1 pound bacon",
            "2 (16 ounce) cans chili beans, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Brown ground beef and chicken in a large skillet until chicken is no longer pink in the center and beef is browned and crumbly, 5 to 7 minutes. Drain grease and transfer meat mixture to a large pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add tomato sauce, crusted tomatoes, water, beer, all chile peppers, onion, bell pepper, green onions, garlic, pepper flakes, chili powder, cayenne, garlic powder, and capsaicin to the meat mixture; bring to a boil. Reduce heat to low and simmer, stirring occasionally, for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop bacon and combine with chili beans in a large bowl; set aside until meat-pepper mixture has simmered for a full hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add bacon-bean mixture to the pot. Simmer, stirring occasionally, for 1 more hour.\n"
            }
        ],
