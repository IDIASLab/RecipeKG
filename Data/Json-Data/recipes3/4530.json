    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/53986/herbal-tuna-salad/",
        "name": "Herbal Tuna Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F386530.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:21:12.000Z",
        "description": "A new and exciting way to dress up your standard tuna salad. Serve as an appetizer with rich crackers or with a lettuce leaf and melba toast.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 hard-cooked eggs",
            "2 tablespoons mayonnaise",
            "2 tablespoons sour cream",
            "1 tablespoon Worcestershire sauce",
            "1 teaspoon prepared mustard",
            "1 teaspoon dried dill weed",
            "1 teaspoon dried rosemary",
            "1 teaspoon grated lemon zest",
            "salt and pepper to taste",
            "2 (5 ounce) cans tuna in water, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a blender or food processor, combine eggs, mayonnaise, sour cream, Worcestershire sauce, and mustard. Season with dill, rosemary, and lemon zest, salt. and pepper. Blend until smooth, then spoon into a medium bowl. Stir in tuna.\n"
            }
        ],
        "recipeCategory": [
