        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15613/ham-and-cheese-salad/",
        "name": "Ham and Cheese Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F876977.jpg",
