        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229672/pomegranate-glazed-carrots/",
        "name": "Pomegranate Glazed Carrots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4086144.jpg",
