        "mainEntityOfPage": "https://www.allrecipes.com/recipe/48944/long-island-iced-tea/",
        "name": "Long Island Iced Tea",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1131884.jpg",
