    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234607/party-meatballs/",
        "name": "Party Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6291742.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2013-09-19T22:40:37.000Z",
        "description": "Meatballs made from beef and pork sausage combined with special ingredient apple butter are sure to please at your next party!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup MUSSELMAN'S\u00ae Apple Butter, divided",
            "1 tablespoon butter",
            "\u00bd cup grated red onion",
            "1 pound pork sausage",
            "1 pound ground beef",
            "2 cups panko bread crumbs",
            "\u00bc cup sour cream",
            "2 eggs",
            "2 teaspoons salt",
            "1 teaspoon black pepper",
            "1 teaspoon allspice",
            "\u00bd cup beef broth",
            "2 tablespoons honey",
            "Cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F and line a large-rimmed baking sheet with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a skillet over medium heat and melt the butter in it. When the butter has melted, add the grated onions and saute for 3-4 minutes, until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix the sausage, ground beef, panko, 1/2 cup apple butter, sour cream, eggs, salt, pepper, and allspice. Add the onion, once it cools. Get your hands in it and mix well, until evenly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Measure out the meatballs into one-ounce portions. Roll tight balls and place them on the baking sheet, close together, but not touching. You should have 40-45 meatballs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 minutes. Meanwhile, mix the remaining 1/2 cup of apple butter, with beef broth, honey, and 1-2 dashes of cayenne pepper. Whisk well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After 15 minutes, remove the meatballs from the oven. Pour the sauce over the top, making sure to cover each meatball, and place back in the oven for 5-7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Once cooked through, shake the baking sheet a little to loosen the meatballs and serve warm.\n"
            }
        ],
