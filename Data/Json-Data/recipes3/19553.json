        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13085/v-eight-vegetable-beef-soup/",
        "name": "V-Eight Vegetable Beef Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F290155.jpg",
            "width": 250,
