        "mainEntityOfPage": "https://www.allrecipes.com/recipe/79447/tangy-cranberry-salsa/",
        "name": "Tangy Cranberry Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F299069.jpg",
            "width": 250,
            "height": 250
