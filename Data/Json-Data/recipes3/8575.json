    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220404/lobster-tomato-sauce/",
        "name": "Lobster Tomato Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5059980.jpg",
            "width": 1512,
            "height": 1512
        },
        "datePublished": "2018-06-08T14:00:29.000Z",
        "description": "Another one of grandma Puglia's treasures! The lobster flavor from the shell infuses into a rich and delicious tomato sauce. You may remove the lobster meat and add to the sauce or serve separately. Serve over linguine.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "1 onion, chopped",
            "1 small garlic clove, crushed",
            "1 tablespoon chopped fresh parsley",
            "6 (6 ounce) lobster tails, thawed",
            "1 (28 ounce) can crushed tomatoes",
            "1 (8 ounce) can tomato sauce",
            "3 tablespoons chopped fresh basil",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large saucepan over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion and garlic in hot oil until lightly browned, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir parsley and lobster tails into onion and garlic; cook until shells turn bright red, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir crushed tomatoes, tomato sauce, basil, salt, and black pepper with lobster tails; simmer over low heat for 1 hour, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove lobster tails from sauce and shake excess sauce off the shells. If desired, remove lobster meat from shells and stir into sauce or serve lobster pieces separately.\n"
            }
        ],
