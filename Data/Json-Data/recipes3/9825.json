    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232428/habanero-cream-sauce/",
        "name": "Habanero Cream Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1937344.jpg",
            "width": 2322,
            "height": 4128
        },
        "datePublished": "2018-06-08T14:08:28.000Z",
        "description": "A spicy cream sauce for Mexican meals. Cream can be substituted for milk, and margarine for butter. I normally serve this drizzled over shredded chicken in tortillas.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 habanero peppers",
            "\u2154 cup milk, divided (or as needed)",
            "2 tablespoons butter",
            "2 tablespoons all-purpose flour",
            "1 pinch salt",
            "1 pinch ground paprika",
            "1 tablespoon chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast peppers in preheated oven until nearly browned, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove and discard seeds and stems from peppers. Transfer peppers to a food processor. Blend peppers and 2 tablespoons milk in the food processor until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over medium heat until bubbly. Stir flour into the melted butter; cook until the mixture starts to brown, 2 to 3 minutes. Add the pepper mixture to the flour mixture. Begin whisking milk into the mixture; cook, whisking regularly, until it begins to thicken, thinning with additional milk as needed. Season with salt and paprika. Sprinkle cilantro over the sauce to serve.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
