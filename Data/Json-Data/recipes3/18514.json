    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270648/kefir-soaked-baked-oatmeal/",
        "name": "Kefir-Soaked Baked Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Kefir-Soaked Baked Oatmeal"
        },
        "datePublished": "2019-02-14T09:02:26.000Z",
        "description": "This oatmeal rises above the rest due to its more easily digestible oats soaked in kefir overnight. Easy to throw together the night before, and requiring little time or energy in the morning, this recipe is great for busy moms.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT8H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "5 cups rolled oats",
            "2\u2009\u00bc cups plain kefir",
            "\u00be cup coconut oil, melted",
            "\u00be cup honey",
            "\u00bd tablespoon ground cinnamon",
            "cooking spray",
            "1 cup chopped apple",
            "3 eggs",
            "1 tablespoon baking powder",
            "\u00bd tablespoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine oats, kefir, coconut oil, honey, and cinnamon in a large bowl. Cover and let soak, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease a 9x13-inch baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir apple, eggs, baking powder, and salt into the oat mixture. Pour into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the top is lightly browned, about 45 minutes.\n"
            }
        ],
