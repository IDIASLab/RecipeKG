    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240251/roasted-cauliflower-and-pancetta-soup/",
        "name": "Roasted Cauliflower and Pancetta Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2057879.jpg",
            "width": 2048,
            "height": 1662
        },
        "datePublished": "2018-06-08T14:18:03.000Z",
        "description": "Flavorful pancetta, cauliflower, and cheese soup.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 bowls",
        "recipeIngredient": [
            "1 large head cauliflower, cut into florets",
            "\u00bc cup butter (such as Kerrygold\u00ae), melted",
            "salt and ground black pepper to taste",
            "5 cloves garlic",
            "1 tablespoon olive oil",
            "6 ounces diced pancetta",
            "1 sweet onion, diced",
            "2 teaspoons dried thyme",
            "1 teaspoon dried rosemary",
            "1 teaspoon ground turmeric",
            "28 fluid ounces chicken stock",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange cauliflower tightly into a 9x13-inch glass baking dish. Pour melted butter over cauliflower and lightly season with salt and pepper. Place garlic cloves into the cauliflower.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until cauliflower is tender and browned, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat. Cook and stir pancetta in the hot oil until browned and crisp, 5 to 10 minutes. Transfer pancetta to paper towel-lined bowl, reserving pancetta drippings in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion in the pancetta drippings until tender, about 5 minutes. Mix thyme, rosemary, and turmeric into onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chicken stock over onion mixture and bring to a boil. Add cauliflower and garlic to chicken stock. Reduce heat to medium and simmer soup until cauliflower is very soft, about 15 minutes. Stir pancetta into soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend soup using an immersion blender until completely smooth. Stir Cheddar cheese, 1 to 2 tablespoons at a time, into soup until melted.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Cauliflower"
        ],
