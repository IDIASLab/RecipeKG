        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2017%2F04%2F400866-Best-Salmon-Bake-Image-via-Bigstock-650x465.jpg",
            "width": 650,
