    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257365/pretty-darn-quick-potato-gratin/",
        "name": "Pretty Darn Quick Potato Gratin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6316064.jpg",
            "width": 2220,
            "height": 2220
        },
        "datePublished": "2017-03-01T08:56:34.000Z",
        "description": "Use your food processor to quickly prep this easy, cheesy potato side dish.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 ounces Gruy\u00e8re or Comt\u00e9 cheese",
            "4 large Yukon Gold potatoes, peeled",
            "1\u2009\u00bd cups half-and-half",
            "1 large garlic clove, peeled and very finely minced",
            "\u00be teaspoon salt",
            "1 pinch freshly grated nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Butter an 8x8-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fit a food processor with shredding disk and use it to shred cheese. Transfer to a small bowl. Fit processor with slicing disk and use it to slice potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put potatoes in a large saucepan with half-and-half, garlic, salt, and nutmeg; stir gently to combine. Bring to a boil over medium-high heat, stirring occasionally. Cook at a gentle boil, stirring often with a heat-safe spatula to prevent sticking, until half-and-half has reduced and thickly coats potatoes, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon into prepared baking dish. Smooth top and sprinkle evenly with cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until potatoes are tender when pierced with a knife and top is golden brown, 25 to 30 minutes. Let stand 5 to 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Potatoes Au Gratin Recipes"
        ],
