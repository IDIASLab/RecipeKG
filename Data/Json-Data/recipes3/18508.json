        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270599/keto-chocolate-mousse/",
        "name": "Keto Chocolate Mousse",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6361947.jpg",
            "width": 1500,
            "height": 1407
