    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268968/mushroom-and-bell-pepper-tacos/",
        "name": "Mushroom and Bell Pepper Tacos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2579322.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2018-11-08T08:49:50.000Z",
        "description": "Who said meatless mushroom tacos cannot be delicious? Try this recipe and you will see for yourself. Quick, delicious, and nutritious appetizer any time of the year.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 tacos",
        "recipeIngredient": [
            "3 tablespoons olive oil, or to taste",
            "1 small red onion, finely chopped",
            "1 pound mushrooms, coarsely chopped",
            "\u2153 cup chopped green bell pepper",
            "\u2153 cup chopped red bell pepper",
            "\u2153 cup chopped yellow bell pepper",
            "2 jalapeno peppers, minced, or to taste",
            "1 teaspoon salt",
            "\u00bd teaspoon black pepper",
            "\u00bd teaspoon chili powder",
            "4 whole wheat tortillas",
            "\u00bd cup shredded Cheddar cheese",
            "2 sprigs cilantro, chopped, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons oil in a skillet over medium-high heat. Add onion and cook until golden brown, 5 to 7 minutes. Stir in mushrooms, bell peppers, and jalapenos. Season with salt, pepper, and chili powder. Cook and stir until vegetables are tender, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly brush tortillas with olive oil. Place tortillas on a grill or skillet over medium heat and heat until char marks start to appear, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mushroom and bell pepper mixture among tortillas. Top with Cheddar cheese and heat until cheese melts, about 2 minutes. Garnish with cilantro.\n"
            }
        ],
