        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9137/green-bean-casserole-ii/",
        "name": "Green Bean Casserole II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3021498.jpg",
