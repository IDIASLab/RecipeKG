    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13216/gazpacho-ii/",
        "name": "Gazpacho II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F41119.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-19T05:44:20.000Z",
        "description": "A delicious cold soup, perfect for spring and summer!",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT2H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 Roma (plum) tomatoes, quartered",
            "1 large cucumber, peeled and halved",
            "1 onion, peeled and halved",
            "1 cup green bell pepper, diced",
            "1 (4 ounce) jar diced pimento peppers, drained",
            "2 (12 fluid ounce) cans tomato juice",
            "\u2153 cup olive oil",
            "\u2153 cup red wine vinegar",
            "\u00bc teaspoon hot pepper sauce",
            "1\u2009\u00bd teaspoons salt",
            "\u215b teaspoon ground black pepper",
            "2 cloves garlic, minced",
            "\u00bd cup croutons",
            "\u00bc cup chopped fresh chives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a blender combine one tomato, half the cucumber, half the onion, a green bell pepper quarter, the pimento and 1/2 cup tomato juice. Blend at high speed for 30 seconds to puree the vegetables.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl mix the pureed vegetables with remaining tomato juice, 1/4 cup olive oil, vinegar, hot pepper sauce, salt and ground black pepper. Cover mixture and refrigerate until it is well chilled (about 2 hours).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile saute the croutons in oil and add the garlic; transfer to a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place remaining chopped tomato, cucumber, onion and green bell pepper in separate bowls. Serve soup in chilled bowls, garnish with chives, and serve chopped vegetables and croutons as accompaniments.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Gazpacho Recipes"
        ],
