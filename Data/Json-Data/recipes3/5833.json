        "mainEntityOfPage": "https://www.allrecipes.com/recipe/86817/mexi-hominy/",
        "name": "Mexi Hominy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F202347.jpg",
