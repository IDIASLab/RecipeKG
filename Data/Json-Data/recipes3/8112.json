        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215592/zucchini-salad-with-yogurt-and-walnuts/",
        "name": "Zucchini Salad with Yogurt and Walnuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F472820.jpg",
            "width": 2000,
