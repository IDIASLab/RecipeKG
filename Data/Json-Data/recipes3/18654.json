    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21411/5-ingredient-crab-salad/",
        "name": "5-Ingredient Crab Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8265381.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-09-22T17:39:08.000Z",
        "description": "This crab salad is excellent served warm or cold.  I make it every time the family gets together.  I won an award in a cooking contest with this easy recipe.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P0DT0H11M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd green bell pepper, chopped",
            "1 onion, chopped",
            "3 tablespoons butter",
            "12 ounces imitation crabmeat",
            "1 cup mayonnaise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium skillet, saute the green pepper and onion in the butter for 3 minutes, or until vegetables are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in imitation crabmeat, and saute for another 3 minutes. Remove from heat, and put mixture into a medium bowl. Stir in mayonnaise. May be served warm or cold.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Seafood Salad Recipes",
            "Crab Salad Recipes"
        ],
