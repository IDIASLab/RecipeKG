    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9767/pistachio-crisps/",
        "name": "Pistachio Crisps",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pistachio Crisps"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "If you're not a pistachio lover, you can substitute your favorite nuts in these cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 teaspoons vanilla extract",
            "\u00bd cup sifted confectioners' sugar",
            "\u215b teaspoon salt",
            "2 cups chopped pistachio nuts",
            "1\u2009\u00bd cups sifted all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the butter and salt while slowly adding the powdered sugar.  Add the nuts, vanilla and flour and mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a lightly floured surface, roll out dough in the shape of logs about 1/2 inch in diameter. Cut into 1 1/2 inch pieces and bake for 15 to  18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll cookies in powdered sugar when they are still warm.  Let cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pistachio Dessert Recipes"
        ],
