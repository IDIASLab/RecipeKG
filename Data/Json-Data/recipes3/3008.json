    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21128/shrimp-toast-rectangles/",
        "name": "Shrimp Toast Rectangles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Shrimp Toast Rectangles"
        },
        "datePublished": "2000-09-16T02:01:49.000Z",
        "description": "A tasty appetizer made with fresh shrimp on toast then fried to a golden brown.  You can use any kind of bread and cut it into any desired shape.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00be pound shrimp, peeled and deveined",
            "2 teaspoons all-purpose flour",
            "salt and pepper to taste",
            "2 egg yolks",
            "2 egg whites",
            "4 slices whole wheat bread",
            "\u00bd cup sesame seeds",
            "2 cups oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium high heat to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop and mash shrimp using a large knife. Mix in flour, egg yolks, salt and pepper. In a separate bowl,  whisk the egg whites to stiff peaks and fold into the shrimp mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice bread into 4 pieces each. Put 2 teaspoons of the shrimp mixture onto each piece and sprinkle with sesame seeds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Starting with the shrimp side down, fry for 3 minutes. Turn, and fry for another 3 minutes or until golden brown. Remove from oil, drain on paper towels and serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
        ],
