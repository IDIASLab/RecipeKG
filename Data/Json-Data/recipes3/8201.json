    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216474/cucumber-hummus/",
        "name": "Cucumber Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F669719.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:57:40.000Z",
        "description": "Light and refreshing appetizer made to eat with crisp veggies and warm pita bread. Serve topped with olives.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT1H10M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "4 cloves garlic",
            "\u00bc cup tahini paste, or to taste",
            "1 lemon, juiced",
            "1 teaspoon extra-virgin olive oil",
            "1 (15 ounce) can garbanzo beans, drained",
            "1 cucumber, coarsely chopped",
            "salt and black pepper to taste",
            "1 pinch paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the garlic, tahini paste, lemon juice, and olive oil into a blender or food processor. Blend until the garlic is finely minced, then add the garbanzo beans and chopped cucumber. Puree until smooth, then season to taste with salt and black pepper; blend to mix. Pour into a serving dish, and refrigerate 1 to 2 hours to allow the flavors to blend. Sprinkle with paprika before serving.\n"
            }
        ],
