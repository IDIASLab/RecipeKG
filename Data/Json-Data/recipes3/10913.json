        "name": "Ellen's Spinach and Fig Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5035032.jpg",
            "width": 4608,
            "height": 3072
