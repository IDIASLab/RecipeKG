        "mainEntityOfPage": "https://www.allrecipes.com/recipe/46028/herbed-mushrooms-with-white-wine/",
        "name": "Herbed Mushrooms with White Wine",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F381619.jpg",
