        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17254/healthier-chicken-a-la-king/",
        "name": "Healthier Chicken a la King",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5838831.jpg",
