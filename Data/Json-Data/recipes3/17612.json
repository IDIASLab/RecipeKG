    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258481/zucchini-carrot-patties-with-bacon/",
        "name": "Zucchini Carrot Patties with Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5119159.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-11-23T03:40:03.000Z",
        "description": "Delicious gluten-free, paleo-friendly zucchini cakes! Bacon and onions take this savory side to another level.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H23M",
        "totalTime": "P0DT0H53M",
        "recipeYield": "2 patties",
        "recipeIngredient": [
            "1 zucchini, grated",
            "1 carrot, grated",
            "salt, or to taste",
            "3 slices bacon",
            "\u00bd onion, finely chopped",
            "1 egg",
            "\u00bc cup almond flour",
            "1 teaspoon minced garlic",
            "1 teaspoon Italian seasoning",
            "2 tablespoons coconut oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle zucchini and carrot with salt and drain in a colander.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Crumble into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onions in the same skillet over medium heat until translucent, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse zucchini and carrot; remove excess liquid by squeezing through cheese cloth or paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place zucchini, carrot, bacon, egg, onion, almond flour, garlic, and Italian seasoning in a large bowl. Mix evenly. Form a 1/2-inch thick patty using 1/4 cup of the mixture. Repeat with remaining patty mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat coconut oil over medium-high heat and cook patties until browned, about 4 minutes per side.\n"
            }
        ],
