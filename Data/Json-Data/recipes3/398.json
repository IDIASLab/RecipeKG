        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7734/apricot-brandy-pound-cake-i/",
        "name": "Apricot Brandy Pound Cake I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F247158.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-06-06T12:24:04.000Z",
        "description": "I collect cake recipes that use a box cake mix as the base.  This cake is rich, delicious and easy!  This cake freezes well.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 bundt cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "1 cup sour cream",
            "\u00bd cup apricot brandy",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon orange extract",
            "\u00bd teaspoon lemon extract",
            "\u00bd teaspoon almond extract",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "4 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease and lightly flour a bundt pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat cake mix, pudding, and eggs until smooth.  Mix brandy and flavorings in sour cream, and add to egg mixture.  Beat at least 2 minutes with an electric mixer on medium speed.  Pour batter into prepared pan\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 50 to 55 minutes, or until tester inserted in the center comes out clean.  Cool cake in pan on wire rack for 20 minutes.  Remove from pan, and continue cooling on wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cake Mix Cake Recipes",
            "Pound Cake"
        ],
