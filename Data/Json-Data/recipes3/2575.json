    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16278/chicken-fried-steak-ii/",
        "name": "Chicken Fried Steak II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F273569.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-01-16T19:14:13.000Z",
        "description": "Green hot pepper sauce is added to this chicken fried steak to give a hint of spice. Best served with mashed potatoes and gravy and biscuits.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (5 ounce) can evaporated milk",
            "2\u2009\u00bd tablespoons hot pepper sauce",
            "\u00be teaspoon salt",
            "2 cups all-purpose flour",
            "2\u2009\u00bd teaspoons paprika",
            "\u00bd teaspoon garlic powder",
            "salt to taste",
            "\u00be teaspoon ground black pepper",
            "3 pounds round steak, pounded to about 1/2 inch thickness",
            "4 cups vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the milk, hot pepper sauce and salt in a medium bowl. Measure one cup of flour into a second medium bowl. In a third medium bowl, combine the remaining flour, paprika, garlic powder, salt and ground black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "First coat the steak in the unseasoned flour, then dip in the milk mixture, and finally coat meat in the seasoned flour. Repeat as necessary if there's more than one piece of meat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a medium skillet over medium high heat. Fry the coated meat in the oil for 3 minutes per side, or until golden brown. Drain meat on paper towels. (Note: Make sure the oil is fully heated before deep frying.)\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks",
            "Round Steak Recipes"
        ],
