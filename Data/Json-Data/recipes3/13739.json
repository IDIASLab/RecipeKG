    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261104/christmas-stuffing/",
        "name": "Christmas Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4372249.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "2018-06-08T14:45:48.000Z",
        "description": "A favorite in our family for years, adjusted to cook in a pressure cooker before baking.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4 large potatoes, cubed",
            "\u00bd large white onion, coarsely chopped",
            "2 tablespoons dried summer savory",
            "\u00bd cup butter, cubed",
            "6 slices bread, cubed",
            "salt and ground black pepper to taste",
            "2 eggs, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine potatoes, onion, and summer savory in a pressure cooker with water to cover. Close cooker securely and place pressure regulator over vent according to manufacturer's instructions. Adjust heat until regulator is gently rocking. Cook at high pressure until potatoes are tender, about 5 minutes. Let pressure release naturally according to manufacturer's instructions; unlock and remove lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir butter, bread, salt, and pepper into the potato mixture. Transfer stuffing to an oven-proof glass dish. Cover with plastic wrap and refrigerate for 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs into the stuffing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, 30 to 40 minutes.\n"
            }
        ],
