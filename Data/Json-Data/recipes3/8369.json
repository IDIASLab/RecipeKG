    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218177/ensalada-rusa/",
        "name": "Ensalada Rusa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9401766.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-08T13:58:42.000Z",
        "description": "My Salvadoran sister-in-law's beet, potato, and egg salad.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 beets, trimmed",
            "4 potatoes, peeled and cubed",
            "1 teaspoon salt",
            "4 eggs",
            "\u00bd cup mayonnaise, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the beets into a saucepan, cover with water, and bring to a boil. Reduce heat to medium, and boil gently until tender, about 20 minutes. Remove from the boiling water and let cool. Peel and dice the beets, and chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the potatoes into a separate saucepan, add water to cover, and stir in salt. Boil the potato cubes until tender but not mushy, about 15 minutes. Drain the potatoes in a colander set in the sink, and chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a saucepan of water to a boil, reduce heat to medium, and stir in 1 to 2 more teaspoons of salt. Very gently lower the eggs into the boiling water, making sure they don't hit the bottom of the pan and crack; simmer for 15 minutes. Remove the eggs to a bowl filled with ice and ice water, and let the eggs chill until thoroughly cold. Peel and dice the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the chilled beets, potato cubes, and eggs together in a salad bowl, and gently stir in mayonnaise to taste.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Potato Salad Recipes",
            "Dairy-Free Potato Salad Recipes"
        ],
