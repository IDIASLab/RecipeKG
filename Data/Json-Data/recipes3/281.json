        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15895/chocolate-bar-pie-i/",
        "name": "Chocolate Bar Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F964165.jpg",
