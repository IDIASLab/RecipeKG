        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9605/peanut-butterchocolate-chip-cookie-bars/",
        "name": "Peanut Butter/Chocolate Chip Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3267322.jpg",
