        "recipeYield": "4 - 5 dozen",
        "recipeIngredient": [
            "3 eggs",
            "2 cups honey",
            "\u00bd cup milk",
            "2 teaspoons baking soda",
            "4\u2009\u00bd cups all-purpose flour",
            "1 teaspoon anise extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs well. Dissolve baking soda with a little milk. Mix together the rest of the ingredients to make a soft dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10  minutes. DO NOT OVER BAKE. Frost with your favorite icing.\n"
            }
        ],
