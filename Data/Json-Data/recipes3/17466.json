        "name": "Spinach 'n' Steak Salad with Chipotle Honey Mustard",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2141492.jpg",
