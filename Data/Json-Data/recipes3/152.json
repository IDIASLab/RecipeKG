    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15192/baked-brie-in-puff-pastry/",
        "name": "Baked Brie in Puff Pastry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F534862.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "1999-12-23T18:42:26.000Z",
        "description": "This wonderful, light puff pastry is filled with melted Brie cheese. Serve with crackers on the side.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 to 10 servings",
        "recipeIngredient": [
            "\u00bd (17.5 ounce) package frozen puff pastry, thawed",
            "1 (8 ounce) wheel Brie cheese",
            "\u00bc cup sliced almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degree C). Lightly grease a 9 inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice the wheel of Brie in half, horizontally, so that the halves are two, flatter wheels of Brie. Lay the puff pastry in the pie pan. Place half of the Brie (rind-side down) onto the pastry dough. Sprinkle almonds evenly over the top. Place the other half of the Brie (rind-side up) over the almonds. Bundle the pastry dough around the Brie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 to 20 minutes. Let cool for 5 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Cheese",
            "Baked Brie Recipes"
        ],
