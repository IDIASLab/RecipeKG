        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12597/orange-pie-ii/",
        "name": "Orange Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4466406.jpg",
            "width": 960,
