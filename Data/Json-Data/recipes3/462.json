    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8161/philadelphia-style-butter-cake/",
        "name": "Philadelphia Style Butter Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2198650.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-03-09T19:35:29.000Z",
        "description": "This is a very rich, sweet, gooey, buttery cake. I store leftovers in the refrigerator and they taste just like cheesecake! Yummy!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9x13 inch cake",
        "recipeIngredient": [
            "1 egg",
            "1 (18.25 ounce) package yellow cake mix",
            "\u00bd cup butter, softened",
            "8 ounces cream cheese, softened",
            "1 (16 ounce) package confectioners' sugar, sifted",
            "2 eggs",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour one 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat together 1 egg, cake mix, and butter for 5 minutes. Mix and pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together cream cheese, confectioners' sugar, 2 eggs, and vanilla extract. Pour this on top of the cake base.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 35-40 minutes until browned. Cool 15 minutes and cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cake Mix Cake Recipes",
            "Yellow Cake"
