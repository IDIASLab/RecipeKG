    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272328/tender-baked-smothered-pork-chops/",
        "name": "Tender Baked Smothered Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tender Baked Smothered Pork Chops"
        },
        "datePublished": "2019-03-27T01:27:20.000Z",
        "description": "Incredibly tender pork chops with unparalleled flavor.",
        "prepTime": "P0DT0H50M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "6 pork chops",
        "recipeIngredient": [
            "6 pork chops",
            "salt and ground black pepper to taste",
            "1 cup all-purpose flour",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon onion powder",
            "4 eggs",
            "1 cup bread crumbs",
            "\u00bc cup vegetable oil",
            "\u00bd cup butter",
            "2 medium shallots, diced",
            "4 cloves garlic, minced",
            "1 cup mushrooms, sliced",
            "\u00bd cup all-purpose flour",
            "\u00bd cup sherry",
            "2 cups chicken broth",
            "1 cup heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season each pork chop with salt and pepper. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, garlic powder, and onion powder in a bowl; whisk eggs together in a separate bowl; place bread crumbs into a third bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a pork chop in the flour bowl, covering each side very lightly with flour. Move to beaten eggs, making sure to cover. Cover with bread crumbs. Repeat with remaining pork chops.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a deep skillet over medium heat. Cook each pork chop in the skillet until well-browned, 3 to 4 minutes per side. Transfer to a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in the same skillet over medium heat. Add shallots and cook until softened, 3 to 5 minutes. Add garlic and cook until fragrant, about 30 seconds. Add mushrooms and cook until they have softened and released their liquid, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add flour to the skillet and stir into a paste, 1 to 2 minutes. Add sherry slowly and stir until well combined; repeat with chicken broth, and then with heavy cream. Simmer until thickened, about 5 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon gravy over pork chops. Cover the baking dish with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 40 minutes. Remove foil and bake for 10 minutes more. Let rest for 5 minutes before serving.\n"
            }
        ],
