        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246433/vegan-tzatziki/",
        "name": "Vegan Tzatziki",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3548486.jpg",
