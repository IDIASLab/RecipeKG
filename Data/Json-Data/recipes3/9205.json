    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228534/seasoned-pumpkin-seeds/",
        "name": "Seasoned Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2500639.jpg",
            "width": 3156,
            "height": 3156
        },
        "datePublished": "2018-06-08T14:04:32.000Z",
        "description": "Not the old roasted and salted pumpkin seeds.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1 pound green hulled pumpkin seeds (pepitas)",
            "1 tablespoon olive oil",
            "1 tablespoon chili powder",
            "1 tablespoon tamari sauce",
            "2 teaspoons garlic powder",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pumpkin seeds, olive oil, chili powder, tamari, and garlic powder in a resealable 1-gallon plastic bag. Seal bag and knead to coat the seeds with seasonings. Spread coated seeds on a baking sheet in an even layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 minutes, then turn the seeds over. Bake until pumpkin seeds are lightly toasted and fragrant, about 15 more minutes. Remove from oven and sprinkle lightly with salt. Cool before serving; store in an airtight container in refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Nuts and Seeds",
            "Pumpkin Seed Recipes"
        ],
