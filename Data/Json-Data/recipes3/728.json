        "name": "Tender Shortbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F584528.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-01-04T20:11:55.000Z",
        "description": "These are great shortbreads for the holidays.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 cups sifted all-purpose flour",
            "\u00bd cup fructose (fruit sugar)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together butter and sugar.  Add 1 3/4 cups flour gradually, reserving 1/2 cup for pastry board.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out on board and gradually work in remaining flour, if necessary.  Roll dough to 1/3 or 1/2 inch thickness; then cut with cookie cutter\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on cookie sheet and bake at 300 degrees F (150 degrees C) for 30 to 35 minutes.\n"
            }
        ],
