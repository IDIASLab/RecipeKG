        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6716/beer-biscuits/",
        "name": "Beer Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7990749.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-06-03T16:14:47.000Z",
        "description": "The beer adds flavor and leavening to these biscuits.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "3 teaspoons baking powder",
            "1 teaspoon salt",
            "\u00bc cup shortening",
            "\u00be cup beer"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together flour, baking powder, and salt.  Cut in shortening until it has cornmeal consistency.  Stir in beer, and knead lightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out to 1/2 inch thickness.  Cut with biscuit cutter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 10 to 12 minutes, or until golden brown.\n"
            }
        ],
