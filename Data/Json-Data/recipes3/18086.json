        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269979/instant-pot-pork-chops/",
        "name": "Instant Pot&reg; Pork Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6501901.jpg",
            "width": 3456,
            "height": 3456
