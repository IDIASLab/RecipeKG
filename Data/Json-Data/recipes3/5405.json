    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78050/hot-dip/",
        "name": "Hot Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4402696.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2018-06-08T13:24:59.000Z",
        "description": "This delicious dip is always best when it's served really warm -- works well with breadsticks and assorted crackers.  I make it during the holidays when JOY is in the air and romance is always in style.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "3 slices bacon",
            "8 ounces sliced fresh mushrooms",
            "\u00bd medium onion, chopped",
            "1 clove garlic, minced",
            "2 tablespoons all-purpose flour",
            "\u00bc teaspoon salt",
            "\u215b teaspoon ground black pepper",
            "1 (8 ounce) package cream cheese, cut into cubes",
            "2 teaspoons Worcestershire sauce",
            "2 teaspoons soy sauce",
            "\u00bd cup sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon slices in a large skillet over medium-high heat. Fry until crisp. Remove bacon, crumble and set aside. Drain off all but 2 tablespoons of the bacon drippings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add mushrooms, onion and garlic to the bacon drippings, and cook over medium heat, stirring, until tender and most of the liquid has evaporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle the flour, salt, and pepper over the top, and stir to blend. Mix in cream cheese, Worcestershire sauce, and soy sauce. Reduce heat to low, and stir until cream cheese is melted. Remove from heat. Stir in the sour cream and bacon. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes"
        ],
