    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241209/pork-and-bean-burrito/",
        "name": "Pork and Bean Burrito",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pork and Bean Burrito"
        },
        "datePublished": "2018-06-08T14:19:16.000Z",
        "description": "This is a delicious burrito that is baked in your oven. The beans and salsa make it extra good!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT2H5M",
        "recipeYield": "8 burritos",
        "recipeIngredient": [
            "1 (10 ounce) can diced tomatoes with green chile peppers (such as RO*TEL\u00ae)",
            "2 onions, chopped, divided",
            "1 (1.25 ounce) package salsa seasoning mix",
            "salt and ground black pepper to taste",
            "2 (4 ounce) pork steaks, or more to taste",
            "\u00bd cup butter",
            "1 (15 ounce) can refried beans",
            "1 burrito seasoning",
            "8 large flour tortillas",
            "1 (15 ounce) can chili, or more as needed",
            "1 (8 ounce) package shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix diced tomatoes with green chile peppers, 1 chopped onion, salsa seasoning mix, salt, and pepper together in a bowl; refrigerate until the salsa flavors blend, at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a pot of water to a boil. Cook pork steaks in the boiling water until cooked through, 10 to 15 minutes. Remove pork steaks from water and cut into pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a skillet over medium heat; cook and stir pork and remaining 1 chopped onion in the hot butter until onion is tender, about 30 minutes. Mix refried beans, burrito seasoning, and 1/2 of the salsa into the pork mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon pork mixture down the center of each tortilla and roll tortilla around the filling. Place burritos in a baking dish. Top burritos with chili and Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted, 10 to 15 minutes. Serve burritos with remaining salsa.\n"
            }
        ],
