    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269902/vegan-al-pastor-tacos/",
        "name": "Vegan Al Pastor Tacos",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Al Pastor Tacos"
        },
        "datePublished": "2018-12-31T13:20:34.000Z",
        "description": "These vegan taco recipes are recreations of some of the top taco meats out there, but made entirely out of plants. Serve with cilantro, onions, and your favorite salsa.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "10 tacos",
        "recipeIngredient": [
            "1 (8 ounce) bag Soy protein isolate",
            "10 peppers dried guajillo chile peppers",
            "4 (7 ounce) cans chipotle chiles in adobo sauce",
            "\u00bc medium onion, chopped",
            "1 tablespoon garlic salt",
            "\u00bc teaspoon dried oregano",
            "\u00bc teaspoon black pepper",
            "\u00bc cup oil",
            "\u00bd cup pineapple chunks, cut into squares",
            "10 (6 inch) flour tortillas, warmed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Once boiling, add soy meat and turn off heat immediately. Let soak for 5 to 10 minutes, then drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, remove stems and seeds from guajillo peppers. Cut peppers in half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a small pot of water to a boil. Place guajillo peppers in the boiling water and boil for 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chipotle chiles in a blender, reserving all adobo sauce. Add guajillo peppers, onion, garlic salt, oregano, and pepper; process until fully blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat. Add soy meat and fry until browned, about 15 minutes. Add pineapple chunks and cook for another 15 minutes. Pour in reserved adobo sauce and cook for 5 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add a large spoonful of the soy mixture to the center of each warmed tortilla.\n"
            }
        ],
