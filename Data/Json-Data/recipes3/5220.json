        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75107/butter-fried-zucchini/",
        "name": "Butter Fried Zucchini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8184267.jpg",
