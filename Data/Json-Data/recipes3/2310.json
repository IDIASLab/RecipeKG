    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13769/hamburger-gravy/",
        "name": "Hamburger Gravy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2080126.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-10-27T03:36:07.000Z",
        "description": "This is so easy and delicious!  We have enjoyed hamburger gravy in our family for years.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds lean ground beef",
            "\u00bc cup all-purpose flour",
            "1 quart milk",
            "salt and pepper to taste",
            "1 teaspoon onion salt",
            "1 small onion, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place hamburger in a large, deep skillet. Crumble and cook over medium high heat until evenly brown. Remove from heat and sprinkle flour over cooked hamburger. Stir until evenly coated and all fat is absorbed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place skillet over medium heat, add 1/2 of milk and stir until gravy begins to thicken. Add remaining milk until desired consistency has been reached and gravy comes to a boil. Season with salt, pepper and onion salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve over mashed or boiled potatoes, toast or biscuits. Add fresh chopped onion if desired.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Gravy Recipes"
        ],
