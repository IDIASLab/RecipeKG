        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix chili, green beans, and cheese. Pour into unbaked crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 375 F (190 degrees C) for 40 minutes or until bubbly and crust is brown.\n"
            }
