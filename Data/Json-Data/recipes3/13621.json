        "name": "Easy Tuna and Bean Kale Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4561713.jpg",
            "width": 1008,
            "height": 1008
