    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268432/biscoff-chocolate-cups/",
        "name": "Biscoff&reg; Chocolate Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F814774.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-10-06T06:48:17.000Z",
        "description": "A Biscoff&reg; twist on the traditional peanut butter cups!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "48 chocolate cups",
        "recipeIngredient": [
            "1 cup cookie butter (such as Biscoff\u00ae)",
            "1 cup confectioners' sugar",
            "\u00bc cup butter, softened",
            "1 teaspoon salt",
            "2 (12 ounce) packages white chocolate chips",
            "2 (12 ounce) packages semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line 2 mini muffin pans with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cookie butter, confectioners' sugar, butter, and salt in a bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt white chocolate chips in a microwave-safe bowl in 15-second intervals, stirring after each interval, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 teaspoon of melted white chocolate in 1/2 of the lined muffin cups. Roll 1/2 the cookie butter mix into teaspoon-sized balls and place on top of the chocolate. Fill each cup with enough white chocolate to cover the cookie balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt semisweet chocolate chips in a microwave-safe bowl in 15-second intervals, stirring after each interval, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 teaspoon of melted semisweet chocolate in the remaining muffin cups. Roll remaining cookie butter mix into teaspoon-sized balls and place on top of the chocolate. Fill each cup with enough semisweet chocolate to cover the cookie balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate cookie cups until set, at least 1 hour.\n"
            }
        ],
