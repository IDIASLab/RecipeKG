    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267975/vegan-peanut-butter-cookies/",
        "name": "Vegan Peanut Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F690205.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-09-13T05:13:15.000Z",
        "description": "These are the best cookies I've ever made. Honest. And they're vegan!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "2 cups whole grain spelt flour",
            "1 teaspoon baking soda",
            "\u00be teaspoon salt",
            "1 cup unsalted natural peanut butter (such as East Wind)",
            "1 cup maple syrup",
            "\u2153 cup olive oil",
            "1\u2009\u00bd teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Place racks in the top half of the oven. Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine spelt flour, baking soda, and salt in a medium mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine peanut butter, maple syrup, olive oil, and vanilla extract in a separate, larger bowl; stir well. Pour in the flour mixture and stir until barely combined and still a little dry. Let sit for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Give dough another good stir. Drop by tablespoonfuls onto the prepared baking sheets. Press each cookie down gently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges appear set, 10 to 11 minutes. Avoid overbaking or cookies may become dry. Let cool on the baking sheets for a few minutes before transferring to cooling racks.\n"
            }
        ],
