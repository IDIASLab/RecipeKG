        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8236/satiny-chocolate-glaze/",
        "name": "Satiny Chocolate Glaze",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2418635.jpg",
