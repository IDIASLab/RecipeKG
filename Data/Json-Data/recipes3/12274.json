    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254569/loaded-baked-red-potato-salad/",
        "name": "Loaded Baked Red Potato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4460533.jpg",
            "width": 2205,
            "height": 2205
        },
        "datePublished": "2018-06-08T14:39:59.000Z",
        "description": "This is a new twist on ho-hum potato salad! I like to use red potatoes because there is no peeling necessary and it looks pretty!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "12 red potatoes, unpeeled",
            "4 slices bacon",
            "1 cup sour cream",
            "1 cup plain yogurt",
            "\u00bd cup shredded Cheddar cheese, or more to taste",
            "\u2153 cup chopped fresh chives",
            "1 (1 ounce) package ranch dressing mix",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes in a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 15 minutes. Drain and cool until easily handled, about 10 minutes. Cut into cubes and place in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Crumble into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bacon, sour cream, yogurt, Cheddar cheese, chives, ranch dressing mix, salt, and black pepper together in a bowl. Pour over potatoes and toss gently to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate potato mixture until chilled, at least 1 hour.\n"
            }
        ],
