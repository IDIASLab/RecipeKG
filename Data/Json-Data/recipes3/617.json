    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9166/old-fashioned-scalloped-corn/",
        "name": "Old-Fashioned Scalloped Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F963753.jpg",
            "width": 1949,
            "height": 1943
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Delicious country style creamed corn casserole, a family favorite! Originally submitted to ThanksgivingRecipe.com.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 (15 ounce) cans creamed corn",
            "2 eggs",
            "1 cup crushed saltine crackers",
            "\u00bd cup butter",
            "\u00bd teaspoon paprika",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Butter one 8x11x2 inch casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium sized mixing bowl, combine the creamed corn, eggs, 1/4 cup of the melted butter and 1/2 of the cracker crumbs. Pour mixture in into the prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix the remaining melted butter, cracker crumbs, paprika and pepper. Sprinkle crumb topping over the casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 40 minutes, until topping browns slightly and corn is bubbly around the edges.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Corn Casserole Recipes"
        ],
