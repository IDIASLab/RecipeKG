    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268606/chocolate-almond-fudge/",
        "name": "Chocolate Almond Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5874135.jpg",
            "width": 2970,
            "height": 1649
        },
        "datePublished": "2018-10-11T02:56:41.000Z",
        "description": "Rich chocolate flavor in a creamy fudge enhanced by the crunchy, nutty flavor of almonds. Add this to a holiday dessert table for rave reviews.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "36 pieces",
        "recipeIngredient": [
            "Cooking spray",
            "2 pounds powdered sugar",
            "1 cup unsweetened cocoa powder",
            "\u00bd teaspoon salt",
            "\u00bd cup Almond Breeze Vanilla almondmilk",
            "\u00be cup vegan buttery sticks, softened",
            "\u00be dash almond flavor",
            "1 cup chopped almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a 9-inch baking pan with foil; spray foil with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large saucepan, mix powdered sugar, cocoa, and salt. Add almondmilk and buttery sticks. Cook over medium-low heat until buttery sticks melt and mixture is all moistened, stirring frequently. Cook and stir until mixture is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat. Beat with spoon until mixture loses its shine, about 5 minutes. Stir in almond flavor and almonds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread in pan. Cool completely, about 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Almond Breeze"
        ],
