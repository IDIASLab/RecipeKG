    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15875/macadamia-clusters/",
        "name": "Macadamia Clusters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F967319.jpg",
            "width": 3431,
            "height": 3456
        },
        "datePublished": "1999-07-13T01:51:47.000Z",
        "description": "I made these for Christmas '98 for work and had to make more for return trips back to work. They are very easy and decadent looking.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "8 ounces vanilla flavored confectioners' coating",
            "2 (3.5 ounce) packages macadamia nuts",
            "\u00bd tablespoon orange zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place candy coating in microwave safe bowl; microwave uncovered on high for 40 to 60 seconds. Stir every 30 seconds, until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in nuts and orange zest.  Drop mixture by teaspoons onto waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow to sit until set, store covered in refrigerator. If desired, melt approximately 1 ounce semisweet chocolate and drizzle over.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Nut Candy Recipes"
        ],
