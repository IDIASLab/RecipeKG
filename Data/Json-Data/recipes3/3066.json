        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21571/quick-clam-sauce/",
        "name": "Quick Clam Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F845448.jpg",
            "width": 1024,
            "height": 768
