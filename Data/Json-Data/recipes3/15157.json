    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266164/air-fryer-meatloaf/",
        "name": "Air Fryer Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4802557.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-07-06T02:48:50.000Z",
        "description": "If you have an air fryer, try this simple recipe for meatloaf in under 1 hour.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "1 egg, lightly beaten",
            "3 tablespoons dry bread crumbs",
            "1 small onion, finely chopped",
            "1 tablespoon chopped fresh thyme",
            "1 teaspoon salt",
            "ground black pepper to taste",
            "2 mushrooms, thickly sliced",
            "1 tablespoon olive oil, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an air fryer to 392 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ground beef, egg, bread crumbs, onion, thyme, salt, and pepper in a bowl. Knead and mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer beef mixture to a baking pan and smooth the top. Press mushrooms into the top and coat with olive oil. Place the pan into the air fryer basket and slide into the air fryer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set air fryer timer for 25 minutes and roast meatloaf until nicely browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let meatloaf rest at least 10 minutes before slicing into wedges and serving.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Beef Meatloaf Recipes"
        ],
