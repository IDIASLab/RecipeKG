    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220484/chicken-baseballs/",
        "name": "Chicken Baseballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2462817.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2018-06-08T14:00:34.000Z",
        "description": "Great for kids. You can add half to a full packet of dry ranch dressing mix to the filling for yet more flavor.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 baseballs",
        "recipeIngredient": [
            "1 (3 ounce) package cream cheese, softened",
            "1 teaspoon margarine",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "2 tablespoons chopped onion",
            "2 tablespoons milk",
            "2 cups cubed, cooked chicken",
            "1 (8 ounce) package refrigerated crescent rolls",
            "2 tablespoons bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the cream cheese, margarine, salt, pepper, milk, and onion together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold the chicken into the cream cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate crescent rolls into 4 rectangles and smooth the perforations.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon 1/2 cup of the chicken mixture into the center of each rectangle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold the corners over the chicken mixture and pinch along the edges to seal into a ball shape. Arrange on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle bread crumbs over the top of the newly formed 'baseballs.'\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until browned, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Chicken Pie Recipes"
        ],
