    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269997/bell-peppers-stuffed-with-mexican-cauliflower-rice/",
        "name": "Bell Peppers Stuffed with Mexican Cauliflower Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bell Peppers Stuffed with Mexican Cauliflower Rice"
        },
        "datePublished": "2019-01-15T03:17:26.000Z",
        "description": "This dish is a new favorite among my family and myself. I wanted to make something that had a Mexican flair, not too spicy or hot, and keep it low carb. This is definitely going to be a repeated recipe in my home.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 stuffed peppers",
        "recipeIngredient": [
            "\u00bd head cauliflower, or more to taste",
            "2 (14.5 ounce) cans diced roasted tomatoes",
            "1 (8 ounce) can tomato sauce",
            "1 teaspoon garlic salt",
            "1 teaspoon Italian seasoning",
            "4 green bell peppers, tops and seeds removed",
            "1 pound hamburger meat",
            "1 (10 ounce) package beef chorizo",
            "4 ounces shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Chop cauliflower into rice-like shapes using a vegetable chopper and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix diced tomatoes, tomato sauce, garlic salt, and Italian seasoning together in a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add bell peppers and cook 5 to 7 minutes. Remove from water and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium heat. Cook and stir hamburger meat in the hot skillet until browned and crumbly, 5 to 7 minutes. Drain and discard grease. Clear an area in the center of the skillet and add chorizo. Cook about 5 minutes; stir into hamburger meat in the skillet. Add chopped cauliflower and cook, stirring frequently, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove skillet from heat and add 1/2 the tomato sauce mixture. Stir thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill each bell pepper with meat mixture and place in a small glass casserole dish. Cover with remaining tomato sauce mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Remove stuffed peppers from the oven and cover with shredded Cheddar cheese. Return to the hot oven and bake until cheese is melted, about 5 minutes more.\n"
            }
        ],
