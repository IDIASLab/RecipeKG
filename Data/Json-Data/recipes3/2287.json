    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13545/fresh-corn-spoon-bread/",
        "name": "Fresh Corn Spoon Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3088718.jpg",
            "width": 1688,
            "height": 1705
        },
        "datePublished": "1998-11-09T01:51:35.000Z",
        "description": "This a great custard-like corn bread that can take the place of a stuffing.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 cups milk",
            "1\u2009\u00bc cups cornmeal",
            "1\u2009\u00bd teaspoons salt",
            "1 cup sweet corn",
            "\u2153 cup chopped green onions",
            "\u00bc cup unsalted butter, cubed",
            "3 eggs, separated"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Butter one 8x8x2- inch glass baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk, cornmeal, and salt in a heavy saucepan over medium heat. Bring mixture to a boil, whisking constantly until mixture begins to thicken, about 4 minutes. Reduce heat to low and cook until the mixture is very thick, stirring frequently, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cornmeal to a large bowl. Stir in corn, green onions, and butter. Add the egg yolks one at a time, stirring after each one.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg whites until stiff peaks form. Fold half of the egg white mixture into the warm cornmeal to lighten it; fold in remaining egg whites. Spread the batter evenly in the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until light, puffed and golden brown, about 30 minutes. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Cornbread Recipes"
