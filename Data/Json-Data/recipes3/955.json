        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12300/heath-bar-pie/",
        "name": "Heath Bar Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2244428.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1998-07-14T17:06:51.000Z",
        "description": "If you like Heath candy bars, you'll love this pie!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT4H30M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) prepared graham cracker crust",
            "\u2153 cup caramel ice cream topping",
            "1\u2009\u00bd cups milk",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "6 (1.4 ounce) bars chocolate covered English toffee, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spread caramel topping into pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat milk and pudding mix for 2 minutes; let stand for 5 minutes. Stir in whipped topping and chopped candy bars. Spoon into crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze for 4 hours or until set. Before serving, let stand for 15 minutes so that pie can easily be cut. Store any leftovers in refrigerator.\n"
            }
        ],
