        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11912/old-fashioned-macaroni-salad/",
        "name": "Old Fashioned Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3796144.jpg",
