        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13153/flat-dumplings/",
        "name": "Flat Dumplings",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9119.jpg",
