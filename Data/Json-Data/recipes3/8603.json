    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220572/braised-orange-chicken/",
        "name": "Braised Orange Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1001509.jpg",
            "width": 1705,
            "height": 1694
        },
        "datePublished": "2018-06-08T14:00:38.000Z",
        "description": "Tender chicken thigh meat simmers in an orange juice and pepper broth for a citrus-inspired meal that's ready in just 30 minutes, but tastes like it cooked for hours.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "4 bone-in chicken thighs",
            "1 onion, chopped",
            "2 cloves garlic, minced",
            "1 (14.5 ounce) can diced tomatoes with juice",
            "2 cups orange juice (no pulp)",
            "\u00bc cup brandy-based orange liqueur (such as Grand Marnier\u00ae)",
            "1 pinch salt and ground black pepper",
            "\u00bc teaspoon red pepper flakes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium heat; cook chicken thighs in the hot oil until browned, about 5 minutes per side. Remove chicken thighs and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion in the same skillet until softened, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in garlic; cook and stir until fragrant, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in tomatoes, orange juice, orange liqueur, salt, black pepper, and red pepper flakes until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return chicken thighs to the skillet, cover, and bring to a boil. Reduce heat to medium-low and simmer until chicken is tender and no longer pink inside, 25 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Thigh Recipes"
