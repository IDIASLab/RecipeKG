        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15959/chocolate-bar-pie-ii/",
        "name": "Chocolate Bar Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9366719.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-12-16T21:54:49.000Z",
        "description": "Everyone young and old will love this pie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (9 inch) prepared graham cracker crust",
            "6 (1.45 ounce) bars milk chocolate with almonds, coarsely chopped",
            "2 cups miniature marshmallows",
            "\u00bd cup milk",
            "1 pinch salt",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chopped chocolate bars, marshmallows, milk, and salt in the top of a double boiler. Heat and stir until mixture is melted and smooth. Remove from heat and allow to cool, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, beat cream until soft peaks form. Fold cream into cooled chocolate mixture. Pour mixture into graham cracker crust. Chill before serving. This pie is also delicious frozen.\n"
            }
        ],
