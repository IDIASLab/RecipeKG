    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10540/aunty-lauras-shortbread/",
        "name": "Aunty Laura's Shortbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1923939.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "1997-11-30T04:42:31.000Z",
        "description": "This is my aunt's shortbread recipe.  It sticks to your teeth and tastes GREAT with milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups butter",
            "1 cup fructose (fruit sugar)",
            "1 cup white rice flour",
            "3 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together butter and sugar.  Mix in rice flour until well blended.  Add all-purpose flour.  Remove from bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough for 5 minutes.  Roll dough out and place in a greased and floured 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prick the top with a fork.  Bake 35 minutes or until golden brown and a knife inserted in the middle comes out clean.  Cut into bars and allow to cool in the pan.\n"
            }
        ],
