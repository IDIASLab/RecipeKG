        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24825/chicken-salad-balsamic/",
        "name": "Chicken Salad Balsamic",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1034759.jpg",
