        "mainEntityOfPage": "https://www.allrecipes.com/recipe/85881/tangy-cucumber-and-avocado-salad/",
        "name": "Tangy Cucumber and Avocado Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F875887.jpg",
            "width": 3264,
