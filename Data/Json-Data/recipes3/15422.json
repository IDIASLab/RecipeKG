    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256079/cinnamon-chocolate-truffles/",
        "name": "Cinnamon Chocolate Truffles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5341335.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2018-07-19T13:04:36.000Z",
        "description": "Rich little bites of cinnamon-y goodness. Super easy to make and great for the holidays! Can be done with or without the walnut topping.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT9H50M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "12 ounces semisweet chocolate",
            "1 (4 ounce) package cream cheese, softened",
            "2 tablespoons heavy whipping cream",
            "2 tablespoons ground cinnamon, divided",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground nutmeg",
            "\u00bd cup finely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine chocolate, cream cheese, and heavy cream in a saucepan over medium heat; cook and stir until chocolate is melted and mixture is smooth, about 5 minutes. Remove from heat and stir in 1 tablespoon cinnamon, vanilla extract, and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chocolate mixture into a small bowl and refrigerate until firm but still pliable, 1 1/2 to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining 1 tablespoon cinnamon and walnuts in a shallow bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll chocolate mixture into balls and set on a plate. Sprinkle walnuts on top, pressing walnuts to make sure they stick. Refrigerate truffles until fully firm, 8 hours to overnight.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Truffle Recipes"
        ],
