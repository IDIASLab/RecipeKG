    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8581/sarahs-chicken-lasagna/",
        "name": "Sarah's Chicken Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3936775.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-05-03T19:45:35.000Z",
        "description": "This recipe is very different from the tomato based lasagna.  It is a creamy lasagna, and very good.  This freezes well, and can be prepared in advance.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 - 10 servings",
        "recipeIngredient": [
            "8 lasagna noodles",
            "3 tablespoons butter",
            "\u00be cup milk",
            "\u00bc cup chopped pimento peppers",
            "\u00bd teaspoon dried basil",
            "3 cups diced, cooked chicken meat",
            "\u00bd cup grated Parmesan cheese",
            "\u00bd cup chopped green bell pepper",
            "\u00bd cup chopped onion",
            "2 (10.75 ounce) cans  condensed cream of chicken soup",
            "1 (6 ounce) can sliced mushrooms, drained",
            "1 (16 ounce) package creamed, small curd cottage cheese",
            "2 cups shredded processed cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook noodles in a large pot of boiling water until done.  Rinse, and drain. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, saute green pepper and onions in butter or margarine over medium heat.  Stir in undiluted soup, milk, mushrooms, pasteurized cheese, pimento, and basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place half of the noodles in 13 x 7 x 2 inch baking dish.  Layer with half of the cream sauce, 1/2 cottage cheese, 1/2 diced chicken, and 1/2 Parmesan cheese.  Repeat layers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
            "Chicken"
        ],
