    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72777/mediterranean-orzo-spinach-salad/",
        "name": "Mediterranean Orzo Spinach Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F30110.jpg",
            "width": 576,
            "height": 576
        },
        "datePublished": "2018-06-08T13:23:40.000Z",
        "description": "A bright, flavorful array of Mediterranean ingredients!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cup uncooked orzo pasta",
            "2 tablespoons extra virgin olive oil, divided",
            "1 pound ground lamb",
            "2 cloves garlic, chopped",
            "1 tablespoon ground coriander",
            "salt and pepper to taste",
            "4 cups fresh spinach leaves, chopped",
            "3 tomatoes, seeded and chopped",
            "1 lemon, zested and juiced",
            "\u00bc cup chopped fresh mint leaves",
            "\u00bc cup chopped fresh parsley",
            "5 green onions, chopped",
            "1 cup crumbled feta cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a pot of lightly salted water to a boil. Add orzo pasta, cook for 5 minutes, or until al dente.  Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a skillet over medium heat. Place the lamb and garlic in the skillet. Season lamb with coriander, salt, and pepper. Cook until evenly browned. Remove from heat, and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix the spinach, tomatoes, lemon juice and zest, mint, parsley, green onions, and remaining olive oil. Toss with the orzo, lamb, and feta cheese to serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Tomato Pasta Salad Recipes"
        ],
