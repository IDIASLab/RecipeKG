        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149567/greek-souvlaki-dressing/",
        "name": "Greek Souvlaki Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9252576.jpg",
