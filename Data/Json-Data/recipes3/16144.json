    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261398/pork-and-vegetable-casserole/",
        "name": "Pork and Vegetable Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5744234.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-09-13T00:08:21.000Z",
        "description": "You can use any mild cheese that melts easily for this casserole. It is one of our family favorites and we make it often.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pork tenderloin",
            "salt and freshly ground black pepper to taste",
            "1 tablespoon rapeseed oil",
            "4 ounces shredded Gruyere cheese",
            "1 cup heavy whipping cream",
            "2 eggs",
            "1 pinch ground nutmeg",
            "1 pound potatoes, peeled and thinly sliced",
            "\u00bd pound mushrooms, sliced",
            "\u00bd pound carrots, thinly sliced",
            "2 tablespoons chopped fresh parsley, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season pork with salt and pepper. Heat oil in a large skillet over medium-high heat and cook pork until browned on all sides, 5 to 8 minutes. Remove from skillet and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Gruyere cheese, cream, eggs, nutmeg, salt, and pepper in a bowl and stir together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice pork and layer pork, potatoes, mushrooms, and carrots into a casserole dish. Pour egg-cheese mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cooked through and lightly browned on top, about 1 hour. Sprinkle with parsley to serve.\n"
            }
        ],
