        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229683/peruvian-aji-amarillo-cheese-sauce/",
        "name": "Peruvian Aji Amarillo Cheese Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1008453.jpg",
