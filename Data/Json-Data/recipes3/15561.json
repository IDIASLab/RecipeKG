        "cookTime": "P0DT0H15M",
        "totalTime": "P1DT1H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 cups uncooked macaroni",
            "\u2153 cup Italian-style salad dressing",
            "\u00bd cup sour cream",
            "1 cup mayonnaise",
            "1 onion, chopped",
            "2 stalks celery, chopped",
            "\u00bd teaspoon garlic powder",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "1 (5 ounce) can tuna, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook macaroni in a large pot of boiling salted water until done. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate macaroni in Italian dressing for 2 to 3 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sour cream, mayonnaise, onion, celery, garlic powder, tuna, and salt and pepper into macaroni. Chill before serving.\n"
            }
        ],
