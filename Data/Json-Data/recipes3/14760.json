    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265543/my-moms-potato-salad-with-carrot-and-egg/",
        "name": "My Mom's Potato Salad with Carrot and Egg",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "My Mom's Potato Salad with Carrot and Egg"
        },
        "datePublished": "2018-06-14T06:54:50.000Z",
        "description": "My mom often makes this potato salad for dinner and it's loved by my family.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 potatoes, peeled and cut into eighths",
            "salt and ground black pepper to taste",
            "1 cucumber, thinly sliced",
            "\u00bd onion, thinly sliced",
            "1 carrot, thinly sliced into rounds",
            "1 tablespoon apple juice",
            "\u2153 tablespoon rice vinegar",
            "2 hard-boiled eggs, chopped",
            "5 tablespoons mayonnaise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak potatoes in a bowl of water to draw out the bitterness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rub salt over cucumber and onion slices; squeeze out excess moisture and pat dry. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain potatoes and place in a pot with carrot. Add enough water to cover. Season with salt. Bring to a boil; reduce heat to low and simmer until potatoes can be easily pierced with a bamboo skewer, 10 to 15 minutes. Remove carrot from the pot when tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain potatoes and return to the pot. Add apple juice and vinegar; mash potatoes with a wooden spatula or fork for about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer potatoes to a bowl. Add cooked carrot, cucumber, onion, and eggs. Mix in mayonnaise. Season with salt and pepper. Refrigerate until cool, 30 minutes to 1 hour. Serve in individual bowls.\n"
            }
        ],
