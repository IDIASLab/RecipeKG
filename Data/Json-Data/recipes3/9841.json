        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232478/quick-antipasto-pasta-salad/",
        "name": "Quick Antipasto Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1012574.jpg",
            "width": 1073,
