        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10896/cloud-nine-butterscotch-squares/",
        "name": "Cloud Nine Butterscotch Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cloud Nine Butterscotch Squares"
        },
        "datePublished": "1998-05-20T21:07:49.000Z",
        "description": "Butterscotch cookie squares with chips.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 (3.4 ounce) package instant butterscotch pudding mix",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd cup milk",
            "1 cup rolled oats",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix the butter or margarine, instant pudding mix, eggs, and vanilla.  Stir well.  Mix in the flour, baking powder, milk, oats and chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in 9 inch pan for 20 to 25 minutes at  350 degrees F (175 degrees C).  Frost with chocolate icing.\n"
            }
        ],
