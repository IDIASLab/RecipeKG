    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254378/asparagus-pea-shoot-spinach-and-potato-soup/",
        "name": "Asparagus, Pea Shoot, Spinach, and Potato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Asparagus, Pea Shoot, Spinach, and Potato Soup"
        },
        "datePublished": "2018-06-08T14:39:47.000Z",
        "description": "Delicious homemade soup that's packed with green garden vegetables and herbs. Use a handful of the herbs of your choice. It's a lovely fresh spring color. Garnish with a little cream.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 large potato, quartered, or more to taste",
            "2\u2009\u00bd cups chicken stock, or more to taste",
            "1 bunch fresh spinach",
            "1 bunch pea shoots",
            "1 tablespoon vegetable oil",
            "1 bunch asparagus, trimmed and chopped",
            "1 onion, finely chopped",
            "2 ribs celery, chopped",
            "2 tablespoons chopped pineapple mint, or to taste",
            "2 tablespoons chopped fresh sage, or to taste",
            "1 tablespoon creme fraiche",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil. Add potato; simmer until tender, about 20 minutes. Drain. Return to the pot and pour in chicken stock. Add spinach and pea shoots.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet. Add asparagus, onion, celery, mint, and sage. Cook and stir until asparagus is tender, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir asparagus mixture into the chicken stock in the pot. Simmer soup over medium-low heat until flavors combine, about 30 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soup into a food processor; process until smooth, about 20 seconds. Pour back into the pot. Stir in creme fraiche. Season with salt and pepper.\n"
            }
        ],
