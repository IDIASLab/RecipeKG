        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11842/tomato-alfredo-sauce-with-artichokes/",
        "name": "Tomato Alfredo Sauce with Artichokes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F12779.jpg",
            "width": 800,
            "height": 600
