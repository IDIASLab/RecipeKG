    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240662/buckwheat-energy-bars/",
        "name": "Buckwheat Energy Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3123099.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2018-06-08T14:18:44.000Z",
        "description": "A healthy, low-sugar snack bar created by my daughter and son-in-law. These are delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 10x15-inch batch",
        "recipeIngredient": [
            "1\u2009\u00bd cups buckwheat flour",
            "1 tablespoon ground cinnamon",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground nutmeg",
            "1 cup honey",
            "1 cup natural peanut butter",
            "\u00bd cup olive oil",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "3 cups quick-cooking oats",
            "1 (12 ounce) bag semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Butter a 10x15-inch baking dish or line a deep baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix buckwheat flour, cinnamon, baking soda, salt, and nutmeg together in a bowl. Beat honey, peanut butter, olive oil, eggs, and vanilla extract together in a separate bowl; mix into flour mixture. Fold oats and chocolate chips into flour mixture; spread into the prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are lightly browned, 15 to 20 minutes. Cool completely on a wire rack before cutting into bars.\n"
            }
        ],
