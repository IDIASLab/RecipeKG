        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14941/garlic-parmesan-olives/",
        "name": "Garlic Parmesan Olives",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F377697.jpg",
