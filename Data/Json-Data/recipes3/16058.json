    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265989/warm-kale-salad/",
        "name": "Warm Kale Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Warm Kale Salad"
        },
        "datePublished": "2018-09-07T06:44:35.000Z",
        "description": "This warm kale salad showcases bacon, shallots, roasted red peppers, and walnuts dressed in a white balsamic vinaigrette.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd cup olive oil",
            "6 teaspoons white balsamic vinegar",
            "4 teaspoons Dijon mustard",
            "2\u2009\u00bd teaspoons honey",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bc teaspoon red pepper flakes",
            "12 strips bacon",
            "4 large shallots, thinly sliced into rings",
            "12 cups packed kale, massaged",
            "3 roasted red peppers, drained and chopped",
            "1 pinch salt and ground black pepper to taste",
            "2 large cloves garlic, minced",
            "\u00bd cup walnuts",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, balsamic vinegar, mustard, honey, salt, black pepper, and red pepper flakes together to make the vinaigrette.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Reserve 3 tablespoons bacon fat in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook shallots in the bacon fat until golden, 3 to 4 minutes. Drain on a paper towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add kale and red peppers to the skillet and saute until warmed through but not wilted, 2 to 3 minutes. Season with salt and pepper. Add garlic; cook and stir until fragrant, about 30 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place kale, red peppers, and garlic in a serving bowl. Top with the vinaigrette, diced bacon, shallots, walnuts, and Parmesan cheese.\n"
            }
        ],
