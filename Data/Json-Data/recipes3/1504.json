        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238251/bloody-mary-tomatoes/",
        "name": "Bloody Mary Tomatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1225447.jpg",
            "width": 5184,
            "height": 3888
