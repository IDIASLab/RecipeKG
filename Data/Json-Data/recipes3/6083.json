    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/93253/fairly-fast-salsa/",
        "name": "Fairly Fast Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F646181.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:28:34.000Z",
        "description": "This is a sure winner tomato salsa recipe.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 quart",
        "recipeIngredient": [
            "5 tomatoes, halved",
            "1 onion, halved",
            "1 jalapeno pepper sliced in half lengthwise and seeded",
            "2 cloves garlic",
            "\u00be cup fresh cilantro leaves",
            "2 tablespoons fresh lime juice",
            "4 teaspoons salt",
            "1 teaspoon pureed chipotle peppers in adobo sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven's broiler and set the oven rack about 6 inches from the heat source. Line a large baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange the tomatoes, onion, and jalapeno pepper on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast the vegetables under the broiler until the skins are blackened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the roasted vegetables, garlic, cilantro, lime juice, salt, and chipotle peppers in a blender; blend to desired consistency. Store unused portions in refrigerator up to 1 week.\n"
            }
        ],
