        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237307/zesty-lime-vinaigrette/",
        "name": "Zesty Lime Vinaigrette",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1997291.jpg",
