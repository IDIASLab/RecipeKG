        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256744/mini-snowball-chip-cookies/",
        "name": "Mini Snowball Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4468007.jpg",
            "width": 5216,
            "height": 3663
        },
        "datePublished": "2018-06-08T14:42:06.000Z",
        "description": "A small but very light cookie that basically melts in your mouth.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups butter, softened",
            "\u00be cup confectioners' sugar",
            "1 tablespoon vanilla extract",
            "\u00bd teaspoon salt",
            "3 cups all-purpose flour",
            "1 (12 ounce) package semisweet chocolate mini morsels",
            "\u00bc cup confectioners' sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, 3/4 cup confectioners' sugar, vanilla extract, and salt in a large bowl; beat with an electric mixer until creamy. Beat in flour gradually. Fold mini morsels into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1 1/4-inch balls; arrange on ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 10 minutes. Sprinkle 1/4 cup confectioners' sugar over cookies while still hot. Cool on the baking sheets for 10 minutes. Transfer to wire racks to finish cooling.\n"
            }
        ],
