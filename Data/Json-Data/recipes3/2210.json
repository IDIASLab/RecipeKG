    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13011/potato-leek-soup-i/",
        "name": "Potato Leek Soup I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F352692.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-01-07T16:11:58.000Z",
        "description": "A rich potato soup with NO milk or cream. Garnish with fresh parsley.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "5 pounds white potatoes, peeled and quartered",
            "6 cups chicken broth",
            "2 leeks, bulb only",
            "\u00bd cup butter",
            "\u00bc cup white wine",
            "salt to taste",
            "\u00bc teaspoon freshly ground white pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook potatoes in chicken stock until soft. Set aside, do not drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put potatoes in the work bowl of a food processor in batches. Add 5 cups of chicken stock from the potato cooking pot.  Puree until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Half the leeks lengthwise, and soak in water to clean.  Finely slice.  Saute in butter until transparent.  Add white wine, and cook for 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a soup pot, combine remaining cup of chicken stock from the potato cooking pot and sauteed leeks.  Stir in pureed potatoes, and bring to a simmer.  Season with salt and white pepper.  Cook to desired consistency, adding more stock if necessary. Garnish with parsley.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Cream Soup Recipes",
            "Potato Leek Soup Recipes"
        ],
