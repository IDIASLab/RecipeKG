    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/142544/creamy-french-american-soup/",
        "name": "Creamy French-American Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F135243.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:47:25.000Z",
        "description": "This soup is an American take on the classic French Onion soup. With the addition of heavy cream, it has a little more fat but lots more flavor!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "2 onions, halved and thinly sliced",
            "1 pint heavy cream",
            "2 cups beef broth",
            "salt and pepper, to taste",
            "3 slices French bread",
            "3 slices provolone cheese",
            "3 slices Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to broiler setting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Add onions and cook until softened, 5 to 8 minutes. Stir in the heavy cream and beef broth. Bring to a boil, then reduce heat to low and simmer, loosely covered, for 20 minutes. Season with salt and pepper, to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each slice of bread with one slice of provolone and one slice of Swiss cheese; place on a cookie sheet. Broil on top oven rack until the cheese browns and the edges of the bread are crispy, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, ladle soup into 3 bowls and top with one slice of the cheese-topped bread.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Onion Soup Recipes"
        ],
