        "mainEntityOfPage": "https://www.allrecipes.com/recipe/187381/cherry-almond-oatmeal/",
        "name": "Cherry Almond Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F385823.jpg",
