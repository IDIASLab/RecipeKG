        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264993/keto-diet-avocado-egg-bake/",
        "name": "Keto Diet Avocado Egg Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7228181.jpg",
