        "recipeIngredient": [
            "\u00bc cup margarine",
            "6 slices stale bread, crumbled",
            "1 small onion, chopped",
            "1 clove garlic, minced",
            "4 cups chopped tomatoes",
            "1 teaspoon salt",
            "\u00bc teaspoon dried sage",
            "\u00bc teaspoon dried basil",
            "\u215b teaspoon ground black pepper"
