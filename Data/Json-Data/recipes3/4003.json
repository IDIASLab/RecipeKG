        "name": "Apple Avocado Salad with Tangerine Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F118200.jpg",
            "width": 250,
            "height": 250
