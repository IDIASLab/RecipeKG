    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241885/healthy-granola-clusters/",
        "name": "Healthy Granola Clusters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2185598.jpg",
            "width": 2612,
            "height": 2612
        },
        "datePublished": "2018-06-08T14:20:33.000Z",
        "description": "Very easy! Great for moms that don't want white flour or sugary snacks! My 3-year-old daughter's favorite 'cookie'.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "40 clusters?",
        "recipeIngredient": [
            "\u00be cup raw sugar",
            "\u00bd cup butter",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups old-fashioned oats",
            "\u00be cup whole wheat flour",
            "\u00bc cup applesauce",
            "1 egg",
            "2 tablespoons molasses",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon ground ginger",
            "\u00bc teaspoon ground cloves",
            "\u00bc teaspoon salt",
            "1 cup chopped walnuts",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat sugar, butter, and vanilla extract together in a bowl using an electric mixer until smooth and creamy. Add oats, whole wheat flour, applesauce, eggs, molasses, baking powder, cinnamon, nutmeg, ginger, cloves, and salt and mix well. Fold walnuts and raisins into mixture; spoon onto a baking sheet, about 1 teaspoon per cluster.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned and crisp, 10 to 13 minutes.\n"
            }
        ],
