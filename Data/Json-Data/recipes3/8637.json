    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220728/raw-vegan-strawberry-pie/",
        "name": "Raw Vegan Strawberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1226656.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "2018-06-08T14:00:46.000Z",
        "description": "A super easy, raw, vegan strawberry pie! It's absolutely delicious. Feel free to add blueberries also.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT2H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 cup almonds",
            "1 cup shredded coconut",
            "1 cup pitted dates",
            "2 tablespoons coconut oil",
            "1 pinch salt",
            "2 cups strawberries",
            "2 cups shredded coconut",
            "2 tablespoons coconut oil",
            "1 pinch salt",
            "3 strawberries, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place almonds, 1 cup coconut, dates, 2 tablespoons coconut oil, and salt in a blender. Puree until ingredients form a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press dough into a pie pan to form the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 2 cups strawberries, 2 cups coconut, 2 tablespoons coconut oil, and salt in a blender. Puree into a cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cream over the crust. Top with sliced strawberries. Refrigerate until cream is thickened, about 2 hours.\n"
            }
        ],
        "recipeCategory": [
