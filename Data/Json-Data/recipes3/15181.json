    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283699/thai-stir-fried-noodles-pad-see-ew/",
        "name": "Thai Stir-Fried Noodles (Pad See Ew)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Thai Stir-Fried Noodles (Pad See Ew)"
        },
        "datePublished": "2018-07-06T11:42:45.000Z",
        "description": "Chicken, broccoli, and rice noodles are cooked in a sweet soy sauce in this quick and easy Thai noodle dish that's ready in just 20 minutes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "1 (6 ounce) package rice stick noodles (rice vermicelli)",
            "2 tablespoons oyster sauce",
            "2 tablespoons water",
            "2 tablespoons dark soy sauce",
            "2 teaspoons soy sauce",
            "2 teaspoons distilled white vinegar",
            "2 teaspoons white sugar",
            "2 tablespoons vegetable oil",
            "2 cloves garlic",
            "5 ounces Chicken, broilers or fryers, thigh, meat only, raw",
            "1 cup small broccoli florets",
            "1 large egg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place rice noodles in a large bowl and cover with boiling water. Soak until tender, 4 to 8 minutes. Rinse thoroughly with water and leave in a colander to drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oyster sauce, water, dark and regular soy sauce, vinegar, and sugar in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a wok over high heat; add oil and mince garlic straight into the wok. Heat until oil is hot and garlic is starting to turn golden, about 1 minute. Add chicken and broccoli; stir-fry for 1 minute. Move chicken and broccoli to one side. Crack egg into the skillet and scramble it, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add noodles and sauce; fold gently to combine, until sauce evenly coats the noodles and caramelizes a bit, and chicken is cooked through, 1 to 2 minutes.\n"
            }
        ],
