        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236817/baked-chicken-and-corn/",
        "name": "Baked Chicken and Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4425466.jpg",
            "width": 3264,
            "height": 1836
        },
        "datePublished": "2018-06-08T14:13:26.000Z",
        "description": "This is an easy way to make a one-dish meal with little effort. It is also very tasty and my kids love it. You can also use mixed vegetables instead of corn.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 (16 ounce) cans whole kernel corn, drained",
            "4 skinless, boneless chicken breast halves",
            "2 tablespoons butter",
            "1 tablespoon chopped garlic",
            "\u00bd teaspoon ground black pepper",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread corn into the bottom of a baking dish; add chicken breasts. Cover chicken with butter, garlic, black pepper, and salt. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink in the center, 30 to 45 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
