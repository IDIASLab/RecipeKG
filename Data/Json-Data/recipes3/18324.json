    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270260/broccoli-cauliflower-fritters/",
        "name": "Broccoli-Cauliflower Fritters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6324418.jpg",
            "width": 2536,
            "height": 2891
        },
        "datePublished": "2019-01-26T10:09:44.000Z",
        "description": "These low-carb veggie fritters are made from shredded cauliflower and broccoli held together with eggs and almond flour.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd pound broccoli, cut into florets",
            "\u00bd pound cauliflower, cut into florets",
            "1\u2009\u00be cups almond flour",
            "2 eggs, beaten",
            "\u00bd onion, finely chopped",
            "1 tablespoon garlic powder",
            "2 tablespoons coconut oil",
            "ground black pepper to taste",
            "coarse sea salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place broccoli and cauliflower in a food processor; pulse until shredded.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine almond flour, beaten eggs, onion, and garlic powder in a large bowl. Mix in shredded broccoli and cauliflower mixture. Roll batter into 2-inch balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat coconut oil in a large pan over medium-high heat. Place fritters in the pan and flatten slightly with a spatula. Cook until crisp, 3 to 4 minutes per side. Transfer to a paper towel-lined plate and season with salt and pepper. Repeat with any remaining fritters.\n"
            }
        ],
