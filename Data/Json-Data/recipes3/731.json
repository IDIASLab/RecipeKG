    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10565/german-wreaths/",
        "name": "German Wreaths",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "German Wreaths"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Made with hard-boiled eggs! Often a Christmas cookie but good any time.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 - 5 dozen",
        "recipeIngredient": [
            "\u00be cup shortening",
            "1\u2009\u00bd cups confectioners' sugar",
            "2 hard-cooked eggs, chopped",
            "2 cups all-purpose flour",
            "2 eggs, separated",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large bowl cream together shortening and confectioners' sugar. Beat in uncooked egg yolks and hard-boiled eggs.  Gradually blend in flour. Cover dough and chill for 8 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pre-heat oven to 350 degrees F (175 degrees C) and lightly grease two cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out dough on floured surface to 1/4 inch thickness. Cut into pencil thick strips about 6 to 8 inches long.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For each cookie, braid three strips and form into a circle pinching ends together. Place cookies 1 inch apart on cookie sheets.  Brush with the beaten eggwhites.  Sprinkle with confectioners' sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 12 to 15 minutes until lightly colored. Cool on wire racks.\n"
            }
        ],
