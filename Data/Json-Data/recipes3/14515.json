    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264064/spicy-ranch-party-mix/",
        "name": "Spicy Ranch Party Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7616524.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-08T14:48:42.000Z",
        "description": "This is the perfect party snack to have lying around. It's salty, a bit spicy, and loaded with bright herb flavor. Store in an airtight container for up to 2 weeks.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "4 tablespoons butter",
            "2 tablespoons Worcestershire sauce",
            "2 cloves garlic, minced",
            "1 tablespoon dried dill",
            "1 tablespoon dried parsley",
            "1 tablespoon dried chives",
            "1 tablespoon salt",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon chipotle chile powder",
            "3 cups crispy wheat cereal squares (such as Wheat Chex\u00ae)",
            "3 cups crispy rice cereal squares (such as Rice Chex\u00ae)",
            "3 cups crispy corn cereal squares (such as Corn Chex\u00ae)",
            "2 cups mixed nuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 200 degrees F (95 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan. Add Worcestershire sauce, minced garlic, dill, parsley, chives, salt, garlic powder, and chipotle powder. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place wheat, rice, and corn cereal squares in a large bowl. Add nuts. Stir in the seasoned butter. Spread mixture evenly on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring once halfway through, until toasted and fragrant, about 1 hour. Let cool before serving.\n"
            }
        ],
