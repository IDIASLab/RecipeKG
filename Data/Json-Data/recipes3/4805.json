    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/64655/bacon-cheeseburgers/",
        "name": "Bacon Cheeseburgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F292235.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2018-06-08T13:22:35.000Z",
        "description": "This takes a little more effort than the average cheeseburger, but is well worth it! Serve on buns with your favorite condiments.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 burgers",
        "recipeIngredient": [
            "1 pound sliced bacon, diced",
            "5 pounds ground beef chuck",
            "1 large onion, chopped",
            "\u00bc cup steak sauce",
            "salt and pepper to taste",
            "1 pound American cheese slices"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat a grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet over medium heat. Fry, stirring occasionally. When the bacon is almost done, add the onion. Cook until the bacon is crisp, and the onion is tender. Remove onion and bacon from the pan with a slotted spoon, and transfer to a food processor. Pulse a couple of times to chop finely. Do not puree. Pour into a large bowl, and mix with steak sauce and ground chuck using your hands. Form into 16 patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place patties on the grill, and cook for 5 minutes per side, or until well done. Place a slice of cheese on top of each one during the last minute.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork"
        ],
