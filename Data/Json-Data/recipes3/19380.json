        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9615/healthy-banana-cookies/",
        "name": "Healthy Banana Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3636087.jpg",
            "width": 5312,
            "height": 2988
