        "mainEntityOfPage": "https://www.allrecipes.com/recipe/108352/caribbean-fruity-salsa/",
        "name": "Caribbean Fruity Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F882444.jpg",
