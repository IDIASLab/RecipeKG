    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258624/breakfast-burritos-for-two/",
        "name": "Breakfast Burritos for Two",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4530107.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-08-07T05:48:25.000Z",
        "description": "An easy breakfast for you and that special someone.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 burritos",
        "recipeIngredient": [
            "4 slices bacon",
            "\u00bc cup refried beans",
            "2 (10 inch) flour tortillas",
            "1 tablespoon vegetable oil",
            "2 eggs",
            "2 tablespoons egg whites",
            "\u00bc cup grated Cheddar cheese",
            "2 tablespoons salsa",
            "2 tablespoons sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 200 degrees F (95 degrees C). Wrap tortillas in aluminum foil; place in the preheated oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook bacon in a skillet over medium heat until browned, about 5 minutes. Remove bacon from skillet. Cook and stir refried beans in bacon grease until heated through, about 1 minute. Cover beans and remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together eggs and egg whites in a bowl until uniform, about 1 minute. Heat oil in a skillet over medium heat, add egg mixture. Cook and stir until almost solid, about 2 minutes; stir in Cheddar cheese and remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each warm tortilla with 2 tablespoons of refried beans, 2 bacon strips, half of the eggs, 1 tablespoon of salsa, and 1 tablespoon of sour cream. Roll tortillas into burritos.\n"
            }
        ],
