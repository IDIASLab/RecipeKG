    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269280/ghorayebah-middle-eastern-vegan-cookies/",
        "name": "Ghorayebah (Middle Eastern Vegan Cookies)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Ghorayebah (Middle Eastern Vegan Cookies)"
        },
        "datePublished": "2018-11-29T02:50:09.000Z",
        "description": "Ghorayebah is a very popular Arabian cookie served on many occasions and holidays. Those cookies are extremely soft and melt in your mouth. Suitable for vegans, since they are made with shortening.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "40 cookies",
        "recipeIngredient": [
            "2 cups vegetable shortening, chilled",
            "1\u2009\u00bc cups confectioners' sugar",
            "3\u2009\u00be cups all-purpose flour",
            "1 teaspoon vanilla extract",
            "2 tablespoons pistachios"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine vegetable shortening and confectioners' sugar in a large bowl. Use the kneading hooks of an electric mixer to mix until smooth, creamy, and pale in color. Slowly mix in flour and vanilla extract using the kneading hooks. Cover dough and refrigerate for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 320 degrees F (160 degrees C). Grease a baking sheet with shortening.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop tablespoons of dough onto the prepared baking sheet. Press a pistachio on top and slightly pat each cookie to flatten.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until very lightly browned, 15 to 20 minutes. Remove from oven and allow to cool completely, about 15 minutes more, before removing from baking sheet.\n"
            }
        ],
