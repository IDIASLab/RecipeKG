    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264826/azeri-yogurt-marinated-chicken-thighs/",
        "name": "Azeri Yogurt-Marinated Chicken Thighs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9161052.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-01T00:40:03.000Z",
        "description": "Tender chicken thighs marinated in rich and creamy yogurt mixed with spices is a popular dish in Azerbaijan.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT5H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup plain Greek yogurt",
            "2 teaspoons garlic powder",
            "2 teaspoons onion powder",
            "1 teaspoon ground turmeric",
            "1 teaspoon cinnamon",
            "1 teaspoon paprika",
            "1 teaspoon dried tarragon",
            "1 teaspoon dried mint",
            "1 teaspoon crushed red pepper flakes",
            "1 large orange, juiced",
            "2 pounds chicken thighs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine yogurt, garlic, onion, turmeric, cinnamon, paprika, tarragon, mint, red pepper flakes, and orange juice in a bowl. Stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each chicken thigh into yogurt mixture and place on a plate. Cover with foil and marinate in the refrigerator for at least 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a baking dish and place chicken thighs inside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink at the bone and the juices run clear, 50 to 55 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Thigh Recipes"
        ],
