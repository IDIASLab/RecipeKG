        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264844/peachy-rye-julep/",
        "name": "Peachy Rye Julep",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5419162.jpg",
