        "mainEntityOfPage": "https://www.allrecipes.com/recipe/39533/creamed-tuna-on-toast/",
        "name": "Creamed Tuna On Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4462974.jpg",
