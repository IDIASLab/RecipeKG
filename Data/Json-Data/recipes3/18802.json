    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/271150/spicy-salmon-with-tomatoes-and-garlic/",
        "name": "Spicy Salmon with Tomatoes and Garlic",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Salmon with Tomatoes and Garlic"
        },
        "datePublished": "2019-03-13T01:52:45.000Z",
        "description": "Easy and delicious salmon dish for everyone who likes it spicy! Feel free to adjust the cayenne pepper to your personal preference.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 (3 ounce) fillets frozen salmon fillets",
            "1 tablespoon butter",
            "salt and ground black pepper to taste",
            "1 teaspoon ground cayenne pepper",
            "3 tablespoons olive oil",
            "2 cloves garlic, minced",
            "1 tomato, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease a shallow baking dish and place frozen fillets inside. Rub fillets with butter and season with salt, pepper, and cayenne pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until salmon flakes easily with a fork, about 16 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat olive oil in a skillet over medium-high heat. Cook and stir garlic until fragrant, about 2 minutes. Add tomato and reduce heat to medium. Cook until tomato has softened, 7 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove salmon from oven and place on 2 plates. Drizzle with tomato-garlic mixture on top of salmon and serve immediately.\n"
            }
        ],
