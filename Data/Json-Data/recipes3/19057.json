    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272412/lemon-cream-cookie-bars/",
        "name": "Lemon Cream Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9423159.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-03-28T06:57:40.000Z",
        "description": "Light lemony bar cookie that is easy to make. You can substitute the oatmeal cookie mix as a variation on this great summer dessert!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT4H5M",
        "recipeYield": "1 8-inch square pan",
        "recipeIngredient": [
            "1 (17.5 ounce) pouch dry sugar cookie mix (such as Betty Crocker\u00ae)",
            "1 stick butter, softened",
            "1 egg",
            "1 (14 ounce) can fat-free sweetened condensed milk",
            "2 lemons, zested and juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease an 8-inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cookie mix into a bowl and add butter. Mix together using a fork until crumbly. Add egg and mix until well blended. Press 1/2 of the mixture into the bottom of the prepared pan; reserve remaining mixture for the topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine condensed milk, lemon juice, and lemon zest in a bowl and mix well; filling may thicken as it sits.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from the oven; it will be puffy. Leave oven on. Let crust cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread lemon filling over the crust in the pan. Top with reserved mixture by crumbling it over the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pan to the hot oven and bake until lightly browned, about 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow to cool completely, about 1 hour. Cover and refrigerate for 2 hours before cutting into bars and serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
