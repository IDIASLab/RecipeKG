    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221312/roasted-roma-vinaigrette/",
        "name": "Roasted Roma Vinaigrette",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F950539.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T14:01:25.000Z",
        "description": "Basic balsamic dressing with a tangy twist.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "2 pounds Roma (plum) tomatoes, quartered",
            "2 shallots, quartered",
            "\u00bc cup garlic cloves",
            "3 tablespoons olive oil",
            "2 sprigs fresh sage",
            "2 sprigs fresh rosemary",
            "salt and ground black pepper to taste",
            "2 cups balsamic vinegar",
            "2 cups canola oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss tomatoes, shallots, garlic, olive oil, sage, and rosemary together in large bowl until everything is coated in olive oil; season with salt and black pepper. Spread the tomato mixture into a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in preheated oven until the garlic cloves are soft, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend the tomato mixture with balsamic vinegar in a blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stream canola oil into the blended mixture with the blender on until the mixture emulsifies into a dressing.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Salad Dressing Recipes",
            "Vinaigrette Dressing Recipes"
        ],
