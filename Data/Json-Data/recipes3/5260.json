    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75748/mamas-macaroni-salad/",
        "name": "Mama's Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F10999.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2018-06-08T13:24:16.000Z",
        "description": "This is a favorite pot-luck macaroni salad with olives and celery blended together to create a sweet and creamy side dish!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT8H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package elbow macaroni",
            "3 cups mayonnaise",
            "\u00be cup prepared yellow mustard",
            "\u2153 cup sweet pickle relish, or to taste",
            "1 (10 ounce) can sliced black olives, drained",
            "1 cup chopped celery",
            "\u00bd cup chopped green onions",
            "salt and pepper to taste",
            "1 pinch paprika, for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add the macaroni, and cook until tender, about 8 minutes. Drain and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together the mayonnaise, mustard, relish, olives, celery and green onions. Mix in the macaroni, and season with salt and pepper to taste. Add more mayonnaise if you like. For best flavor, cover and refrigerate over night. Sprinkle with paprika just before serving.\n"
            }
        ],
