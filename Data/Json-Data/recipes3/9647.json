        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231421/batter-cakes/",
        "name": "Batter Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F969893.jpg",
            "width": 2848,
            "height": 2848
