    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241784/caramelized-onion-and-mushroom-tarte-tatin/",
        "name": "Caramelized Onion and Mushroom Tarte Tatin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2205115.jpg",
            "width": 1632,
            "height": 1224
        },
        "datePublished": "2018-06-08T14:20:23.000Z",
        "description": "Easy to prepare, very gourmet appetizer with puffed pastry, goat cheese, caramelized onions, and sauteed cremini mushrooms.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bc cup unsalted butter",
            "1 large sweet onion (such as Vidalia\u00ae), sliced",
            "2 tablespoons olive oil",
            "1 (8 ounce) package crimini mushrooms, sliced",
            "1 pinch salt and ground black pepper to taste",
            "1 (17.5 ounce) package frozen puff pastry, thawed",
            "1 (11 ounce) log goat cheese",
            "1 egg, beaten",
            "1 pinch thyme, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium-low heat; cook and stir onion in hot butter until soft and caramelized, 30 to 60 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat; cook and stir mushrooms until browned and tender yet crisp, about 10 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll puff pastry out on a lightly-floured work surface. Cut out 5-inch circles using a round cutter and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned and puffed, about 12 minutes. Transfer puffs to a wire rack to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat goat cheese and egg together in a bowl until smooth. Season with thyme. Spread goat cheese mixture over cooled pastry puffs; top each with caramelized onions and mushrooms. Place pastries back onto baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the oven until warmed through, about 5 minutes.\n"
            }
        ],
