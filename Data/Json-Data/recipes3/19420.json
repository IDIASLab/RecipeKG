    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10493/coconut-meringues/",
        "name": "Coconut Meringues",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4466617.jpg",
            "width": 2000,
            "height": 2000
        },
        "datePublished": "1998-01-23T16:50:08.000Z",
        "description": "Very good if you like meringue stuff!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 egg whites",
            "\u215b teaspoon cream of tartar",
            "\u00be cup confectioners' sugar",
            "\u00bc cup shredded coconut",
            "\u00bc teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small bowl with mixer at high speed, beat egg whites and cream of tartar until soft peaks form.  Beating at high speed, gradually sprinkle in sugar, 2 Tablespoons at a time, beating until sugar is completely dissolved. (Whites should stand in stiff glossy peaks.) With rubber spatula, fold in coconut and extract until mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop mixture by rounded teaspoonfuls onto large cookie sheet, about 1 inch apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 1 hour or until dry.  Remove to rack to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Fruit Cookie Recipes",
            "Coconut"
        ],
