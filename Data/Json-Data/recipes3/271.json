    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15844/heavenly-shrimp-dip/",
        "name": "Heavenly Shrimp Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F347958.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-11-22T04:28:59.000Z",
        "description": "This shrimp and cream cheese dip is easy to prepare and always popular. For best results, serve the cream cheese at room temperature and the cocktail sauce well chilled.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 to 15 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese",
            "1 (8 ounce) jar cocktail sauce",
            "\u00bd pound shrimp, peeled and deveined",
            "1 lime",
            "1 (16 ounce) package buttery round crackers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cream cheese in center of serving platter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together cocktail sauce and shrimp. Pour mixture over cream cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice lime in half; squeeze one half over cocktail sauce. Slice other half into wedges for garnish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place crackers and lime wedges around edge of plate and serve.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Shrimp Dip Recipes"
