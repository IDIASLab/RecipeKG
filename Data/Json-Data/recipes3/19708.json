    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14648/mothers-pot-roast/",
        "name": "Mother's Pot Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F422264.jpg",
            "width": 3872,
            "height": 2592
        },
        "datePublished": "1999-10-07T18:41:01.000Z",
        "description": "The best vegetable gravy you ever tasted--and the roast isn't bad either! We always had this meal with mounds of mashed potatoes. Great on a cold wintry evening!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT7H0M",
        "totalTime": "P0DT7H10M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2\u2009\u00bd pounds tip round roast",
            "salt and pepper to taste",
            "1 (15 ounce) can tomato sauce",
            "1 onion, cut into thin strips",
            "2 bay leaves",
            "3 tablespoons all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray slow cooker with non-stick cooking spray. Season roast with salt and pepper. Place meat in pot with fat side up. Pour tomato sauce over roast. Place onion rings over all. Toss in bay leaves. Cover and cook 1 hour on high.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After 1 hour, reduce heat to low and cook 6 to 8 more hours. Carefully lift meat out of pot and remove to a warm platter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour drippings through strainer into medium-sized saucepan and discard material in strainer. Whisk in flour to liquid. Cook, stirring constantly over medium heat until thickened. Season to taste with salt and pepper; serve alongside roast.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Beef",
            "Pot Roast Recipes"
        ],
