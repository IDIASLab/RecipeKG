        "recipeIngredient": [
            "1 cup mayonnaise",
            "\u00bc cup minced onion",
            "\u00bc cup minced dill pickle",
            "2 tablespoons dill pickle juice",
            "salt and ground black pepper to taste",
            "celery salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir the mayonnaise, onion, pickle, and pickle juice together in a bowl. Season with salt, black pepper, and celery salt to taste. Cover, and refrigerate for 1 hour before serving.\n"
            }
