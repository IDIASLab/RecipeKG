    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257263/spicy-chicken-burgers/",
        "name": "Spicy Chicken Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6699802.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-06-08T14:42:32.000Z",
        "description": "This is a healthy, spicy ground chicken burger topped with smashed avocado and a homemade coleslaw. It is the best burger you will ever have!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 burgers",
        "recipeIngredient": [
            "1 tablespoon chili powder",
            "1\u2009\u00bd teaspoons salt",
            "1\u2009\u00bd teaspoons ground cumin",
            "1\u2009\u00bd teaspoons minced garlic",
            "1 teaspoon ground black pepper",
            "1 pound ground chicken",
            "1 egg",
            "1 large avocado - halved, pitted, and peeled",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon salt",
            "1 (8 ounce) container light sour cream",
            "1\u2009\u00bd cups shredded cabbage",
            "4 whole wheat Kaiser rolls",
            "1 large tomato, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix chili powder, 1 1/2 teaspoon salt, cumin, minced garlic, and black pepper together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle 3/4 of the chili powder mixture over ground chicken in a large bowl. Add egg; mix well and shape into 4 patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate. Grill patties until browned and juices run clear, about 5 minutes per side. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash avocado in a small bowl; season with garlic powder and 1/4 teaspoon salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining chili powder mixture, sour cream, and shredded cabbage together in a bowl to make coleslaw.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast Kaiser rolls lightly on the grill. Place chicken patties on the buns. Top each patty with mashed avocado, a large scoop of coleslaw, and sliced tomato.\n"
            }
        ],
