        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258015/3-cheese-tortilla-pizza/",
        "name": "3 Cheese Tortilla Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4563084.jpg",
            "width": 3629,
