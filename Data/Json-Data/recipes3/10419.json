    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236407/bacon-breakfast-bowls/",
        "name": "Bacon Breakfast Bowls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1836491.jpg",
            "width": 1532,
            "height": 1796
        },
        "datePublished": "2018-06-08T14:12:35.000Z",
        "description": "I love this because it's a quick and easy breakfast on-the-go consisting of 3 of my favorite breakfast foods combined: bacon, eggs, and cheese!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "12 strips bacon",
            "8 eggs, beaten",
            "salt and ground black pepper to taste",
            "\u00bd cup shredded Cheddar cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Prepare 12 muffin cups with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook bacon in a large skillet over medium-high heat, turning occasionally, until evenly browned but not yet crispy, 7 to 10 minutes. Drain the bacon slices on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line the walls of each muffin cup with a bacon strip.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs with salt and pepper in a large bowl; ladle into muffin cups to about 3/4-full. Sprinkle cheddar cheese over the top of each portion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until golden brown on top and the egg doesn't jiggle, 20 to 35 minutes. Remove from oven and use a butter knife or spoon to remove from muffin tin promptly so that they don't stick.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs"
        ],
