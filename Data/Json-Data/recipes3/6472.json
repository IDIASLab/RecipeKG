    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/131198/tropical-shrimp-scampi/",
        "name": "Tropical Shrimp Scampi",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F192957.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:44:04.000Z",
        "description": "This fruity shrimp scampi has a citrus kick.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (16 ounce) package linguine pasta",
            "\u00bd cup olive oil",
            "\u00bd cup pineapple juice",
            "\u00bd cup no-pulp orange juice",
            "5 teaspoons grated orange zest",
            "5 teaspoons lemon zest",
            "1 teaspoon salt",
            "1 teaspoon pepper",
            "5 cloves garlic, peeled",
            "1 pound medium shrimp - peeled and deveined",
            "2 tablespoons chopped fresh parsley",
            "2 tablespoons grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil over high heat.  Stir in the linguine, and return to a boil. Cook until the pasta has cooked through, but is still firm to the bite, about 11 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While pasta is cooking, combine the olive oil, pineapple juice, orange juice, orange zest, lemon zest, salt, pepper, and garlic in a blender.  Blend on high speed until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauce into a large skillet over medium-high heat.  Bring to a simmer and cook for 2 minutes.  Add the shrimp and parsley; cook until shrimp are pink and cooked through, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain the linguini, and place on a serving platter.  Spoon the shrimp and sauce over the pasta, and top with a sprinkle of Parmesan cheese.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Shrimp",
            "Shrimp Scampi"
        ],
