    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7962/apple-cake-ii/",
        "name": "Apple Cake II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F688794.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-08-24T12:17:09.000Z",
        "description": "A nice no-added-sugar apple cake sweetened with fruit juice.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 9x5-inch loaf pan",
        "recipeIngredient": [
            "1 cup self-rising flour",
            "\u00bd cup butter, cut into 1-inch chunks",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground nutmeg",
            "\u215d cup chopped walnuts",
            "1 cup chopped raisins",
            "1\u2009\u00bd cups grated apple",
            "3 eggs",
            "\u00bc cup pineapple juice",
            "1 tablespoon grated lime zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5-inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the flour in a bowl and mix well; rub in the butter until the pieces are the size of small peas. Stir in the cinnamon and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the walnuts, raisins, and apples. Add the grated lime rind, eggs, and pineapple juice. Beat well until thoroughly combined. Spoon the mixture into the prepared pan and smooth the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for about 1 1/4 hours, or until a tester inserted in the center of the cake comes out clean. Turn out and cool on wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
