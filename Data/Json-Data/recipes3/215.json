        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15570/peanut-butter-dreams/",
        "name": "Peanut Butter Dreams",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F316111.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-22T16:51:23.000Z",
        "description": "This no bake cookie is a simple, chocolate-coated treat!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 5 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "4 cups confectioners' sugar",
            "2 cups creamy peanut butter",
            "3 cups crisp rice cereal",
            "2 cups semisweet chocolate chips",
            "4 tablespoons shortening"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, mix butter, sugar, peanut butter and crisp rice cereal together thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll into small balls and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips and shortening in a small saucepan over low heat. Remove from heat. Coat the small balls in the chocolate mixture. Refrigerate for several hours.\n"
            }
        ],
