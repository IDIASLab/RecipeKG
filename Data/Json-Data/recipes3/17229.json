    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267545/pumpkin-chutney/",
        "name": "Pumpkin Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Chutney"
        },
        "datePublished": "2018-11-01T13:02:35.000Z",
        "description": "This pumpkin chutney is sweet, savory, and nicely spiced. One of my favorite chutneys, this is especially great for an autumn-themed dinner party or Thanksgiving.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 pints",
        "recipeIngredient": [
            "1 (3 pound) pumpkin, peeled and cubed",
            "salt",
            "1 (1 inch) piece fresh ginger root",
            "12 peppercorns",
            "\u00bd pound apples - peeled, cored and chopped",
            "\u00bd pound shallots, chopped",
            "\u00bd pound golden raisins",
            "\u00bd pound demerara sugar",
            "1\u2009\u00bc cups malt vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place pumpkin in a pan, cover liberally with salt, and leave overnight. The following day, drain and rinse under cold water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ginger and peppercorns on a piece of muslin and tie together with kitchen twine to make a spice bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pumpkin, apples, shallots, raisin, sugar, vinegar and spice bag in a saucepan over medium-low heat. Cook until pumpkin is soft and chutney has thickened, about 1 hour. Remove and discard spice bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chutney into sterilized jars, cover with lids, and allow to cool to room temperature. Store in the refrigerator.\n"
            }
        ],
