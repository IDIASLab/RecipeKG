    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10106/hello-dolly-cookies/",
        "name": "Hello Dolly Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F103832.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-11-14T01:56:27.000Z",
        "description": "Easy bar cookie - sweet and delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup graham cracker crumbs",
            "1 cup semisweet chocolate chips",
            "1 cup chopped walnuts",
            "1 cup shredded coconut",
            "\u00bc cup butter, melted",
            "1 (14 ounce) can sweetened condensed milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).  Lightly grease a 9 x 9 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine graham crackers, chocolate chips, walnuts and coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour melted butter over the dry ingredients and stir.  Press into prepared baking pan.  Drizzle the condensed milk over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 - 30 minutes, until lightly colored on top.  Let cool before cutting into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
