    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11025/peanut-butter-balls-iv/",
        "name": "Peanut Butter Balls IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F532499.jpg",
            "width": 1944,
            "height": 1944
        },
        "datePublished": "1998-07-31T01:36:20.000Z",
        "description": "My family makes these every Christmas, they're one of our favorites.  The maple flavoring is the key ingredient!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "\u00bc cup butter",
            "1\u2009\u00bd cups peanut butter",
            "4 cups confectioners' sugar",
            "1 teaspoon vanilla extract",
            "1 teaspoon maple flavored extract",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the butter, peanut butter, confectioners' sugar, vanilla  and maple flavoring.  Cream well and knead well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the chocolate chips over low heat.  Roll the dough into 1 inch balls.  Dip into melted chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate overnight (or at least for about 15 minutes) on a cookie sheet lined with wax paper.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
