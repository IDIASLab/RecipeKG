    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14215/cucumber-and-yogurt-salad/",
        "name": "Cucumber And Yogurt Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F72961.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-13T09:29:36.000Z",
        "description": "Cucumbers in a creamy yogurt dressing. Serve very cold.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT1H15M",
        "recipeYield": "7 servings",
        "recipeIngredient": [
            "3 cucumbers",
            "2 cloves garlic, minced",
            "salt to taste",
            "2 tablespoons dried mint",
            "1 cup plain yogurt",
            "1 tablespoon olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel, quarter lengthwise, and seed cucumbers.  Slice thinly.  Mix together with garlic.  Layer cucumber slices in a salad bowl, sprinkling each layer lightly with salt.  Let stand for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour off the liquid that has formed, pressing cucumber firmly.  Crumble mint over cucumbers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat yogurt until smooth; blend in a drizzle of olive oil if desired.  Pour over cucumbers.  Refrigerate until thoroughly chilled.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Cucumber Salad Recipes"
        ],
