    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218911/oyster-and-mushroom-stuffing/",
        "name": "Oyster and Mushroom Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Oyster and Mushroom Stuffing"
        },
        "datePublished": "2018-06-08T13:59:19.000Z",
        "description": "Old family Thanksgiving favorite.  Made with bread crumbs, oysters, and mushrooms.  Delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "\u00bd cup margarine",
            "1 large onion, chopped",
            "2 cups chopped celery",
            "1 (12 ounce) package fresh mushrooms, sliced",
            "8 cups dry bread crumbs",
            "1 egg, beaten",
            "1 pint shucked oysters, chopped",
            "\u00bd cup chicken broth",
            "1 teaspoon poultry seasoning",
            "1 teaspoon dried sage",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a large skillet over medium heat, and cook and stir the onion and celery until the celery is tender, about 10 minutes. Stir in the mushrooms, and cook and stir until they are tender and release their juice, 5 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, mix the bread crumbs, egg, and oysters together, and stir in the cooked vegetables with juices and margarine, chicken broth, poultry seasoning, sage, salt, and black pepper until well combined. Place stuffing into a 2-quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the stuffing is hot, the oysters are cooked, and the top is browned, about 45 minutes.\n"
            }
        ],
