    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78142/twenty-minute-chicken/",
        "name": "Twenty Minute Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1052417.jpg",
            "width": 1067,
            "height": 1067
        },
        "datePublished": "2018-06-08T13:24:59.000Z",
        "description": "Chicken cooked in a pan and then baked in the oven with mushrooms and onions. Fast and absolutely delicious!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 boneless, skinless chicken breast halves",
            "\u00bd large onion, chopped",
            "2 (10 ounce) packages sliced fresh button mushrooms",
            "\u00bc cup olive oil",
            "salt and freshly ground black pepper to taste",
            "1 clove garlic, chopped",
            "1 cup shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Wash chicken breasts, pat dry with paper towels, and cut each breast in half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons oil in a large skillet over medium-high heat. Sear chicken briefly, turning until no longer pink, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, in another large skillet, heat 2 tablespoons oil over medium-high heat. Stir in mushrooms, onions, and garlic, if using; cook until they are nice and soft, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour contents of both skillets into a baking dish, sprinkle cheese over the top, and bake about 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
