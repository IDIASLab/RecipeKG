        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9991/faux-reos/",
        "name": "Faux-reos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F397687.jpg",
            "width": 3056,
            "height": 2292
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Better than the real thing?  You decide.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 (18.25 ounce) packages devil's food cake mix",
            "1\u2009\u00bd cups shortening",
            "4 eggs",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine all ingredients and mix well.  Batter will be thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into balls the size of nickels.  Place on greased cookie sheets about 2 inches apart.  Bake for 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When cookies are cool, frost the bottoms of half of the cookies with Faux-reo filling(see footnote) and stick bottom-to-bottom with another unfrosted cookie.\n"
            }
        ],
