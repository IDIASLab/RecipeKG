        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23026/restaurant-style-mac-and-cheese/",
        "name": "Restaurant Style Mac and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F794691.jpg",
