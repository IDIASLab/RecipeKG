        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236744/simple-shrimp-tostadas/",
        "name": "Simple Shrimp Tostadas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5030017.jpg",
