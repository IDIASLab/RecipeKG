        "name": "6-Ingredient Banana Oat PB Breakfast Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4486047.jpg",
            "width": 2478,
            "height": 2486
