        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9130/festive-onions/",
        "name": "Festive Onions",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5880949.jpg",
            "width": 2305,
