        "totalTime": "P0DT0H10M",
        "recipeYield": "2 to 3 dozen",
        "recipeIngredient": [
            "9 cups crispy rice cereal squares",
            "\u00bd cup peanut butter",
            "1 cup semi-sweet chocolate chips",
            "1\u2009\u00bd cups confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan over low heat, melt the chocolate; add peanut butter and mix until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, add cereal and stir until coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour powdered sugar into large plastic bag, add coated cereal and shake until well coated. Store in airtight container.\n"
            }
        ],
