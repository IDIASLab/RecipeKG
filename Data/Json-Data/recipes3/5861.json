    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/86963/carrot-cashew-quiche/",
        "name": "Carrot Cashew Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F371329.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:26:56.000Z",
        "description": "One of my mom's favorites for buffets or brunches and afternoon lunches. Served with a nice salad, this is a nice light meal.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 cup sliced carrots",
            "1 cup cashews",
            "\u00bd cup honey",
            "3 eggs",
            "1\u2009\u00bd cups heavy cream",
            "\u00bd teaspoon nutmeg",
            "\u00bd teaspoon salt",
            "\u00be cup shredded Cheddar cheese",
            "1 (9 inch) pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a skillet over medium heat. Stir in the carrots and cashews, and cook until carrots are tender and cashews are golden brown. Mix in the honey.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, beat together the eggs, heavy cream, nutmeg, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Evenly spread the cheese in the bottom of the pie crust. Layer the carrot mixture over the cheese, and top with the egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 40 minutes in the preheated oven, until a toothpick inserted in the center comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Quiche"
        ],
