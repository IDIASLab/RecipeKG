        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23799/pepper-parmesan-dressing/",
        "name": "Pepper-Parmesan Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F741861.jpg",
