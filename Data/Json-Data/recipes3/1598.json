    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283333/ballymore-irish-lamb-stew/",
        "name": "Ballymore Irish Lamb Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5408127.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-05-09T02:55:13.000Z",
        "description": "This hearty Irish lamb stew with vegetables is perfect for celebrating St. Patrick's Day, or any chilly weeknight.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT2H20M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds boneless lamb chops",
            "salt and freshly ground black pepper to taste",
            "2 tablespoons vegetable oil",
            "\u00bd cup Irish stout beer (such as Guinness\u00ae)",
            "1 pound new potatoes",
            "1 pound baby carrots",
            "1 (8 ounce) package pearl onions",
            "4 cups lamb stock",
            "2 tablespoons brown roux",
            "2 tablespoons finely chopped parsley "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season lamb chops with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large Dutch oven over medium heat until hot but not smoking; add chops. Sear on each side, 2 to 3 minutes. Remove from the pan and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour beer into the pot and cook for 1 minute, scraping up any browned bits from the bottom of the pan with a wooden spoon. Add lamb back to the pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss potatoes, carrots, and onions with salt and pepper in a mixing bowl and add to the pot. Cover with lamb stock and bring to a boil. Cover and reduce heat to medium-low. Simmer until lamb falls off the bone, about 2 hours. Stir in roux and continue to cook for 10 minutes. Stir in parsley and spoon into serving bowls.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Irish Stew Recipes"
        ],
