    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257765/chicken-parmesan-burger/",
        "name": "Chicken Parmesan Burger",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4508148.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-08T14:42:59.000Z",
        "description": "This recipe tastes just like a chicken Parm! I was trying to find something that was healthy and tasty and this is it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H17M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "4 burgers",
        "recipeIngredient": [
            "1 pound ground chicken",
            "1 egg",
            "\u00bd cup Italian-seasoned bread crumbs",
            "\u00bd cup grated Parmesan cheese",
            "3 tablespoons tomato paste, divided",
            "1 tablespoon Italian seasoning",
            "1 teaspoon garlic powder",
            "1 teaspoon onion powder",
            "salt and ground black pepper to taste",
            "1 tablespoon olive oil",
            "1 onion, chopped",
            "1 cup crushed tomatoes",
            "1 cup chicken stock",
            "4 slices provolone cheese",
            "4 whole wheat hamburger buns, split"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine chicken, egg, bread crumbs, Parmesan cheese, 1 tablespoon tomato paste, Italian seasoning, garlic powder, and onion powder in a large bowl. Season with salt and pepper. Form mixture into 4 patties. Press gently to flatten.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a large skillet. Cook patties until browned, about 4 minutes per side. Transfer to a large plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion in the drippings left in the skillet until translucent, 3 to 4 minutes. Stir in remaining 2 tablespoons tomato paste, crushed tomatoes, and chicken stock. Cook until thickened and reduced by half, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return patties to the skillet; cover with tomato sauce. Place 1 slice of provolone cheese over each patty. Cover skillet and cook until cheese melts slightly, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place patties on buns; spoon tomato sauce on top.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes",
            "Chicken"
        ],
