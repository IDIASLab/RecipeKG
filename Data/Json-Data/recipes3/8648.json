    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220792/cheesy-chicken-pockets/",
        "name": "Cheesy Chicken Pockets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4580791.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2018-06-08T14:00:50.000Z",
        "description": "This is a mixture of cheese, chicken, and a bit of spice in a phyllo pastry. They are a great finger food, and give a nice flavorful taste.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bc cup vegetable oil",
            "\u00bd red onion, chopped",
            "1 jalape\u00f1o pepper, seeded and chopped",
            "2 cloves garlic, chopped",
            "salt and ground black pepper to taste",
            "3 cups cooked, diced chicken breast meat",
            "1 cup shredded Cheddar-Monterey Jack cheese blend",
            "\u00bd (8 ounce) package cream cheese",
            "1 dash hot pepper sauce",
            "15 sheets phyllo dough, thawed",
            "cooking spray",
            "1 egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion, jalapeno pepper, and garlic until tender, about 3 minutes. Season with salt and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chicken, Cheddar-Monterey Jack cheese, cream cheese, hot pepper sauce, and onion mixture in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray 5 phyllo sheets each with cooking spray. Stack these 5 phyllo sheets on top of each other and cut into 4 smaller squares.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chicken mixture into each of the 4 squares; fold phyllo dough over and seal the edges. Repeat with remaining phyllo dough and chicken mixture until you have 12 filled pockets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush beaten egg on tops of filled phyllo pockets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place filled phyllo pockets on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Pastries"
        ],
