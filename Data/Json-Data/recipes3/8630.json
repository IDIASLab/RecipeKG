    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220715/deep-fried-asparagus/",
        "name": "Deep-Fried Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F817677.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "2018-06-08T14:00:45.000Z",
        "description": "This is a fancier version of the other fried asparagus on here, and is more like an appetizer. Serve with Ranch dressing for dipping.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cups vegetable oil for frying, or as needed",
            "1 cup milk",
            "2 eggs",
            "1\u2009\u00bd cups biscuit mix",
            "\u00bd cup grated Parmesan cheese",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 bunch fresh asparagus, trimmed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a large skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk and eggs together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir biscuit mix, Parmesan cheese, salt, and black pepper together in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip asparagus into milk mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll and coat asparagus in dry ingredient mixture. Repeat with a second layer for each asparagus.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully place coated asparagus in the hot oil; fry until golden brown, 2 to 4 minutes.\n"
            }
        ],
