    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270761/sweet-rice-flour-shortbread-cookies/",
        "name": "Sweet Rice Flour Shortbread Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sweet Rice Flour Shortbread Cookies"
        },
        "datePublished": "2019-02-23T09:31:17.000Z",
        "description": "Adapted from an old family recipe, you'll need to make these gluten-free shortbread cookies in large batches to keep them around long enough for company!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups cornstarch",
            "1\u2009\u00bd cups icing sugar",
            "1 cup sweet rice flour (mochiko)",
            "1 cup tapioca starch",
            "\u2154 cup sorghum flour",
            "\u2153 cup fine white rice flour",
            "2\u2009\u00bc cups butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift cornstarch, icing sugar, sweet rice flour, tapioca, sorghum, and white rice flours together in a large bowl. Blend in butter until a soft, smooth dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1-inch-thick logs. Wrap in plastic and refrigerate until firm, 30 to 60 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice cold dough into 1/8-inch rounds. Place rounds 1 1/2 inches apart on a cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are firm and tops are still pale, 15 to 20 minutes. Transfer to a rack to cool.\n"
            }
        ],
