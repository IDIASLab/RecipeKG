        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218364/sunset-sangria/",
        "name": "Sunset Sangria",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1049350.jpg",
            "width": 1440,
