        "recipeIngredient": [
            "\u00bd (16 ounce) package frozen Italian-style meatballs",
            "1 (15 ounce) can asparagus, drained",
            "1 (14.5 ounce) can chicken broth",
            "1 (10.75 ounce) can  tomato soup",
            "\u00bd cup uncooked instant rice",
            "1 tablespoon Italian seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix meatballs, asparagus, chicken broth, tomato soup, instant rice, and Italian seasoning in a large bowl. Pour into a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until rice is tender, about 30 minutes.\n"
            }
