        "totalTime": "P0DT0H15M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 cups mayonnaise",
            "1 cup sweet pickle relish",
            "\u00bd teaspoon freshly ground black pepper",
            "2 teaspoons salt",
            "2 pounds smoked boneless ham, diced",
            "1 onion, diced",
            "1 small green bell pepper, diced",
            "2 stalks celery, diced"
