        "mainEntityOfPage": "https://www.allrecipes.com/recipe/173244/detox-salad/",
        "name": "Detox Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060336.jpg",
            "width": 2346,
