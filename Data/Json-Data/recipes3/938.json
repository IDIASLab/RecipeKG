    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12136/strawberry-pie-i/",
        "name": "Strawberry Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2379877.jpg",
            "width": 1681,
            "height": 1377
        },
        "datePublished": "1998-04-08T18:42:47.000Z",
        "description": "This recipe came to me from a good friend of my mother.  So easy to make and so delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 nine inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie crust, baked",
            "24 marshmallows",
            "1 tablespoon milk",
            "1 (10 ounce) package frozen strawberries, thawed and pureed",
            "1 cup heavy whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan, stir marshmallows with milk over low heat until marshmallows are dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir strawberries into the melted marshmallows.  In a medium bowl, whip cream until soft peaks form. Fold whipped cream into the marshmallow mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon filling into pie shell, and refrigerate for 4 to 6 hours.  Serve cold.\n"
            }
        ],
