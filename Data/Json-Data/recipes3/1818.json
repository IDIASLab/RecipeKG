        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/265385/keto-steak/",
        "name": "Keto Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1103472.jpg",
            "width": 1500,
            "height": 1000
        },
        "datePublished": "2018-06-08T05:20:05.000Z",
        "description": "This is a favorite in my house and preferred over restaurant steaks. You get the quality and benefits of a steak cooked in grass-fed butter right in your own home. Paleo, low-carb, ketogenic friendly.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 pound beef sirloin steak, at least 1-inch thick ",
            "salt and pepper to taste",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove steak from the refrigerator at least 1 hour before cooking to allow it to come to room temperature. Season generously with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 200 degrees F (95 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat an ovenproof skillet over medium-high heat. Add butter. Sear steak for 2 minutes; flip and cook on the other side, 2 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place steak in the oven and cook for 10 to 20 minutes, depending on steak thickness and desired doneness. Let steak rest for 10 minutes before serving.\n"
            }
        ],
