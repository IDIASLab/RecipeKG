    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10296/cherry-chip-cookies-i/",
        "name": "Cherry Chip Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1015.jpg",
            "width": 250,
            "height": 285
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "A chip cookie with a difference! Light, crispy and eggless, these cookies are a great after-school treat with a cold mug of milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup margarine",
            "1 cup confectioners' sugar",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd cup rolled oats",
            "1 cup cherry baking chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine margarine, confectioner's sugar and vanilla.  Beat until fluffy.  Add flour, baking soda and oats.  Mix well.  Stir in cherry flavored chips (other flavors work well, too!).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded teaspoonful onto ungreased cookie sheets and flatten slightly with fingers.  Bake for 12 minutes.  Remove immediately from cookie sheets and onto wire racks to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
