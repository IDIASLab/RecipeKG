    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247351/fried-zucchini-flowers/",
        "name": "Fried Zucchini Flowers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5561193.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2018-06-08T14:27:40.000Z",
        "description": "In our family, we grow zucchini just for the flowers. This recipe has been our family favorite. Hope you enjoy them as much as we do.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "zucchini flowers",
            "1 cup all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "2 eggs",
            "\u00bd cup milk",
            "1 teaspoon olive oil",
            "vegetable oil for frying",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Clean flowers and dry with a paper towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking powder, and salt together in a bowl. Whisk eggs, milk, and olive oil together in a separate bowl; stir in flour mixture until batter is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a deep skillet over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip flowers in the batter and fry in the hot oil until golden and crispy, 2 to 5 minutes, working in batches as needed. Transfer fried flowers to a paper towel-lined plate; sprinkle salt over each.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Zucchini Appetizer Recipes"
        ],
