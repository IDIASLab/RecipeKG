        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240627/spicy-hot-corn/",
        "name": "Spicy Hot Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1843337.jpg",
