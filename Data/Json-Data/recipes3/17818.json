        "name": "Almond Butter & Jam Overnight Oats",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6106324.jpg",
            "width": 960,
            "height": 960
