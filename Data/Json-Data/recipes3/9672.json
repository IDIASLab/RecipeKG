    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231540/perfect-baked-poutine/",
        "name": "Perfect Baked Poutine",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Perfect Baked Poutine"
        },
        "datePublished": "2018-06-08T14:07:26.000Z",
        "description": "Oven-baked potatoes make this a less guilty indulgence. This dish makes it on our table weekly, playing sidekick to chicken fingers, burgers, dogs ...",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 tablespoons extra-virgin olive oil",
            "1 teaspoon salt",
            "1 teaspoon freshly ground black pepper",
            "\u00bd teaspoon garlic powder",
            "1 teaspoon dried parsley",
            "5 red potatoes, scrubbed and cut into wedges",
            "2 (.75 ounce) packets brown gravy mix",
            "2 cups low-sodium chicken broth",
            "2 cups cheese curds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine extra-virgin olive oil, salt, black pepper, garlic powder, and parsley in a large zip lock bag; add cut red potatoes and shake until potatoes are covered.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a cookie sheet with foil; lightly cover with oil. Arrange potatoes on cookie sheet. Bake potatoes, flipping every 20 minutes, until golden brown, about 45 minutes to 1 hour. Transfer fries to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk brown gravy mix with chicken broth in a saucepan, place over medium heat, and bring to a boil, whisking often. Reduce heat to low; simmer for 1 minute, continuing to whisk. Let stand off the heat for 1 minute for gravy to thicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer cheese curds over fries and top with prepared gravy.\n"
            }
        ],
