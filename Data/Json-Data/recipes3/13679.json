    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260676/country-sausage-breakfast-pizza/",
        "name": "Country Sausage Breakfast Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4573127.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-08T14:45:26.000Z",
        "description": "The great tastes of biscuits and gravy on a pre-made pizza crust. Scrambled eggs and cheese make it a filling meal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H19M",
        "totalTime": "P0DT0H29M",
        "recipeYield": "2 8-inch pizzas",
        "recipeIngredient": [
            "1 (10 ounce) package mini 8-inch baked pizza crusts (such as Boboli\u00ae)",
            "\u00bc cup sage pork sausage",
            "2 tablespoons butter, divided",
            "1 tablespoon all-purpose flour",
            "\u00bc cup half-and-half",
            "\u00bd teaspoon ground black pepper",
            "\u00bc teaspoon salt",
            "\u215b teaspoon onion powder",
            "3 large eggs",
            "3 tablespoons shredded Swiss cheese",
            "3 tablespoons shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pizza crusts on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 5 minutes. Remove from oven and leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir sausage until browned, 5 to 7 minutes. Remove sausage with a slotted spoon; drain on a paper towel-lined plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat of skillet to medium-low. Melt 1 tablespoon butter in the skillet. Add flour; stir until dissolved to make a roux, about 1 minute. Add half-and-half; stir to remove browned bits from bottom of the skillet. Cook and stir until thick, about 2 minutes. Season gravy with pepper, salt, and onion powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the drained sausage to the skillet; stir to coat with the gravy. Remove from heat; divide sausage gravy evenly over the pizza crusts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the remaining butter in a small nonstick saucepan over low heat. Pour in eggs; cook and stir until scrambled, 3 to 4 minutes. Remove from heat; divide scrambled eggs evenly over the pizzas. Top with Swiss cheese and Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until cheese is melted, 3 to 4 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Breakfast Pizza Recipes"
        ],
