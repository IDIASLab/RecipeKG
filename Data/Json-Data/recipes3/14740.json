    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246764/cajun-shrimp-alfredo/",
        "name": "Cajun Shrimp Alfredo",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4786588.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2018-06-13T13:03:00.000Z",
        "description": "Spicy and creamy....a great combination! Homemade Alfredo is always best, but the Knorr&reg; mix plus Parmesan is a quick easy alternative. Serve over angel hair pasta.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd pound andouille sausage, diced",
            "\u00bc onion, diced",
            "\u00bc cup diced celery",
            "\u00bc cup diced red bell pepper",
            "2 teaspoons Cajun seasoning",
            "\u00bc teaspoon dried sage",
            "1 pound raw shrimp, peeled and deveined ",
            "1\u2009\u00be cups milk",
            "1 (1.6 oz) package dry Alfredo sauce mix (such as Knorr\u00ae)",
            "1 tablespoon butter",
            "\u00bd cup freshly grated Parmesan cheese",
            "2 teaspoons minced garlic, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sausage, onion, celery, red bell pepper, Cajun seasoning, and sage in a skillet; cook and stir until sausage is cooked through and celery and bell pepper are softened, 5 to 10 minutes. Add shrimp and cook until cooked through and bright pink, about 5 minutes. Remove skillet from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk, Alfredo sauce mix, and butter together in a saucepan; bring to a boil. Reduce heat to low and simmer sauce, stirring occasionally, until smooth and thickened, about 2 minutes. Stir Parmesan cheese and garlic into sauce until cheese is melted. Stir shrimp mixture into sauce.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces",
            "Creamy",
            "Alfredo"
        ],
