        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260068/spiced-sugar-cookies/",
        "name": "Spiced \"Sugar\" Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559953.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-08-30T04:24:24.000Z",
        "description": "Warm spices punch the flavor of basic cookie dough making these cookies perfect for the holidays.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00bd cup unsalted butter, softened",
            "\u00bc cup Pyure Organic All Purpose Stevia Blend",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon ground allspice",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat butter with Pyure Organic All Purpose Stevia Blend until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in egg until well blended. Beat in vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, cinnamon, baking powder, nutmeg, allspice and salt together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir into butter mixture in two additions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap dough in plastic wrap and refrigerate for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll on lightly floured surface or between sheets of waxed paper to 1/4-inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using desired cookie cutter, cut out shapes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Place cookies 2-inches apart on parchment paper-lined baking sheets. Bake for 10 to 12 minutes or until golden. Let cool on racks.\n"
            }
        ],
