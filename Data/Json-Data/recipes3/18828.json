    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/271152/spinach-and-cream-cheese-stuffed-chicken-breast/",
        "name": "Spinach and Cream Cheese Stuffed Chicken Breast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1964541.jpg",
            "width": 3555,
            "height": 2372
        },
        "datePublished": "2019-03-14T07:53:10.000Z",
        "description": "This was a last-minute decision for my son's birthday dinner...mixing ingredients found in the refrigerator and kitchen...the family LOVED it! Adjust filling ingredients to your taste.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 chicken breasts",
        "recipeIngredient": [
            "cooking spray",
            "2 tablespoons olive oil",
            "2 cloves garlic, chopped",
            "\u00bd onion, chopped",
            "4 ounces chopped cooked ham",
            "4 ounces fresh spinach",
            "4 ounces cream cheese",
            "4 ounces shredded Swiss cheese",
            "4 large boneless chicken breast fillets",
            "4 ounces Italian-style salad dressing",
            "8 toothpicks, or as needed",
            "\u00bd sleeve round buttery crackers, crushed fine",
            "4 ounces shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over medium heat. Add oil, garlic, and onion. Cook and stir until tender, 1 to 3 minutes. Add ham and spinach leaves and stir until wilted, about 1 minute. Chop slightly in the skillet. Remove from heat and stir in cream cheese until melted and mixture has cooled a bit. Add Swiss cheese and set filling aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each chicken breast to 1/2 inch thickness and place in a bowl. Pour Italian dressing over chicken. Place 1 chicken breast on a cutting board and pound thinner. Add about 1/4 of the filling and fold chicken over to form a packet. Seal using toothpicks and place in the prepared baking pan. Repeat with remaining chicken breasts and filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine crushed crackers and Cheddar cheese together in a bowl. Cover filled chicken breasts with cracker mixture, mashing them into the breasts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 45 minutes. Remove from oven and let sit for 10 minutes before transferring from the pan to a serving platter to allow filling to set up. Remove toothpicks before serving.\n"
            }
        ],
