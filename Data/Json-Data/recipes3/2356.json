    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14083/roasted-acorn-squash/",
        "name": "Roasted Acorn Squash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F183809.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-13T22:43:15.000Z",
        "description": "A late summer or fall dish that goes well with brown rice.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 medium acorn squash",
            "3 tablespoons butter",
            "2 onions, thinly sliced",
            "3 cloves garlic, minced",
            "1 teaspoon ground coriander seed",
            "\u00bd teaspoon freshly grated nutmeg",
            "\u00bd teaspoon salt",
            "freshly ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut acorn squash in half. Roast them cut side up on a baking sheet for 50 minutes or until the flesh is just tender. Let the squash cool for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium heat melt the butter; saute the onions. Stirring occasionally, cook the onions for 10 minutes, or until they begin to brown at the edges. Mix in the garlic, coriander, and nutmeg. Cook the mixture 2 minutes more; then remove the pan from the heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the seeds and stringy middle out of the squash, and discard these. Spoon out the flesh, chop it and add it to the onion mixture. Discard the skins. Heat and stir the squash-onion mixture, then season it with salt and pepper. Serve the squash hot.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash",
            "Winter Squash",
            "Acorn Squash"
        ],
