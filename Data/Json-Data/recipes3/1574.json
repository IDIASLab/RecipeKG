        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 cups cold orange juice",
            "2 cups frozen peach chunks",
            "ice",
            "\u00bd cup peach-flavored bourbon liqueur (such as Southern Comfort \u00ae)",
            "\u00bd cup peach schnapps",
            "\u00bd cup vodka",
            "2 cups cold lemon-lime flavored soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place orange juice, peach chunks, and ice in a blender; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peach-flavored bourbon liqueur, peach schnapps, and vodka into a large pitcher. Add orange-peach mixture; stir well to blend.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soda slowly into pitcher to prevent foaming.\n"
            }
