        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241804/baked-tofu-slices/",
        "name": "Baked Tofu Slices",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2282402.jpg",
            "width": 2592,
            "height": 2592
        },
        "datePublished": "2018-06-08T14:20:25.000Z",
        "description": "A baked tofu dish with a spicy soy-based sauce on top! Best served with rice.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "12 slices",
        "recipeIngredient": [
            "1 (18 ounce) package extra-firm tofu, slice 1/2-inch thick",
            "1 tablespoon olive oil",
            "1 pinch salt",
            "1 pinch ground black pepper",
            "2 tablespoons soy sauce",
            "2 tablespoons Sriracha hot sauce, or to taste",
            "1\u2009\u00bd teaspoons sesame oil",
            "1 teaspoon chile-garlic sauce",
            "1 teaspoon sesame seeds",
            "\u00bd teaspoon minced garlic",
            "1 green onion, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange tofu slices onto a baking sheet. Brush olive oil over the top of each slice. Sprinkle salt and pepper over the tofu.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake tofu in preheated oven until lightly browned on top, 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir soy sauce, Sriracha, sesame oil, chile-garlic sauce, sesame seeds, garlic, and green onion together in a bowl; spoon atop the tofu slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return baking sheet to oven and continue baking tofu until topping is warmed, 3 to 5 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Asian"
        ],
        "recipeCuisine": [
            "Asian"
        ],
