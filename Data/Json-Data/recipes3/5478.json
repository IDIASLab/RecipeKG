    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/80819/fast-and-delicious-black-bean-soup/",
        "name": "Fast and Delicious Black Bean Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2019277.jpg",
            "width": 3025,
            "height": 2725
        },
        "datePublished": "2018-06-08T13:25:24.000Z",
        "description": "A fast and easy recipe for black bean soup sure to delight your taste buds.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "1 medium onion, chopped",
            "1 green bell pepper, chopped",
            "1 large carrot, peeled and thinly sliced",
            "3 cloves garlic, minced",
            "2 (15 ounce) cans black beans",
            "1 (14 ounce) can chicken broth",
            "2 cups cubed cooked ham",
            "\u00bc teaspoon cumin",
            "\u00bd teaspoon salt",
            "ground black pepper to taste",
            "1 cup shredded Cheddar cheese",
            "1 cup sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large pot over medium heat. Stir in the onion, bell pepper, carrot, and garlic, and cook 5 minutes, until tender. Mix in 1 can black beans and chicken broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a blender, puree remaining can of beans until smooth. Mix into the pot. Bring soup to a boil. Reduce heat to low. Mix in ham, cumin, salt, and pepper. Simmer 20 minutes. Garnish with Cheddar cheese and sour cream to serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas",
            "Black Bean Soup Recipes"
        ],
