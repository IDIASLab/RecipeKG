    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255027/japanese-beef-tongue-stir-fry/",
        "name": "Japanese Beef Tongue Stir-Fry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Japanese Beef Tongue Stir-Fry"
        },
        "datePublished": "2018-06-08T14:40:38.000Z",
        "description": "This is a Japanese stir-fry dish with pork or beef tongue, lots of garlic, and spring onions. If you prefer to use pork fillet you can. Serve hot.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H13M",
        "totalTime": "P0DT0H33M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon salt, or as needed",
            "\u00bd pound beef tongue, very thinly sliced",
            "1\u2009\u00bd teaspoons sesame oil, or as needed",
            "2 cloves garlic, sliced",
            "2 cups sliced spring onions",
            "1 tablespoon sake (Japanese rice wine)",
            "\u00bd teaspoon dashi powder",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rub 1 tablespoon salt onto beef tongue; place on a plate. Let sit until flavors combine, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a pot of water to a boil. Dip beef tongue into boiling water with a slotted spoon for 30 seconds. Remove, draining excess water; transfer to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat sesame oil in a skillet over medium heat. Add garlic; cook and stir until fragrant, 1 to 2 minutes. Stir in onions; cook and stir until soft, 2 to 3 minutes. Add beef tongue and more sesame oil if needed; cook and stir until tongue is browned, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sake, dashi powder, salt, and pepper into the skillet; simmer until flavors combine, 2 to 3 minutes.\n"
            }
        ],
