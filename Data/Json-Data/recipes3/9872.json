    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232588/shrimp-spaghetti-with-crumbs/",
        "name": "Shrimp Spaghetti with Crumbs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1061225.jpg",
            "width": 1200,
            "height": 1200
        },
        "datePublished": "2018-06-08T14:08:37.000Z",
        "description": "This is a spaghetti that meets a garlic bread! And add shrimp for a wholesome meal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (16 ounce) package spaghetti",
            "\u2154 cup olive oil",
            "4 cloves garlic, halved",
            "1 cup dry bread crumbs",
            "2 tablespoons butter",
            "1 cup cooked shrimp",
            "\u00bd teaspoon dried rosemary",
            "\u00bd teaspoon dried basil",
            "salt and ground black pepper to taste",
            "1 tablespoon chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally, until cooked through but firm to the bite, about 12 minutes. Drain and transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2/3 cup olive oil and garlic in a skillet over medium-low heat until garlic is fragrant, about 5 minutes. Remove from heat and discard garlic cloves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss bread crumbs with about 2 tablespoons garlic olive oil in a bowl and spread bread crumbs over a baking sheet. Pour remaining garlic olive oil over spaghetti and toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake bread crumbs in the preheated oven until toasted, about 5 minutes. Transfer bread crumbs to the bowl with spaghetti and toss.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a clean skillet over medium heat; cook and stir shrimp, rosemary, and basil until shrimp is coated and heated through, about 5 minutes. Toss shrimp with spaghetti mixture and season with salt and pepper. Garnish with parsley.\n"
            }
        ],
