        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/109782/jalapeno-steak/",
        "name": "Jalapeno Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F134260.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:30:28.000Z",
        "description": "Jalapenos mixed with the other ingredients give a spicy and delicious kick to grilled steak. Marinate this steak in the fridge for up to 24 hours before cooking.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT8H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 jalapeno peppers, stemmed",
            "4 cloves garlic, peeled",
            "1\u2009\u00bd teaspoons cracked black pepper",
            "1 tablespoon coarse salt",
            "\u00bc cup lime juice",
            "1 tablespoon dried oregano",
            "1\u2009\u00bd pounds top sirloin steak"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine jalapenos, garlic, pepper, salt, lime juice and oregano in a blender. Blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place steak in a shallow pan or large resealable plastic bag. Pour jalapeno marinade over the steak, and turn to coat. Cover pan or seal bag; marinate in the refrigerator 8 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat, and lightly oil the grill grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain and discard marinade. Grill steak 5 minutes per side, or to desired doneness.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks",
            "Sirloin Steak Recipes"
        ],
