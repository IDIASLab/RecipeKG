        "recipeIngredient": [
            "\u00bc cup chopped uncooked bacon",
            "1 pound fresh green beans, trimmed",
            "\u00bc cup chopped onion",
            "\u00bc cup butter",
            "\u00bc cup chicken broth",
            "1 clove garlic, minced",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine bacon, green beans, onion, butter, chicken broth, garlic, salt, and pepper in a saucepan. Cover and simmer on medium heat until beans are tender, 15 to 20 minutes.\n"
            }
        ],
