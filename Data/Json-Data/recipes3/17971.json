        "name": "Roasted Rainbow Carrots with Mushrooms and Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5497601.jpg",
            "width": 2592,
