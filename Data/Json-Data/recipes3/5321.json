        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare Fast Chicken Soup Base. Bring to a simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add these, then simmer until tender, 10-20 minutes: 3/4 cup long-grain white rice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Before removing from heat, stir in: 1 cup (5 ounces) frozen green peas and 1/2 cup chopped fresh parsley.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Final touch: Add salt and pepper, to taste.\n"
            }
