    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/69429/chili-with-ground-pork/",
        "name": "Chili with Ground Pork",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F654386.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:23:05.000Z",
        "description": "A hearty chili, made with tons of veggies and extra lean ground pork.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 pound lean ground pork",
            "2 tablespoons olive oil",
            "4 medium onions, chopped",
            "4 cloves garlic, minced",
            "1 (8 ounce) can mushroom pieces",
            "1 (14.5 ounce) can wax beans",
            "1 (15 ounce) can sweet peas",
            "1 green bell pepper, chopped",
            "1 red bell pepper, chopped",
            "1 (28 ounce) can tomato sauce",
            "chili powder to taste",
            "ground nutmeg to taste",
            "dried marjoram to taste",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the olive oil in a large, deep skillet over medium heat, and cook the ground pork until evenly browned. Reserving the juices in the skillet, remove pork, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the onions and garlic into the skillet, and cook in the pork juices over medium heat until tender. Mix in the mushrooms, wax beans, peas, green bell pepper, and red bell pepper. Cook and stir until tender and heated through.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the pork to the skillet. Mix in the tomato sauce. Season with chili powder, nutmeg, marjoram, and salt. Reduce heat, and simmer about 45 minutes to allow the flavors to blend.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chili Recipes",
            "Pork Chili Recipes"
        ],
