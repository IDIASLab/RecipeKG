    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19522/peasant-soup/",
        "name": "Peasant Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1018714.jpg",
            "width": 2731,
            "height": 2736
        },
        "datePublished": "2000-01-08T19:38:37.000Z",
        "description": "Hearty vegetable soup for a crowd. Makes about 5 to 7 quarts of soup, enough for 15 people as a meal. Great with crusty rye bread and even better if made the day before it is served.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT2H10M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "\u00bd pound bacon, cut into small pieces",
            "2 onions, chopped",
            "\u00bc cup chopped shallots",
            "1 leek, sliced",
            "2 stalks celery, chopped, with leaves",
            "5 cloves garlic, sliced",
            "2 cups dry mixed beans, soaked overnight",
            "3 carrots, sliced",
            "1 turnips, cubed",
            "\u00bd small head cabbage, finely shredded",
            "4 quarts vegetable stock",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large pot over medium high heat. Stir in the bacon and saute just to render the fat out of the bacon; do not cook till crisp. Then add the onions, shallots, leek, celery and garlic and saute for about 5 minutes, stirring frequently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Then, add the beans, carrots, turnip and cabbage along with enough vegetable stock to cover (about 4 quarts). Let simmer for 2 hours, or until vegetables are to desired tenderness. Season to taste with salt and pepper.\n"
            }
        ],
