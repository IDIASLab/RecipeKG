        "mainEntityOfPage": "https://www.allrecipes.com/recipe/148853/smoky-steak-marinade/",
        "name": "Smoky Steak Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F290038.jpg",
