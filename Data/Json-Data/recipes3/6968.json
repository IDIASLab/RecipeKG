        "cookTime": null,
        "totalTime": "P0DT0H5M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "\u00bd cup Worcestershire sauce",
            "\u00bc cup liquid smoke",
            "1\u2009\u00bd tablespoons salt",
            "3 tablespoons seasoned salt",
            "2 tablespoons garlic powder",
            "2\u2009\u00bd tablespoons onion powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together Worcestershire sauce, liquid smoke, salt, seasoned salt, garlic powder, and onion powder in a bowl until the salts have dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To use, pour the marinade over up to 2 pounds of meat in a resealable plastic bag. Seal the bag, and refrigerate overnight. The following morning, discard the marinade, and cook the meat as desired.\n"
            }
        ],
