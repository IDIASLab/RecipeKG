    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245025/creamy-pumpkin-sauce/",
        "name": "Creamy Pumpkin Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3118606.jpg",
            "width": 2437,
            "height": 2212
        },
        "datePublished": "2018-06-08T14:22:54.000Z",
        "description": "This sauce is great over pumpkin or cheese ravioli!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 teaspoons oil",
            "1 shallot, chopped",
            "2 tablespoons butter",
            "2 tablespoons all-purpose flour",
            "1 cup light cream",
            "1 pinch salt",
            "1 pinch ground cinnamon",
            "1 cup pumpkin puree",
            "1 cup chicken broth, or more as needed",
            "1 tablespoon finely chopped fresh sage",
            "1\u2009\u00bd teaspoons chopped fresh thyme"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat; cook and stir shallots until soft, about 10 minutes. Add butter and allow to melt. Whisk flour into butter mixture until incorporated. Stir cream, salt, and cinnamon into butter mixture and continue whisking until incorporated, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pumpkin and 1/2 cup chicken broth into cream mixture, adding more broth, 1/4 cup at a time, until desired consistency is reached. Simmer sauce until desired consistency is reached, 5 to 10 minutes. Stir sage and thyme into sauce.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces",
            "Creamy"
        ],
