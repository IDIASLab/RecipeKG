    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/94171/jimmy-dean-sausage-stuffing/",
        "name": "Jimmy Dean Sausage Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F982848.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-10-04T13:02:15.000Z",
        "description": "Jimmy Dean&reg; sausage is the secret ingredient in this flavorful stuffing dish seasoned with rosemary and garlic.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 (16 ounce) packages Regular Flavor Jimmy Dean Pork Sausage",
            "8 ounces fresh mushrooms, sliced",
            "1\u2009\u00bd cups diced onions",
            "\u00be cup diced or coarsely grated carrots",
            "1\u2009\u00bd cups diced celery",
            "3 cloves garlic, minced",
            "1 (10 ounce) package frozen spinach, thawed and drained",
            "2 teaspoons dried rosemary",
            "2 teaspoons poultry seasoning",
            "1 teaspoon black pepper",
            "1 (15 ounce) package herbed dry bread cubes",
            "2 cups chicken broth",
            "1 egg, lightly beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan or stockpot, cook sausage over medium-high heat, stirring frequently until thoroughly cooked and no longer pink. Add mushrooms, onion, carrots, celery and garlic; cook 10-12 minutes or until onion is translucent, stirring frequently. Transfer mixture to a large bowl; add remaining ingredients and stir well. Pour into a buttered 4-quart casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake covered for 60 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Jimmy Dean"
        ],
