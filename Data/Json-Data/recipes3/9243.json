        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228772/jalapeno-corn-bread/",
        "name": "Jalapeno Corn Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7450375.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-08T14:04:45.000Z",
        "description": "Wonderful cornbread with a hint of spice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (8.25 ounce) can cream-style corn",
            "1 cup yellow cornmeal",
            "\u00be cup buttermilk",
            "\u00bd cup corn oil",
            "3 ounces shredded Cheddar cheese",
            "3 eggs",
            "2 jalapeno peppers, minced",
            "1 teaspoon salt",
            "\u00bd teaspoon baking soda",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Move an oven rack to the center position in oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream-style corn, cornmeal, buttermilk, corn oil, Cheddar cheese, eggs, jalapeno peppers, salt, and baking soda in a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put butter into a 9-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in preheated oven, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour corn bread batter into the baking dish over melted butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until golden brown, about 35 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Cornbread Recipes"
        ],
