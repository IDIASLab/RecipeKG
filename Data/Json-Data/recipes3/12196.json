        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254256/vegetarian-indian-egg-curry/",
        "name": "Vegetarian Indian Egg Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegetarian Indian Egg Curry"
        },
        "datePublished": "2018-06-08T14:39:41.000Z",
        "description": "Hard boiled eggs in a spicy tomato sauce. Garnish with cilantro and serve with rice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons oil",
            "\u00bd teaspoon mustard seeds",
            "\u00bd teaspoon cumin seeds, or to taste",
            "\u00bd teaspoon fenugreek seeds",
            "1 red onion, diced",
            "2 cups diced tomatoes",
            "3 dried red chile peppers",
            "1 clove garlic",
            "1 teaspoon grated fresh ginger ",
            "1 teaspoon chili powder",
            "1 teaspoon salt",
            "\u00bd teaspoon ground coriander",
            "\u00bd teaspoon cumin",
            "3 hard-boiled eggs, sliced",
            "\u00bc bunch fresh cilantro, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a pan over medium heat. Add mustard seeds, cumin seeds, and fenugreek seeds; cook and stir until the seeds begin to sputter, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add onion to the pan; cook and stir until the onion turns golden, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir tomatoes, red chile peppers, garlic, ginger, chili powder, salt, ground coriander, and cumin into the onion mixture. Cook and stir until the tomato begins to break down, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir eggs into the curry; reduce heat to low and simmer until flavors are mixed, about 5 minutes. Garnish with cilantro.\n"
            }
        ],
