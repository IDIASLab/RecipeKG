    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24630/shrimp-toast-ii/",
        "name": "Shrimp Toast II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2236377.jpg",
            "width": 1280,
            "height": 1280
        },
        "datePublished": "2018-06-08T13:15:46.000Z",
        "description": "A shrimp mixture is spread on bread slices, coated with bread crumbs and fried to golden brown. These flavorful appetizers are a unique treat, and they may be made ahead of time and frozen.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "32 shrimp toasts",
        "recipeIngredient": [
            "1 pound cooked shrimp, peeled and deveined",
            "1 onion, chopped",
            "\u00bc inch piece fresh ginger root, minced",
            "2 egg whites",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "8 slices white bread, crusts trimmed",
            "\u00bd cup fine dry bread crumbs",
            "1 quart oil for frying",
            "1 lemon, for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a blender or food processor, blend shrimp, onion, ginger, egg whites, salt and pepper until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each white bread slice into 4 triangles. Spread both sides of triangles with shrimp mixture and coat with bread crumbs. Cover and chill in the refrigerator approximately 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large, heavy skillet to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small batches, fry bread triangles 3 minutes on each side, or until golden brown. Drain on paper towels. Garnish with lemon juice before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
        ],
