    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220226/bucks-italian-grilled-asparagus/",
        "name": "Buck's Italian-Grilled Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F791974.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2018-06-08T14:00:18.000Z",
        "description": "This cheesy Italian-style asparagus basically made people respect me as a chef. The best asparagus dish out there.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "1 tablespoon olive oil",
            "\u00bd pound fresh asparagus, trimmed",
            "1 teaspoon hot pepper sauce",
            "\u00bc cup slivered almonds",
            "2 pinches Italian herb seasoning",
            "\u00bc onion, chopped",
            "salt and ground black pepper to taste",
            "\u00be cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large skillet with olive oil over medium heat; place asparagus spears in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When asparagus starts to sizzle slightly, drizzle in the hot pepper sauce and add almonds, Italian seasoning, and onion to the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Move the asparagus around in the skillet with a spoon to expose almonds and onion to the butter and oil; cook until onion is translucent, about 5 minutes. Reduce heat to low, and season with salt and black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle asparagus with Parmesan cheese and heat until cheese melts, about 5 more minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Asparagus"
