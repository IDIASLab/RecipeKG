        "name": "Grilled Garlic Scapes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5505160.jpg",
            "width": 2305,
            "height": 2305
