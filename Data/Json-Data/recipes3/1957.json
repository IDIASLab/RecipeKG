    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9522/spiced-nuts-i/",
        "name": "Spiced Nuts I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F55705.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-13T16:43:48.000Z",
        "description": "After tasting commercial spiced nuts, my mother invented a recipe and came up with the perfect combination. Please make this recipe as stated and trust that the amounts ARE correct as written.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 to 4 cups",
        "recipeIngredient": [
            "\u00bd cup unsalted butter",
            "4 cups pecans",
            "2 tablespoons Worcestershire sauce",
            "1 tablespoon soy sauce",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground allspice",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a 2-quart microwave safe pan, melt the butter and stir in nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the Worcestershire sauce, soy sauce, salt, cinnamon, allspice, cloves and garlic powder. Mix well and microwave on high for 6 to 8 minutes, stirring every 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve warm or at room temperature; store in a tightly sealed container.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Nuts and Seeds"
        ],
