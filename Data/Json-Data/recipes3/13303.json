    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259030/raw-kale-breakfast-salad/",
        "name": "Raw Kale Breakfast Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Raw Kale Breakfast Salad"
        },
        "datePublished": "2018-06-08T14:44:02.000Z",
        "description": "A new twist on eggs and bacon for breakfast.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "3 eggs, at room temperature",
            "3 slices bacon",
            "2 bunches kale",
            "\u00bc cup balsamic vinaigrette dressing, or more to taste",
            "\u2153 cup halved Muscat grapes",
            "2 tablespoons chopped fresh parsley",
            "salt and ground black pepper to taste",
            "\u00bc cup toasted pumpkin seeds",
            "2 tablespoons toasted pumpkin seeds",
            "\u00bc cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a small pot of water to a boil. Add eggs. Remove from heat; let stand for 15 minutes. Transfer eggs to a bowl of ice water to cool, about 5 minutes. Drain and chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-low heat, turning occasionally, until evenly browned, about 10 minutes. Blot with paper towels to remove grease. Chop into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove thick stems from kale. Chop leaves into thin, ribbon-like pieces. Place in a large bowl, pour in balsamic vinaigrette, and toss until evenly coated. Add eggs, bacon, Muscat grapes, parsley, salt, and pepper; toss well. Add 1/4 cup plus 2 tablespoons pumpkin seeds and Parmesan cheese; toss well.\n"
            }
        ],
