        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17104/tangy-barbecued-chicken/",
        "name": "Tangy Barbecued Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F256216.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-31T03:05:53.000Z",
        "description": "A local restaurant serves chicken that tastes about the same, and it is the only barbecued chicken you will ever want to eat again! Just sit back and enjoy the tangy, crisp flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 half chickens",
        "recipeIngredient": [
            "1\u2009\u00bd cups cider vinegar",
            "5 tablespoons vegetable oil",
            "1\u2009\u00bd tablespoons poultry seasoning",
            "2 tablespoons salt",
            "4 chicken halves (1 leg and 1 breast each)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for indirect medium heat and lightly oil grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan over medium low heat, combine the vinegar, oil, poultry seasoning and salt. Stir often, until the sauce just starts to bubble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the chicken halves on the grill and brush with the sauce, turning them every 7 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue this over indirect medium heat for about 45 minutes, or until internal temperature reaches 180 degrees F (80 degrees F).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken"
        ],
