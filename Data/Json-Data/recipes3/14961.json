    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237291/grain-free-peanut-butter-cookies/",
        "name": "Grain-Free Peanut Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1126132.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-06-27T13:02:51.000Z",
        "description": "Hearty peanut butter cookies made from flaxseed meal. No sugar added! Gluten-free! These make a great breakfast!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00be cup flaxseed meal",
            "\u00bd cup coconut flour",
            "2 teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "1 teaspoon salt",
            "1 cup peanut butter",
            "\u2154 cup unsweetened applesauce",
            "2 eggs",
            "\u00bc cup butter, melted",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flaxseed meal, coconut flour, baking powder, cinnamon, and salt together in a bowl. Stir peanut butter, applesauce, eggs, butter, and vanilla extract together in a separate bowl; mix into flaxseed meal mixture until dough is just mixed. Spoon dough onto a baking sheet, pressing a wet fork into each cookie to flatten.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are lightly browned, 10 to 15 minutes.\n"
            }
        ],
