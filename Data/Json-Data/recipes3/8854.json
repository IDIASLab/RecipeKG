    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222375/emilys-pizza-burgers/",
        "name": "Emily's Pizza Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F837104.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2018-06-08T14:02:16.000Z",
        "description": "Fun, kid-friendly recipe that my 13-year old makes for us! She is so proud of herself that she makes dinner for us. Hope you enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 burgers",
        "recipeIngredient": [
            "2 pounds ground beef chuck",
            "24 slices mozzarella cheese",
            "12 hamburger buns",
            "2 (6.5 ounce) cans tomato sauce",
            "1 teaspoon dried oregano",
            "\u00bd teaspoon dried minced onion",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bc teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape beef chuck into 12 patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill patties on preheated grill until no longer pink in the center, 3 to 4 minutes on each side. Place 2 slices mozzarella cheese on each patty about 1 minute before removing from grill. Place patties on hamburger buns.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix tomato sauce, oregano, minced onion, salt, pepper, and garlic powder in a bowl; cover and heat in the microwave on high until heated through, about 5 minutes. Spread tomato sauce mixture over burgers to serve.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes",
            "Hamburgers"
