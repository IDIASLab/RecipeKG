    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262179/sunflower-salad/",
        "name": "Sunflower Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4931152.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-06-08T14:46:55.000Z",
        "description": "This eye-catching Russian layered chicken and mushroom salad is perfect for any party or buffet. The salad is decorated to look like a sunflower, hence the name.",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT3H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 chicken legs, skin removed",
            "salt to taste",
            "1 tablespoon vegetable oil",
            "1 onion, minced",
            "3\u2009\u00bd cups minced mushrooms",
            "1 cup mayonnaise",
            "2 cloves garlic, crushed",
            "3 hard-boiled eggs, yolks and whites separated and grated",
            "1\u2009\u00bc cups grated Cheddar cheese",
            "1 (4.25 ounce) can pitted black olives, drained",
            "1 (8 ounce) bag potato chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken legs into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until no longer pink at the bone, 15 to 20 minutes. Drain and let cool. Shred chicken meat and season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat. Cook and stir onion until soft, about 5 minutes. Add mushrooms and cook until soft, 5 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mayonnaise and garlic together in a bowl. Mix 1/4 cup of the mayonnaise with the chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread chicken-mayonnaise mixture in the shape of a disk on a serving plate. Cover with onion-mushroom mixture and 1/4 cup of mayonnaise. Add grated egg whites and 1/4 cup of mayonnaise. Sprinkle Cheddar cheese on top; cover with remaining mayonnaise. Sprinkle grated egg yolks evenly over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut olives into 4 wedges each and place on top to resemble sunflower seeds. Refrigerate salad for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potato chips around the salad to resemble sunflower petals.\n"
            }
        ],
