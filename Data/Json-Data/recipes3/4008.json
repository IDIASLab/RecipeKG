        "mainEntityOfPage": "https://www.allrecipes.com/recipe/39302/beaufort-stew/",
        "name": "Beaufort Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F792536.jpg",
