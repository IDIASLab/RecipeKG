    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254377/vegan-mini-doughnuts/",
        "name": "Vegan Mini Doughnuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5460649.jpg",
            "width": 2198,
            "height": 2305
        },
        "datePublished": "2018-06-08T14:39:47.000Z",
        "description": "These cute mini doughnuts are a sweet treat and also vegan! Decorate or not to your heart's content.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "12 doughnuts",
        "recipeIngredient": [
            "cooking spray",
            "1 cup all-purpose flour",
            "2\u2009\u00bd teaspoons baking soda",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon salt",
            "1 pinch ground cinnamon, or to taste",
            "\u00bd cup soy milk",
            "\u00bc cup vegetable oil",
            "\u00bd teaspoon apple cider vinegar",
            "\u00bd teaspoon vanilla extract",
            "1 cup confectioners' sugar",
            "1 tablespoon soy milk",
            "1 dash vanilla extract, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a mini doughnut pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking soda, nutmeg, salt, and cinnamon together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix soy milk, vegetable oil, vinegar, and vanilla extract together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the flour mixture and the soy milk mixture and stir until smooth; pour into a mini doughnut pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until doughnuts are light brown and the tops spring back when touched, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine confectioners' sugar, soy milk, and vanilla extract together in a bowl; stir until glaze is combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip doughnuts into glaze and cool on a wire rack.\n"
            }
        ],
