    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262510/sweet-potato-cauliflower-shepherds-pie/",
        "name": "Sweet Potato-Cauliflower Shepherd's Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5016706.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-06-08T14:47:16.000Z",
        "description": "A lighter version of traditional British shepherd's pie using sweet potatoes and cauliflower, the perfect blend of sweet and savory.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound sweet potatoes, peeled and chopped",
            "\u00bd pound cauliflower florets, chopped",
            "\u00bc cup chicken broth",
            "3 tablespoons butter",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2 tablespoons light olive oil",
            "2 carrots, diced",
            "\u00bd onion, diced",
            "2 cloves garlic, minced",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "1\u2009\u00bd pounds ground beef",
            "2 tablespoons tapioca starch",
            "1 cup chicken broth",
            "\u00bd cup peas",
            "\u00bd cup corn",
            "3 tablespoons tomato paste",
            "\u00bd cup freshly grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sweet potatoes in a pot and add water to cover. Bring to a boil and cook for 15 minutes. Add cauliflower and more water if needed; cook until all vegetables are tender, about 15 minutes more. Drain. Mash sweet potatoes and cauliflower with 1/4 cup broth, butter, 1 teaspoon salt, and 1/2 teaspoon black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium-high heat. Add carrots and onion; saute until softened, about 5 minutes. Add garlic, 1 teaspoon salt, and 1/2 teaspoon black pepper; cook 5 minutes more. Add beef; cook and stir until browned and crumbly, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle tapioca starch over the beef mixture in the skillet. Add 1 cup broth, peas, corn, and tomato paste. Bring to a boil. Reduce heat and simmer for 10 to 15 minutes. Spread beef mixture evenly in a baking dish. Top with mashed sweet potato mixture. Sprinkle Parmesan cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until slightly browned on top, about 25 minutes. Let stand 15 minutes before serving.\n"
            }
        ],
