    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237164/annas-easter-ham-pie/",
        "name": "Anna's Easter Ham Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Anna's Easter Ham Pie"
        },
        "datePublished": "2018-06-08T14:14:02.000Z",
        "description": "This recipe was passed down to me by Anna, my husband's Italian grandmother. It was passed down to her by her mother, and she was kind enough to share it. She calls it an 'Easter pie,' but it's great any time of year.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "2 prepared pie crusts",
            "1 pound ricotta cheese",
            "4 eggs, beaten",
            "\u00bd cup grated Parmesan cheese",
            "\u00bc cup milk",
            "\u00bc cup white wine",
            "2 tablespoons prepared mustard",
            "1 (8 ounce) package fresh basket cheese, chopped",
            "1 pound roasted ham, chopped",
            "\u00bd pound Italian-style ham, chopped",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a deep-dish pie plate with 1 pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat ricotta cheese, eggs, Parmesan cheese, milk, wine, and mustard together in a large bowl until smooth. Stir basket cheese, roasted ham, and Italian-style ham into the ricotta mixture; season with salt and pepper. Pour the mixture into the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut remaining pie crust into 3/4-inch strips and arrange to make a lattice over the top of the pie. Crimp and flute edges.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour. Leave pie out to cool to room temperature before refrigerating to chill completely.\n"
            }
        ],
