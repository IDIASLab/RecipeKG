        "name": "Southwestern Flank Steak Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5461084.jpg",
            "width": 3648,
            "height": 2736
