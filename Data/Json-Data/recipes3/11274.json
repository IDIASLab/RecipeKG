    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240828/kellys-hot-hash/",
        "name": "Kelly's Hot Hash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2004650.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2018-06-08T14:18:56.000Z",
        "description": "This works great as a paleo breakfast! Microwave a small bowl full of hash, dash on some Buffalo Tabasco&reg;, and enjoy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ground turkey",
            "2 tablespoons olive oil",
            "1 leek, minced",
            "1 red bell pepper, minced",
            "1 carrot, peeled and minced",
            "1 small zucchini, peeled and cut into small dice",
            "4 crimini mushrooms, minced",
            "1 pound baby kale, roughly chopped",
            "salt and ground black pepper to taste",
            "1 dash Buffalo-style hot pepper sauce (such as Tabasco\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium heat. Cook and stir turkey in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease. Transfer browned turkey to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in the same skillet over medium heat. Cook and stir leek and red bell pepper until leek is soft, about 5 minutes; add carrot and cook until carrot is softened, about 5 minutes more. Stir zucchini and crimini mushrooms with leek mixture; cook and stir until mushrooms are completely tender, about 10 minutes. Top vegetable mixture with kale and stir into mixture as it wilts; cook and stir another 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return browned turkey to the skillet, stir with vegetables and season with hot sauce, salt, and pepper; cook and stir until the turkey is reheated, 2 to 3 minutes.\n"
            }
        ],
