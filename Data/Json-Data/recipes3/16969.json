    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268655/easy-beet-chips/",
        "name": "Easy Beet Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5859075.jpg",
            "width": 4608,
            "height": 3072
        },
        "datePublished": "2018-10-17T02:39:09.000Z",
        "description": "A different way to use up some beets!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "6 medium beets, scrubbed and ends trimmed",
            "\u00bd teaspoon salt, or more to taste ",
            "1 tablespoon olive oil",
            "\u00bc teaspoon garlic powder",
            "\u215b teaspoon cracked black pepper",
            "\u00bc teaspoon dill"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Thinly slice beets to about 1/16-inch thickness using a mandoline. Place beet slices in a bowl and add salt. Toss to combine. Allow beets to sit about 30 minutes. Drain excess liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour olive oil over beets and toss to combine. Mix together garlic powder, black pepper, and dill in a small bowl. Sprinkle over beets and toss to coat. Place beet slices on baking sheets in a single layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crispy, 40 to 50 minutes. Sprinkle with additional salt, if desired.\n"
            }
        ],
