    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236627/risotto-with-nuts/",
        "name": "Risotto with Nuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Risotto with Nuts"
        },
        "datePublished": "2018-06-08T14:13:03.000Z",
        "description": "Delicious risotto made with vegetable broth and a variety of nuts. Serve as a side dish or filling main course.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup pine nuts",
            "1 cup hazelnuts",
            "1 cup almonds",
            "1 cup cashews",
            "1 cup walnuts",
            "1 cup chestnuts",
            "1 tablespoon olive oil",
            "3 small onions, finely chopped",
            "1 clove garlic, crushed",
            "1 teaspoon minced fresh parsley",
            "salt and ground black pepper to taste",
            "1\u2009\u00bd cups sliced fresh mushrooms",
            "1 teaspoon butter",
            "1 cup rice",
            "5 fluid ounces white wine, or as needed",
            "5 cups vegetable stock"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Spread pine nuts, hazelnuts, almonds, cashews, and walnuts onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring occasionally, until nuts are roasted and fragrant, 5 to 10 minutes. Remove nuts from oven and cool slightly. Crush nuts using a rolling pin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Boil chestnuts in a pot of water until tender, about 10 minutes. Drain and finely chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium-high heat; saute onions and garlic until onions are tender and garlic is lightly browned, about 5 minutes. Remove garlic and stir parsley, salt, and pepper into onion; add mushrooms. Reduce heat to low and continue cooking until mushrooms are softened, 5 to 10 minutes. Remove skillet from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a separate large skillet over medium-high heat; saute rice until toasted and fragrant, about 2 minutes. Stir wine and vegetable stock, 1 cup at a time, into rice, allowing stock to evaporate before each addition until rice is tender and all the liquid has been absorbed, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir mushroom mixture, roasted nuts, and boiled chestnuts into rice mixture and remove from heat.\n"
            }
        ],
