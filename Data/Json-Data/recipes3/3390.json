    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24572/sausage-mushroom-quiche/",
        "name": "Sausage Mushroom Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F185132.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:15:43.000Z",
        "description": "One of the best quiches I have ever had. Delicious and filling. Also great for dinner.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound small fresh button mushrooms",
            "1 pound ground pork breakfast sausage",
            "\u00bd cup chopped fresh parsley",
            "3 eggs",
            "1 cup half-and-half cream",
            "\u00bd cup grated Parmesan cheese",
            "\u00bc teaspoon salt",
            "1 (9 inch) unbaked 9 inch pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Prepare the mushrooms by snipping off the stems.  Cut in half if large.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crumble the sausage in a large skillet, add the mushrooms, cook on medium-high heat until the meat and mushrooms are lightly browned and all the liquid from the mushrooms has evaporated.  Drain off the grease.  Add the parsley.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat the eggs, adding the cream, cheese and salt.  Pour into the mushroom/sausage mixture; blending well. Pour mixture into the pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 25 to 30 minutes, until crust is well browned and the filling is set. Let stand 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Quiche"
        ],
