    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247358/durian-cookies/",
        "name": "Durian Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Durian Cookies"
        },
        "datePublished": "2018-06-08T14:27:40.000Z",
        "description": "We Asians love our durian. Here's a recipe for Durian lovers! The cookies will never last for an hour if left on the counter unattended! They're that good!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "100 cookies",
        "recipeIngredient": [
            "5 pods durian, seeded ",
            "1 tablespoon heavy whipping cream",
            "1\u2009\u00be cups confectioners' sugar",
            "7\u2009\u00bd ounces butter",
            "4 ounces cream cheese, softened",
            "2 egg yolks",
            "1 tablespoon vanilla extract",
            "3 cups all-purpose flour",
            "1 tablespoon cream of tartar",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine durian and heavy cream in a saucepan over low heat; heat until durian is softened and no chunks remain, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer durian mixture to a bowl of a stand mixer fitted with a paddle attachment. Add confectioners' sugar, butter, and cream cheese; beat together until light and fluffy. Beat in egg yolks and vanilla extract. Add flour, cream of tartar, and salt; mix until dough is evenly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon dough into a piping bag fitted with round tip. Pipe out small circles of dough onto the lined baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly golden brown, 10 to 15 minutes. Let rest on the baking sheets for 5 minutes before transferring to wire racks to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
