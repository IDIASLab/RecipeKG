    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218226/dads-onion-pie/",
        "name": "Dad's Onion Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F656587.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:58:44.000Z",
        "description": "Very easy, delicious, economical onion pie!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 single 9-inch pie crust",
            "3 tablespoons butter",
            "3 large sweet onions, thinly sliced",
            "2 eggs",
            "\u00bd cup milk",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups sour cream, divided",
            "3 tablespoons all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Fit the pie crust into a 9-inch pie dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a large skillet over medium-low heat, and cook, stirring often, until the onion slices are slightly browned at the edges, about 15 minutes. Allow to cool, and arrange the onion slices into the bottom of the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, beat the eggs thoroughly with milk, salt, and about 1 1/4 cup of sour cream. Mix the remaining 1/4 cup of sour cream with flour in a small bowl until thoroughly combined, and whisk the flour mixture into the egg-sour cream mixture. Pour the mixture over the onions in the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 45 minutes.\n"
            }
        ],
