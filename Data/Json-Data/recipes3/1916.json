        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8960/vinegar-chicken/",
        "name": "Vinegar Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F563816.jpg",
