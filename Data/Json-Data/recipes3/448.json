        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8058/ooey-goey-cake/",
        "name": "Ooey Goey Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1549252.jpg",
            "width": 2448,
            "height": 2696
        },
        "datePublished": "1998-09-02T06:21:35.000Z",
        "description": "This cake was passed on to my mother by my grandmother. It is one of my personal favorites. I hope it becomes one of yours.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -13x9 inch cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "\u00bd cup melted butter",
            "1 egg",
            "8 ounces cream cheese",
            "2 eggs",
            "3\u2009\u00be cups confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease and flour one 13x9 inch pan. Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together cake mix, melted butter, and 1 egg. Press into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In separate bowl mix cream cheese, 2 eggs, and confectioners' sugar. Pour over first layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 40 minutes.\n"
            }
        ],
