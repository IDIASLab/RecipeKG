    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261357/baked-rice-pilaf/",
        "name": "Baked Rice Pilaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Baked Rice Pilaf"
        },
        "datePublished": "2018-06-08T14:46:02.000Z",
        "description": "A bit of curry and lots of veggies make this a great prep-ahead side dish. Add chopped rotisserie chicken for a one-dish meal.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "2 tablespoons butter",
            "4 stalks celery, chopped",
            "\u00bd large sweet onion, chopped",
            "1 tablespoon minced garlic",
            "2 cups uncooked jasmine rice",
            "4 green onions, chopped",
            "\u00bd cup golden raisins",
            "2 teaspoons curry powder, or to taste",
            "1 teaspoon salt",
            "12 saffron threads, or to taste",
            "4 dashes cayenne pepper, or to taste",
            "3 cups chicken stock"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil and butter in a skillet over medium-high heat. Cook celery, sweet onion, and garlic until the onion is translucent, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine rice, green onions, raisins, curry powder, salt, saffron, and cayenne pepper in a 9x13-inch baking pan; stir well. Stir in the celery mixture and chicken stock. Cover pan tightly with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until rice is tender, about 35 minutes. Let stand for 5 minutes; fluff with a fork.\n"
            }
        ],
