        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9820/double-nut-chocolate-chip-cookies/",
        "name": "Double Nut Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F30331.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1997-07-30T14:17:31.000Z",
        "description": "Cookie recipe using cake mix.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package moist yellow cake mix",
            "\u00bd cup butter",
            "1 egg",
            "1 cup semisweet chocolate chips",
            "\u00bd cup chopped pecans",
            "1 cup sliced almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F.  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a sauce pan over low heat.  Pour into bowl and combine with cake mix and egg.  Beat on low with an electric mixer until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in chocolate chips, pecans and 1/4 cup of almonds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into balls.  Press top of ball in remaining almonds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 9-11 minutes or until lightly browned.\n"
            }
        ],
