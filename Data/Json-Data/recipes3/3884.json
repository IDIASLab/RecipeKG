    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/34913/creamy-mushroom-peas/",
        "name": "Creamy Mushroom Peas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F949216.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:17:57.000Z",
        "description": "This is an great side with mashed potatoes and chicken!  Company loves this one and so does my husband.  Not the boring everyday plain peas.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cups frozen green peas",
            "2 tablespoons butter",
            "1 cup sliced fresh mushrooms",
            "1 small chopped onion",
            "1 tablespoon all-purpose flour",
            "1 cup light cream",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1 pinch ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a small saucepan with one inch of water. Bring to a boil, add peas, and cook until tender, about 5 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a medium saucepan over medium heat.  Add mushrooms and onions, and cook for a few minutes, or until tender. Sprinkle flour over the mushrooms, and cook for 1 minute, stirring constantly. Gradually stir in cream, and season with salt, pepper and nutmeg. Cook, stirring until smooth and thick. Stir in peas, and remove from heat. Let stand for 5 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Green Peas"
        ],
