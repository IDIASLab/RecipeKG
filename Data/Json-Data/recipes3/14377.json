    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263436/western-omelet/",
        "name": "Western Omelet",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5253175.jpg",
            "width": 4680,
            "height": 4680
        },
        "datePublished": "2018-06-08T14:48:10.000Z",
        "description": "This is the ultimate Western omelet! You can even put red bell peppers and sausage in it, if you like.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "1 omelet",
        "recipeIngredient": [
            "2 teaspoons butter, divided",
            "\u2153 cup sliced fresh mushrooms",
            "\u00bc cup chopped green bell pepper",
            "2 green onions, sliced on the bias",
            "\u00bc cup cooked ham strips",
            "salt and ground black pepper to taste",
            "2 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt 1 teaspoon butter in a small skillet over medium heat. Add mushrooms, bell pepper, and green onions; cook until tender, about 5 minutes. Stir in ham until heated through, about 1 minute. Season with salt and pepper. Set filling mixture aside in a small bowl and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs together in a bowl; season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the same skillet over medium-high heat. Add remaining 1 teaspoon butter; heat until foaming. Pour in eggs and cook for 30 seconds. Lift the edges of the omelet so that the uncooked egg runs under the cooked edges and comes into contact with the hot skillet. Shake and tilt the skillet to move the uncooked egg. Repeat until the top is set but still moist and soft, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the filling over one side of the omelet. Fold the other half over the filling and slide omelet onto a plate.\n"
            }
        ],
