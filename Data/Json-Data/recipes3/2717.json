        "recipeIngredient": [
            "\u00bd cup mayonnaise",
            "2\u2009\u00bd tablespoons distilled white vinegar",
            "1 apple - peeled, cored, and chopped",
            "1 large stalk celery, diced",
            "\u00bd cup raisins",
            "\u00bd cup chopped walnuts",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, whisk together the mayonnaise and vinegar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the apples, celery, raisins and walnuts if desired. Add salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together well and serve chilled.\n"
            }
