    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219653/kentucky-lace-cakes/",
        "name": "Kentucky Lace Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F761088.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:59:43.000Z",
        "description": "These cornmeal pancakes are one of Papa's favorite recipes and really good with chili or beans, etc. Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1\u2009\u00bc cups milk",
            "1 tablespoon vinegar",
            "1 egg",
            "1 cup cornmeal",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon baking soda",
            "1 tablespoon butter, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the milk and vinegar in a bowl; set aside until the milk sours, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the egg, cornmeal, salt, and baking soda into the milk mixture until evenly mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt some of the butter in a large skillet over medium heat. Pour 1/2 cup of the cornmeal mixture onto the skillet and cook until bubbles appear on the surface, 2 to 3 minutes. Flip with a spatula and cook until browned on the other side, 1 to 2 minutes more. Repeat until all batter is used.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Pancake Recipes",
            "Whole Grain Pancake Recipes"
        ],
