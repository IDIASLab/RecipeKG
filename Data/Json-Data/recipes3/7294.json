    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/165322/zippy-orange-rosemary-chicken/",
        "name": "Zippy Orange Rosemary Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F221676.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:51:05.000Z",
        "description": "This takes chicken to a whole new level!  Orange marmalade, horseradish and rosemary married together make this fast and easy chicken recipe a spicy, sweet and savory hit I know your family will love...even on a weeknight.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "4 (4 ounce) skinless, boneless chicken breasts",
            "salt and pepper to taste",
            "\u00bd cup orange marmalade",
            "1 tablespoon prepared horseradish",
            "1 tablespoon dried rosemary"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 375 degrees F (190 degrees C). Spray an 8x8 inch baking dish with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season chicken breasts with salt and pepper; place in prepared dish. Combine orange marmalade, horseradish, and rosemary in a small bowl; pour over chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the chicken in the preheated oven until no longer pink in the center, and the juices run clear, 30 to 35 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
