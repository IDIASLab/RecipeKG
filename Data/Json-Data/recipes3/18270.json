    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270207/spelt-banana-bread/",
        "name": "Spelt-Banana Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6287107.jpg",
            "width": 2672,
            "height": 2326
        },
        "datePublished": "2019-01-25T08:20:40.000Z",
        "description": "If you are looking for a variation on banana bread, this recipe incorporating spelt flour for added nutrition, and both mashed and diced bananas for texture, is for you.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "cooking spray",
            "2 cups spelt flour",
            "2 teaspoons ground cinnamon",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon kosher salt",
            "2 large eggs",
            "\u00bd cup sour cream",
            "\u00bd cup unsweetened applesauce",
            "\u00bd cup pure maple syrup",
            "1\u2009\u00bc teaspoons vanilla extract",
            "4 ripe bananas",
            "1 cup chopped pecans",
            "2 tablespoons finely chopped pecans, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Coat an 8 1/2x4 1/2-inch loaf pan with cooking spray and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine spelt flour, cinnamon, baking powder, baking soda, and salt in a medium-sized bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a large bowl until light yellow in color. Stir in sour cream, applesauce, maple syrup, and vanilla extract. Mash 2 bananas and add to the egg mixture. Dice remaining bananas and add to the bowl. Stir in 1 cup pecans. Pour flour mixture into the bowl with the egg-banana mixture and and stir just until flour is incorporated; do not over mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into the prepared loaf pan and sprinkle 2 tablespoons pecans on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted in the center of the loaf comes out clean, 60 to 65 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pan on a rack to cool, about 10 minutes. Turn loaf out onto the rack to cool completely.\n"
            }
        ],
