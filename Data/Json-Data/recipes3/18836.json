    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/271068/pop-pi/",
        "name": "Pop Pi",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6436222.jpg",
            "width": 2535,
            "height": 2535
        },
        "datePublished": "2019-03-14T11:39:47.000Z",
        "description": "For those who enjoy spinach, bacon, and cheese.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 pies",
        "recipeIngredient": [
            "2 slices bacon",
            "1 teaspoon butter",
            "\u00bc cup sliced mushrooms",
            "1 cup chopped cooked spinach",
            "\u2153 cup cottage cheese",
            "\u00bd cup shredded Cheddar cheese",
            "salt and ground black pepper to taste",
            "1 (8 ounce) package crescent roll dough"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until crispy and evenly browned, about 10 minutes. Drain on paper towels and roughly chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in small frying pan over medium heat. Add mushrooms and cook until tender and juicy, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the bacon, mushrooms, spinach, cottage cheese, and Cheddar cheese in a bowl. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the edges of 2 crescent dough triangles together; roll out into a 6x5-inch rectangle. Place 1/4 of the spinach mixture on one end, leaving a 1/2-inch margin on the edges. Fold opposite end over filling, pressing down and crimping edges with a fork to seal. Repeat with remaining crescent dough and filling. Transfer to an ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, 20 to 25 minutes.\n"
            }
        ],
