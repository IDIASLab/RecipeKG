        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9659/chocolate-coconut-bars/",
        "name": "Chocolate Coconut Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7755782.jpg",
