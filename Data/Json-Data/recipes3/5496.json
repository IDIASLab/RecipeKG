        "mainEntityOfPage": "https://www.allrecipes.com/recipe/81203/jacked-up-tuna/",
        "name": "Jacked-Up Tuna",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F969018.jpg",
