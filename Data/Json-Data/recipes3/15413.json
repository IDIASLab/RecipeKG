    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244315/chocolicious-ice-cream-dairy-free/",
        "name": "Chocolicious Ice Cream (Dairy-Free)",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolicious Ice Cream (Dairy-Free)"
        },
        "datePublished": "2018-07-19T13:04:32.000Z",
        "description": "Chocolate ice cream with chia seeds and hemp milk! A healthy alternative ice cream for vegans and lactose-intolerant individuals.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT5H40M",
        "recipeYield": "1 1/2 quarts",
        "recipeIngredient": [
            "3 cups hemp milk",
            "3 tablespoons chia seeds",
            "\u00bd cup cocoa powder",
            "\u00bd cup agave nectar",
            "2 ounces semi-sweet baking chocolate",
            "2 teaspoons almond extract",
            "2 teaspoons vanilla extract",
            "\u00bd teaspoon salt",
            "1 pinch ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat hemp milk in a saucepan over medium heat until warmed, about 5 minutes. Remove saucepan from heat and stir chia seeds into hemp milk. Let mixture sit, stirring occasionally every hour, until thickened, 2 to 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cocoa powder, agave nectar, baking chocolate, almond extract, vanilla extract, salt, and pepper into chia-milk mixture; blend in a high-powered blender until smooth. Cool mixture in refrigerator for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend mixture in an ice cream maker according to manufacturer's instructions until frozen, 25 to 30 minutes.\n"
            }
        ],
