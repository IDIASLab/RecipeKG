    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9161/moms-great-green-beans/",
        "name": "Mom's Great Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1959879.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Just plain green beans that taste great. Originally submitted to ThanksgivingRecipe.com.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 - 8 servings",
        "recipeIngredient": [
            "2 (15 ounce) cans green beans",
            "1 cube beef bouillon cube",
            "1 tablespoon Worcestershire sauce",
            "1 teaspoon soy sauce",
            "1 tablespoon butter",
            "\u215b teaspoon garlic pepper seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Drain the green beans, reserving 1/2 of the liquid. Dissolve the bouillon cube in the reserved liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place green beans and liquid in a saucepan. Add Worcestershire sauce, soy sauce, butter or margarine and garlic pepper seasoning to taste. Simmer gently for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove to serving bowl leaving beans sitting in their liquid. Use a slotted spoon to serve.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Green Beans"
        ],
