    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12875/sydneys-squash/",
        "name": "Sydney's Squash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2375724.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-11-04T17:17:41.000Z",
        "description": "This recipe takes some prep time but it is worth it. Delicious!!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 pounds yellow squash, chopped",
            "1 onion, chopped",
            "4 tablespoons butter",
            "1 cup heavy whipping cream",
            "8 ounces American cheese, cubed",
            "1 tablespoon cornstarch",
            "\u00bd teaspoon salt",
            "\u00bd pound small shrimp, peeled and deveined",
            "20 buttery round crackers, crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pot of salted water, boil squash and onions until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine butter or margarine and cream; stir constantly over a low heat. Mix in cheese, cornstarch, salt and pepper. Stir until sauce thickens.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place squash and onions in a 2-quart baking dish. Mix in shrimp and cheese sauce. Sprinkle casserole with cracker crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered for 20 to 30 minutes or until hot.\n"
            }
        ],
