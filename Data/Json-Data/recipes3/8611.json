    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220616/kicked-up-potato-pancakes/",
        "name": "Kicked-Up Potato Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1035026.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "2018-06-08T14:00:41.000Z",
        "description": "Shredded potato with cheese, ham and more. Tasty and wonderful! I invented this during an Iron chef cooking contest I had with my family - and won!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 large potatoes, peeled and shredded",
            "\u00be cup shredded deli smoked ham",
            "3 eggs",
            "\u00be cup shredded mozzarella cheese",
            "1 teaspoon onion powder",
            "1 teaspoon garlic powder",
            "salt and ground black pepper to taste",
            "olive oil for frying",
            "1 green onion, chopped",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine potatoes, ham, eggs, mozzarella cheese, onion powder, garlic powder, salt, and black pepper in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-low heat. Pour enough olive oil to cover bottom of pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop up about 1/2 cup of potato mixture per pancake and form into a patty. Repeat, making 8 patties in all.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 4 pancakes at time in hot olive oil. Cook until browned, about 4 minutes on each side. Transfer to paper towel-lined plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with green onion and Parmesan cheese.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Pancakes"
        ],
