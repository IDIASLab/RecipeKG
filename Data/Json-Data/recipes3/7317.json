        "mainEntityOfPage": "https://www.allrecipes.com/recipe/167020/grilled-okra-salad/",
        "name": "Grilled Okra Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F154289.jpg",
