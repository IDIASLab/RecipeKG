        "name": "Blue Green and Red Potato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F948342.jpg",
            "width": 250,
            "height": 250
