        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281087/frozen-watermelon-martinis/",
        "name": "Frozen Watermelon Martinis",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8469010.jpg",
            "width": 0,
            "height": 0
