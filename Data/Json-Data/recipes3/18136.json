        "name": "CAMPBELL'S&reg; BBQ Bacon Slow Cooker Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6303498.jpg",
            "width": 960,
