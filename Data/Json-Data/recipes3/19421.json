        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10535/festive-shortbread/",
        "name": "Festive Shortbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7486842.jpg",
            "width": 0,
