    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10302/spritz-cookies-iii/",
        "name": "Spritz Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F745850.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-11-11T01:54:14.000Z",
        "description": "Cookie press cookies topped with red and green candied cherries.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "\u2154 cup sifted confectioners' sugar",
            "1 egg",
            "1 egg yolk",
            "1 teaspoon almond extract",
            "2\u2009\u00be cups sifted all-purpose flour",
            "72 red and green candied cherries, quartered"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together butter and confectioner's sugar in bowl until light and fluffy, using electric mixer at medium speed.  Beat in egg, egg yolk and almond extract; blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually stir flour into creamed mixture, mixing well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fit flower crown design into cookie press.  Place 1/2 of the dough in cookie press at a time, forcing dough through press, about 1 inch apart, on ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1/4 of a red or green candied cherry in center of each.  Bake in 400 degrees F (200 degrees C) oven 7-10 minutes or until set but not browned.  Remove from baking sheets; cool on racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Butter Cookie Recipes",
            "Spritz Cookie Recipes"
        ],
