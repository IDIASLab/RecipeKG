    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219889/no-mayo-3-cheese-spinach-dip/",
        "name": "No Mayo 3 Cheese Spinach Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "No Mayo 3 Cheese Spinach Dip"
        },
        "datePublished": "2018-10-04T13:03:58.000Z",
        "description": "Creamy spinach dip that tastes just like the one at the restaurant.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) jar Alfredo sauce",
            "1\u2009\u00bd (8 ounce) packages cream cheese, softened",
            "1 (10 ounce) package frozen chopped spinach, thawed and drained",
            "1 red bell pepper, chopped",
            "\u00bd red onion, chopped",
            "1 cup shredded mozzarella cheese",
            "\u00bd cup grated Romano cheese",
            "1 cup shredded Cheddar cheese",
            "5 (6 inch) pita breads, cut into wedges",
            "1 tablespoon vegetable oil",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the Alfredo sauce, cream cheese, spinach, red bell pepper, red onion, mozzarella cheese, and Romano cheese into a blender. Cover the blender and blend until well-mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour dip into a baking dish; sprinkle the Cheddar cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly and the cheese is melted, about 30 minutes. The dip will be runny but will thicken as it cools.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, arrange the pita wedges on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle with vegetable oil, and season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pita wedges are toasted, about 10 minutes. Serve with dip.\n"
            }
        ],
