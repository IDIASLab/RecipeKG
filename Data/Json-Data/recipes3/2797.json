    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18420/easy-tomato-pie/",
        "name": "Easy Tomato Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9383873.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-04-01T02:13:16.000Z",
        "description": "This succulent, savory pie is loaded with tomatoes, Swiss cheese and cottage cheese, all accented with basil, parsley and onion flakes. You can enjoy this pie year round: use canned tomatoes and dried basil in the winter, but be sure to treat yourself to fresh in the summer!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) unbaked pie shell",
            "2 eggs, beaten",
            "1 cup whole milk",
            "1 teaspoon salt",
            "\u00bd teaspoon dried basil",
            "\u00bc teaspoon dried parsley",
            "1\u2009\u00bd tablespoons dried minced onion",
            "\u00bd cup cottage cheese",
            "5 plum tomatoes, diced",
            "1\u2009\u00bc cups shredded Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, whisk together eggs, milk, salt, basil, parsley, onion flakes and cottage cheese. Spread tomatoes evenly across bottom of pastry shell, then sprinkle with Swiss cheese. Pour egg mixture over cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 45 minutes, until set in center.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Tomatoes"
        ],
