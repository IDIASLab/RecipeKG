    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233331/utokias-cauliflower-in-bell-pepper-and-cheese-sauce/",
        "name": "Utokia's Cauliflower in Bell Pepper and Cheese Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Utokia's Cauliflower in Bell Pepper and Cheese Sauce"
        },
        "datePublished": "2013-07-01T23:09:13.000Z",
        "description": "Cauliflowerets and red bell pepper are baked with a creamy cheese sauce in foil packets for an easy, cheesy side dish.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 sheet (18x24-inches) Reynolds Wrap\u00ae Heavy Duty Aluminum Foil",
            "4 cups cauliflowerets",
            "1 cup pasteurized processed cheese sauce",
            "\u00bd cup chopped red bell pepper",
            "2 ice cubes",
            "Salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cauliflowerets in center of  Reynolds Wrap&reg; Heavy Duty Aluminum Foil.  Spoon cheese sauce evenly over cauliflowerets; top with bell pepper and ice cubes.  Sprinkle with salt and pepper, if desired.  Place packet on baking sheet with 1-inch sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring up sides of foil; fold down two times.  Double fold both ends of foil to seal packet, leaving room for heat circulation inside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 20 to 25 minutes or until cauliflowerets are at desired texture.  Open packet carefully by cutting along top folds with a sharp knife, allowing steam to escape.  Stir to coat evenly.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Reynolds&reg;"
        ],
