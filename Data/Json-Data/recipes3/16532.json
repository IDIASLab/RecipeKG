    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/74551/spinach-and-yogurt-soup/",
        "name": "Spinach and Yogurt Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4519022.jpg",
            "width": 3453,
            "height": 3456
        },
        "datePublished": "2018-10-04T13:01:53.000Z",
        "description": "A good soup for a weekend lunch, accompanied by tuna sandwiches and crisp radishes!  Yogurt gives the soup a tart accent.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd tablespoons butter or margarine",
            "1 medium onion, finely chopped",
            "2 teaspoons all-purpose flour",
            "\u2153 teaspoon salt",
            "\u00bc teaspoon dried tarragon",
            "1 pinch ground nutmeg",
            "1 pinch cayenne pepper",
            "1 (16 ounce) package frozen chopped spinach, thawed ",
            "2 cups chicken broth",
            "\u00be cup plain yogurt",
            "2 slices  lemon, cut in half for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large saucepan over medium heat. Add the onion, and cook until tender, stirring occasionally. Stir in the flour, salt, tarragon, nutmeg and cayenne, and heat until fragrant. Stir in spinach (undrained) and chicken broth. Bring to a boil, then reduce heat to low, and simmer for about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the soup from the heat, and puree in a food processor or blender in batches. Return to the saucepan, and whisk in yogurt. Heat through, but do not boil. Taste and adjust seasonings if necessary. Ladle into bowls, and float a lemon slice on top of each serving.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Spinach Soup Recipes"
        ],
