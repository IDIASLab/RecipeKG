        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11812/corned-beef-noodle-casserole/",
        "name": "Corned Beef Noodle Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F195258.jpg",
            "width": 250,
            "height": 250
