        "recipeIngredient": [
            "cooking spray",
            "1 small sweet onion, chopped",
            "2 teaspoons minced garlic",
            "1 (14.5 ounce) can diced tomatoes",
            "1 (13 ounce) package smoked sausage, chopped into bite-size pieces",
            "salt and ground black pepper to taste",
            "3 cups cooked grits",
            "1 teaspoon hot sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over medium heat. Lightly grease with cooking spray. Add onion and garlic. Cook and stir until softened, about 6 minutes. Stir in tomatoes, sausage, salt, and pepper; cook until sausage starts to brown, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place grits in a serving bowl; cover with the sausage mixture. Top with hot sauce.\n"
            }
        ],
