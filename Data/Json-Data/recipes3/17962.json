    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284868/lionels-microwave-fudge/",
        "name": "Lionel's Microwave Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lionel's Microwave Fudge"
        },
        "datePublished": "2018-12-25T04:48:26.000Z",
        "description": "My father received this recipe from his aunt and it has been a family tradition. My father used to take 2 1/2 hours for each batch, but it can be made with a microwave in just 10 minutes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 cups white sugar",
            "2 tablespoons all-purpose flour",
            "\u00bd teaspoon salt",
            "\u00bd cup butter, melted",
            "\u00bd cup milk",
            "1 (1 ounce) square unsweetened chocolate",
            "cooking spray",
            "\u00bd cup chopped walnuts",
            "2 tablespoons butter",
            "1 teaspoon vanilla"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar, flour, and salt together in a 2-quart glass mixing bowl. Stir in 1/2 cup melted butter and milk, then add chocolate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave on high power for 5 minutes. Remove from the microwave and stir until chocolate is melted and thoroughly mixed in.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the microwave and cook to the soft ball stage or 245 degrees F (118 degrees C), about 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile spray an 8x11-inch pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Quickly add walnuts, 2 tablespoons butter, and vanilla; stir until mixture begins to lose its sheen. Pour into the prepared pan and allow to cool, about 1 hour, before cutting into pieces.\n"
            }
        ],
