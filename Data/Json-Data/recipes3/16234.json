        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17038/spicy-cheese-ball/",
        "name": "Spicy Cheese Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Cheese Ball"
        },
        "datePublished": "2000-01-29T23:56:34.000Z",
        "description": "This is a great cheesy and flavorful appetizer for holidays or any other time. Betcha can't eat just one as they are delicious. Great served with buttery round crackers. Extras can be frozen.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "7 balls",
        "recipeIngredient": [
            "1 (2 ounce) can black olives, drained and chopped",
            "1 (4 ounce) jar diced pimento peppers, drained",
            "8 green onions, finely chopped",
            "1 pound butter, softened",
            "8 (8 ounce) packages cream cheese, softened",
            "4 ounces blue cheese, crumbled",
            "1 cup chopped almonds or other nut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine olives, pimentos and green onions; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter or margarine, cream cheese and blue cheese; stir in olive mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into balls or logs and roll in chopped nuts; serve.\n"
            }
        ],
