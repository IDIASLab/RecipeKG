    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266797/keto-cauliflower-hash-browns/",
        "name": "Keto Cauliflower Hash Browns",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7618866.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-09-07T03:20:55.000Z",
        "description": "Cauliflower, cheese, and bacon come together in this keto alternative to hash browns.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 patties",
        "recipeIngredient": [
            "3 cups grated cauliflower",
            "1 cup shredded Cheddar cheese",
            "1 large egg",
            "\u00bc cup real bacon bits",
            "1 tablespoon diced chives",
            "\u00bd teaspoon salt",
            "\u215b teaspoon ground black pepper",
            "1 pinch cayenne pepper",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place grated cauliflower in a microwave-safe bowl. Cook on high for 2 minutes. Let cool for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wring cauliflower in a clean dish towel, squeezing out as much moisture as possible. Transfer cauliflower to a large mixing bowl. Add Cheddar cheese, egg, bacon bits, chives, salt, pepper, and cayenne. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a large baking sheet with cooking spray. Divide cauliflower mixture into 6 equal portions, making sure to leave space between each one. Flatten with your hands and shape into ovals.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned, about 15 minutes. Turn broiler on low and broil until crispy, about 5 minutes. Let cool for 5 minutes to firm up.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Cauliflower",
            "Roasted"
        ],
