        "name": "Low Carb Lamb Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F03%2F12%2F228257-lowcarblambburgers-passano-alr0321-9142-2000.jpg",
            "width": 2000,
