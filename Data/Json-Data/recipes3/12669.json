    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256235/slow-cooker-philly-cheesesteak-dip/",
        "name": "Slow Cooker Philly Cheesesteak Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4510171.jpg",
            "width": 4128,
            "height": 2322
        },
        "datePublished": "2018-06-08T14:41:42.000Z",
        "description": "Easy to make, delicious Philadelphia favorite in a fun dip form! Enjoy with tortilla chips or baguette slices.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H58M",
        "totalTime": "P0DT1H18M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 pound cream cheese, softened and cut into 2-inch pieces",
            "1 (8 ounce) package processed cheese (such as Velveeta\u00ae), cut into 1-inch pieces, or more to taste",
            "3 tablespoons olive oil",
            "1 large yellow onion, chopped",
            "1 large green bell pepper, chopped",
            "1 tablespoon Worcestershire sauce",
            "1 pound thinly sliced rib-eye steak, cut into 1-inch pieces",
            "salt and ground black pepper to taste",
            "1 (15 ounce) jar pasteurized process cheese spread (such as Cheez Whiz\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and processed cheese in a slow cooker on Low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in large skillet over medium heat; cook and stir onion and green bell pepper until tender, about 8 minutes. Add Worcestershire sauce and simmer. Mix rib-eye steak, salt, and pepper into onion mixture; cook and stir until steak is browned, about 5 minutes. Drain extra grease. Stir process cheese spread into steak mixture until melted. Pour cheese-steak mixture into the slow cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 45 minutes.\n"
            }
        ],
