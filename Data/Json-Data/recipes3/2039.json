        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11727/mostaccioli-with-spinach-and-feta/",
        "name": "Mostaccioli with Spinach and Feta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F591266.jpg",
