    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259135/cauliflower-rice-biryani-style/",
        "name": "Cauliflower Rice (Biryani-Style)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4556539.jpg",
            "width": 3888,
            "height": 2592
        },
        "datePublished": "2018-06-08T14:44:09.000Z",
        "description": "Indian-style cauliflower rice. Optional additions - sliced red onion, boiled egg, avocado, other biryani things.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 head cauliflower, broken into florets",
            "3 tablespoons butter",
            "1 clove garlic, minced, or to taste",
            "\u00bd teaspoon cumin",
            "\u00bd teaspoon ground coriander",
            "\u00bd teaspoon garam masala",
            "\u00bd teaspoon ground turmeric",
            "\u00bc teaspoon minced fresh ginger, or to taste",
            "1 pinch cayenne pepper, or more to taste",
            "salt and ground black pepper to taste",
            "1 lime, cut into wedges",
            "\u00bc cup chopped fresh cilantro, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Process cauliflower florets in a blender or food processor until broken into small pieces about the size of rice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a skillet over medium-high heat; add cauliflower rice, garlic, cumin, coriander, garam masala, turmeric, ginger, cayenne pepper, salt, and black pepper. Cook, stirring occasionally, until cauliflower is softened, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove skillet from heat and add lime wedges and cilantro.\n"
            }
        ],
