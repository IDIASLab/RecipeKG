    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/135891/sweet-and-sour-turkey-patties/",
        "name": "Sweet and Sour Turkey Patties",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1119784.jpg",
            "width": 1600,
            "height": 1067
        },
        "datePublished": "2018-06-08T13:45:06.000Z",
        "description": "These lean turkey patties are topped with a sweet and sour sauce that is sure to please even the pickiest eaters!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 patties",
        "recipeIngredient": [
            "1 pound ground turkey",
            "\u00bd cup dry bread crumbs",
            "\u00bd onion, chopped",
            "1 egg, beaten",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bd cup maple syrup",
            "\u00bd cup ketchup",
            "3 tablespoons mustard",
            "\u00bc cup vinegar",
            "\u00bd cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C), and grease a 9x12 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the ground turkey, bread crumbs, onion, egg, salt, and pepper in a bowl, and form into 6 patties. Place the patties in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the maple syrup, ketchup, mustard, vinegar, and oil in a bowl, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the patties in the preheated oven for 20 minutes, remove from oven, and turn them over.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle the patties with the maple syrup mixture, return to the oven, and bake until the patties are browned and the sauce is bubbling and thickened, about 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
