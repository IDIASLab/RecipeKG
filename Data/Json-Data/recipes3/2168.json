        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12809/simple-shrimp-pasta-salad/",
        "name": "Simple Shrimp Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2069511.jpg",
            "width": 2722,
            "height": 2722
        },
        "datePublished": "1999-06-21T20:56:05.000Z",
        "description": "A supremely simple Shrimp Pasta salad!  It can be whipped up in no time at all and is excellent served with Garlic Cheese Bread!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package dry fusilli pasta",
            "1 pound cooked medium shrimp, peeled and deveined",
            "1 tomato, chopped",
            "1 green bell pepper, chopped",
            "2 tablespoons mayonnaise",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil, add pasta and cook until al dente. Rinse under cold water and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine shrimp, tomato, green pepper, mayonnaise and salt and pepper to taste. Mix well. Add pasta and toss to coat. Chill for approximately one hour and serve.\n"
            }
        ],
