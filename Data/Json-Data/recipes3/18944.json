    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272228/cumin-spiced-sweet-potatoes/",
        "name": "Cumin-Spiced Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6455765.jpg",
            "width": 1080,
            "height": 1080
        },
        "datePublished": "2019-03-22T23:41:03.000Z",
        "description": "These sweet potatoes are warming, savory, and sweet. Perfect as a main meal or side dish!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "2 sweet potatoes",
        "recipeIngredient": [
            "1 tablespoon cornmeal",
            "2 teaspoons ground cumin",
            "2 medium sweet potatoes, peeled and chopped into 1 1/2-inch cubes",
            "2 tablespoons olive oil",
            "1 tablespoon butter",
            "1 pinch sea salt, or to taste",
            "2 tablespoons sour cream, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cornmeal and cumin together in a bowl. Add sweet potatoes and toss until coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil and butter in a large, non-stick frying pan over medium heat until the butter is melted and starts to bubble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the sweet potatoes to the frying pan and ensure that they are evenly distributed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat to medium-low. Cover the frying pan with a lid and cook, covered, for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Uncover and flip the sweet potatoes. Continue cooking until easily pierced with a knife, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the pan with tongs and onto a plate. Sprinkle with sea salt and serve with sour cream on the side.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
