        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23972/bow-ties-with-veggies/",
        "name": "Bow Ties with Veggies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F253230.jpg",
            "width": 250,
