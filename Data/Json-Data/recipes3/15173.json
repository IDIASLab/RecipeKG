    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266170/leftover-meatloaf-parmesan/",
        "name": "Leftover Meatloaf Parmesan",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Leftover Meatloaf Parmesan"
        },
        "datePublished": "2018-07-06T05:40:32.000Z",
        "description": "A great way at use up leftover meatloaf. My husband does not eat leftovers, except when I make this.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 eggs",
            "\u00bc cup milk",
            "1 cup Italian-seasoned bread crumbs",
            "\u00bc cup grated Parmesan cheese",
            "1 pound leftover meatloaf, cut into 8 slices",
            "2 tablespoons vegetable oil",
            "1 (28 ounce) jar spaghetti sauce",
            "1 cup shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and milk together in a shallow bowl. Stir bread crumbs and Parmesan cheese together in another shallow bowl. Dip meatloaf slices, one at a time, into the egg mixture and bread crumb mixture to coat all sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large oven-safe skillet over medium-high heat. Cook the coated meatloaf slices until browned, turning once halfway, about 5 minutes. Drain off oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour spaghetti sauce evenly over the browned meatloaf slices. Sprinkle with mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until sauce is heated through and cheese is melted, 8 to 10 minutes.\n"
            }
        ],
