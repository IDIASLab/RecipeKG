    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282722/white-beans-and-greens/",
        "name": "White Beans and Greens",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "White Beans and Greens"
        },
        "datePublished": "2019-01-10T04:51:03.000Z",
        "description": "A sprinkling of Parmesan and savory bits of bacon add tons of flavor to tender Swiss chard and creamy white beans cooked in a zesty tomato-based sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 slices bacon",
            "1 (14 ounce) can fire-roasted diced tomatoes",
            "1 medium onion, chopped",
            "1 tablespoon minced garlic",
            "1 bunch Swiss chard including stems, roughly chopped",
            "\u00bd cup chicken broth",
            "1 (15.5 ounce) can great northern beans, rinsed and drained",
            "1 (15.5 ounce) can cannellini beans, rinsed and drained",
            "2 tablespoons grated Parmesan cheese, or to taste",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and chop when cool enough to handle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add tomatoes, onion, and garlic to the bacon fat. Cook over medium-high until the onion has softened and turned translucent, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add Swiss chard and chicken stock, cover, and steam until chard is tender, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add beans and chopped bacon. Cook until liquid has evaporated, about 3 minutes. Sprinkle with Parmesan and season with salt and pepper.\n"
            }
        ],
