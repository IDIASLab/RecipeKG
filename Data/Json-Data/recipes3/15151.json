        "recipeIngredient": [
            "\u00bd cup mayonnaise",
            "\u00bc cup lime juice",
            "\u00be tablespoon salt",
            "\u00bc teaspoon ground black pepper",
            "4 (6 ounce) fillets red snapper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine mayonnaise, lime juice, salt, and black pepper in a bowl. Rub mixture evenly onto the fillets and place them on a plate. Marinate in the refrigerator for 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill or griddle for medium heat and lightly grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place fillets on the cooking surface for 30 seconds. Turn over and continue to cook until opaque and cooked through, 8 to 10 minutes.\n"
            }
