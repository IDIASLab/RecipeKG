    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241220/lemon-and-almond-butter-bars/",
        "name": "Lemon and Almond Butter Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon and Almond Butter Bars"
        },
        "datePublished": "2018-04-14T00:34:21.000Z",
        "description": "Tasty granola bars for anyone who likes to snack! These bars can be modified to whatever flavor you desire! Wrap bars in plastic wrap for storage. Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "15 bars",
        "recipeIngredient": [
            "2 lemons, zested and juiced",
            "1\u2009\u00bd cups honey",
            "1\u2009\u00bd cups almond butter",
            "1 cup shredded unsweetened coconut",
            "1 cup quick-cooking oats"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix lemon juice and lemon zest in a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour honey into a microwave-safe bowl and heat in microwave until melted; stir with the lemon juice mixture. Add almond butter to the mixture and stir until smooth. Fold coconut into the mixture; add oats and stir to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the mixture into a medium-sized baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until browned along the edges, about 30 minutes. Cool to room temperature before cutting into bars.\n"
            }
        ],
