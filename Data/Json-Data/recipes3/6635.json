        "mainEntityOfPage": "https://www.allrecipes.com/recipe/140283/no-mayonnaise-ranch-dressing/",
        "name": "No-Mayonnaise Ranch Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F840450.jpg",
