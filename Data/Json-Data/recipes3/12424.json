    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255132/chicken-salad-pretzel-bites/",
        "name": "Chicken Salad Pretzel Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3900526.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2018-06-08T14:40:45.000Z",
        "description": "A quick and easy appetizer served on crispy pretzels.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT2H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large cooked chicken breast",
            "1 stalk celery",
            "\u00bd small onion",
            "2 green onions",
            "\u00bd cup mayonnaise",
            "2 teaspoons dried tarragon",
            "salt and ground black pepper to taste",
            "12 thin pretzel crackers (such as Snack Factory\u00ae Pretzel Crisps\u00ae)",
            "3 slices provolone cheese, cut into quarters",
            "4 cherry tomatoes, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pulse chicken breast in a food processor until finely chopped. Transfer to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine celery, onion, and green onions in the food processor; pulse until finely chopped. Add to chicken in the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir mayonnaise, tarragon, salt, and black pepper into the chicken mixture in the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill chicken salad until flavors combine, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each pretzel cracker with a piece of provolone cheese and a heaping tablespoon of chicken salad. Garnish with a slice of cherry tomato.\n"
            }
        ],
