    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269644/hoisin-asian-meatballs/",
        "name": "Hoisin Asian Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6099217.jpg",
            "width": 3744,
            "height": 3744
        },
        "datePublished": "2019-01-05T06:29:09.000Z",
        "description": "This easy recipe only needs a handful of ingredients and can be made in under 30 minutes. Treat your guests, your kids, or yourself with this delicious, quick finger food recipe. Garnish with sesame seeds if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "\u00bd cup panko bread crumbs",
            "\u00bc cup soy sauce, divided",
            "1 egg",
            "3 cloves garlic, minced, divided",
            "1 teaspoon minced fresh ginger root",
            "\u00bd cup hoisin sauce",
            "1 green onion, minced",
            "2 tablespoons rice vinegar",
            "2 teaspoons sesame oil",
            "1 (16 ounce) package frozen fried rice (such as Kahiki\u00ae Japanese-Style Fried Rice)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine beef, bread crumbs, 1 tablespoon soy sauce, egg, 2 cloves minced garlic, and ginger in a bowl and mix thoroughly. Form into golf-ball-sized meatballs and place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until meatballs are no longer pink inside, 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine remaining soy sauce, remaining garlic, hoisin sauce, green onion, vinegar, and sesame oil in a small bowl. Whisk sauce until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place frozen fried rice bag on a plate and heat in the microwave oven until thoroughly heated, 6 to 8 minutes. Peel back film and stir. Re-cover and heat for 1 minute more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cooked meatballs to a serving bowl. Pour sauce over meatballs to coat. Serve meatballs over fried rice.\n"
            }
        ],
