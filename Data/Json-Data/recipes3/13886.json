    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261514/cinnamon-chip-bread/",
        "name": "Cinnamon Chip Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4549077.jpg",
            "width": 3984,
            "height": 2241
        },
        "datePublished": "2018-06-08T14:46:12.000Z",
        "description": "Very similar to Great Harvest&reg;'s cinnamon chip bread. I have spent over a year tweaking and working on this recipe and I must say I think it might be perfect.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT4H40M",
        "recipeYield": "1 round loaf",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "1\u2009\u00bc cups milk",
            "\u2153 cup butter",
            "\u00bc cup honey",
            "1 egg",
            "\u00bc cup instant potato flakes",
            "1 tablespoon instant yeast",
            "1 tablespoon powdered milk",
            "1\u2009\u00bd teaspoons salt",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon baking powder",
            "1 cup cinnamon chips (such as Hershey's\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place flour, milk, butter, honey, egg, potato flakes, yeast, milk, salt, vanilla extract, cinnamon, and baking powder in a bread machine in the order listed by the manufacturer. Run Dough cycle. Remove dough from the machine after the cycle is done, about 1 hour 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate dough until cool, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer dough to a bowl of a stand mixer with a paddle attachment; mix in cinnamon chips using the dough hook.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dough in a round pie plate; let rise until about doubled in volume, 1 to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Cover dough loosely with a tent of aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until an instant-read thermometer inserted into the center reads 175 degrees F (80 degrees C), 40 to 50 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
