    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219991/cajun-shrimp-with-tangy-peach-sauce/",
        "name": "Cajun Shrimp with Tangy Peach Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F843999.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:59:57.000Z",
        "description": "Deliciously breaded shrimp accompanied by a spicy sweet sauce. Not your typical fried shrimp basket. Best served immediately, doesn't keep! Perfect for fondue.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups vegetable oil for frying",
            "1 (12 ounce) jar peach preserves",
            "2 tablespoons prepared horseradish",
            "2 tablespoons prepared spicy mustard",
            "1\u2009\u00bc cups all-purpose flour",
            "1 tablespoon baking powder",
            "1 teaspoon salt",
            "1 teaspoon garlic powder",
            "1 teaspoon paprika",
            "\u00bd teaspoon cayenne pepper",
            "\u00bd teaspoon black pepper",
            "\u00bc teaspoon dried basil",
            "2 large eggs",
            "1 cup beer",
            "2 pounds medium shrimp, peeled and deveined"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the peach preserves, horseradish, and mustard together in a small bowl until well blended; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the flour, baking powder, salt, garlic powder, paprika, cayenne pepper, black pepper, basil, eggs, and beer in a large bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the shrimp in batches to the batter, turning to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove with a slotted spoon, shaking off excess batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry the shrimp in the preheated oil until golden, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the fried shrimp to a paper towel-lined plate to drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve with the peach sauce.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Shellfish",
            "Shrimp"
        ],
