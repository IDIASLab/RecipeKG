    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/34830/salmon-patties-iii/",
        "name": "Salmon Patties III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F31549.jpg",
            "width": 1500,
            "height": 1150
        },
        "datePublished": "2018-06-08T13:17:57.000Z",
        "description": "Salmon patties that are quick and easy. Great with eggs for breakfast, or serve with a salad for a light summer dinner.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd pound salmon",
            "1 red potato, peeled and chopped",
            "1 shallot, minced",
            "1 egg, beaten",
            "\u00bc cup Italian seasoned bread crumbs",
            "1 teaspoon dried Italian seasoning",
            "salt and pepper to taste",
            "\u00bd cup cornflake crumbs",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C) and lightly grease a small baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place salmon in the prepared baking dish, cover, and bake 20 minutes, or until easily flaked with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potato in a small saucepan with enough water to cover, and bring to a boil. Cook 15 minutes, or until tender. Drain and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix salmon, potato, shallot, egg, and bread crumbs. Season with Italian seasoning, salt, and pepper. Place the cornflake crumbs in a small bowl. Form the salmon mixture into 1 inch balls, and roll in the cornflakes to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a medium saucepan over medium heat. Press the balls into patties, and fry 3 to 5 minutes per side, or until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Fish Cake Recipes"
        ],
