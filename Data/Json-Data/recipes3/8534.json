    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220200/pan-fried-halibut/",
        "name": "Pan Fried Halibut",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8081136.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-06-08T14:00:16.000Z",
        "description": "This recipe takes an often boring fish and turns it into something you'd be pleased to get at a restaurant.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 egg",
            "1 cup all-purpose flour",
            "2 tablespoons dried herbes de Provence",
            "1 teaspoon seafood seasoning (such as Old Bay\u00ae)",
            "1 teaspoon salt",
            "ground black pepper",
            "1 pound skinless, boneless halibut fillets",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk egg in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, herbes de Provence, seafood seasoning, salt, and black pepper in a separate small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut halibut into 4 equal pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large frying pan over medium-low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each piece of halibut in whisked egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dredge all sides of each piece in flour mixture to evenly coat; tap off excess flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place coated pieces immediately in the hot olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the halibut until lightly browned, about 5 minutes; turn and cook until fish is opaque and flakes easily with a fork, another 2 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Halibut"
        ],
