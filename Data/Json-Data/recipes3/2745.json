    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17879/sausage-gravy-ii/",
        "name": "Sausage Gravy II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3699714.jpg",
            "width": 4000,
            "height": 2250
        },
        "datePublished": "2000-04-29T16:32:12.000Z",
        "description": "Here's a simple recipe for sausage gravy to serve over hot biscuits. Season with your favorite herbs and spices to taste.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound ground pork sausage",
            "\u00bc cup chopped onion",
            "1 teaspoon garlic powder to taste",
            "1 teaspoon dried parsley",
            "salt and pepper to taste",
            "2 tablespoons all-purpose flour",
            "1 cup milk as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium high heat, combine the sausage and onion and saute for 5 to 10 minutes, or until sausage is browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season with garlic powder, parsley and salt and pepper to taste. Add enough flour to just make the meat somewhat sticky. Then stir in milk until mixture is to desired consistency. Allow to heat through and serve.\n"
            }
        ],
