    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262399/vegan-tahini-shortbread-cookies/",
        "name": "Vegan Tahini Shortbread Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4580536.jpg",
            "width": 3036,
            "height": 4048
        },
        "datePublished": "2018-10-09T23:25:15.000Z",
        "description": "These vegan tahini shortbread cookies are the perfect afternoon snack with coffee or tea.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H25M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00be cups unbleached all-purpose flour",
            "1 cup tahini",
            "\u00be cup vegan margarine",
            "\u00be cup confectioners' sugar",
            "\u00bd cup sesame seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend flour, tahini, margarine, and sugar together in a food processor. Shape dough into 2 logs 1 inch in diameter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sesame seeds in a dish. Roll each log in sesame seeds. Wrap logs in plastic wrap; chill dough in the refrigerator, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C). Grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut dough into 1/4-inch-thick slices. Place on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 12 to 15 minutes.\n"
            }
        ],
