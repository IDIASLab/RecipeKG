        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13850/cranberry-jalapeno-relish/",
        "name": "Cranberry-Jalapeno Relish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7348376.jpg",
