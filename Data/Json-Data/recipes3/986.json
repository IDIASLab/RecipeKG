    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12599/fresh-tomato-pie/",
        "name": "Fresh Tomato Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8249508.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-08-02T15:49:59.000Z",
        "description": "A great way to use surplus tomatoes from your garden. Tasty served warm or cold! You may want to add chopped fresh herbs for flavor.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell",
            "7 ripe tomatoes, sliced",
            "1 yellow onion",
            "\u00be cup mayonnaise",
            "\u2153 cup shredded mozzarella cheese",
            "\u2153 cup grated Parmesan cheese",
            "ground black pepper to taste",
            "2 teaspoons fresh basil",
            "2 teaspoons fresh oregano"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the pastry shell for 8 to 10 minutes or until browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice onion and place in the bottom of pastry shell. Slice tomatoes and arrange over onions. Add black pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine mozzarella, parmesan and mayonnaise. Spread this mixture evenly  over tomatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 20-25 minutes or until golden brown. Once cooked, garnish with fresh herbs.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
