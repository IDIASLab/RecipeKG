    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269332/banana-pudding-with-vanilla-wafers/",
        "name": "Banana Pudding with Vanilla Wafers",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Banana Pudding with Vanilla Wafers"
        },
        "datePublished": "2018-11-24T06:43:47.000Z",
        "description": "Banana pudding recipe from Kristy's mom.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cups white sugar",
            "6 tablespoons all-purpose flour",
            "4 large egg yolks",
            "6 tablespoons water",
            "4 cups milk",
            "5 large <a href=\"https://www.allrecipes.com/article/how-to-ripen-bananas-faster/\" aria-label=\"ripe bananas\">ripe bananas</a>, mashed, or more to taste",
            "1 teaspoon vanilla",
            "1 (12 ounce) package vanilla wafers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sugar and flour in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine egg yolks and water in a second bowl and beat with an electric mixer until light and fluffy. Add the sugar mixture and mix until batter is well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour milk into a saucepan and bring to a boil over medium heat. Add batter and stir until mixture thickens and comes to a boil, about 5 minutes. Remove from heat. Stir in bananas and vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer vanilla wafers and banana pudding alternately into a glass bowl. Cool before serving.\n"
            }
        ],
