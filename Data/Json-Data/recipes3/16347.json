    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268129/pork-chops-and-kale/",
        "name": "Pork Chops and Kale",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5798259.jpg",
            "width": 3264,
            "height": 1952
        },
        "datePublished": "2018-09-28T01:39:10.000Z",
        "description": "Pork chops are briefly sauteed and then simmered in a honey-mustard sauce with kale for a yummy mid-week dinner.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 pork chops",
            "salt and freshly ground black pepper to taste",
            "1 tablespoon vegetable oil, or as needed",
            "1 tablespoon butter",
            "1 shallot, minced",
            "1 sprig fresh thyme, chopped",
            "1 sprig fresh rosemary, chopped",
            "4\u2009\u00bd cups kale, ribs removed, chopped",
            "1 tablespoon coarse-grain mustard",
            "1 tablespoon honey",
            "2 tablespoons dry sherry",
            "2 tablespoons hard apple cider"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season pork chops generously with salt and pepper. Heat oil in a large skillet over medium-high heat and cook pork chops until browned, 2 to 3 minutes per side. Remove from skillet and place on a plate. Cover with aluminum foil to keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in the same skillet and reduce heat. Add shallot and cook until soft and translucent, about 5 minutes. Add thyme and rosemary. Cook and stir for 1 minute. Add kale in batches and cook until wilted, about 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together mustard, honey, sherry, and cider in a cup. Place pork chops on top of kale in the skillet and pour mustard sauce on top. Bring to a boil and reduce heat. Cover and cook over low heat until pork is cooked through, about 10 minutes more.\n"
            }
        ],
