        "mainEntityOfPage": "https://www.allrecipes.com/recipe/207344/apple-cider-sweet-potatoes/",
        "name": "Apple Cider Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F860085.jpg",
