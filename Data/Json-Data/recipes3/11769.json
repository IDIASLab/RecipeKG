    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245446/rainbow-potato-pancakes/",
        "name": "Rainbow Potato Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rainbow Potato Pancakes"
        },
        "datePublished": "2015-11-16T21:07:10.000Z",
        "description": "Make amazing potatoes with this healthy dish: Rainbow Potato Pancakes. With a combination of russet and purple potatoes, you can create this savory treat at home in under 20 minutes!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "14 servings",
        "recipeIngredient": [
            "3 cups grated russet potatoes",
            "3 tablespoons grated yellow onion",
            "1\u2009\u00bd teaspoons kosher salt",
            "5 tablespoons potato starch",
            "2 eggs, beaten",
            "\u215b teaspoon ground white pepper",
            "1 cup vegetable oil for frying, or more as needed",
            "Fine salt, to taste",
            "4 cups grated purple potatoes",
            "3 tablespoons grated red onion",
            "1\u2009\u00bd teaspoons kosher salt",
            "5 tablespoons potato starch",
            "2 eggs, beaten",
            "\u215b teaspoon ground black pepper",
            "1 teaspoon balsamic vinegar",
            "1 cup vegetable oil for frying, or more as needed",
            "Fine salt, to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "For the russet pancakes, preheat vegetable oil in a skillet until it reaches 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grate potatoes and onion, set aside. Place measured amounts into a clean bar towel or cloth. Squeeze out as much moisture as possible.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs and whisk in potato starch, salt, and white pepper until smooth. Quickly fold onion and potatoes into the egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a 1/4 cup measuring cup to portion out a potato pancake. Place into hot oil and flatten with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry until golden brown and flip with a slotted spatula (about 4 minutes per side). Fry for an additional time until the second side is golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a slotted spatula to remove the pancake to a drain pan or rack. Sprinkle with fine salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use the same directions to make the purple potato pancakes using the ingredients listed above.\n"
            }
        ],
