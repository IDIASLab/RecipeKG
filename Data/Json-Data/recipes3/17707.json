    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285010/rons-mini-taco-bites/",
        "name": "Ron's Mini Taco Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6060620.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-12-01T20:46:16.000Z",
        "description": "Yummy mini taco bites! Tortilla scoop chips with taco mixture. Add some salsa if you wish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "\u00be cup water",
            "1 (1 ounce) packet taco seasoning",
            "1 (16 ounce) can refried beans",
            "salt and ground black pepper to taste",
            "1 (10 ounce) bag scoop-shaped tortilla chips",
            "1 (8 ounce) package shredded Cheddar cheese",
            "1 (8 ounce) container sour cream, or more to taste",
            "1 large tomato, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir ground beef in the hot skillet until browned and crumbly, 5 to 7 minutes. Transfer to a paper towel-lined plate to drain. Wipe grease from the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water and taco seasoning mix in the same skillet and bring to a boil. Reduce heat and simmer, stirring occasionally, for 5 minutes. Add cooked beef along with refried beans; mix well. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Assemble tortilla scoops on individual plates. Scoop a small amount of beef mixture into each scoop, filling to the top. Add Cheddar cheese, diced tomato, and sour cream on top.\n"
            }
        ],
