    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/162786/cop-cobbler/",
        "name": "Cop Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F215162.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:50:47.000Z",
        "description": "A twist on a old favorite, great for dessert or even breakfast! Yummy with a scoop of vanilla ice cream and a cup of hot coffee!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "24 glazed doughnuts",
            "1 (14 ounce) can sweetened condensed milk",
            "2 eggs, beaten",
            "1 (29 ounce) can sliced peaches, not drained",
            "1 teaspoon ground cinnamon",
            "1 pinch salt",
            "\u00bd cup butter, melted",
            "1 (16 ounce) package confectioners' sugar",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). Grease a 9x13 inch baking dish, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Tear the doughnuts into bite-size pieces, and place them in a large bowl. Pour in the sweetened condensed milk, eggs, peaches with juice, cinnamon, and salt, and mix. Spoon the mixture into the greased baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 1 hour, until a knife inserted in the center comes out clean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the cobbler is baking, make a glaze by combining the melted butter, confectioners' sugar, and vanilla extract in a bowl. Stir until smooth. Drizzle the glaze over the baked cobbler.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Peach Dessert Recipes"
        ],
