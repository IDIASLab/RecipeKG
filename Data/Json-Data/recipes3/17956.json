        "name": "Slow-Cooked Green Beans in the Instant Pot\u00ae",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9421670.jpg",
