    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72565/dilled-creamed-potatoes/",
        "name": "Dilled Creamed Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F698304.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:23:38.000Z",
        "description": "This recipe was given to me by my mother-in-law. It is always served at family get togethers and BBQs. You can use regular peeled and cubed potatoes if you can't find baby potatoes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 pounds new potatoes",
            "2 tablespoons olive oil",
            "1 small onion, diced",
            "3 cloves garlic, minced",
            "1 quart heavy cream",
            "1 cup chopped fresh dill",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the potatoes in a large pot with enough water to cover. Bring to a boil, and cook 15 minutes, or until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the olive oil in a skillet over medium heat, and cook the onion and garlic until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain potatoes, and return to the pot. Pour in the cream, and mix in the onion, garlic, and dill. Bring to a boil, reduce heat to low, and simmer 20 minutes, stirring occasionally, until thickened. Season with salt and pepper.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato"
        ],
