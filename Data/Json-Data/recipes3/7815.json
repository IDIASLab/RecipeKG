    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/212933/filipino-chicken-relleno/",
        "name": "Filipino Chicken Relleno",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Filipino Chicken Relleno"
        },
        "datePublished": "2018-06-08T13:55:16.000Z",
        "description": "This lemon-rubbed baked Filipino chicken is stuffed with ground pork, Spanish chorizo, raisins, and Cheddar, and served with its own broth. It is usually served for special occasions.",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "1 whole chicken",
        "recipeIngredient": [
            "1 (4.5 pound) whole chicken",
            "2 tablespoons soy sauce",
            "1 lemon, juiced",
            "1 pound ground pork",
            "1 onion, diced",
            "1 tomato, diced",
            "2 large eggs",
            "1 carrot, cut into matchsticks",
            "1 link chorizo de bilbao (spicy Spanish semi-cured sausage), finely chopped",
            "\u00bd cup shredded Cheddar cheese",
            "1 (1.5 ounce) box raisins",
            "2 cloves garlic, minced",
            "salt and ground black pepper to taste",
            "1 cup chicken broth",
            "1 tablespoon corn flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove breast bones from chicken. Rinse and pat dry with a paper towel. Place chicken in a roasting pan. Pour soy sauce and lemon juice over chicken, rubbing liquid into the cavity as well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground pork with onion, tomato, eggs, carrot, chorizo, Cheddar cheese, raisins, garlic, salt, and pepper in a bowl. Stuff chicken with the mixture. Sew the opening to secure the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink at the bone and the juices run clear, about 60 minutes. Cover chicken with aluminum foil if it is getting brown too quickly. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C). Cool briefly before slicing crosswise.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour drippings from the chicken and broth in a pot over medium-high heat. Bring to a boil. Add corn flour; stir constantly until sauce thickens, 5 to 8 minutes. Serve with the chicken.\n"
            }
        ],
