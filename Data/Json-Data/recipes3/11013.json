    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239372/simply-mashed-potato-soup/",
        "name": "Simply Mashed Potato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1526591.jpg",
            "width": 980,
            "height": 980
        },
        "datePublished": "2018-06-08T14:16:48.000Z",
        "description": "This tasty potato soup is quick and easy and can be dressed with whatever toppings you want. I like bacon, cheese, and chives!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4\u2009\u00bd tablespoons butter",
            "1\u2009\u00bd cups frozen chopped onions",
            "1\u2009\u00bd (14.5 ounce) cans chicken broth",
            "1 (24 ounce) package frozen hash brown potatoes (such as Ore-Ida\u00ae Steam n' Mash\u00ae) Garlic Seasoned Potatoes)",
            "3 tablespoons all-purpose flour",
            "1\u2009\u00bd teaspoons dried basil",
            "\u00be teaspoon salt",
            "\u00be teaspoon ground black pepper",
            "\u00be teaspoon garlic salt",
            "1\u2009\u00bd dashes hot sauce (such as Louisiana\u00ae)",
            "1 (12 ounce) can evaporated milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large skillet over medium heat; cook and stir onions until softened, about 5 minutes. Slowly pour chicken broth over onions; add potatoes, flour, basil, salt, pepper, garlic salt, and hot sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring chicken broth to a boil; reduce heat to low and simmer, stirring and mashing potatoes often using a potato masher, until soup is cooked through and smooth, 20 to 25 minutes. Stir evaporated milk into soup.\n"
            }
        ],
