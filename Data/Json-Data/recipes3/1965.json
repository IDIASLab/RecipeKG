    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9667/fruit-drops-i/",
        "name": "Fruit Drops I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fruit Drops I"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These fruit-filled cookies have applesauce, peaches, raisins and pineapple.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "\u00bd cup vegetable oil",
            "\u00bc cup honey",
            "\u00bc cup unsweetened applesauce",
            "1 egg",
            "2 peaches",
            "1 cup rolled oats",
            "1 cup chopped raisins",
            "\u00bc cup crushed pineapple, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel, pit and slice peaches.  Place in small saucepan and add water until just covered.  Sprinkle with 3 teaspoons of sugar and boil.  Reduce heat and cook until peaches soften.  Remove from heat and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small saucepan, heat oil and honey over low heat.  Pour into bowl and beat in egg, applesauce and mashed peaches. Mix in flour, cinnamon and salt until well blended. Fold in oats, raisins and pineapple, stirring well.  If the dough seems too dry, add more applesauce 1 tablespoon at a time. Drop by spoonfuls 2 inches apart on cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 to 20 minutes until lightly colored.\n"
            }
        ],
