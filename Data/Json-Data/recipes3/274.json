        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15859/bacon-chestnuts/",
        "name": "Bacon Chestnuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F670576.jpg",
            "width": 2136,
            "height": 2136
