    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/269404/amaretto-shortbread-cookie/",
        "name": "Amaretto Shortbread Cookie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7340043.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-12-19T01:07:49.000Z",
        "description": "This is a classic Christmas almond shortbread cookie with amaretto.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "1 cup unsalted butter at room temperature",
            "1 cup confectioners' sugar",
            "2 teaspoons grated orange zest",
            "1 teaspoon amaretto liqueur",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon salt",
            "2 cups sifted all-purpose flour",
            "\u00be cup sliced almonds, lightly toasted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine butter, sugar, orange zest, amaretto, vanilla extract, and salt in a large bowl; beat using an electric mixer until smooth and creamy. With mixer on low speed, add flour and mix until a dough starts to form. Mix in almonds using a spatula. Form dough into a log and freeze for 40 minutes, then place in the refrigerator for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into small balls and flatten slightly. Place balls 1-inch apart on an ungreased baking sheet. Bake until edges are golden, about 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool on the baking sheet for 10 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
