        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263459/vegan-coleslaw/",
        "name": "Vegan Coleslaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5268493.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2018-06-08T14:48:11.000Z",
        "description": "A simple vegan coleslaw without any mayo. I add caraway seeds to my coleslaw as it helps with digestion. It tastes best after it has marinated for a few hours.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT4H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 medium head cabbage, finely sliced",
            "2 large carrots, peeled and shredded",
            "1 teaspoon salt",
            "6 tablespoons sunflower seed oil",
            "4 tablespoons white vinegar",
            "1 tablespoon confectioners' sugar",
            "1 teaspoon caraway seeds",
            "freshly ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cabbage and carrots in a large bowl, season with salt, and allow to sit for 30 to 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze out as much liquid as possible from cabbage and carrots with your hands; drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together oil, vinegar, confectioners' sugar, caraway seeds, and pepper and pour over the salad. Mix well. Allow to sit to intensify the flavor, about 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Coleslaw Recipes",
            "No Mayo"
        ],
