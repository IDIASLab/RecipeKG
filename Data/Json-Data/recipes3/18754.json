    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270727/grandma-ninis-skillet-chicken/",
        "name": "Grandma Nini's Skillet Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7342034.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2019-03-08T09:09:50.000Z",
        "description": "An easy fix for a quick chicken dish, and tastes just like what Grandma always makes when we come to visit. I served with Cuban-style seasoned black beans and rice. So good!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd tablespoons olive oil, divided",
            "\u00bd tablespoon balsamic vinegar",
            "1 teaspoon chili powder",
            "1 teaspoon paprika",
            "\u00bd teaspoon garlic powder",
            "\u00bc teaspoon onion powder",
            "1 pinch salt and ground black pepper to taste",
            "1 pound boneless, skinless chicken thighs",
            "\u00bc medium yellow onion, chopped",
            "2 cloves garlic, minced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 tablespoon olive oil, balsamic vinegar, chili powder, paprika, garlic powder, and onion powder in a large resealable plastic bag. Season with salt and pepper. Add chicken thighs. Seal bag and shake until chicken thighs are evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate in the refrigerator, 15 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in a skillet over medium heat. Add onion and garlic; saute until fragrant and translucent, about 5 minutes. Add chicken thighs to the skillet. Cover and cook until inside is no longer pink and juices run clear, about 25 minutes.\n"
            }
        ],
