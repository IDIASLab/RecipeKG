        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "2 onions, chopped",
            "1 green bell pepper, chopped",
            "\u00bd pound fresh mushrooms, sliced",
            "1 cup chopped celery",
            "1 (28 ounce) can crushed tomatoes",
            "1 (15 ounce) can kidney beans",
            "1 (16 ounce) package wide egg noodles",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook noodles in a large pot of boiling water until done.  Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brown meat in a large skillet.  Add onions, pepper, mushrooms, and celery; cook until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine hamburger mixture with tomatoes, beans, and cooked noodles in a 3 - 4 quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 minutes.  Top with cheddar cheese, and bake 30 minutes longer.\n"
            }
        ],
