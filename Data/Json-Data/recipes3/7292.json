        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/165225/cucumber-mango-salsa/",
        "name": "Cucumber-Mango Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2240880.jpg",
            "width": 3648,
