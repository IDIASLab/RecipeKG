        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound pork sausage",
            "2 cups milk",
            "1 chopped onion",
            "1 clove garlic, minced",
            "1 teaspoon mixed spice",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place pork in a large, deep skillet. Cook over medium high heat until evenly brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain off fat. Add milk, onion, garlic, mixed spice, salt and pepper. Continue simmering on medium-low heat, stirring frequently, until milk evaporates. Let cool and serve.\n"
            }
        ],
