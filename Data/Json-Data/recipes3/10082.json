        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233915/delicious-ham-omelette/",
        "name": "Delicious Ham Omelette",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3559249.jpg",
