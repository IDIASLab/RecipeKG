    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24639/onion-strips/",
        "name": "Onion Strips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F136238.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-06-08T13:15:46.000Z",
        "description": "Onions are flavored in a mild spice mixture, then baked on top of pizza crust with Swiss cheese. Cut the pizza into strips and serve it as tasty appetizer bites! This is good hot or cold.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 large onions, sliced into rings",
            "1 cube beef bouillon, crumbled",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon onion powder",
            "salt and pepper to taste",
            "1 unbaked pizza crust",
            "8 ounces shredded Swiss cheese",
            "1 (14 ounce) can pizza sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a medium saucepan over low heat. Slowly cook and stir onions in the butter, and mix in beef bouillon, garlic powder, onion powder, salt and pepper. Continue cooking about 5 minutes, until onions are soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange onion mixture evenly on the pizza crust. Top with Swiss cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place topped pizza crust on a medium baking sheet. Bake in the preheated oven 15 minutes, or until cheese is melted and crust is golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, cut into strips and serve with pizza sauce.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Cheese"
        ],
