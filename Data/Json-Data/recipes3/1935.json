    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9062/apple-and-onion-dressing/",
        "name": "Apple and Onion Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F666358.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Delicious dressing great on its own or baked with your favorite meat. Originally submitted to ThanksgivingRecipe.com.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 - 10 servings",
        "recipeIngredient": [
            "7 cups white bread cubes",
            "1 cup raisins",
            "\u00be cup butter",
            "1 cup chopped onion",
            "1 cup chopped celery",
            "1 clove garlic, minced",
            "3 cups diced apple without peel",
            "\u00bc cup chopped fresh parsley",
            "1\u2009\u00bd teaspoons salt",
            "\u00bc teaspoon paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly butter a 2 quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover raisins with boiling water and let sit for 5 minutes. Drain well then toss them with the bread cubes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a skillet melt the butter and saute the onions, garlic, and celery for about 4 to 5 minutes. Stir the sauteed onions into the bread cube mixture. Add the diced apples, parsley, salt and paprika and stir carefully. Place stuffing mix in  the prepared dish or you can alternately place it in a large roaster with your favorite uncooked meat (pork chops, turkey, chicken, etc.).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the dressing for 30 to 40 minutes. If you are baking the dressing with meat cook it until the meat is completely done all the way through.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Stuffing and Dressing Recipes",
            "Vegetarian Stuffing and Dressing Recipes"
        ],
