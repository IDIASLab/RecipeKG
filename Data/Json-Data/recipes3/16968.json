    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268659/cuban-sandwich-bites/",
        "name": "Cuban Sandwich Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5855396.jpg",
            "width": 3892,
            "height": 2756
        },
        "datePublished": "2018-10-17T02:36:15.000Z",
        "description": "All of the rich, delicious flavor of a Cuban sandwich in one bite-sized appetizer. Perfect for game day or holiday parties!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 bites",
        "recipeIngredient": [
            "1 baguette, cut into 1/2-inch slices",
            "3 tablespoons yellow mustard",
            "3 tablespoons Dijon mustard",
            "1\u2009\u00bd pounds Swiss cheese, sliced, divided",
            "4 ounces dill pickle slices",
            "1 pound honey ham, sliced",
            "1 pound cooked pork loin, sliced",
            "\u00bc cup mayonnaise",
            "\u00bc teaspoon garlic powder",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Place a cooling rack on a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix yellow and Dijon mustards together and spread evenly over baguette slices. Top 1/2 of the bread slices with 1/2 of the Swiss cheese and place on the rack on the baking sheet. Top each with pickle slices, ham, pork loin, and remaining cheese. Top sandwiches with remaining baguette slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mayonnaise over the tops of the sandwiches and sprinkle with garlic powder. Spray the bottom of a second baking pan with cooking spray and place upside-down on top of the sandwiches.  Weigh down the top pan with a heavy, oven-safe item such as a cast iron skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 minutes. Remove top pan and bake until golden brown, about 5 minutes more.\n"
            }
        ],
