    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19413/olive-blasta-pasta/",
        "name": "Olive Blasta Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F258152.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-04-15T21:55:07.000Z",
        "description": "The distinct flavors of black olives and sun dried tomatoes intermingled with chicken make this a wonderful recipe for all pasta lovers!  Try it topped with feta.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "4 ounces fettuccini pasta",
            "2 skinless, boneless chicken breast halves - cut into bite size pieces",
            "2 green onions, chopped",
            "\u00bd teaspoon dried basil",
            "\u00bd cup sliced black olives",
            "2 tablespoons olive oil",
            "\u00bd teaspoon minced garlic",
            "2 tablespoons grated Parmesan cheese",
            "10 sun-dried tomatoes, softened",
            "1 tablespoon minced fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium-high heat, cook chicken until brown and juices run clear, 5 to 10 minutes.  Stir in green onions, basil, olives, olive oil, garlic, Parmesan, sun-dried tomatoes and parsley; cook 5 minutes, or until garlic is golden and whites of onions are translucent.  Toss chicken mixture with pasta; serve.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
        ],
