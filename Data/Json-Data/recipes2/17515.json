        "recipeIngredient": [
            "1\u2009\u00bd fluid ounces premium gin",
            "\u00bd fluid ounce cointreau",
            "\u00bd fluid ounce rock candy syrup",
            "2 fluid ounces half-and-half",
            "1 fluid ounce freshly squeezed orange juice",
            "1 fluid ounce fresh sour",
            "1 dash orange flower water",
            "1 egg white",
            "\u00bd teaspoon vanilla extract",
            "nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stick blend all ingredients until well mixed. Pour into an ice filled, 16 ounce glass. Garnish with fresh nutmeg.\n"
