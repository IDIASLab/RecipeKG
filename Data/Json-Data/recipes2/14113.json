        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19520/tuna-casserole-i/",
        "name": "Tuna Casserole I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F602273.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-01-10T20:12:15.000Z",
        "description": "My father made this when I was small, and it was always a family favorite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cups crushed potato chips, divided",
            "2 (5 ounce) cans tuna, drained",
            "1 (15 ounce) can sweet peas, drained",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "6 slices American cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 1 1/2 quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1 cup crushed potato chips evenly on bottom of casserole dish. Next, layer 1 can of tuna, half of the peas, and half of the soup. Place slices of cheese on top of this. Repeat layering with tuna, peas and soup. Finish with a layer of chips, and top with slices of cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 20 to 25 minutes, or until bubbly and golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Tuna"
        ],
