        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215495/persimmon-cream/",
        "name": "Persimmon Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F521524.jpg",
            "width": 250,
            "height": 250
