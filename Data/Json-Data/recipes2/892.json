    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237301/balsamic-herbed-halibut/",
        "name": "Balsamic Herbed Halibut",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1126106.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2014-04-11T20:21:54.000Z",
        "description": "I like this dish a couple times a month; I serve it with rice or coleslaw. This is my personal recipe.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 (6 ounce) halibut fillets",
            "1 tablespoon seafood seasoning (such as Old Bay\u00ae)",
            "\u00be cup balsamic vinegar",
            "1 lemon, zested and juiced",
            "1 tablespoon chopped fresh parsley",
            "1 tablespoon chopped fresh thyme",
            "1 tablespoon chopped fresh rosemary",
            "\u00bd sweet onion, sliced",
            "1 teaspoon oil, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle halibut with seafood seasoning; place into a container with a lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk balsamic vinegar, lemon juice and zest, parsley, thyme, and rosemary together; pour over halibut. Cover; marinate fish in the refrigerator for 2 to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a baking dish; scatter onion evenly in the bottom of the dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over high heat; fry halibut just until browned, about 1 minute per side. Remove halibut and place on bed of onions in prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 8 minutes. Remove fish; pour about half the marinade over the fish, discarding remaining marinade. Return halibut to the oven; bake until fish flakes easily with a fork, about 7 minutes more.\n"
            }
        ],
