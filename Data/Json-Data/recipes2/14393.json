        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21112/cranberry-glazed-pork-ribs/",
        "name": "Cranberry Glazed Pork Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F186089.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-06-29T00:12:53.000Z",
        "description": "Ruby red cranberry sauce becomes a tangy glaze in this easy summertime recipe!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (8 ounce) can whole cranberry sauce",
            "1 cinnamon stick",
            "1 tablespoon prepared Dijon-style mustard",
            "1 teaspoon grated orange zest",
            "1\u2009\u00bd pounds country style pork ribs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small saucepan stir cranberry sauce, cinnamon stick, mustard and orange zest over medium heat for 5 minutes or till bubbly. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On preheated coals (medium heat) with a drip pan, place ribs on grill. Brush with sauce. Cover and grill for 45 to 60 minutes, brushing occasionally with sauce, until ribs are tender and no pink remains.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining sauce. Remove and discard cinnamon stick. Serve sauce with ribs.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes",
            "Country-Style Ribs"
        ],
