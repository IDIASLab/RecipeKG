    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22274/artichoke-pie/",
        "name": "Artichoke Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F72161.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-15T15:26:27.000Z",
        "description": "Can be used as an appetizer or with the main course.  Always a big hit with my family and friends.  Extremely easy to make.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 clove garlic, minced",
            "2 (6 ounce) cans artichoke hearts, drained",
            "\u00bd cup Italian seasoned bread crumbs",
            "\u00bd cup grated Parmesan cheese, divided",
            "1 (9 inch) unbaked 9 inch pie crust",
            "3 eggs, beaten",
            "1 (8 ounce) package mozzarella cheese, shredded"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat.  Saute garlic until it starts to brown.  Stir in the artichoke hearts and cook 10 minutes before adding the bread crumbs and half of the parmesan cheese.  When heated through, transfer half of artichoke mixture to pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour eggs over artichoke mixture and sprinkle in the rest of the parmesan cheese.  Spoon the rest of the artichoke mixture into the pie and top with mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 45 minutes, or until crust begins to brown.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Vegetarian Pie"
        ],
