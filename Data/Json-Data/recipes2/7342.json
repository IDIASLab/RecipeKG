    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254248/whiskey-sidecar-with-broiled-lemon-juice/",
        "name": "Whiskey Sidecar with Broiled Lemon Juice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3669872.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2016-06-18T05:52:12.000Z",
        "description": "Broiled lemon adds nice depth to this take on a sidecar cocktail.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "1 drink",
        "recipeIngredient": [
            "1 lemon, halved",
            "3 tablespoons superfine sugar",
            "1 lemon wedge",
            "2 fluid ounces whiskey",
            "1 fluid ounce orange liqueur ",
            "ice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler. Place lemon halves, cut-side up, on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil until lemon halves are blackened on the cut side, about 10 minutes. Allow lemons to cool enough to handle. Squeeze juice from the lemons into a measuring cup. Measure out 3/4 fluid ounce broiled lemon juice, reserving the rest for another use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sugar into a shallow bowl. Run lemon wedge around the rim of a cocktail glass; dip into sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine whiskey, orange liqueur, and 3/4 fluid ounce broiled lemon juice in a cocktail shaker and add ice; cover and shake for 20 seconds. Strain drink into the prepared cocktail glass.\n"
            }
        ],
