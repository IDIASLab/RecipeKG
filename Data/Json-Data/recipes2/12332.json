    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260246/honey-shrimp-stir-fry/",
        "name": "Honey Shrimp Stir-Fry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4557522.jpg",
            "width": 1500,
            "height": 1000
        },
        "datePublished": "2017-09-12T04:17:21.000Z",
        "description": "No need to order take out, this delicious Honey Shrimp Stir-Fry comes together in no time!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon soy sauce",
            "\u00bd tablespoon fish sauce",
            "\u00bd tablespoon sesame seeds",
            "1 tablespoon cornstarch",
            "1 tablespoon Aunt Sue\u2019s\u00ae Raw & Unfiltered Honey",
            "\u00bc cup chicken stock",
            "1 tablespoon canola oil",
            "1 red bell pepper, chopped",
            "1 green bell pepper, chopped",
            "1 yellow bell pepper, chopped",
            "1 onion, chopped",
            "1 cup button mushrooms, chopped",
            "1 carrot, chopped",
            "\u00bd zucchini, chopped",
            "\u00bd cup sugar snap peas",
            "\u00bc pound peeled and deveined shrimp",
            "2 cloves garlic, finely minced",
            "4 cups cooked white rice",
            "Chopped scallions for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together soy sauce, fish sauce, sesame seeds, cornstarch, honey, and chicken stock. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat canola oil in a large skillet over medium high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chopped peppers, onion, mushrooms, carrot, and zucchini to skillet and saute for about five minutes, or until vegetables have began to soften and take on color.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using tongs or a spatula, push vegetables to the sides of the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add shrimp to the center and saute till pink and translucent, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add garlic to the skillet, and cook for about a minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "And soy sauce-honey mixture and sugar snap peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook for another two minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve over steamed rice, topped with chopped scallion.\n"
            }
        ],
