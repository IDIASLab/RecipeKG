    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261845/peanut-butter-candy-bar-cookies/",
        "name": "Peanut Butter Candy Bar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Peanut Butter Candy Bar Cookies"
        },
        "datePublished": "2017-12-09T04:47:35.000Z",
        "description": "Perfect candy bar cookies/brownies for the chocolate and peanut butter lover!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H33M",
        "totalTime": "P0DT0H53M",
        "recipeYield": "28 small bars",
        "recipeIngredient": [
            "1 (16 ounce) package peanut-shaped sandwich cookies (such as Nutter Butter\u00ae), crushed",
            "\u00bd cup butter",
            "1 (14 ounce) can sweetened condensed milk",
            "\u00bd cup creamy peanut butter",
            "1 tablespoon vanilla extract",
            "5 (1.5 ounce) packages peanut butter cups, coarsely chopped",
            "1 cup semisweet chocolate chips",
            "\u00bd cup honey roasted peanuts",
            "2 (2.1 ounce) bars chocolate-covered crispy peanut butter candy (such as Butterfinger\u00ae), coarsely chopped",
            "\u00bd cup sweetened flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a 9x13-inch baking pan with aluminum foil extending over the ends of the pan; coat with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine crushed cookies and butter in a bowl. Press crumb mixture into the bottom of the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is set, 6 to 8 minutes. Remove crust and leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine condensed milk, peanut butter, and vanilla extract together in a bowl, stirring until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle peanut butter cups, chocolate chips, peanuts, chocolate-covered peanut butter candy, and coconut over the baked crust. Drizzle condensed milk mixture over the coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until lightly browned, about 27 minutes. Remove to a wire rack and let cool in pan. Pull aluminum foil to lift uncut brownies out of pan. Peel foil away from sides; cut brownies into bars.\n"
            }
        ],
