    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285897/grandmas-corned-beef/",
        "name": "Grandma's Corned Beef",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5228800.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-03-17T00:36:46.000Z",
        "description": "A honey-mustard glaze on this corned beef brisket brings superb flavor to an old Irish standard.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT2H35M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (3 pound) corned beef brisket",
            "2 cups water",
            "\u00bc cup honey",
            "1 tablespoon country Dijon mustard"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place brisket and water in a Dutch oven. Cover tightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Simmer in the preheated oven for 1 hour. Turn brisket over and continue cooking, covered, until meat is tender, 1 1/2 to 2 more hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and turn on the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer brisket, fat-side up, to a rack set in a broiler pan. Mix together honey and Dijon mustard; brush 1/2 of the mixture over the top of the brisket.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the roasting pan under the broiler, making sure the surface of the meat is 3 to 4 inches from the heat. Broil for 3 minutes. Brush with remaining honey-mustard and broil until brisket is glazed to perfection, about 2 minutes more.\n"
            }
        ],
