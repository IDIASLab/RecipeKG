    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255408/spice-cake-whoopie-pies/",
        "name": "Spice Cake Whoopie Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3936354.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-09-15T03:12:12.000Z",
        "description": "Try these easy whoopies once and you'll be making them for potlucks, game days, birthdays or every day!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H14M",
        "totalTime": "P0DT0H44M",
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "1 (15.25 ounce) package spice cake mix (dry)",
            "1\u2009\u00bd cups MUSSELMAN'S\u00ae Apple Butter",
            "\u00bd cup vegetable oil",
            "2 eggs",
            "8 ounces cream cheese, softened",
            "4 tablespoons unsalted butter, softened",
            "3 cups powdered sugar",
            "2 tablespoons milk",
            "1 teaspoon vanilla"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F. Line a baking sheet with parchment paper or a silpat liner.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together the cake mix, apple butter, oil and eggs until smooth. Let stand for 3-4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon the batter onto the prepared baking sheet, a heaping rounded tablespoon at a time. Space them about 2 inches apart. (Using a small scoop will keep the size consistent.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for about 14 minutes and let them cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost the flat side of each \"pie\" half with either the marshmallow fluff (see Note below) or the cream cheese frosting, and place halves together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make the cream cheese frosting, beat the cream cheese, butter and powdered sugar until fluffy. Add the milk and vanilla, and beat for another 1-2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Store whoopie pies in sealed container in refrigerator.\n"
            }
        ],
