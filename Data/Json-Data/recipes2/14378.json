        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21042/tortilla-rollups-iii/",
        "name": "Tortilla Rollups III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F563656.jpg",
            "width": 250,
            "height": 250
