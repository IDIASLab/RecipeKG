        "recipeIngredient": [
            "1 (750 milliliter) bottle Chardonnay wine",
            "1 (750 milliliter) bottle sweet red wine",
            "2\u2009\u00bd cups simple syrup",
            "2 cups orange juice",
            "1 cup rum",
            "1 apple, thinly sliced",
            "1 orange, thinly sliced",
            "10 strawberries, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir Chardonnay, red wine, simple syrup, orange juice, rum, apple slices, and orange slices together in a large pitcher.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate wine mixture for at least 3 hours to overnight. Add strawberries just before serving.\n"
            }
