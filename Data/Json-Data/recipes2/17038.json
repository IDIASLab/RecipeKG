        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/55457/baja-sauce/",
        "name": "Baja Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7082075.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-03-21T16:31:15.000Z",
        "description": "This is a tasty white sauce to use on Mexican dishes.",
        "prepTime": "P0DT0H25M",
        "cookTime": null,
        "totalTime": "P0DT3H25M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "\u00bc red bell pepper, seeded and chopped",
            "1 jalapeno pepper, sliced",
            "2 tablespoons diced onion",
            "1 cup mayonnaise",
            "1 tablespoon vinegar",
            "\u00bc teaspoon cracked black pepper",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon cumin"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bell pepper, jalapeno pepper, and onion in the bowl of a food processor; puree until smooth.  Stir together 4 teaspoons of this puree with the mayonnaise, and vinegar in a small bowl.  Season with pepper, garlic powder, and cumin.  Cover, and refrigerate for at least 3 hours to allow flavors to develop.\n"
            }
        ],
