    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255613/multi-potato-poblano-chowder/",
        "name": "Multi-Potato Poblano Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Multi-Potato Poblano Chowder"
        },
        "datePublished": "2016-10-01T03:58:22.000Z",
        "description": "A zesty chowder made with a good sampling of potatoes! This recipe was part of a contest for Potatoes USA developed and cooked by Beth Baker, Angie Fuller, and Sarah Stone.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H48M",
        "totalTime": "P0DT1H33M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd pound bacon, chopped",
            "\u00be cup chopped onion",
            "2 red potatoes, chopped",
            "\u00bd (16 ounce) package frozen diced potatoes",
            "3\u2009\u00bd cups chicken broth",
            "5 small yellow potatoes, chopped",
            "2 russet potatoes, chopped",
            "1 poblano pepper, halved and seeded",
            "\u00bd cup Alfredo sauce",
            "\u00bd cup Chardonnay wine",
            "\u00bd cup grated Parmesan cheese",
            "2 cloves garlic, crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir bacon and onion in a large pot until tender, about 5 minutes. Add red potatoes; cook and stir until crispy, 6 to 8 minutes. Stir in diced potatoes and cook until golden brown, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring chicken broth to a boil in a saucepan. Add yellow potatoes and russet potatoes; cook until tender, about 10 minutes. Stir into the bacon mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with aluminum foil. Place poblano pepper cut-side down on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook under the preheated broiler until skin has blackened and blistered, 5 to 8 minutes. Place into a bowl and tightly seal with plastic wrap. Let cool, about 15 minutes. Peel off skin and chop flesh.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chopped pepper, Alfredo sauce, Chardonnay, Parmesan cheese, and garlic in a saucepan over medium heat. Cook and stir until simmering, about 4 minutes. Pour over bacon and potato mixture. Simmer until flavors combine, about 10 minutes.\n"
            }
        ],
