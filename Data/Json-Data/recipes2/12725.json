    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283665/roasted-leg-of-pork/",
        "name": "Roasted Leg of Pork",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4585573.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-11-21T21:11:42.000Z",
        "description": "Mouthwatering and succulent roasted pork leg that just melts in your mouth, it's so tender.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2\u2009\u00bd tablespoons olive oil",
            "2 tablespoons dried sage",
            "2 tablespoons garlic powder",
            "2 teaspoons salt",
            "1 tablespoon onion powder",
            "1 (5 pound) pork leg with skin",
            "1 large onion",
            "6 cloves garlic, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 430 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine olive oil, sage, garlic powder, onion powder, and salt in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse pork leg and pat dry with a paper towel. Rub seasoned olive oil onto the pork leg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice onion and separate layers into rings. Place onion rings and garlic halves in the bottom of a roaster. Place pork on top of onions and garlic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven for 40 minutes. Reduce oven temperature to 355 degrees F (180 degrees C) and roast until an instant-read thermometer inserted into the center reads at least 145 degrees F (63 degrees C), about 1 hour 20 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and slice and serve.\n"
            }
        ],
