    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282510/sharis-shredded-chicken-enchiladas/",
        "name": "Shari's Shredded Chicken Enchiladas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8969638.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-01-24T14:09:43.000Z",
        "description": "These shredded chicken enchiladas are a family favorite and easy to prepare.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 enchiladas",
        "recipeIngredient": [
            "cooking spray",
            "1 cup sour cream",
            "1 cup green salsa (such as Frontera\u00ae Tomatillo)",
            "2 teaspoons olive oil",
            "1 small onion, chopped",
            "2 cloves garlic, minced",
            "3 cups shredded cooked chicken breasts",
            "1\u2009\u00bd cups shredded Mexican cheese blend, divided",
            "1 (4 ounce) can chopped green chile peppers",
            "8 (8 inch) flour tortillas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a 9x13-inch baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sour cream and salsa together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat. Saute onion and garlic in the hot oil until soft, 5 to 7 minutes. Stir in cooked chicken, 1 cup of the sour cream mixture, 1/2 of the Mexican cheese, and chile peppers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon about 1/3 cup of the mixture down the center of each tortilla. Roll tortillas and place, seam sides-down, into the prepared dish. Top with remaining sour cream mixture and cheese. Cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through and bubbly, 30 to 40 minutes.\n"
            }
        ],
