        "mainEntityOfPage": "https://www.allrecipes.com/recipe/107352/three-ingredient-green-bean-casserole/",
        "name": "Three-Ingredient Green Bean Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8898218.jpg",
