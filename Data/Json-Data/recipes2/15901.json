    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26825/hot-crab-rounds/",
        "name": "Hot Crab Rounds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F844525.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-03-21T16:21:49.000Z",
        "description": "Toasted crabmeat rounds with melted mozzarella will drive you into feeding frenzy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (6 ounce) can crabmeat, drained and flaked",
            "\u2153 cup creamy salad dressing, e.g. Miracle Whip \u2122",
            "\u00bd teaspoon Worcestershire sauce",
            "1 teaspoon cocktail sauce",
            "\u00bd cup thinly sliced celery",
            "2 tablespoons sweet pickle relish",
            "1 French baguette, cut into 1/2 inch slices",
            "1 (6 ounce) package sliced mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together crabmeat, creamy salad dressing, Worcestershire sauce, cocktail sauce, celery and pickle relish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange French baguette slices on a medium baking sheet in a single layer. Broil until lightly toasted, 2 to 5 minutes, in the preheated broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread even amounts of the crabmeat mixture on the toasted baguette slices. Top with mozzarella slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven 15 minutes, or until cheese is melted and lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Canapes and Crostini Recipes"
        ],
