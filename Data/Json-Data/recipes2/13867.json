        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17890/butterscotch-chocolate-cake/",
        "name": "Butterscotch Chocolate Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F16841.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2000-03-07T19:29:21.000Z",
        "description": "A chocolate cake with butterscotch and whipped topping. It is easy and 'to die for' Plan ahead as it needs to chill in the refrigerator for 2 hours.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (18.25 ounce) package chocolate cake mix",
            "1 (16 ounce) jar butterscotch topping",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "3 (2.16 ounce) bars chocolate-covered crispy peanut butter candy"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare and bake cake according to package directions, using a 9x13 inch pan.  Cool on a wire rack for 30 minutes.  Using the end of a wooden spoon handle, poke 12 holes in warm cake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour butterscotch topping over cake. Allow to cool completely.  Spread with whipped topping, sprinkle with candy bars.  Refrigerate for at least 2 hours before serving.\n"
            }
        ],
