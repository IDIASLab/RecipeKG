        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238504/classic-bellini-cocktail/",
        "name": "Classic Bellini Cocktail",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1244556.jpg",
            "width": 968,
            "height": 1296
