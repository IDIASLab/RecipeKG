    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283408/easy-mostaccioli-salad/",
        "name": "Easy Mostaccioli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Mostaccioli Salad"
        },
        "datePublished": "2015-07-12T20:51:01.000Z",
        "description": "Perfect for parties, picnics, and potlucks, this chilled mostaccioli salad has a tasty cucumber-onion dressing.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (16 ounce) package mostaccioli pasta",
            "6 drops yellow food coloring",
            "1 medium cucumber, chopped",
            "1 small onion, chopped",
            "1\u2009\u00bd cups white vinegar",
            "1 cup white sugar",
            "1 tablespoon dried parsley flakes",
            "1 teaspoon garlic salt",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 teaspoon monosodium glutamate (MSG)",
            "1\u2009\u00bd teaspoons yellow mustard",
            "\u00bd cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add penne and food coloring to the boiling water and cook, stirring occasionally, until pasta is tender yet firm to the bite, about 11 minutes. Rinse and drain pasta.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cucumber, onion, vinegar, sugar, parsley, garlic salt, salt, pepper, MSG, and mustard in a blender; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pasta in a large bowl. Add oil and toss to coat, then toss with cucumber-onion dressing. Refrigerate 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir before serving.\n"
            }
        ],
