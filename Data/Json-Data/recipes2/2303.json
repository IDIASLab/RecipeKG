        "name": "NO YOLKS&reg; Skillet Salsa Chicken with Noodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7058588.jpg",
