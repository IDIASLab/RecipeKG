        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16829/sherbet-shower-surprise/",
        "name": "Sherbet Shower Surprise",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F256667.jpg",
            "width": 250,
            "height": 250
