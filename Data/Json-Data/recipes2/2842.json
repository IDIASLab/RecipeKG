        "name": "Sweet and Spicy Sriracha Popcorn Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2140280.jpg",
            "width": 0,
