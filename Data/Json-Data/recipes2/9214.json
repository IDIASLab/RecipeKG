        "mainEntityOfPage": "https://www.allrecipes.com/recipe/144778/chocolate-covered-cherry-martini/",
        "name": "Chocolate Covered Cherry Martini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9021994.jpg",
