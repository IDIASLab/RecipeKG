        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255336/easy-homemade-pad-thai/",
        "name": "Easy Homemade Pad Thai",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3926830.jpg",
            "width": 1706,
            "height": 960
        },
        "datePublished": "2016-09-09T00:15:31.000Z",
        "description": "Delicious homemade Pad Thai doesn't get any more doable than this. Create the perfect blend of Asian-inspired pure flavor with ginger and red pepper. Add crunchy peanut butter and toss with vegetables and noodles for a flavor-loaded dish that rivals Thai restaurant cuisine.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 ounces pad Thai rice noodles",
            "2 teaspoons vegetable oil",
            "1 egg",
            "\u00bd cup water",
            "2 tablespoons crunchy peanut butter",
            "2 tablespoons soy sauce",
            "2 tablespoons lime juice",
            "2 teaspoons packed brown sugar",
            "\u00bd teaspoon McCormick\u00ae Ginger, Ground",
            "\u00bc teaspoon McCormick\u00ae Red Pepper, Crushed",
            "1 cup bean sprouts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook noodles as directed on package.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in wok or large skillet on medium-high heat. Add egg, cook and stir 30 seconds (egg will not be fully cooked).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in cooked noodles and remaining ingredients, except bean sprouts. Cook and stir until most of the liquid has been absorbed by the noodles, about 1 minute. Stir in bean sprouts. Serve with crushed peanuts, sliced green onions and lime wedges, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "McCormick&reg;"
        ],
