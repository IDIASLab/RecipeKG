        "recipeYield": "1 1/2 cups",
        "recipeIngredient": [
            "1 cup pineapple juice",
            "\u00bd cup soy sauce",
            "2 tablespoons fresh lime juice",
            "1 teaspoon ground cumin",
            "\u00bd teaspoon minced garlic"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the pineapple juice, soy sauce, lime juice, cumin, and garlic in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate up to 2 pounds of meat in the marinade for at least 4 hours before using.\n"
            }
