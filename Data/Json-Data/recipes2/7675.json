        "recipeIngredient": [
            "1 sheet waxed paper",
            "\u2154 cup honey",
            "\u00be cup creamy peanut butter",
            "4 cups granola"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a 9x13-inch baking pan with waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat honey in a saucepan over medium heat until bubbly, 2 to 3 minutes. Add peanut butter; stir until thoroughly mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place granola in a large bowl. Pour honey mixture over granola; stir until well combined. Transfer granola mixture to prepared baking pan; press firmly. Let stand until firm, 15 to 20 minutes.\n"
            }
