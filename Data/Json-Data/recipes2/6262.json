        "name": "Sarah's Rice Pilaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F04%2F28%2F203951_sarahs-rice-pilaf_abapplez.jpg",
            "width": 960,
