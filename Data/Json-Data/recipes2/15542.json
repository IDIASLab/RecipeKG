        "recipeIngredient": [
            "\u00bc cup olive oil",
            "1 cup minced onion",
            "\u00bc teaspoon onion powder",
            "2 cloves garlic, minced",
            "2 tablespoons dried oregano",
            "2 (6 ounce) cans tomato paste",
            "2 (15 ounce) cans tomato sauce"
