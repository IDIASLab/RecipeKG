    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239261/triple-citrus-cupcakes/",
        "name": "Triple Citrus Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1994917.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2014-09-05T23:21:25.000Z",
        "description": "Lemon, lime and orange make these cupcakes triple delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "24 cupcakes",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup butter, softened",
            "1\u2009\u00bc cups sugar",
            "2 tablespoons freshly grated orange peel",
            "1 teaspoon Spice Islands\u00ae Pure Vanilla Extract",
            "4 eggs",
            "\u00bc cup Mazola\u00ae Corn Oil",
            "\u00bc cup milk",
            "2 cups flour",
            "2 teaspoons Argo\u00ae Baking Powder",
            "\u00bd teaspoon salt",
            "3 cups powdered sugar",
            "\u00bc cup butter, softened",
            "1 teaspoon Spice Islands\u00ae Pure Vanilla Extract",
            "1 teaspoon freshly grated lemon peel",
            "2 tablespoons lime juice, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cake: Preheat oven to 325 degrees F. Mix cream cheese, butter, sugar, orange peel and vanilla using an electric mixer until light and fluffy, about 3 to 4 minutes. Add eggs one at a time, beating after each addition. Beat in oil and milk. Combine flour, baking powder and salt. Add to orange mixture beating just until mixed. Portion into paper lined muffin tins; fill cups 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 to 35 minutes, or until a wooden toothpick inserted in centers of cupcake comes out clean. Let cool 10 minutes in pan. Remove warm cakes from pan and finish cooling on wire rack (a minimum of 30 minutes). Frost cupcakes when cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frosting: Mix all ingredients with an electric mixer, until frosting is of desired spreading consistency.\n"
            }
        ],
