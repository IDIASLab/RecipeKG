        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284584/mini-oreo-cheesecakes/",
        "name": "Mini Oreo&reg; Cheesecakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3068746.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-01-03T04:28:27.000Z",
        "description": "Miniature Oreo&reg; cheesecakes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT4H5M",
        "recipeYield": "18 mini cheesecakes",
        "recipeIngredient": [
            "1 (14 ounce) package chocolate sandwich cookies (such as Oreo\u00ae)",
            "2 (8 ounce) packages cream cheese, softened",
            "1 (8 ounce) container sour cream",
            "\u00bd cup white sugar",
            "2 large eggs",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 275 degrees F (135 degrees C). Line 18 muffin cups with paper liners. Place 1 sandwich cookie in each liner. Crumble remaining cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, sour cream, sugar, eggs, and vanilla in a mixing bowl; beat with an electric mixer until smooth. Spoon cheesecake mixture into each liner on top of cookies filling 90% full. Place desired amount of cookie crumbles on top of each mini cheesecake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 22 minutes. Remove from the oven and cool in the pan for 30 minutes. Place in the refrigerator to chill until completely set, about 3 hours.\n"
            }
        ],
