    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276482/holiday-standing-rib-roast/",
        "name": "Holiday Standing Rib Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Holiday Standing Rib Roast"
        },
        "datePublished": "2017-12-01T00:02:56.000Z",
        "description": "Perfect prime rib with a delicious crust.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT3H10M",
        "totalTime": "P0DT4H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (4 pound) beef rib roast",
            "2 tablespoons ground black pepper",
            "2 tablespoons kosher salt",
            "2 tablespoons packed light brown sugar",
            "1 tablespoon dried oregano",
            "1 teaspoon smoked paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove roast from the refrigerator 30 minutes before you want to start cooking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 200 degrees F (95 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pepper, salt, brown sugar, oregano, and paprika together in a small bowl until well combined. Rub seasoning mix all over the roast to completely cover it. Transfer to a roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven for 3 hours (45 minutes per pound), until the internal temperature reads exactly 125 degrees F (52 degrees C). Remove from the oven and cover with aluminum foil. Let rest for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase the oven temperature to 550 degrees F (288 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove foil and put the roast back in the oven until the crust gets crispy, 6 to 8 minutes. Cut and serve immediately.\n"
            }
        ],
