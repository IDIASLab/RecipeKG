        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220204/cannoli-cream-dessert/",
        "name": "Cannoli Cream Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1153568.jpg",
            "width": 1450,
            "height": 1750
        },
        "datePublished": "2016-08-05T13:04:47.000Z",
        "description": "This is my version of an elegant cannoli dessert! It is perfect for a romantic evening!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 cup part-skim ricotta cheese",
            "\u00bc cup confectioners' sugar",
            "\u00bd teaspoon vanilla extract",
            "\u00bd teaspoon almond extract",
            "\u00bd cup graham cracker crumbs",
            "2 tablespoons butter, melted",
            "\u00bc cup canned peach slices, drained and chopped",
            "2 tablespoons grated semi-sweet chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together ricotta cheese, confectioners' sugar, and vanilla and almond extracts in a bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs and melted butter together in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon about 1/4 of the graham cracker mixture into 2 wine glasses.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer about 1/4 the cheese mixture on top, then repeat the layers with the remaining cracker crumbs and ricotta cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with chopped peaches and grated chocolate. Chill until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes"
        ],
