    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244108/french-onion-bruschetta/",
        "name": "French Onion Bruschetta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2296237.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-05-21T21:49:10.000Z",
        "description": "If you like French Onion soup you'll love this easy-to-make French Onion Bruschetta!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 -12 servings",
        "recipeIngredient": [
            "1 (8 ounce) baguette",
            "2 cups julienned onion",
            "2 tablespoons butter",
            "1 tablespoon vegetable oil",
            "\u00bc cup sherry, Marsala, or Madeira wine",
            "\u00bd tablespoon Better Than Bouillon\u00ae Roasted Beef Base",
            "2 tablespoons chopped fresh parsley",
            "\u215b teaspoon freshly ground black pepper",
            "1 cup shredded Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Add the butter and oil to a large saute pan over medium high heat. Add the onions to the pan and saute until lightly browned, about 20 to 25 minutes, stirring occasionally. Add the wine to the pan and reduce for 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the beef base, parsley and black pepper to the pan and stir. Remove the pan from the heat and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425  degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice the baguette into 1/4-inch-thick pieces and place onto a sheet pan. Spread each slice of bread with the onion mixture and top with 1 tablespoon of the Swiss cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the sheet pan directly into the oven and bake for 5 minutes or until the cheese is melted and bubbly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully remove the sheet pan from the oven and place the bruschetta onto a serving platter. Serve immediately.\n"
            }
        ],
