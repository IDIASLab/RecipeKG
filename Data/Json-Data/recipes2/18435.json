    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/83758/turkey-red-grape-salad/",
        "name": "Turkey Red Grape Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2016.jpg",
            "width": 450,
            "height": 338
        },
        "datePublished": "2018-03-21T16:43:27.000Z",
        "description": "A quick and easy noodle salad with delicious sweet red grapes, turkey, and a special kick of celery.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (8 ounce) package mostaccioli pasta",
            "1\u2009\u00bd cups mayonnaise",
            "1 cup sour cream",
            "1 tablespoon minced celery root",
            "2 cups halved seedless red grapes",
            "2 cups diced cooked turkey",
            "1 cup chopped celery",
            "1 head romaine lettuce leaves, torn into bite size pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add the pasta, and cook for 8 to 10 minutes, or until tender. Drain, and run under cold water to cool. Drain again, and transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, stir together the mayonnaise, sour cream and celery root. Stir this into the noodles to coat. Fold in the grapes, turkey and celery. Chill until serving.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, place a bed of romaine lettuce onto serving plates. Top with the turkey pasta salad.\n"
            }
        ],
