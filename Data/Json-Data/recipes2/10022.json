    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258105/kikis-favorite-deep-fried-buttermilk-chicken-tenders/",
        "name": "Kiki's Favorite Deep-Fried Buttermilk Chicken Tenders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4515068.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2017-05-06T04:32:07.000Z",
        "description": "Lightly crispy, very moist deep-fried chicken. Serve with dipping sauces like honey mustard, hot mustard, or sweet and sour sauce.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT2H13M",
        "recipeYield": "16 tenders",
        "recipeIngredient": [
            "1 cup buttermilk",
            "2 tablespoons seasoned salt, divided",
            "16 chicken breast tenders",
            "2 cups panko bread crumbs",
            "freshly ground black pepper to taste",
            "2 cups vegetable oil for frying, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix buttermilk and 1 tablespoon seasoned salt together in a bowl. Stir in chicken tenders. Cover and refrigerate for at least 2 and up to 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 170 degrees F (75 degrees C). Line a baking sheet with paper towels; place a cooling rack on sheet and set in oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining 1 tablespoon seasoned salt, panko bread crumbs, and pepper together in a large, flat bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove tenders from marinade one at a time and shake to remove excess buttermilk. Roll pieces in panko mixture and place on a large platter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 370 degrees F (190 degrees C). Fry chicken pieces 3 or 4 at a time in hot oil until browned and crispy, about 3 minutes. Transfer to cooling rack in oven to keep them warm as you fry.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Chicken"
        ],
