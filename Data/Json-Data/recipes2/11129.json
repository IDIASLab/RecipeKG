    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283869/pickled-eggs-and-beets/",
        "name": "Pickled Eggs and Beets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4537561.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-07-17T00:31:00.000Z",
        "description": "I grow beets to make this. Tasty eaten alone, or as part of a garden salad.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P1DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "8 large eggs",
            "4 large fresh beets",
            "1 cup water",
            "1 cup white sugar",
            "1 cup white vinegar",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground cloves",
            "1 pinch ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove roots and greens from beets. Place beets in a Dutch oven, cover with water, and bring to a boil. Cook for 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place eggs in a large saucepan and cover with water. Heat to boiling over high heat, then reduce to a simmer and cook for 20 minutes. Rinse in cold water and add a handful of ice to chill the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove beets from boiling water and rinse. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water, sugar, and vinegar in a saucepan. Add cinnamon, salt, cloves, and pepper; bring to a boil. Reduce heat and simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel and slice beets and place in a glass or plastic container. Peel eggs and place in the same container. Pour vinegar mixture over top. Cover and chill in the refrigerator for 24 hours, mixing occasionally to ensure eggs and beets remain covered by liquid.\n"
            }
        ],
