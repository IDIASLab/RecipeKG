        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4588967.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2017-10-12T01:02:07.000Z",
        "description": "Easier, but dressed up version of jalapeno poppers. Great for the big game!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "30 jalapeno popper bites",
        "recipeIngredient": [
            "30 mini phyllo tart shells",
            "1 (8 ounce) package cream cheese, softened",
            "1 (4 ounce) can diced green chiles",
            "\u00bd cup shredded Cheddar cheese",
            "2 jalapeno peppers, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Arrange phyllo shells on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and diced green chiles in a bowl and mix until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill each phyllo shell with about 1 tablespoon of the cream cheese mixture and top each lightly with shredded Cheddar cheese. Garnish each filled shell with 1 jalapeno slice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted, 5 to 8 minutes.\n"
            }
        ],
