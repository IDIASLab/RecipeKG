    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213746/aunt-cins-egg-noodle-lasagna/",
        "name": "Aunt Cin's Egg Noodle Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Aunt Cin's Egg Noodle Lasagna"
        },
        "datePublished": "2017-10-28T03:38:44.000Z",
        "description": "A crowd favorite in our family, this casserole has all the goodness of lasagna with much less work! Also, it feeds a crowd without breaking the bank.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H51M",
        "totalTime": "P0DT1H11M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "1 (12 ounce) package extra-wide egg noodles",
            "1 (12 ounce) container cottage cheese",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup sour cream",
            "1 pound ground beef",
            "1 (28 ounce) jar pasta sauce",
            "2 (6 ounce) cans tomato paste",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil. Stir in egg noodles and return to a boil. Cook noodles uncovered, stirring occasionally, until tender yet firm to the bite, about 6 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x13-inch baking pan and layer cooked egg noodles on the bottom.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cottage cheese, cream cheese, and sour cream together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium heat. Add ground beef; cook and stir until nearly browned, about 5 minutes. Stir in pasta sauce and tomato paste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the cottage cheese mixture over the cooked noodles in the baking pan. Top with the ground beef mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Sprinkle Cheddar cheese on top and bake until melted and bubbling, about 10 minutes more.\n"
            }
        ],
