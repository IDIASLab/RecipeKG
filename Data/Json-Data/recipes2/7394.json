    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254390/halibut-cakes/",
        "name": "Halibut Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Halibut Cakes"
        },
        "datePublished": "2016-06-30T00:03:09.000Z",
        "description": "These yummy halibut cakes work as an appetizer or as the main dish at any meal. Full of vegetables, full of flavor, they are very versatile, can be made ahead of time, freeze well, and you can choose to bake or pan-fry the cakes.",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H21M",
        "totalTime": "P0DT0H56M",
        "recipeYield": "30 halibut cakes",
        "recipeIngredient": [
            "cooking spray",
            "20 fluid ounces lemon-lime soda",
            "1 pound halibut, cut into 1-inch chunks",
            "1 tablespoon olive oil",
            "\u00bc onion, diced",
            "\u00bc cup diced zucchini",
            "1 carrot, diced",
            "1 rib celery, diced",
            "2 cloves garlic, minced",
            "1 teaspoon seasoned salt (such as Johnny's\u00ae), or to taste",
            "\u00bd cup seasoned bread crumbs",
            "\u00bc cup mayonnaise",
            "1 egg",
            "1 teaspoon dried parsley",
            "\u00bc teaspoon ground cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with water and lemon-lime soda; bring to a boil. Add halibut chunks, a few at a time, and cook until they float to the top, 30 to 45 seconds. Transfer to a colander to drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium-low heat. Add onion, zucchini, carrot, celery, and garlic; cook and stir until softened, about 5 minutes. Sprinkle seasoned salt on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Flake halibut chunks into a large bowl. Stir in onion mixture, bread crumbs, mayonnaise, egg, parsley, and cayenne pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop halibut mixture onto greased baking sheet with a cookie scoop; flatten into cakes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake halibut cakes in the preheated oven until golden, 10 to 12 minutes.\n"
            }
        ],
