    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258136/bratwurst-pot-pie/",
        "name": "Bratwurst Pot Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4514043.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-05-05T01:07:05.000Z",
        "description": "Bratwurst provides a smoky note to this pot pie with mushrooms and veggies in a creamy sauce and topped with a crumbled pretzel crust.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "3 links Farmland\u00ae Beer Bratwurst",
            "1 pound mushrooms, sliced",
            "1\u2009\u00bd cups diced yellow onion",
            "\u00be cup diced carrots",
            "\u00be cup diced celery",
            "4 garlic cloves, sliced",
            "\u00bd teaspoon red pepper flakes",
            "1 teaspoon fresh thyme leaves",
            "3 tablespoons all-purpose flour",
            "2 cups chicken stock",
            "1 cup heavy cream",
            "1 tablespoon whole-grain mustard",
            "2 cups Cheddar cheese, grated",
            "Salt and pepper, to taste",
            "3 soft pretzels",
            "3 tablespoons melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In heavy-bottomed saute pan, heat vegetable oil over medium-high heat. Add sausages and cook until browned on all sides until just cooked through. Remove, and once cooled, slice into rounds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To same pan, add mushrooms and cook until well browned. Add onions, carrots and celery. Cook until softened, add garlic, and cook for additional 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add red pepper flakes and fresh thyme, followed by flour. Reduce heat and cook for 2 more minutes to cook raw taste from flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chicken stock and heavy cream, and bring to simmer, whisking frequently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add mustard and cheddar cheese, and stir to incorporate. Add reserved sausage and season with salt and pepper. Transfer to individual ovenproof serving dishes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In food processor, or by hand, roughly chop pretzels to make crumbs. Toss with melted butter and top each serving dish with crumb mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until golden brown and warmed through, approximately 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Pot Pie Recipes"
        ],
