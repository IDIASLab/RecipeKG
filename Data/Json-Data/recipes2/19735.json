    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263798/philly-cheese-steak-sliders/",
        "name": "Philly Cheese Steak Sliders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7988359.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-04-13T01:48:30.000Z",
        "description": "Quick, easy, and tasty Philly cheese steak sliders. Great for a busy weeknight or game day.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 sliders",
        "recipeIngredient": [
            "1 (12 ounce) beef skirt steak, thinly sliced",
            "1 (12 count) package Hawaiian bread rolls",
            "\u00bd large green bell pepper, chopped",
            "\u00bd large onion, chopped",
            "\u00bc cup mayonnaise",
            "8 slices pepper Jack cheese",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over medium-high heat. Add steak, bell pepper, and onion. Cook until steak is hot and reaches your desired color, 7 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line an 8x8-inch baking pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice the connected rolls in half widthwise to look like 2 giant slices of bread. Place the bottom half into the bottom of the prepared baking pan. Spread mayonnaise evenly on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 4 slices of pepper Jack cheese over the mayonnaise. Layer the steak mixture and the remaining cheese on top. Cover with the top half of the bread. Spread melted butter on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and bread is browned, about 20 minutes. Slice into individual portions.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Sandwich Recipes",
            "Cheese"
        ],
