        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14359/sweet-pea-and-noodle-salad/",
        "name": "Sweet Pea and Noodle Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F649321.jpg",
