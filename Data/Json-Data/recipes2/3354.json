    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281980/beet-salad-with-mixed-greens/",
        "name": "Beet Salad with Mixed Greens",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Beet Salad with Mixed Greens"
        },
        "datePublished": "2015-07-05T14:32:08.000Z",
        "description": "Mustard greens salad with great flavor.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 bunch red beets, trimmed and washed",
            "2 tablespoons extra-virgin olive oil, divided",
            "sea salt and cracked black pepper to taste",
            "2 cups chopped fresh mustard greens",
            "2 cups chopped beet greens",
            "2 cups chopped romaine lettuce",
            "1 medium grapefruit, segmented",
            "\u00bc cup chopped red onion, or to taste",
            "\u00bc cup extra-virgin olive oil",
            "1 clove garlic, minced",
            "1 teaspoon lemon juice, or to taste",
            "2 tablespoons crumbled goat cheese, or to taste",
            "1 tablespoon chopped fresh dill, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Place a sheet pan on the lower oven rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place beets on a sheet of aluminum foil. Drizzle with 1 tablespoon oil. Fold edges of the foil around the beets to make a sealed pouch.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pouch on the center rack of the oven, so the sheet pan below can catch any drippings. Roast until beets are tender when pierced with a knife, 50 to 60 minutes. Remove beets from the oven. Open the pouch and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rub a paper towel on the cooled beets to remove skin. Cut into small pieces. Drizzle with remaining 1 tablespoon olive oil and season with salt and pepper. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop grapefruit segments. Transfer fruit and any accumulated juices into a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mustard greens, beet greens, romaine lettuce, chopped grapefruit and juice, red onion, 1/4 cup olive oil, garlic, and lemon juice in a large bowl; mix well. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide greens among 6 salad bowls. Top with roasted beets and garnish with goat cheese and dill.\n"
            }
        ],
