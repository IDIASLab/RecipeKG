        "mainEntityOfPage": "https://www.allrecipes.com/recipe/162831/birdseed-treats/",
        "name": "Birdseed Treats",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9334537.jpg",
