    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143106/speedy-bagel-tuna-melt/",
        "name": "Speedy Bagel Tuna Melt",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9402976.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2016-12-13T09:33:59.000Z",
        "description": "A delicious tuna melt with some Dijon mustard to spice things up. Positively kid-friendly!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 (5 ounce) can chunk light tuna, drained",
            "2 tablespoons mayonnaise",
            "1 teaspoon Dijon mustard",
            "1 bagel, split and toasted",
            "\u00bd cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the tuna, mayonnaise, and mustard in a medium bowl, using a fork to mash the ingredients together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top each bagel half with half of the tuna mixture. Sprinkle the tops with shredded Cheddar cheese. Place bagel halves on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the bagel halves in preheated oven until the cheese melts, about 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Sandwich Recipes",
            "Tuna"
        ],
