    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/44388/amaretto-roasted-chicken/",
        "name": "Amaretto Roasted Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F554901.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-03-21T16:27:23.000Z",
        "description": "A wonderful, simple twist on your basic roasted chicken that will please your party guests and make the house smell divine! Use this as your main dish for Christmas parties and guests will beg for the recipe!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (4 pound) whole chicken",
            "\u00bc cup butter",
            "1 large lemon, halved",
            "1 head garlic, halved",
            "\u00bd cup amaretto liqueur"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the chicken in a baking pan, and rub with 2 tablespoons butter. Place remaining butter and 1/2 the lemon in the chicken cavity. Squeeze juice from remaining lemon half over chicken. Distribute squeezed lemon half and both halves of the garlic head in the pan around the chicken. Pour 1/4 cup amaretto over chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken 1 hour and 25 minutes in the preheated oven, or until juices run clear.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from pan, and set aside. Squeeze juice from the lemon half inside chicken into the pan juices. Mash the garlic. Discard lemon rids and garlic pulp. Pour remaining amaretto into the pan, and mix to make a gravy. Serve chicken with the gravy.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Whole Chicken Recipes"
        ],
