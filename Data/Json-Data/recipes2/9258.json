    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235147/el-dorado-shrimp/",
        "name": "El Dorado Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1920104.jpg",
            "width": 1536,
            "height": 2048
        },
        "datePublished": "2017-02-10T08:10:54.000Z",
        "description": "The unique combination of coffee liqueur, bacon, and creamy cheese sets off these shrimp in a melt-in-your-mouth meal.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup coffee-flavored liqueur (such as Kahlua\u00ae)",
            "4 ounces Chihuahua cheese, sliced into strips",
            "10 large shrimp, peeled and deveined, or more to taste",
            "10 slices smoked bacon, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring coffee-flavored liqueur to a simmer in a small saucepan over medium heat. Simmer until liqueur is reduced by half, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack in the middle of the oven and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a slice of Chihuahua cheese on the back of each shrimp and wrap shrimp and cheese with 1 bacon strip; set on a baking sheet. Repeat for remaining shrimp.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil on the middle rack in the preheated broiler until bacon cooks and cheese melts, about 5 minutes. Transfer cooked shrimp to a plate and drizzle with liqueur reduction.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
