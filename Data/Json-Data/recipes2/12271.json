        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255908/pickled-egg-salad-sandwich/",
        "name": "Pickled Egg Salad Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4441482.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2017-10-21T13:00:31.000Z",
        "description": "A zippy change from an ordinary egg salad sandwich.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H15M",
        "recipeYield": "1 sandwich",
        "recipeIngredient": [
            "2 pickled eggs, cut into small pieces",
            "2 tablespoons light mayonnaise",
            "1 tablespoon minced celery",
            "1 garlic dill pickle, minced",
            "2 teaspoons capers",
            "2 teaspoons minced chives",
            "\u00bd teaspoon pickled egg juice",
            "1 dash hot pepper sauce",
            "2 slices whole wheat bread",
            "1 teaspoon butter, or to taste",
            "1 red lettuce leaf",
            "1 tomato, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix pickled eggs, mayonnaise, celery, pickle, capers, chives, pickle juice, and hot pepper sauce together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast bread lightly; spread butter on both slices. Layer lettuce and tomato on 1 slice. Spread egg mixture on the other slice. Press sandwich together and cut in half.\n"
            }
        ],
