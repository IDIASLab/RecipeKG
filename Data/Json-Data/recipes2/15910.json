        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26867/moms-sandwich-spread/",
        "name": "Mom's Sandwich Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1003488.jpg",
