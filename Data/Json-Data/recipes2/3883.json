        "recipeYield": "1 cocktail",
        "recipeIngredient": [
            "3 fluid ounces gin",
            "1 fluid ounce dry vermouth",
            "1 cup ice",
            "3 pimiento-stuffed green olives, drained",
            "1 lemon twist"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine gin and vermouth in a cocktail mixing class. Add ice and stir until chilled. Strain into a chilled cocktail glass.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with three olives on a pick or a lemon twist.\n"
            }
