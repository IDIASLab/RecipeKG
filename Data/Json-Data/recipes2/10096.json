        "recipeIngredient": [
            "24 RITZ Crackers",
            "6 bocconcini (1-inch fresh mozzarella cheese balls), each cut into 4 slices",
            "24 fresh basil leaves",
            "12 cherry tomatoes, halved",
            "6 pitted black olives, quartered",
            "1 green onion, green part only cut into 48 thin strips",
            "2 tablespoons balsamic glaze",
            "\u00bc teaspoon black sesame seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Top each cracker with 1 EACH cheese slice and basil leaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 1 tomato half to each for the lady bug's body, 1 olive piece for the head and 2 green onion strips for the antennae.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush with balsamic glaze; sprinkle with sesame seed.\n"
            }
