    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26925/reuben-rollups/",
        "name": "Reuben Rollups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F724955.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-03-21T16:21:54.000Z",
        "description": "Delicious, not terribly authentic, refreshing wraps - a great way to dress up leftover meat! Try shredded pork or steak! These wraps can be dressed up or down as the situation warrants.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "8 rollups",
        "recipeIngredient": [
            "8 (12 inch) flour tortillas",
            "1 cup Russian-style salad dressing",
            "2 pounds prepared coleslaw",
            "2 cups chopped cooked corned beef",
            "2 cups shredded Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Warm tortillas in microwave to soften; keep covered with a damp towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread one side of each tortilla with 2 tablespoons Russian-style salad dressing. Arrange equal amounts of the cole slaw in the center of each tortilla. Sprinkle equal amounts corned beef over the cole slaw. Top with equal amounts of Swiss cheese. Tightly roll tortillas around the fillings. Chill in the refrigerator until serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Wraps and Rolls"
        ],
