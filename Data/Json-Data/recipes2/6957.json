        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223182/protein-no-bake-drops/",
        "name": "Protein No Bake Drops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6505672.jpg",
