        "cookTime": null,
        "totalTime": "P0DT0H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "1 (4.25 ounce) can chopped black olives, undrained",
            "1 tablespoon mayonnaise",
            "4 ounces Borden\u00ae Pepper Jack cheese, shredded",
            "1 (16 ounce) package corn chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese, chopped black olives, and mayonnaise in a mixing bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in shredded pepper Jack cheese until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve with corn chips.\n"
            }
