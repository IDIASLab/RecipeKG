        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24381/japanese-ginger-salad-dressing/",
        "name": "Japanese Ginger Salad Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9216512.jpg",
