    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/247179/carne-asada-salad/",
        "name": "Carne Asada Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3566076.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-05-05T04:54:02.000Z",
        "description": "Marinate steak in a mixture of spices, lime juice and tomato sauce for flavor you'll love. Pair it up with fresh greens and a lively dressing for a steak salad you'll want to make again and again.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) can tomato sauce",
            "1 lime, juiced",
            "3 cloves garlic, minced",
            "1 teaspoon ground cumin",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon cayenne pepper",
            "1 pound Sutton & Dodge\u2122 choice Angus Beef flank, skirt or sirloin steak",
            "1 (9 ounce) bag ready-to-eat spinach leaves",
            "\u00bd cup shredded carrot",
            "\u00bd cup Market Pantry\u2122 shredded sharp Cheddar cheese",
            "Lime or southwestern vinaigrette (such as Archer Farms\u2122 Chipotle Lime Dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine tomato sauce, lime juice, garlic, cumin, salt, black pepper and cayenne in large plastic food storage bag. Add steak; seal bag. Massage bag to coat steak. Refrigerate 8 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat grill. Grill steak, about 6 minutes per side for medium, until desired doneness, turning halfway through. Let rest then thinly slice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To assemble salad, toss spinach, carrot and cheese in large bowl. Top with slices of grilled steak. Drizzle dressing over salad. Garnish with fresh cilantro leaves and squeeze of fresh lime, if desired.\n"
            }
        ],
