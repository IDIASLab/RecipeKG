    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231366/crispy-baked-chicken/",
        "name": "Crispy Baked Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2408041.jpg",
            "width": 2272,
            "height": 2272
        },
        "datePublished": "2016-06-04T14:04:33.000Z",
        "description": "An easier way to get that crispy chicken.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 slices bread",
            "1 cup grated Parmesan cheese",
            "2 teaspoons dried oregano",
            "1\u2009\u00bd teaspoons dried basil",
            "1\u2009\u00bd teaspoons minced garlic",
            "\u00bd cup grated Parmesan cheese",
            "2 tablespoons vegetable oil",
            "4 skinless, boneless chicken breast halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread into a food processor and blend until crumbled. Transfer bread crumbs to a shallow bowl. Add 1 cup Parmesan cheese, oregano, basil, and garlic; stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press chicken breast halves into bread crumb mixture until coated; arrange on a large baking dish. Drizzle chicken with vegetable oil and sprinkle with remaining 1/2 cup Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken breasts are no longer pink in the center and the juices run clear, about 30 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
