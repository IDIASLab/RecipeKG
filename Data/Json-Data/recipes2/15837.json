        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26438/melanies-chili/",
        "name": "Melanie's Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F58501.jpg",
            "width": 250,
            "height": 250
