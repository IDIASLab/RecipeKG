    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/63772/creamy-orange-chicken/",
        "name": "Creamy Orange Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1111448.jpg",
            "width": 1920,
            "height": 2560
        },
        "datePublished": "2018-03-21T16:33:39.000Z",
        "description": "Chicken breast, mandarin oranges, and chives sauteed with orange liqueur and cream. Great served over rice or pasta. A MUST TRY!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "3 tablespoons olive oil",
            "\u00bd cup flour",
            "2 skinless, boneless chicken breast halves - pounded to 1/2 inch thickness",
            "2 fluid ounces orange flavored liqueur, or to taste",
            "\u00bd cup canned mandarin orange segments, drained",
            "\u00bc cup chopped fresh chives",
            "\u00bd cup heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat. Lightly coat chicken breasts in flour, shaking off excess, and brown in oil on both sides. Stir in cointreau, oranges, and heavy cream. Reduce heat to medium, and simmer until liquid has reduced by half. Remove chicken from pan when not longer pink in center, and allow the sauce to reduce another 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in chives, season to taste with salt and pepper.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
