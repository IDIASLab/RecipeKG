    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220658/black-bean-burgers/",
        "name": "Black Bean Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F848007.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2016-06-04T13:32:24.000Z",
        "description": "This is a great recipe, even for meat eaters!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "8 burgers",
        "recipeIngredient": [
            "2 (15 ounce) cans black beans, drained",
            "\u00bc cup minced jalapeno pepper, or more to taste",
            "1\u2009\u2153 cups plain breadcrumbs",
            "1 red bell pepper, diced",
            "1 small onion, diced",
            "2 eggs, slightly beaten",
            "2 tablespoons ground cumin",
            "2 tablespoons chili powder",
            "6 cloves garlic, minced",
            "\u00bc cup liquid smoke flavoring",
            "\u00bc cup ranch dressing mix",
            "2 tablespoons olive oil",
            "8 slices smoked Gouda cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend black beans and jalapeno pepper in a food processor until the beans are completely pureed; transfer to a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix breadcrumbs, red bell pepper, onion, eggs, cumin, chili powder, garlic, liquid smoke flavoring, and ranch dressing mix into the black bean mixture until evenly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill mixture in freezer until firm enough to shape into patties, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture into 8 portions; shape into patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat; cook burgers in hot oil until cooked through, about 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top burgers with smoked Gouda cheese to serve.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes",
            "Veggie"
        ],
