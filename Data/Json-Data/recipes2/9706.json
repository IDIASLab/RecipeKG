    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244537/payday-bars/",
        "name": "PayDay&reg; Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2547451.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2017-04-05T13:01:32.000Z",
        "description": "Sinfully delicious bars that taste just like a PayDay&reg; candy bar. The salty mixed with the sweet is heavenly! No baking in oven needed!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "35 bars",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) jar dry-roasted peanuts, divided",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (12 ounce) package peanut butter chips",
            "3 tablespoons butter",
            "3 cups miniature marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray the bottom of a 9x13-inch baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 1/2 of the peanuts into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir sweetened condensed milk, peanut butter chips, and butter in a saucepan over medium heat until melted, about 5 minutes. Add marshmallows and stir until smooth. Pour mixture over peanuts and top with remaining peanuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until chilled, 1 to 2 hours. Cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Nut Candy Recipes"
        ],
