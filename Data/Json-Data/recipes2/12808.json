        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261800/thanksgiving-leftovers-ramen/",
        "name": "Thanksgiving Leftovers \"Ramen\"",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5187047.jpg",
