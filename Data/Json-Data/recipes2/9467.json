        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "6 scoops vanilla ice cream, divided",
            "2 tablespoons strawberry syrup, or to taste",
            "4 fresh strawberries, hulled",
            "2 bananas",
            "2 tablespoons chocolate syrup, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place 3 scoops of vanilla ice cream into a blender; blend until smooth. Add strawberry syrup; blend until evenly distributed, about 1 minute. Add strawberries; blend for 1 minute. Pour mixture into a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place remaining 3 scoops of vanilla ice cream into a blender; blend until smooth. Add chocolate syrup; blend until evenly distributed, about 1 minute. Add bananas; blend for 30 seconds. Pour strawberry mixture back into the blender; blend together, about 30 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture evenly among 3 glasses.\n"
            }
        ],
