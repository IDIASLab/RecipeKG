        "recipeIngredient": [
            "2 cups chopped almonds",
            "1\u2009\u00bd cups dried cranberries",
            "\u00bd cup sweetened condensed milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir almonds, cranberries, and sweetened condensed milk together in a bowl. Press nut mixture into the bottom of a 9x6-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and set, about 25 minutes.\n"
            }
