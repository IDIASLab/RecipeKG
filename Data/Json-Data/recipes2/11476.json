    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260181/spicy-egg-minced-lamb-roll/",
        "name": "Spicy Egg & Minced Lamb Roll",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4555869.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-09-08T05:06:51.000Z",
        "description": "Stuff these tortilla rolls with a generous helping of flavourful lamb, egg, and veggies for an appetizer that can easily double as an entr&eacute;e.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons Patak's Mild Curry Paste",
            "4 eggs",
            "7 ounces minced lamb",
            "1 onion, diced",
            "3 tomatoes, diced",
            "2 tablespoons olive oil",
            "1 tablespoon chopped fresh cilantro",
            "4 flour tortillas or naan bread",
            "Salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a pan and fry the diced onions for 2 minutes, then add the minced lamb and fry for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the Patak's Mild Curry Paste and continue to fry for another 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the diced tomatoes and cook whilst stirring continuously on medium heat for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the lamb becomes tender scramble the eggs in a separate bowl, then mix in with the meat. Add the chopped coriander and remove from the heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the tortillas or naan bread, place the lamb mixture along the centre of each and roll them up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Optional: Place on the grill for 1 to 2 minutes on each side to add a bit of crisp to the tortillas or naan bread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve hot, as a starter or as a main dish with a side salad.\n"
            }
        ],
