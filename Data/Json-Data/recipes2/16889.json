    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/52598/zippy-zoo-marinade/",
        "name": "Zippy-Zoo Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F848872.jpg",
            "width": 1397,
            "height": 1316
        },
        "datePublished": "2018-03-21T16:30:09.000Z",
        "description": "A most flavorful and spicy marinade that has a Thai-style influence with a ginger kick. Great on pork and chicken.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 tablespoons soy sauce",
            "2 tablespoons barbecue sauce",
            "2 tablespoons vegetable oil",
            "1 teaspoon white wine",
            "1 teaspoon smooth peanut butter",
            "1 teaspoon corn syrup",
            "\u00bd teaspoon Worcestershire sauce",
            "2 teaspoons black pepper",
            "1 teaspoon minced fresh ginger root",
            "1 teaspoon chili powder",
            "1 teaspoon onion powder",
            "\u00bd teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a shallow dish, combine soy sauce, barbecue sauce, oil, wine, peanut butter, corn syrup, and Worcestershire sauce. Season with black pepper, ginger, chili powder, onion powder, and garlic powder. Mix until smooth. Marinate meat for 2 to 6 hours.\n"
            }
        ],
        "recipeCategory": [
