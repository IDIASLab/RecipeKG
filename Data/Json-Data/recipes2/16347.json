        "mainEntityOfPage": "https://www.allrecipes.com/recipe/36549/honey-garlic-vinaigrette/",
        "name": "Honey Garlic Vinaigrette",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2407947.jpg",
