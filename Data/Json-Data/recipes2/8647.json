        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282873/southern-stir-fry/",
        "name": "Southern Stir-Fry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9356546.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2016-11-24T04:24:06.000Z",
        "description": "Who says stir-fry has to be Asian? Enjoy this zesty blend of rice, corn, black-eyed peas, and spinach.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "1 (15 ounce) can black-eyed peas, rinsed and drained",
            "1 cup finely chopped red onion",
            "1 cup frozen corn",
            "1 cup cooked white rice, cold",
            "1\u2009\u00bd teaspoons chopped fresh thyme leaves",
            "\u00bd teaspoon garlic salt",
            "\u215b teaspoon cayenne pepper",
            "2 cups lightly packed fresh spinach leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a wok or 12-inch skillet over medium-high heat. Add oil and rotate wok or skillet to coat the sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add black-eyed peas, onion, corn, rice, thyme, garlic salt, and cayenne; stir-fry until heated through, 3 to 4 minutes. Add spinach and stir-fry until it begins to wilt, 1 to 2 minutes.\n"
            }
        ],
