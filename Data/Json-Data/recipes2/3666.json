    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238951/cheesy-chicken-toast-sandwich/",
        "name": "Cheesy Chicken Toast Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5367319.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2015-07-25T06:05:00.000Z",
        "description": "Chicken is baked in four-cheese pasta sauce, covered in a four-cheese blend and served in between two pieces of Texas cheese toast.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (24 ounce) jar four-cheese pasta sauce (such as Classico\u00ae)",
            "4 skinless, boneless chicken breast halves",
            "2 cups fiesta-style four-cheese blend",
            "8 slices Texas-style cheese toast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour enough four-cheese pasta sauce into the bottom of a casserole dish to cover in a light layer. Arrange chicken breasts into the casserole dish. Pour remaining sauce over the chicken breasts to cover. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until no longer pink in the center and the juices run clear, 30 to 40 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover chicken breasts with a layer of cheese and continue baking uncovered until the cheese melts, about 10 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange cheese toast onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cheese toast in preheated oven until cheese is melted and the toast is browned around th edges, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sandwich each chicken breast between two slices of the toast with the cheesy sides touching the chicken.\n"
            }
        ],
