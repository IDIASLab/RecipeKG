    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238888/heavenly-chicken-garlic-sandwiches/",
        "name": "Heavenly Chicken Garlic Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1369460.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "2016-06-04T13:53:22.000Z",
        "description": "If you love garlic butter and cheese, this sandwich will hit the spot.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 sandwiches",
        "recipeIngredient": [
            "\u00bd cup butter",
            "5 cloves garlic cloves, peeled and minced",
            "4 hoagie rolls, split",
            "2 pounds cooked chicken breast, cubed",
            "1\u2009\u00bd cups shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter with garlic in a microwave-safe glass or ceramic bowl in 30-second intervals, stirring after each melting, for 1 to 3 minutes (depending on your microwave).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place hoagie roll halves on a baking sheet and brush each with garlic-butter mixture. Divide chicken between each hoagie roll bottom; sprinkle cheese on each hoagie roll top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in the preheated oven until cheese is melted and bubbling, 10 to 20 minutes. Press top and bottom sandwich portions together.\n"
            }
        ],
