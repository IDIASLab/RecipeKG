    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283444/plum-dessert/",
        "name": "Plum Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Plum Dessert"
        },
        "datePublished": "2013-11-10T18:37:47.000Z",
        "description": "This plum dessert is one of my favorites!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "butter-flavored cooking spray",
            "2 pounds plums",
            "1 teaspoon vanilla sugar, or as needed",
            "\u2154 cup vanilla sugar",
            "\u00bd cup unbleached all-purpose flour",
            "1 tablespoon butter-flavored granules",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00be cup fresh orange juice",
            "\u00bc cup liquid egg substitute",
            "2 tablespoons canola oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Liberally coat a 7x11-inch baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Without peeling, slice plums and discard pits. Place plums in a bowl and toss with 1 teaspoon vanilla sugar, depending on their sweetness. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift 2/3 cup vanilla sugar, flour, butter granules, baking powder, cinnamon, and salt together in a large bowl. Stir until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk orange juice and liquid egg together in a 2-cup liquid measure until well mixed. Blend in oil. Pour into the dry ingredients and stir until thoroughly blended but not overmixed. Pour into the prepared baking dish and cover with sugared plums.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cake is golden and plums are soft and juicy, 55 to 60 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and allow to cool for 30 minutes. Slice into 12 pieces and serve.\n"
            }
        ],
