    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223290/rich-and-creamy-dairy-free-strawberry-ice-cream/",
        "name": "Rich and Creamy Dairy Free Strawberry Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rich and Creamy Dairy Free Strawberry Ice Cream"
        },
        "datePublished": "2017-03-16T13:02:19.000Z",
        "description": "Sweet dairy-free frozen dessert with a smooth, rich taste. Even the dairy lovers in our family enjoy this treat!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT9H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00be cup turbinado sugar",
            "1 teaspoon unflavored gelatin",
            "1 (14 ounce) can unsweetened coconut milk",
            "1\u2009\u00bc cups rice milk",
            "4 egg yolks, beaten",
            "2 cups hulled strawberries",
            "2 teaspoons pure vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine turbinado sugar and gelatin together in a large saucepan; add coconut milk and rice milk. Cook and stir milk mixture over medium heat until almost boiling and sugar is dissolved, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop about 1/2 cup hot milk mixture into a bowl with the beaten egg yolks; whisk together. Slowly pour egg mixture back into the saucepan, stirring constantly. Lower heat to medium-low to avoid curdling; cook and stir until mixture is smooth, about 3 minutes; do not boil. Remove from heat and cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place strawberries in a blender and pulse several times to chop before leaving it on to puree.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir strawberry puree and vanilla extract into milk mixture. Refrigerate until very cold, 8 to 48 hours. Pour chilled strawberry mixture into a 4-quart ice cream maker and follow manufacturer's instructions.\n"
            }
        ],
