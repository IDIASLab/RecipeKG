    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234421/smore-eyeballs/",
        "name": "S'more Eyeballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1697041.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "2017-09-30T13:01:12.000Z",
        "description": "A deliciously spooky treat that is easy to make and the kids love! Get s'more eyeballs before they are gone; these get eaten quickly!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "8 whole graham crackers, crushed",
            "6 tablespoons butter, melted",
            "\u00bc cup confectioners' sugar",
            "2 (1.5 ounce) bars milk chocolate candy bars (such as Hershey's\u00ae)",
            "12 large marshmallows, cut in half crosswise",
            "1 dash red food coloring, or as needed",
            "24 semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir graham cracker crumbs, butter, and confectioners' sugar together in a bowl; mix until evenly moistened and press into an ungreased mini-muffin pan to form shallow cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the edges are bubbling, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Break each chocolate bar into 12 equal pieces; place one piece into each graham cracker cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Paint red circles and lines to resemble blood-shot eyes on top of each marshmallow using red food coloring. Place painted marshmallows, painted-sides up, into graham cracker cups. Press 1 chocolate chip into the center of each marshmallow.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until marshmallows are slightly softened, 1 to 2 minutes. Cool in pan for 15 minutes before removing.\n"
            }
        ],
