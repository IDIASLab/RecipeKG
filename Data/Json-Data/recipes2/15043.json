        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24273/party-cucumber-sandwiches/",
        "name": "Party Cucumber Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9418909.jpg",
