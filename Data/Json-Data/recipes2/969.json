        "name": "Creamed Spinach and Mushrooms for One",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1125137.jpg",
            "width": 1632,
