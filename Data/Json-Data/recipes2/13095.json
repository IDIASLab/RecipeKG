    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284111/pork-banh-mi/",
        "name": "Pork Banh Mi",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pork Banh Mi"
        },
        "datePublished": "2018-01-10T08:29:17.000Z",
        "description": "Delicious Vietnamese-style sandwich.",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "4 sandwiches",
        "recipeIngredient": [
            "6 stalks green onions, white and light green parts only, thinly sliced",
            "2 cloves garlic, thinly sliced",
            "\u00bc cup fish sauce",
            "2 tablespoons white sugar",
            "1 tablespoon honey",
            "1 teaspoon ground black pepper",
            "4 (8 ounce) bone-in pork chops",
            "4 medium carrots, thinly peeled ",
            "1\u2009\u00bd medium cucumbers, cut into thin strips",
            "1 medium red onion, thinly sliced",
            "\u00be cup rice vinegar",
            "4 teaspoons chili-garlic sauce, divided",
            "1 tablespoon vegetable oil",
            "2 (6 ounce) French baguettes, cut in half lengthwise",
            "1 tablespoon mayonnaise, or to taste",
            "1 tablespoon hoisin sauce, or to taste",
            "3 medium jalapeno peppers, seeded and thinly sliced",
            "\u00bd cup coarsely chopped fresh cilantro"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine green onions, garlic, fish sauce, sugar, honey, and pepper in a large glass or ceramic bowl. Add pork chops and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator for 2 to 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "With 30 minutes left to marinade, combine carrots, cucumbers, red onion, rice vinegar, and 2 teaspoons chili-garlic sauce in a medium bowl. Stir to combine and let pickle until needed for sandwiches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pork chops from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat. Add pork chops and cook until golden brown, 4 to 5 minutes per side. Transfer to a plate. Remove meat from the bones and thinly slice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mayonnaise, hoisin sauce, and remaining chili-garlic sauce on the top halves of the baguettes. Layer pork, pickled vegetables, cilantro, and jalapenos on the bottom halves. Carefully close the baguettes. Slice each in half to make 4 sandwiches.\n"
            }
        ],
