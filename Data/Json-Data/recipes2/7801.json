        "recipeIngredient": [
            "\u00bc cup butter",
            "\u00bc cup mayonnaise",
            "3 cloves garlic, or more to taste",
            "1 (8 ounce) loaf French bread, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, mayonnaise, and garlic in a food processor; blend until smooth. Spread mixture on both sides of French bread slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast on the preheated grill until browned and crispy, about 5 minutes per side.\n"
            }
