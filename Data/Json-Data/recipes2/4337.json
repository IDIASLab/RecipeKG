    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/212382/cajun-potato-latkes/",
        "name": "Cajun Potato Latkes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F606765.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2015-11-19T07:12:04.000Z",
        "description": "A blend of Cajun flavors with a traditional Chanukah favorite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups peeled, shredded potatoes",
            "\u00bc onion, shredded",
            "\u2153 cup chopped green onion",
            "2 eggs, beaten",
            "2 tablespoons all-purpose flour, or as needed",
            "1\u2009\u00bd teaspoons Cajun seasoning, or to taste",
            "1 cup canola oil, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the potatoes in a cloth, and wring them to remove as much moisture as possible. Mix the potatoes, onion, green onion, eggs, flour, and Cajun seasoning together in a bowl until thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large, heavy skillet over medium heat until it shimmers (oil should be about 1/3 inch deep). Drop about 2 tablespoons of the potato mixture per patty into the hot oil, and flatten the potatoes to make the patties 1/4 to 1/2-inch thick. Brown the patties in the hot oil until golden and crisp on the bottom, about 5 minutes, then flip and cook on the other side. Drain the latkes on paper towels, and serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Pancakes"
        ],
