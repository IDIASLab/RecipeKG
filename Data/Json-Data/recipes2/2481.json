    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241051/chocolate-raspberry-cheesecake/",
        "name": "Chocolate Raspberry Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2099396.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2015-01-24T00:55:18.000Z",
        "description": "This triple layer cheesecake is the perfect ending to any meal! Treat your family or impress your guests with this sweet & creamy dessert.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (3 ounce) packages cream cheese, softened",
            "1 (14 ounce) can sweetened condensed milk",
            "1 egg",
            "3 tablespoons ReaLemon\u00ae lemon juice from concentrate",
            "1 teaspoon vanilla extract",
            "1 cup fresh or frozen raspberries",
            "1 (6 ounce) ready-made graham cracker crumb crust",
            "2 ounces semisweet chocolate",
            "\u00bc cup whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 350 degrees F. With mixer, beat cream cheese until fluffy. Gradually beat in sweetened condensed milk until smooth. Add egg, ReaLemon&reg; and vanilla and mix well.  Press raspberries firmly into bottom of crust, reserving a few whole berries for garnish. Slowly pour cream cheese mixture over fruit.  Bake 30 to 35 minutes or until center is almost set. Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chocolate Glaze: In small saucepan, over low heat, melt semisweet chocolate with whipping cream. Stir until thickened and smooth. Remove from heat. Glaze over cheesecake. Chill. Garnish as desired.\n"
            }
        ],
