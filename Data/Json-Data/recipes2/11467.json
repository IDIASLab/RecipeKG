        "totalTime": "P0DT0H40M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "3 medium ripe bananas",
            "2 cups rolled oats",
            "1 cup chopped dried fruit",
            "\u2153 cup oil",
            "1 teaspoon vanilla extract",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line two cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash bananas in a bowl. Add oats, dried fruit, oil, and vanilla; mix well. Season with salt if desired. Let sit for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of dough 2 inches apart onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 20 minutes.\n"
            }
        ],
