    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230655/hearty-crab-pizza/",
        "name": "Hearty Crab Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1004415.jpg",
            "width": 2731,
            "height": 2736
        },
        "datePublished": "2016-06-04T13:42:25.000Z",
        "description": "A delicious pizza for crab lovers with a special sauce and healthy ingredients! Light butter and fat-free cheese substitute nicely.",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd teaspoon corn meal",
            "1 pre-baked pizza crust",
            "3 tablespoons butter",
            "\u00bd onion, minced",
            "1 tablespoon minced garlic",
            "1 tablespoon dried basil",
            "1 tablespoon dried oregano",
            "1 tablespoon grated Parmesan cheese",
            "\u00bd cup shredded mozzarella cheese",
            "1 (8 ounce) package imitation crabmeat, chopped",
            "1 tomato, chopped",
            "1 cup whole kernel corn",
            "1\u2009\u00bd cups shredded mozzarella cheese",
            "dried Italian seasoning, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease a pizza pan and sprinkle corn meal into pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Poke several holes into pizza crust and place into the prepared pizza pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is heated through, about 5 minutes. Remove crust and turn oven heat down to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium-high heat; cook and stir onion and garlic in hot butter until onion is tender, about 5 minutes. Stir basil, oregano, and Parmesan cheese into onion and garlic. Spread onion mixture evenly over pizza crust; top with 1/2 cup shredded mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat imitation crab in the saucepan over medium-high heat until warmed through, about 2 minutes. Sprinkle imitation crab over pizza, followed by tomato and corn kernels. Top pizza with remaining 1 1/2 cup mozzarella cheese. Season with Italian seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until cheese is melted and crust is golden brown, 10 to 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish"
        ],
