        "mainEntityOfPage": "https://www.allrecipes.com/recipe/55358/pumpkin-pie-for-dieters/",
        "name": "Pumpkin Pie for Dieters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2534052.jpg",
