        "recipeIngredient": [
            "1 (750 milliliter) bottle Shiraz red wine",
            "\u00bd cup white sugar",
            "\u00bd cup vodka",
            "\u00bd cup gin",
            "\u00bd cup rum",
            "\u00bd cup tequila",
            "1 medium orange, halved and sliced",
            "1 medium lemon, juiced",
            "1 liter ginger ale"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix wine with sugar in a bowl to dissolve. Add vodka, gin, rum, and tequila.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut orange and lemon in half, then into slices. Stir into sangria. Refrigerate for 8 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add ginger ale. Serve over a lot of ice.\n"
            }
