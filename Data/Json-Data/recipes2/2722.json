    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241453/chocolate-delite/",
        "name": "Chocolate Delite",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Delite"
        },
        "datePublished": "2015-03-06T23:16:51.000Z",
        "description": "Delicious chocolaty, cool dessert that's great for summer gatherings, but is a hit any time of the year. Only downside: never any leftovers! Any pudding can be used; butterscotch is excellent!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT3H0M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups finely chopped pecans, divided",
            "1 cup self-rising flour",
            "\u00bd cup margarine, at room temperature",
            "1 cup confectioners' sugar",
            "2 cups frozen whipped topping, thawed, divided",
            "8 ounces cream cheese, softened",
            "2 (3.5 ounce) packages instant chocolate pudding mix",
            "3 cups milk",
            "2 tablespoons dark chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour 2 tablespoons pecans into a small bowl; set aside. Stir remaining pecans, flour, and margarine together in a bowl; press pecan mixture into the bottom of a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crust is golden, 15 to 20 minutes. Cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat confectioners' sugar, 1 cup whipped topping, and cream cheese together in a bowl until smooth; spread mixture over cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat milk and pudding mix together in a bowl until thick and stiff; spread pudding over cream cheese mixture. Stir remaining whipped topping and reserved pecans together in a bowl; spread over pudding layer. Sprinkle chocolate chips over the top. Refrigerate until chilled, at least 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes"
        ],
