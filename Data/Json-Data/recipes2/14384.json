    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21067/polish-mistakes/",
        "name": "Polish Mistakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F959294.jpg",
            "width": 958,
            "height": 960
        },
        "datePublished": "2000-04-27T07:02:52.000Z",
        "description": "A great appetizer that is loved by all, especially men. Serve these meaty, cheesy delights on game day! For later use, place the topped bread on a cookie sheet in the freezer for 1 hour. Once frozen, the slices may be stored in freezer bags.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "1 pound sausage",
            "1 teaspoon minced onion",
            "1 pound processed cheese food, cubed",
            "1 teaspoon dried oregano",
            "1\u2009\u00bd tablespoons Worcestershire sauce",
            "1 (1 pound) loaf cocktail rye bread"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ground beef, sausage and onion in a large, deep skillet. Cook over medium high heat until meat is evenly brown and onions are soft. Drain liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in the processed cheese food, oregano and Worcestershire sauce. Stir continuously until the cheese is melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "If the bread is not sliced, cut it into thin slices. Spoon approximately 1 tablespoon of the mixture onto each slice. Place the slices in a single layer on a baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil the slices 10 minutes, or until the cheese is slightly browned and the bread is toasted.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Pork"
        ],
