        "name": "No-Cook Chicken Lettuce Wraps",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3310111.jpg",
            "width": 1994,
            "height": 2667
