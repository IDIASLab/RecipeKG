        "totalTime": "P0DT0H31M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 apple, chopped",
            "1 onion, chopped",
            "2 tablespoons chopped fresh sage",
            "2 tablespoons chopped fresh rosemary",
            "\u00bd teaspoon sea salt",
            "1 pound ground sausage",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine apple, onion, sage, rosemary, and sea salt in a food processor; pulse until well mixed. Add sausage; pulse 3 or 4 times until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll sausage mixture into meatballs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large skillet over medium heat. Cook meatballs in batches, covered, until browned, 3 to 4 minutes per side.\n"
            }
        ],
