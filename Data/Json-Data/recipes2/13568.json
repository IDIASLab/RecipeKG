    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283395/smoked-corned-beef-brisket/",
        "name": "Smoked Corned Beef Brisket",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Smoked Corned Beef Brisket"
        },
        "datePublished": "2018-03-19T23:56:43.000Z",
        "description": "A homemade dry rub of coriander, mustard seed, fennel, and paprika really brings out the flavor in this smoked corned beef brisket.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT5H0M",
        "totalTime": "P2DT5H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 tablespoons ground coriander",
            "1\u2009\u00bd tablespoons fennel seeds",
            "1 tablespoon brown sugar",
            "1 tablespoon ground paprika",
            "2 teaspoons garlic powder",
            "2 teaspoons onion powder",
            "1\u2009\u00bd teaspoons mustard seed",
            "1 (4 pound) corned beef brisket",
            "cherry wood chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine coriander, fennel seeds, brown sugar, paprika, garlic powder, onion powder, and mustard seed in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place brisket in a glass baking dish and rub spice mixture all over. Cover with plastic wrap and place in the refrigerator for 2 to 3 days.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak wood chips in water for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat a smoker to 240 degrees F (116 degrees C) according to manufacturer's instructions. Maintain temperature for 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain wood chips and place on coals. Place a water pan on the smoker and add water to the depth of the fill line. Place brisket on the lower cooking grate and cover with the smoker lid. Smoke for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pull brisket from the smoker and wrap tightly in foil. Return to the smoker until an instant-read thermometer inserted into the thickest part registers at least 190 to 200 degrees F (88 to 93 degrees C), about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove brisket from the smoker and let rest for 40 minutes. Slice corned beef against the grain.\n"
            }
        ],
