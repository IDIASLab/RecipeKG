    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24456/chocolate-rum-balls-ii/",
        "name": "Chocolate Rum Balls II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6349304.jpg",
            "width": 2051,
            "height": 2051
        },
        "datePublished": "2018-03-21T16:18:01.000Z",
        "description": "Soft fudge-like rum balls that the whole family will love!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT0H15M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "3 (1 ounce) squares unsweetened baking chocolate",
            "1 cup almonds",
            "1 (3 ounce) package cream cheese",
            "1 cup confectioners' sugar",
            "1 teaspoon instant coffee granules",
            "\u00bc cup rum",
            "\u00bd cup chocolate sprinkles (jimmies)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In the microwave or in a bowl over simmering water, melt chocolate, stirring occasionally until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Process the almonds in a food processor. Transfer to a medium bowl and stir in the cream cheese, confectioners' sugar, instant coffee and rum. Blend in the melted chocolate. If dough is too soft to form balls, chill for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1 inch balls. Roll the balls in the chocolate sprinkles. Store in a covered container in the refrigerator until serving time.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes",
            "Rum"
        ],
