    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22164/easy-broccoli-casserole-i/",
        "name": "Easy Broccoli Casserole I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1037343.jpg",
            "width": 1334,
            "height": 1295
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Broccoli casserole that is good with cheese and/or rice added!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 (10 ounce) packages frozen chopped broccoli",
            "1 (10.75 ounce) can  condensed cream of celery soup",
            "\u00bc cup chopped onion",
            "\u00bd cup sour cream",
            "2 tablespoons all-purpose flour",
            "1 cup herb-seasoned dry bread stuffing mix",
            "1 tablespoon melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place broccoli in a large saucepan with enough water to cover.  Heat over medium-high heat until water begins to boil; remove from heat and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine broccoli, soup, onion, sour cream and flour.  Mix well and transfer to a 2 quart casserole dish.  In a small bowl combine stuffing mix and butter; spread over broccoli mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 minutes.  Let stand for 5 minutes before cutting.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Broccoli Casserole Recipes"
        ],
