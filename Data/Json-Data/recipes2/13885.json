    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18046/lentil-tomato-soup/",
        "name": "Lentil Tomato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F375315.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-04-12T19:15:18.000Z",
        "description": "This is my kinda 'comfort food' with a slight Mediterranean flair. It freezes well and is one of those dishes that tastes just as good, if not better, the next day! We have this with fresh French bread, and it was heaven!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "4 tablespoons unsalted butter",
            "1 onion, chopped",
            "1 (28 ounce) can whole peeled tomatoes, with liquid",
            "2 (14.5 ounce) cans chicken broth",
            "2 cups dry brown lentils",
            "\u00bd cup red wine",
            "4 cloves garlic, minced",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a large pot over medium high heat. Place the onions in the pot and saute for 10 minutes, or until onions are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the tomatoes in a food processor or blender and puree until smooth. Pour this into the pot with the onion along with the chicken broth and the lentils\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring to a boil, reduce heat to low and let simmer for 20 minutes. Then, stir in the wine, garlic, nutmeg and cloves. Simmer for at least 25 more minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Tomato Soup Recipes"
        ],
