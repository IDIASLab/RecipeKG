        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20861/cheesy-creamed-spinach-casserole/",
        "name": "Cheesy Creamed Spinach Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F211650.jpg",
