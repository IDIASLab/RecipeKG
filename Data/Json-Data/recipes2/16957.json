        "recipeYield": "1 serving",
        "recipeIngredient": [
            "\u00bd cup vegetarian refried beans",
            "1 large whole-wheat tortilla",
            "2 romaine lettuce leaves",
            "\u00bd avocado, peeled and sliced",
            "\u00bc cup fresh pico de gallo"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir refried beans in a saucepan over medium-low heat until hot, 2 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Warm tortilla in a skillet over low heat until softened, 1 to 2 minutes. Transfer tortilla to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer lettuce onto tortilla. Spread warmed refried beans, avocado slices, and pico de gallo on top of lettuce. Roll tortilla around the fillings into a burrito shape.\n"
            }
        ],
