    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24318/chicken-dressing/",
        "name": "Chicken Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2781089.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2018-03-21T16:17:43.000Z",
        "description": "This recipe came from my Grandmother and is a family tradition--a great dish which is eaten instead of stuffing here in the South.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "6 cups chicken broth, divided",
            "4 cups crumbled cornbread",
            "5 biscuits, crumbled",
            "\u00bd cup margarine, melted",
            "\u00bd cup chopped celery",
            "1 small onion, finely chopped",
            "3 eggs, beaten",
            "1 teaspoon sage",
            "1 teaspoon black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, bring the chicken broth to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine the crumbled cornbread and biscuits.  Pour in 3 cups of boiling broth; cover and let stand.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat margarine in a medium skillet over medium heat.  Saute celery and onions until tender.  Stir into cornbread mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To the cornbread mixture add the remaining broth, eggs, sage and black pepper.  Pour into a large iron skillet or roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 hour, or until set and well browned.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Stuffing and Dressing Recipes"
