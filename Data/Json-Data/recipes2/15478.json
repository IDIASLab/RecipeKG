        "totalTime": "P0DT12H40M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "\u00bd cup margarine, softened",
            "\u00bd cup canned pumpkin puree",
            "\u00bc cup confectioners' sugar",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, using an electric mixer, beat margarine for 30 seconds, or until creamy.  Gradually blend in the pumpkin, confectioners' sugar, cinnamon and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and chill overnight.  Let stand at room temperature for 30 minutes for easier spreading.\n"
            }
        ],
