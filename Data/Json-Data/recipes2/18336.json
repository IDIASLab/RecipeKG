        "mainEntityOfPage": "https://www.allrecipes.com/recipe/81952/jacks-beanie-weenies/",
        "name": "Jack's Beanie-Weenies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1003783.jpg",
            "width": 1140,
