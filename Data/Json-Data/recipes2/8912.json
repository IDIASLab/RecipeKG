    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280762/garlic-pulled-pork/",
        "name": "Garlic Pulled Pork",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4458591.jpg",
            "width": 3264,
            "height": 1836
        },
        "datePublished": "2017-01-05T12:14:01.000Z",
        "description": "This garlic pulled pork is my spin on kalua pork...enjoy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT16H5M",
        "totalTime": "P0DT16H25M",
        "recipeYield": "14 servings",
        "recipeIngredient": [
            "6 pounds bone-in pork butt",
            "2 tablespoons minced garlic",
            "1 tablespoon liquid mesquite smoke flavoring (such as Wright's\u00ae)",
            "1 tablespoon sea salt",
            "\u00bd teaspoon ground black pepper",
            "1 medium head green cabbage",
            "1 medium sweet onion, sliced (such as Vidalia\u00ae)",
            "1 cup water",
            "1 teaspoon chicken soup base (such as Better than Bouillon\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove bone from pork butt. Combine garlic, liquid smoke, salt, and black pepper in a bowl and rub into pork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the bottom of a slow cooker with green cabbage leaves. Place 1 layer of onion slices on top. Add pork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water and chicken base in a microwave-safe measuring cup and microwave on high until dissolved, about 45 seconds. Pour over pork. Break up remaining onion slices and place on top. Cover with another layer of cabbage leaves. Cover the lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low for 8 hours. Turn pork over and, if needed, remove any excess liquid to leave the top 2 inches uncovered. Cover and continue to cook until fork-tender, about 8 hours more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer pork to a large bowl and shred with 2 forks, adding juices from the slow cooker to moisten. Serve alone or over the cabbage mixture.\n"
            }
        ],
