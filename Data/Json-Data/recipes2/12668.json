        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "5 carrots, shredded",
            "2 red bell peppers, diced",
            "2 green bell peppers, diced",
            "1 onion, diced",
            "1 shallot, chopped",
            "3 cloves garlic, crushed",
            "4 cups vegetable broth",
            "1 cup quinoa",
            "1 pinch dried basil, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large saucepan over medium-high heat. Add carrots, red bell peppers, green bell peppers, onion, and shallot. Cook and stir until softened, about 5 minutes. Add garlic; cook and stir until fragrant, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable broth and quinoa into the saucepan. Cover and simmer until quinoa is soft, about 20 minutes. Sprinkle dried basil on top.\n"
            }
        ],
