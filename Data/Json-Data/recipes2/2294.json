    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283374/slow-cooker-apple-crisp-with-cake-mix/",
        "name": "Slow Cooker Apple Crisp with Cake Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Slow Cooker Apple Crisp with Cake Mix"
        },
        "datePublished": "2014-12-18T19:44:18.000Z",
        "description": "We absolutely love this stuff. We love the way the sauce caramelizes around the edges. I am going to make a bunch of copies of this recipe so everyone who tries it will quit asking for it.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u2154 cup white sugar, or to taste",
            "2 tablespoons cornstarch",
            "4 teaspoons apple pie spice, divided",
            "12 cups peeled, cored, and chopped apples",
            "\u00bc cup lemon juice",
            "1 (18.25 ounce) package yellow cake mix",
            "1 cup packed brown sugar",
            "\u00bd cup unsalted butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sugar, cornstarch, and 2 teaspoons apple pie spice in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place apples in a large slow cooker. Stir in spice mixture and toss with lemon juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cake mix, brown sugar, and remaining apple pie spice together in a medium bowl. Stir in melted butter until thoroughly blended; mixture will be crumbly. Sprinkle over apples in the cooker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook until apples are tender, on Low for 4 hours or on High for 2 hours. Partially uncover the slow cooker and continue to cook until the topping has hardened, about 1 hour more.\n"
            }
        ],
