        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256617/serrano-hot-sauce/",
        "name": "Serrano Hot Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7206925.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-04-12T13:02:45.000Z",
        "description": "This hot but flavorful hot sauce was made due to the large harvest of serrano peppers from my garden.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P1DT0H21M",
        "recipeYield": "1 1/2 cups",
        "recipeIngredient": [
            "14 serrano chile peppers, stemmed",
            "6 cloves garlic, smashed",
            "1\u2009\u00bd tablespoons kosher salt",
            "1\u2009\u00bd cups distilled white vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place serrano peppers, garlic, and salt in a food processor; pulse until finely chopped. With the motor running, slowly pour the vinegar through the feed tube; process until sauce is smooth, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sauce into a saucepan; bring to a boil and cook for 1 minute. Pour sauce into a clean jar and store in the refrigerator for at least 1 day.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain sauce through a fine-mesh strainer; store in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments"
        ],
