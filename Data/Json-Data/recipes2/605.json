        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283977/easy-teriyaki-beef-kabobs/",
        "name": "Easy Teriyaki Beef Kabobs",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Teriyaki Beef Kabobs"
        },
        "datePublished": "2014-01-30T20:56:44.000Z",
        "description": "Super easy teriyaki beef kabobs, just serve with rice or salad for an instant meal.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT4H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound beef top round steak",
            "\u00bc cup light brown sugar",
            "\u00bc cup soy sauce",
            "2 tablespoons lemon juice",
            "1 tablespoon oil",
            "\u00bc teaspoon ground ginger",
            "1 clove garlic, minced",
            "1 small pineapple, cut into 1-inch chunks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut steak into 1-inch chunks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, soy sauce, lemon juice, oil, garlic, and ginger in a large glass or ceramic bowl. Add steak and pineapple chunks and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator for at least 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler. Line a sheet pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Thread steak and pineapple chunks onto metal skewers and place on the prepared sheet pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until cooked through, turning halfway, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "BBQ & Grilling",
            "Skewers and Kabobs"
        ],
