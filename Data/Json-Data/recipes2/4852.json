        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1 cup pecans",
            "1 cup coconut milk",
            "1 cup almond milk",
            "1 tablespoon maple syrup",
            "1 teaspoon ground cinnamon",
            "1 pinch ground nutmeg, or more to taste",
            "1 pinch pumpkin pie spice, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak pecans in water in a glass jar or bowl until softened, 8 hours or overnight; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend pecans, coconut milk, almond milk, maple syrup, cinnamon, nutmeg, and pumpkin pie spice together in a blender until smooth.\n"
            }
        ],
