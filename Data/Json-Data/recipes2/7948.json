        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234328/refrigerator-raspberry-oatmeal/",
        "name": "Refrigerator Raspberry Oatmeal",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1127072.jpg",
