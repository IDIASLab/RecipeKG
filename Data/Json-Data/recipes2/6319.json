        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229828/honey-ginger-green-beans/",
        "name": "Honey Ginger Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2575268.jpg",
