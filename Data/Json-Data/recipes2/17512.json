        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place less than a drop of Monin Raspberry Syrup in the bottom of a Martini glass.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add Stoli Razberi, Triple Sec, white cranberry juice, and a squeeze of lime into an ice-filled shaker and shake vigorously. Strain and pour mixture into martini glass. Garnish with 3 raspberries.\n"
            }
