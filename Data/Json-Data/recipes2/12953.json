    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284891/fudgy-chocolate-crackle-cookies/",
        "name": "Fudgy Chocolate Crackle Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4702763.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-12-16T01:18:37.000Z",
        "description": "Fudgy chocolate cookies with white chocolate chips, coated in sugar. Dough can be prepared the day before and chilled overnight.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "25 cookies",
        "recipeIngredient": [
            "1 cup white sugar",
            "2 large eggs",
            "3 tablespoons vegetable oil",
            "1 teaspoon vanilla extract",
            "\u00bc cup margarine, melted",
            "1 cup all-purpose flour",
            "9 tablespoons unsweetened cocoa powder",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 cup white chocolate chips",
            "3 tablespoons white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix 1 cup sugar, eggs, oil, and vanilla together in a large bowl until well blended. Stir in melted margarine. Stir in flour, cocoa, baking powder, and salt. Mix until well blended. Stir in white chocolate chips. Mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and chill until dough is firm enough to shape into balls, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1 1/2-inch balls. Roll balls in remaining 3 tablespoons sugar. Place 2 inches apart on ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crackled on top and slightly firm to the touch, about 20 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
