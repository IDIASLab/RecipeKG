    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240212/steak-taco/",
        "name": "Steak Taco",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8025413.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2014-11-10T21:13:24.000Z",
        "description": "Slices of grilled flank steak are served on tortillas with avocado, pickled onions, lettuce, and cabbage.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (3/4 pound) flank steak, trimmed",
            "\u00bd teaspoon freshly ground black pepper",
            "Cooking spray",
            "1 tablespoon fresh lime juice",
            "\u00bd tablespoon olive oil",
            "2 cloves garlic, minced",
            "\u00bc teaspoon kosher salt",
            "1 cup chopped romaine lettuce",
            "1 cup chopped red cabbage",
            "1 Avocado from Mexico, sliced",
            "1 (16 ounce) jar pickled red onions",
            "Grilled scallions for garnish",
            "6 (6 inch) corn tortillas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle steak with pepper, pressing to adhere.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat grill pan over medium-high heat. Coat pan with cooking spray. Add steak to pan; grill 6 minutes on each side or until desired degree of doneness. Place steak on a cutting board; let stand 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut steak diagonally across grain into thin slices. Mix lime juice, olive oil and garlic and drizzle over steak; sprinkle with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat tortillas on grill pan coated with cooking spray 20 seconds on each side or until lightly charred.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat scallions on grill until lightly charred.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide steak among tortillas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with romaine, red cabbage, avocado and pickled red onions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with grilled scallion.\n"
            }
        ],
