    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218691/grammys-clam-fritters/",
        "name": "Grammy's Clam Fritters",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F704371.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2015-11-19T07:13:03.000Z",
        "description": "Minced clams are the savory star in these golden brown clam fritters.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "24 fritters",
        "recipeIngredient": [
            "oil for frying",
            "1 cup all-purpose flour",
            "2 teaspoons baking powder",
            "1 egg",
            "1 cup minced clams",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C). Whisk the flour and baking powder together in a bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg in a separate bowl until smooth. Stir in the minced clams and black pepper to taste. Stir in the flour to form a batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop the fritters into the hot oil by the heaping tablespoonful. Fry until golden on both sides, about 1 minute per side. Drain on a paper towel-lined plate to serve.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Clams"
        ],
