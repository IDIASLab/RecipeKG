    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25516/cheese-pennies/",
        "name": "Cheese Pennies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3499067.jpg",
            "width": 2322,
            "height": 4128
        },
        "datePublished": "2018-03-21T16:20:00.000Z",
        "description": "These flavorful, round cheese bites are a zesty baked snack that you'll want to pop down one after another.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "30 pennies",
        "recipeIngredient": [
            "1 cup butter, melted",
            "2 cups all-purpose flour",
            "1 (16 ounce) package shredded Cheddar cheese",
            "1 (1 ounce) envelope dry onion soup mix"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, knead together butter, all-purpose flour, Cheddar cheese and dry onion soup mix. Divide into two equal portions. Form each portion into a log shape. Wrap with wax paper. Chill at least 1 hour in the refrigerator, until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the log shapes into slices approximately 1/4 inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange slices in a single layer on a large baking sheet. Bake 20 to 25 minutes in the preheated oven, or until lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Cracker Recipes"
        ],
