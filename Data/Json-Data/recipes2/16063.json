        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32209/hot-shredded-chicken-sandwiches/",
        "name": "Hot Shredded Chicken Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F243238.jpg",
