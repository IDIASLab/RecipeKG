        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/53834/greek-lentil-salad/",
        "name": "Greek Lentil Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4535655.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2018-03-21T16:30:39.000Z",
        "description": "This is a delicious salad or side dish that may be kept for up to 3 days in the refrigerator, if covered. Just moisten with additional salad dressing when ready to use.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 (15 ounce) cans brown lentils",
            "1 cup crumbled feta cheese",
            "\u00bd (16 ounce) bottle Greek, or other vinaigrette salad dressing",
            "1 English cucumber, peeled and diced",
            "1 beefsteak tomato, diced",
            "1 large red onion, diced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Strain lentils and place in a large bowl. Pour in 1/2 of the salad dressing, and stir to coat. Place in the refrigerator while preparing other vegetables. Add feta cheese, cucumber, tomato, and red onion. Cover, and refrigerate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in remaining dressing when ready to serve. May be kept, covered, in the refrigerator for up to 3 days.\n"
            }
        ],
