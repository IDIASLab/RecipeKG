        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "3 tablespoons sour cream",
            "1 (.7 ounce) package dry Italian-style salad dressing mix",
            "1 (1 pound) loaf cocktail rye bread",
            "2 cucumbers, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, blend cream cheese, sour cream and dry Italian-style salad dressing mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange cocktail rye bread slices on a medium serving dish. Spread with the cream cheese mixture. Top each with a cucumber slice.\n"
            }
