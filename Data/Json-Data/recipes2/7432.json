    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/214697/flag-cake/",
        "name": "Flag Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3725885.jpg",
            "width": 3105,
            "height": 2327
        },
        "datePublished": "2016-06-30T13:06:22.000Z",
        "description": "I liked the looks of the popular Wave Your Flag Cake, but I didn't like all the sugar and chemicals.  This is my all-natural version.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT3H30M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "1 (10.75 ounce) loaf prepared pound cake, sliced",
            "4 cups mixed berry fruit juice",
            "4 teaspoons agar-agar powder",
            "1 cup sliced fresh strawberries",
            "1 cup fresh blueberries",
            "1 pint heavy cream",
            "\u00bc cup honey",
            "\u2153 cup fresh blueberries",
            "3 cups fresh strawberries, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cover the bottom of a 9x13-inch pan with cake slices. Add the fruit juice to a saucepan and sprinkle the agar-agar over the juice; let soak for 15 minutes. Bring the juice to simmer and stir until the agar-agar has dissolved. Remove the pan from the heat and let sit until the juice starts to thicken. Stir 1 cup sliced strawberries and 1 cup blueberries into the slightly thickened juice. Pour the fruit and juice mixture over the pound cake slices and let sit until the juice has set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, whip the heavy cream until foamy and peaks are just about to form. Beat in the honey until stiff peaks form. Spread the whipped cream over the cake. Arrange the blueberries on the whipped cream for the stars of the flag. Place the strawberry halves on the whipped cream to resemble the stripes of a flag.\n"
            }
        ],
