    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285098/curry-cauliflower-casserole/",
        "name": "Curry Cauliflower Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Curry Cauliflower Casserole"
        },
        "datePublished": "2013-12-28T23:16:30.000Z",
        "description": "There was once a great curry cauliflower casserole recipe similar to this on this site, but when I could no longer find it, I attempted to recreate from memory.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "1 tablespoon olive oil, or more to taste",
            "1 bunch green onions, chopped",
            "1 medium onion, chopped",
            "1 head cauliflower, cut into florets",
            "1 (16 ounce) package shredded Cheddar cheese",
            "1 (10.5 ounce) can condensed cream of celery soup",
            "\u00bd cup mayonnaise",
            "\u00bd cup sour cream",
            "3 tablespoons curry powder, or more to taste",
            "4 cloves garlic, chopped",
            "1 teaspoon dried basil",
            "1 teaspoon dried oregano",
            "1 teaspoon salt",
            "1 teaspoon black pepper",
            "\u00bc teaspoon celery seed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat. Add green onions and regular onion; saute until translucent, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cooked onions to a large bowl and mix with cauliflower florets, Cheddar cheese, condensed soup, mayonnaise, sour cream, curry powder, garlic, basil, oregano, salt, pepper, and celery seed. Transfer to a 9x13-inch casserole and cover with a lid or foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly, about 45 minutes. Let cool 10 minutes before serving.\n"
            }
        ],
