        "name": "Grilled Ham and Cheese With a Twist",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1090412.jpg",
            "width": 1550,
