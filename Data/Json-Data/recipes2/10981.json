        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259031/low-carb-zucchini-chips/",
        "name": "Low-Carb Zucchini Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4541668.jpg",
