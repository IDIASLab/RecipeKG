    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282169/chicken-pot-pie-casserole-with-noodles/",
        "name": "Chicken Pot Pie Casserole with Noodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chicken Pot Pie Casserole with Noodles"
        },
        "datePublished": "2015-11-05T03:15:24.000Z",
        "description": "This chicken pot pie casserole combines a Pennsylvania Dutch noodle pot pie with a more traditional crust-style pot pie for a hearty and comforting meal.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "1 (8 ounce) package no-cook lasagna noodles",
            "3 (10.5 ounce) cans condensed cream of chicken soup",
            "3 cups milk",
            "2 tablespoons dried parsley flakes",
            "2 tablespoons dried minced onion",
            "1 tablespoon onion powder",
            "1 tablespoon chicken soup base",
            "2 teaspoons garlic powder",
            "2 teaspoons ground black pepper",
            "1 teaspoon salt",
            "2 (2 pound) whole rotisserie chickens, meat shredded and bones discarded",
            "1 (8 ounce) package shredded Cheddar cheese",
            "\u00bd (8 ounce) package shredded Italian cheese blend",
            "1 (10 ounce) can refrigerated buttermilk biscuits, separated and cut into quarters"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Break lasagna noodles into pieces. Grease a large oven-proof dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine condensed soup, milk, parsley, minced onion, onion powder, soup base, garlic powder, pepper, and salt in a large mixing bowl; stir until smooth. Stir in broken lasagna noodles until combined. Gently fold in shredded chicken until incorporated. Carefully pour chicken mixture into the prepared dish, smoothing out the top evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until noodles are almost tender, 25 to 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and sprinkle with 1/2 of the Cheddar cheese and all of the Italian cheese. Layer the biscuit chunks in a single layer over top, and sprinkle with remaining Cheddar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake until biscuit dough is thoroughly cooked, 15 to 20 minutes. Tent with foil if the edges start to get too brown. Remove from the oven and let cool for 5 to 10 minutes before serving.\n"
            }
        ],
