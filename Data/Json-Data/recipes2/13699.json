        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16837/grilled-vidalia-onions/",
        "name": "Grilled Vidalia Onions",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F666855.jpg",
