    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14179/chicken-seashell-salad/",
        "name": "Chicken Seashell Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3561158.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "1999-07-29T02:34:18.000Z",
        "description": "Refreshing chicken salad made with pineapple, apples, mandarin oranges, and seashell macaroni.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups seashell pasta",
            "\u00bc cup cashews",
            "3 boneless, chicken breast halves - cooked, skinned ",
            "3 ounces pineapple tidbits, juice reserved",
            "2 small Gravenstein apples, peeled, cored and diced",
            "\u00be cup chopped celery",
            "1 (3 ounce) can mandarin oranges, drained",
            "1 cup mayonnaise",
            "\u00bd teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook macaroni in a large pan of boiling water until al dente. Drain, and rinse.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place nuts in a dry skillet.  Toast over medium heat, turning frequently, until nuts are fragrant and lightly browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine pasta, chicken, pineapple, apples, celery, mandarin oranges, and toasted nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix together mayonnaise, reserved pineapple juice, and almond flavoring. Stir into pasta mixture, and mix well. Chill.\n"
            }
        ],
