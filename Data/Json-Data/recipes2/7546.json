        "name": "Watermelon, Feta and Cashew Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3778621.jpg",
            "width": 1257,
            "height": 960
