    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/108006/tomato-bread-salad-with-basil-and-capers/",
        "name": "Tomato-Bread Salad with Basil and Capers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1132306.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2018-03-21T16:54:05.000Z",
        "description": "Capers lend this panzanella-style tomato salad the perfect salty bite!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "5 cups 1/2-inch cubed French or Italian bread",
            "1\u2009\u00bd pounds tomatoes, stemmed and cut into medium dice",
            "2 medium garlic cloves, minced",
            "\u00bd large red onion, cut into small dice",
            "\u00bd cup torn fresh basil leaves",
            "\u00bc cup drained capers",
            "\u00bc cup olive oil",
            "2 tablespoons red wine vinegar",
            "1 pinch ground black pepper, to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Adjust oven rack to center position and heat oven to 250 degrees.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread cubes on a cookie sheet with a rim; bake until bread is dried out, about 30 minutes, then set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile dice and salt the tomatoes in a medium bowl; stir in the garlic and let stand until juicy, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare remaining ingredients; add to tomatoes and toss. Add bread cubes; toss again. Adjust seasonings, including pepper to taste. Let stand 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Tomato Salad Recipes"
        ],
