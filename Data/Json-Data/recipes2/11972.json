    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/178992/shepherds-pie/",
        "name": "Shepherd's Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5773537.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2008-10-14T22:23:11.000Z",
        "description": "Here is a shepherd's pie, made easy, that your family will love.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 medium onion, chopped",
            "1 (10.75 ounce) can  Campbell's\u00ae Condensed Cream of Mushroom Soup (Regular or 98% Fat Free)",
            "1 tablespoon ketchup",
            "\u215b teaspoon ground black pepper",
            "1 cup frozen peas and carrots",
            "1 cup milk",
            "2 tablespoons butter",
            "1\u2009\u2153 cups instant mashed potato flakes or buds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook beef and onion in 10-inch skillet over medium-high heat until well browned, stirring to break up meat. Pour off fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir soup, ketchup, black pepper and peas and carrots into skillet. Spoon beef mixture in 9-inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat milk and butter in 2-quart saucepan over medium-high heat to a boil. Remove from heat. Slowly stir in potatoes. Spoon potatoes over beef mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F for 15 minutes or until potatoes are lightly browned.\n"
            }
        ],
        "recipeCategory": [
