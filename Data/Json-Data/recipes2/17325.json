    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/63428/pickle-steak/",
        "name": "Pickle Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2422461.jpg",
            "width": 1704,
            "height": 1705
        },
        "datePublished": "2018-03-21T16:33:30.000Z",
        "description": "Savory, creamy steak with the flavor of pickles and onions.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "12 dill pickle spears",
            "1 large onion, sliced and halved",
            "3 (1/4 inch thick) round steaks, cut into 4 pieces and pounded thin",
            "toothpicks",
            "\u00bc cup vegetable oil",
            "\u00bd cup all-purpose flour",
            "2 eggs, beaten",
            "3 (10.75 ounce) cans  condensed cream of mushroom soup, undiluted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a medium baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place one pickle spear and an equal amount of halved onion slices on one side of each piece of steak. Roll, and secure with toothpicks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil in skillet over medium heat. Place the flour and eggs in two separate bowls. Dip rolled steak first in the eggs, then in the flour to coat. Fry coated steaks in the skillet until browned on all sides. Transfer to the baking dish. Pour cream of mushroom soup into the skillet, and scrape up browned bits. Pour evenly over the steaks in the baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 45 minutes in the preheated oven, to a minimum temperature of 160 degrees F (72 degrees F).\n"
            }
        ],
