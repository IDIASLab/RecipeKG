    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283639/chocolate-chip-birthday-cookie-cake/",
        "name": "Chocolate Chip Birthday Cookie Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Chip Birthday Cookie Cake"
        },
        "datePublished": "2014-05-14T01:21:55.000Z",
        "description": "This is a very easy, yummy cake to make for birthdays, any parties, and just for fun. Enjoy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 cake",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "1 cup firmly packed light brown sugar",
            "\u00bd cup unsalted butter, at room temperature",
            "1 large egg",
            "1 teaspoon vanilla extract",
            "1 cup semisweet chocolate chips",
            "\u00bd cup chilled heavy cream",
            "1 tablespoon white sugar",
            "\u00bc teaspoon vanilla extract",
            "11 large fresh strawberries, hulled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease and flour a round cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, baking powder, and salt together in a small bowl with a wooden spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar and butter together with an electric mixer for about 3 minutes. Add egg and vanilla; mix until blended. Add dry ingredients until completely mixed. Stir in chocolate chips with a wooden spoon. Scrape the dough into the prepared cake pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out mostly clean with just a few crumbs on it, about 30 minutes. Remove from the oven and place on a cooling rack for 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip the chilled cream with an electric mixer on low speed until thick. Mix in sugar and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread whipped cream on top of the cake and add strawberries.\n"
            }
        ],
