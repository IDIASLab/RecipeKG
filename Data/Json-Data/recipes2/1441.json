    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238717/veggie-stroganoff/",
        "name": "Veggie Stroganoff",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7298411.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2014-07-22T16:38:09.000Z",
        "description": "The elements of classic Stroganoff - beefy flavour, mushrooms and creamy sauce, served on a bed of noodles -  appear in this veggie recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H18M",
        "totalTime": "P0DT0H28M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 tablespoons Spectrum Naturals\u00ae Canola Oil, divided",
            "1 medium onion, chopped",
            "1 large clove garlic, minced",
            "1 (8 ounce) package sliced button or cremini mushrooms",
            "1 (170 gram) package Yves Veggie Cuisine\u00ae Beef Veggie Tenders",
            "2 tablespoons all-purpose flour",
            "1 cup Imagine\u00ae Organic Vegetable Broth",
            "1 teaspoon Dijon mustard",
            "\u00bd teaspoon ground thyme",
            "\u00bd teaspoon seasoning salt",
            "\u00bd cup The Greek Gods\u2122 Traditional Plain Yogurt or sour cream",
            "1 tablespoon chopped fresh parsley",
            "1 (8 ounce) package Egg or egg-free noodles, cooked according to package directions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large non-stick skillet, heat 1 tablespoon (15 mL) of the oil over medium heat. Add onion; cook for 6 minutes, stirring occasionally, until soft. Add garlic and mushrooms; cook for 2 minutes, stirring often.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the Beef Veggie Tenders; cook for 1 minute. Remove everything to a large plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add remaining 2 tablespoons (25 mL) oil to the skillet. Stir in flour; cook for about 2 minutes, stirring, until lightly browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in broth, Dijon, thyme and seasoning salt. Cook for 2 minutes, until thickened. Return mixture to skillet. Cover and simmer over low heat for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in yogurt; warm through. Sprinkle top with parsley. Serve over hot cooked noodles.\n"
            }
        ],
