    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/142654/chicken-with-figs-in-a-port-wine-sauce/",
        "name": "Chicken with Figs in a Port Wine Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1040095.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2015-07-25T06:02:09.000Z",
        "description": "This is a delicious dish, with Middle Eastern flavors. Easy to make and a firm favorite when entertaining.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "\u00bd cup flour for dredging",
            "5 skinless, boneless chicken breast halves",
            "2 tablespoons butter",
            "\u00bc cup port wine",
            "\u00bd cup dry white wine",
            "\u00bd cup chicken stock",
            "6 dried figs (stems removed)",
            "2 tablespoons heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the flour in a shallow dish. Dredge the chicken breasts in the flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a large skillet over medium-high heat.  Cook the chicken in the preheated skillet until golden brown, about 2 minutes per side. Pour the port, white wine, and chicken stock over the chicken, and scatter the figs around the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the liquid to a boil and reduce heat to low, simmer until chicken is fully cooked, about 15 minutes. Remove the chicken from the skillet and cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the sauce in the skillet to a boil over medium-high heat. Reduce the heat to medium-low and simmer for 5 minutes. Whisk in the cream and pour over the chicken to serve.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
