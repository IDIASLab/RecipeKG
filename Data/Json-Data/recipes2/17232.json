        "mainEntityOfPage": "https://www.allrecipes.com/recipe/59789/sex-with-an-alligator/",
        "name": "Sex with an Alligator",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1905448.jpg",
