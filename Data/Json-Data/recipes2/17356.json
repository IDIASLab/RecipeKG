    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/64628/chocolate-covered-blueberries/",
        "name": "Chocolate Covered Blueberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2342390.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "2018-03-21T16:34:11.000Z",
        "description": "Chocolate Covered Blueberries! These are unbelievably wonderful. Eat your fruit and have some chocolate too!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "36 candies",
        "recipeIngredient": [
            "1 cup semi-sweet chocolate chips",
            "1 tablespoon shortening",
            "2 cups fresh blueberries, rinsed and dried"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in a glass bowl in the microwave, or in a metal bowl set over a pan of simmering water. Stir frequently until melted and smooth. Remove from the heat, and stir in the shortening until melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet  with waxed paper. Add blueberries to the chocolate, and stir gently to coat. Spoon small clumps of blueberries onto the waxed paper. Refrigerate until firm, about 10 minutes. Store in a cool place in an airtight container. These will last about 2 days.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Blueberry Dessert Recipes"
        ],
