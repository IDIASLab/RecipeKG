        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242066/easy-baked-zucchini-chips/",
        "name": "Easy Baked Zucchini Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2230395.jpg",
