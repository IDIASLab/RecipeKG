    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256553/skillet-breakfast-pizza/",
        "name": "Skillet Breakfast Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4579704.jpg",
            "width": 1400,
            "height": 970
        },
        "datePublished": "2016-12-17T06:58:21.000Z",
        "description": "Skillet breakfast pizza is hot, crispy pizza dough that is oozing with veggies and cheesy bacon yumminess. Plus, it's low in calories!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H39M",
        "totalTime": "P0DT0H54M",
        "recipeYield": "1 12-inch pizza",
        "recipeIngredient": [
            "5 slices low-sodium bacon",
            "\u00bd teaspoon olive oil",
            "\u00bd onion, thinly sliced",
            "1 (16 ounce) package refrigerated pizza dough (such as Trader Joe's\u00ae)",
            "1 cup part-skim ricotta cheese",
            "1 teaspoon garlic powder",
            "1 cup shredded part-skim mozzarella cheese",
            "\u00bc teaspoon freshly ground black pepper",
            "cooking spray",
            "1 (6 ounce) package fresh baby spinach"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain on paper towels; crumble into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium-high heat. Add onion; cook and stir until translucent and starting to brown, 7 to 8 minutes. Remove from heat; transfer to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out pizza dough into a 12-inch circle and press into the bottom and 1 inch up the sides of a well seasoned 10-inch cast iron skillet. Fold the edges of the dough under and crimp like a pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ricotta cheese and garlic powder together in a bowl; spread over the bottom of the crust. Scatter bacon, onion, mozzarella cheese, and black pepper on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cast iron skillet over high heat until mozzarella cheese starts to melt, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer skillet to the preheated oven; bake pizza until crust is lightly browned, about 18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat and coat with cooking spray. Add spinach; cook, stirring frequently, until wilted, about 1 minute. Transfer to paper towels to drain excess moisture. Place spinach over pizza; slice into 8 wedges.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Meat and Seafood",
            "Bacon"
        ],
