    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255870/honey-sweetened-granola/",
        "name": "Honey-Sweetened Granola",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4076382.jpg",
            "width": 4968,
            "height": 4974
        },
        "datePublished": "2016-10-21T04:17:33.000Z",
        "description": "This is a simple yet tasty granola that is sweetened with honey and no sugar. The house smells wonderful and sweet when it is baking in the oven.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "cooking spray",
            "4 cups old-fashioned oats (such as Quaker Oats\u00ae)",
            "\u00bc cup all-purpose flour",
            "\u00bc cup shredded coconut",
            "\u00bc cup chopped pecans",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd cup honey",
            "\u00bc cup coconut oil",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease a rimmed baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, flour, shredded coconut, pecans, and cinnamon together in a large bowl. Stir in honey, coconut oil, and vanilla extract until evenly incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread oat mixture on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly golden, about 10 minutes. Stir gently. Continue baking, stirring every 5 minutes, until golden brown, 10 to 15 minutes. Let cool until crispy, about 15 minutes. Store in airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Cereals",
            "Granola Recipes"
        ],
