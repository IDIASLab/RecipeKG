        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71634/cielos-spring-salmon/",
        "name": "Cielo's Spring Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3539570.jpg",
