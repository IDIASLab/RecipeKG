    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229078/cauliflower-bean-casserole/",
        "name": "Cauliflower Bean Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2853022.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2015-06-17T06:01:44.000Z",
        "description": "A simple casserole dish that will please again and again! Beware, this is for those not conscious of the fat content!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 head cauliflower, cut into florets",
            "1 (9 ounce) package frozen cut green beans",
            "1 (12 ounce) package bacon, cut in half crosswise",
            "1 small onion, chopped",
            "\u00bd cup butter, softened",
            "1 cup mayonnaise",
            "1 cup grated mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil; cook cauliflower until tender yet firm to the bite, about 10 minutes. Add green beans; cook until tender, about 5 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels, reserving bacon grease in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir onion in the skillet with bacon grease over medium heat until tender and lightly browned, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cauliflower, green beans, bacon, and onion together in a casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir butter and mayonnaise together in a bowl; pour over cauliflower mixture. Stir to coat; top with mozzarella cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and bubbling, 20 minutes.\n"
            }
        ],
