        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237617/banana-mango-berry-fluff/",
        "name": "Banana-Mango-Berry Fluff",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2268676.jpg",
            "width": 3456,
            "height": 3456
