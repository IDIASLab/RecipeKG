    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285613/tennessee-meatloaf-and-mashed-potato-casserole/",
        "name": "Tennessee Meatloaf and Mashed Potato Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Tennessee Meatloaf and Mashed Potato Casserole"
        },
        "datePublished": "2014-01-05T20:53:39.000Z",
        "description": "This meatloaf and mashed potato casserole is how we do true comfort food in the South. You don't need any sides, it's already added in! Hope you enjoy as much as we do!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9-inch casserole",
        "recipeIngredient": [
            "cooking spray",
            "1\u2009\u00bd pounds ground beef",
            "\u00bd cup chopped onion",
            "2 slices white bread, torn into small pieces",
            "\u2153 cup ketchup",
            "1 large egg",
            "1 teaspoon Worcestershire sauce",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "3 cups leftover mashed potatoes",
            "\u00bd cup shredded Cheddar cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Coat a 9-inch square baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground beef, onion, bread pieces, ketchup, egg, Worcestershire, salt, and pepper together in a large bowl until well combined. Spread in the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until browned, 35 to 40 minutes. Remove from the oven and drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mashed potatoes evenly over the beef. Sprinkle Cheddar cheese over top. Return to the oven and bake until beef is fully cooked and potatoes are hot, about 30 more minutes.\n"
            }
        ],
