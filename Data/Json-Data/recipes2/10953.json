        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258953/veggie-sweet-potato-burgers/",
        "name": "Veggie Sweet Potato Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4554081.jpg",
            "width": 3036,
            "height": 4048
