    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215690/apple-pie-granola/",
        "name": "Apple Pie Granola",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Pie Granola"
        },
        "datePublished": "2017-07-29T00:59:06.000Z",
        "description": "My family loves granola, so I've been experimenting with my recipe to create new variations. This tasty granola gets its sweet flavor from dried apples and apple juice concentrate. Store in an airtight container.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 cups",
        "recipeIngredient": [
            "cooking spray",
            "2\u2009\u00bd cups old-fashioned oats",
            "\u00bd cup oat bran",
            "\u00bd cup sliced almonds",
            "cooking spray",
            "\u2153 cup honey",
            "\u2153 cup apple juice concentrate",
            "\u2153 cup vegetable oil",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "1 cup crispy rice cereal",
            "\u00bd cup toasted wheat germ",
            "\u00bd cup chopped dried apples",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Line a rimmed 10x15-inch baking sheet with aluminum foil. Grease with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats, oat bran, and almonds in a large bowl. Pour onto the prepared baking sheet; coat with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring once, until toasted and fragrant, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine honey, apple juice concentrate, and oil in a small saucepan. Cook and stir over low heat until heated through, about 5 minutes. Remove from heat; stir in cinnamon and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir rice cereal, wheat germ, and apples into the oat mixture. Drizzle honey mixture on top. Stir to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 15 to 20 minutes. Stir in raisins. Place baking sheet on a wire rack to cool. Break granola into pieces.\n"
            }
        ],
