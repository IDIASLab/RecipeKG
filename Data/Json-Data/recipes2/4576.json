    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235338/easy-thanksgiving-cookies/",
        "name": "Easy Thanksgiving Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1071472.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2015-11-19T07:16:07.000Z",
        "description": "Easy Thanksgiving cookies. Everyone will think you're a gourmet. These are so simple, yet so delicious. Your friends will think that you spent all day with dozens of ingredients. Enjoy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "18 cookies",
        "recipeIngredient": [
            "1 (18.25 ounce) package orange cake mix (such as Duncan Hines\u00ae Orange Supreme)",
            "1 cup sweetened dried cranberries, or more to taste",
            "\u00bd cup vegetable oil",
            "2 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cake mix, dried cranberries, vegetable oil, and eggs together in a bowl until dough is smooth. Drop 1 to 2 tablespoons batter per cookie onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cooked through, 6 to 10 minutes. Cool cookies on baking sheet for about 5 minutes; transfer to a sheet of waxed paper to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
