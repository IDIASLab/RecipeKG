    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283268/steak-tips-in-the-oven/",
        "name": "Steak Tips in the Oven",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Steak Tips in the Oven"
        },
        "datePublished": "2016-05-27T14:56:52.000Z",
        "description": "Steak tips in the oven with a very flavorful steak tip marinade that is best suited for a hearty winter dish but can be enjoyed anytime. Feel free to experiment with the ingredients because I don't think you can really go wrong.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT6H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00be cup soy sauce",
            "\u00bc cup olive oil",
            "\u00bc cup balsamic vinegar",
            "2 tablespoons brown mustard",
            "2 tablespoons maple syrup",
            "2 tablespoons molasses",
            "1 tablespoon sesame oil",
            "1 teaspoon ground cinnamon",
            "3 whole cloves",
            "salt and ground black pepper to taste",
            "1 cup beef broth, or as needed",
            "2 pounds sirloin steak tips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk soy sauce, olive oil, balsamic vinegar, mustard, maple syrup, molasses, sesame oil, cinnamon, cloves, salt, and pepper together in a large glass or ceramic bowl. Add steak tips and toss to evenly coat. Cover the bowl with plastic wrap and marinate in the refrigerator for 6 to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove steak tips from the marinade and shake off excess. Discard the remaining marinade. Put steak tips into a baking dish and fill the bottom of the dish with beef broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until steak tips are beginning to firm and are hot and slightly pink in the center, about 25 minutes, flipping halfway. An instant-read thermometer inserted into the center should read 140 degrees F (60 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks"
        ],
