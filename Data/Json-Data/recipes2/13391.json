    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262722/southwestern-avocado-bell-pepper-quiche-cups/",
        "name": "Southwestern Avocado-Bell Pepper Quiche Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5130547.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2018-02-17T06:10:14.000Z",
        "description": "Shake up the morning routine with this new twist of avocado-bell pepper quiche cups!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 quiches",
        "recipeIngredient": [
            "4 large eggs",
            "\u00bd cup Greek yogurt",
            "2 egg whites",
            "4 tablespoons taco seasoning, reduced sodium",
            "1 cup shredded Cheddar cheese",
            "1 large avocado - pitted, peeled, and diced",
            "1 cup shredded Cheddar cheese",
            "\u00bd onion, chopped",
            "1 (4 ounce) can diced green chiles, drained",
            "1 medium red bell pepper - halved, seeds and ribs removed",
            "1 medium green bell pepper - halved, seeds and ribs removed",
            "1 medium yellow bell pepper - halved, seeds and ribs removed",
            "1 medium orange bell pepper - halved, seeds and ribs removed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs, yogurt, egg whites, and taco seasoning in a large bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently stir avocado, Cheddar cheese, onion, and green chiles into the bowl with the egg mixture. Do not over-stir; keep avocado in diced form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place red, green, yellow, and orange bell pepper halves on the baking sheet. Spoon egg mixture into the bell pepper halves, filling them to just below the edges.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until egg mixture is just set, 35 to 40 minutes. Remove from oven and let cool slightly, 5 to 10 minutes.\n"
            }
        ],
