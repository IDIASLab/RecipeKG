    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239945/nutty-caramel-apple-pops/",
        "name": "Nutty Caramel Apple Pops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1577737.jpg",
            "width": 1000,
            "height": 1000
        },
        "datePublished": "2014-10-24T18:09:41.000Z",
        "description": "This delicious, diminutive delectable has all the satisfaction of the ubiquitous caramel apple in one bite without the messy chin! These fun and festive mini caramel apples are perfect for your Halloween party!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "16 pops",
        "recipeIngredient": [
            "2 large Granny Smith apples, peeled",
            "4-inch lollipop sticks",
            "1 (11 ounce) package individually wrapped caramels, unwrapped",
            "2 tablespoons milk",
            "3 tablespoons chopped nuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop 6 to 8 balls out of each apple using a melon baller. Dry each apple ball with a paper towel and arrange on the prepared baking sheet. Poke each ball with a lollipop stick and refrigerate while making the caramel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place caramels in a saucepan over medium-low heat; stir in milk until sauce is smooth, 3 to 4 minutes. Remove saucepan from heat and cool caramel slightly, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip apple pop in the cooled caramel, allowing excess caramel to drip back into saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour nuts into a shallow bowl and roll caramel apple pops in the nuts until coated; arrange, stick-side up, on the waxed paper.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Caramel Apple Recipes"
        ],
