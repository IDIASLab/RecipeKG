    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238687/tri-tip-marinade/",
        "name": "Tri-Tip Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1978246.jpg",
            "width": 2288,
            "height": 2288
        },
        "datePublished": "2016-06-04T14:14:16.000Z",
        "description": "This is my family's tri-tip steak marinade. This marinade can be used on most cuts of beef, but we would always use it for tri-tip.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P1DT0H45M",
        "recipeYield": "25 servings",
        "recipeIngredient": [
            "3 (2 liter) bottles lemon-lime soda (such as 7 Up\u00ae)",
            "20 fluid ounces soy sauce",
            "2 white onions, sliced",
            "2 lemons, sliced",
            "1 clove garlic, minced",
            "1 (3 pound) beef tri-tip roast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine lemon-lime soda, soy sauce, onions, lemons, and garlic in a large pot; add roast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate roast in the refrigerator at least 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill to 250 degrees F (120 degrees C) to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the roast from the marinade and shake to remove excess moisture. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook roast on the grill until desired doneness and an internal temperature of at least 145 degrees F (65 degrees C) are reached, at least 30 minutes.\n"
            }
        ],
