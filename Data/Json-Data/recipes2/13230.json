        "name": "Delicious Instant Pot&reg; Coconut Milk Yogurt",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5009168.jpg",
            "width": 2048,
