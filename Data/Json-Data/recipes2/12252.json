    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260891/jerezs-caramel-cookies/",
        "name": "Jerez's Caramel Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Jerez's Caramel Cookies"
        },
        "datePublished": "2017-10-20T08:29:12.000Z",
        "description": "A very sweet cookie with a Latin American flavor of rum and milk caramel. The special texture of these cookies will certainly please your taste bus. The milk caramel jam is a Hispanic sweet that we call dulce de leche.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "45 cookies",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "\u00bc cup confectioners' sugar",
            "1 cup butter",
            "\u00be cup milk caramel jam (dulce de leche)",
            "2 egg yolks",
            "3 tablespoons rum"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour and confectioners' sugar together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix butter and milk caramel jam together in a separate bowl using a fork. Beat until smooth with an electric mixer. Beat in egg yolks and rum. Mix in sifted mixture gradually until well blended. Refrigerate dough, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out dough to 1/4-inch thickness on a lightly floured surface. Cut into shapes using cookie cutters. Arrange cookies on the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until firm to the touch, about 15 minutes. Cool on the baking sheets for 5 minutes. Remove to wire racks to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes",
            "Rum"
        ],
