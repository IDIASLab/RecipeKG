    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221859/smoked-salmon-terrine/",
        "name": "Smoked Salmon Terrine",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Smoked Salmon Terrine"
        },
        "datePublished": "2017-09-09T02:07:57.000Z",
        "description": "Delish appetizer great for entertaining and for before holiday feasts. This one won't last long on the plate! Top with capers and serve with rice crackers.",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H45M",
        "recipeYield": "1 loaf pan",
        "recipeIngredient": [
            "1 (10 ounce) bag fresh spinach, or to taste",
            "3 (8 ounce) packages cream cheese, at room temperature  ",
            "1 bunch green onions, chopped",
            "1 (8 ounce) can water chestnuts, drained and chopped",
            "1 tablespoon chopped fresh dill",
            "1 (16 ounce) package smoked salmon, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place spinach in a pot with water to cover. Boil until wilted; drain well. Chop into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese in a bowl; cut using 2 butter knives until paste-like in consistency. Mix in the spinach, green onions, water chestnuts, and dill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a loaf pan with of plastic wrap. Arrange a layer of smoked salmon in the pan; spread a layer of the cream cheese mixture over the salmon. Repeat layering with the remaining salmon and cream cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill in the refrigerator, at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Invert the pan onto a serving dish; peel off the plastic wrap.\n"
            }
        ],
