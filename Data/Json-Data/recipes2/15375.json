        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24901/blue-cheese-dip-ii/",
        "name": "Blue Cheese Dip II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F282676.jpg",
