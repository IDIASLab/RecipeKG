    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24455/amazingly-yummy-surprise-inside-cookies/",
        "name": "Amazingly Yummy Surprise Inside Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1485475.jpg",
            "width": 1536,
            "height": 2048
        },
        "datePublished": "2018-03-21T16:18:01.000Z",
        "description": "These amazingly yummy cookies have a chocolate/caramel surprise in the middle!  So easy to make.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2 cups butter, softened",
            "2\u2009\u00be cups all-purpose flour",
            "1 (18.25 ounce) package yellow cake mix",
            "1 (13 ounce) package chocolate covered caramel candies (e.g. Rolo TM)",
            "\u00bc cup confectioners' sugar for rolling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat the butter and flour together until soft and smooth. Stir in the cake mix to form a soft dough. Roll dough into 1 inch balls. Press a candy into the center of each ball, covering completely. Place 2 inches apart onto an ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 minutes in the preheated oven. Allow cookies to cool on baking sheet for 5 minutes before removing to a wire rack to cool completely. Roll cookies in confectioners sugar while still warm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cake Mix Cookie Recipes"
        ],
