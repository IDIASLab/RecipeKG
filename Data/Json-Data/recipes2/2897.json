    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232972/broccoli-with-buttery-crumbs/",
        "name": "Broccoli with Buttery Crumbs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1072842.jpg",
            "width": 3888,
            "height": 2592
        },
        "datePublished": "2015-04-01T06:00:26.000Z",
        "description": "This quick and easy dish even gets picky kids to try broccoli!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (14 ounce) bags frozen broccoli florets",
            "1 cup melted butter, divided",
            "1 teaspoon seasoned salt",
            "2 cups croutons"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a pot of salted water to a boil. Cook broccoli in boiling water until heated through, about 5 minutes; drain. Transfer broccoli to a casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour about half the butter over the broccoli; season with seasoned salt. Top broccoli with croutons. Drizzle remaining butter over croutons.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until hot and croutons are lightly browned, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Broccoli"
        ],
