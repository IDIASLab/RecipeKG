    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238009/julianas-surprise-sandwich-cookies/",
        "name": "Juliana's Surprise Sandwich Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Juliana's Surprise Sandwich Cookies"
        },
        "datePublished": "2014-12-09T07:00:15.000Z",
        "description": "These are dipped sandwich sugar cookies!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup roasted pecan bits",
            "1 (18 ounce) package refrigerated sugar cookie dough",
            "1 (11 ounce) package semisweet chocolate chips",
            "\u00bd cup apricot preserves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pulse pecans in a food processor until finely ground.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookie dough onto a work surface and knead pecans into dough. Roll dough back into a log, similar to the original shape. Slice dough into 1/4-inch-thick slices and arrange on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are golden around the edges, about 10 minutes. Cool cookies on a wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in the top of a double boiler over simmering water, stirring frequently and scraping down the sides with a rubber spatula to avoid scorching.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread apricot preserves on the top of half of the cooled cookies. Place a second cookie over the apricot layer creating a sandwich. Dip half of each sandwich cookie into the melted chocolate and place on a sheet of waxed paper to dry.\n"
            }
        ],
