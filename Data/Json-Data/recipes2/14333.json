        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20794/broccoli-tofu-pitas/",
        "name": "Broccoli Tofu Pitas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4460707.jpg",
