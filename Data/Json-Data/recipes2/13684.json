    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16778/yummy-garlic-croutons/",
        "name": "Yummy Garlic Croutons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F212297.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-02-25T21:11:59.000Z",
        "description": "I eat Caesar salads just for the these croutons!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H13M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (1 pound) loaf day old French bread",
            "2 tablespoons garlic powder",
            "2 tablespoons dried basil",
            "\u00bd cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut bread into small cubes and place onto a shallow baking pan or cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle oil over the bread to lightly cover and sprinkle with garlic powder and basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (200 degrees C) for 5 to 10 minutes or until lightly brown and toasted.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Crouton Recipes"
        ],
