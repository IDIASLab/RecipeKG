        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246889/super-easy-peanut-butter-cookies/",
        "name": "Super-Easy Peanut Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3534210.jpg",
            "width": 3425,
            "height": 1848
        },
        "datePublished": "2016-04-08T20:15:49.000Z",
        "description": "When we need cookies in a snap, this is our best go-to recipe - easy, fast and super delicious!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1 cup KRAFT Smooth Peanut Butter",
            "\u00bd cup white sugar",
            "1 egg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 325 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix all ingredients with large spoon until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll into 24 balls; place, 4 inches apart, on baking sheets. Flatten with fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 20 min. or until lightly browned. (Do not overbake.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool 5 min. on baking sheets; transfer to wire racks. Cool completely.\n"
            }
        ],
