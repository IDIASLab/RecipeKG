    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260686/sausage-gravy-breakfast-pizza/",
        "name": "Sausage Gravy Breakfast Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7755424.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-10-04T06:27:53.000Z",
        "description": "Soft crust, with bacon, sausage, egg, hash browns, and gravy. Oh so good!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 14-inch round pizza and half 9x13-inch pizza",
        "recipeIngredient": [
            "3 cups frozen hash brown potatoes, thawed",
            "1 (16 ounce) package pork sausage (such as Purnell's \"Old Folks\"\u00ae)",
            "1 (12 ounce) package reduced-sodium bacon, or to taste",
            "10 eggs",
            "2 (8 ounce) packages refrigerated crescent rolls",
            "1 (1.5 ounce) package sausage-flavored gravy mix (such as McCormick\u00ae)",
            "2 cups shredded pizza cheese blend (such as Kroger\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Add hash browns, sausage, and bacon. Fry until lightly browned, 5 to 7 minutes. Transfer to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour eggs into the hot skillet. Cook and stir until scrambled, 5 to 7 minutes. Transfer to the plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a 14-inch round pizza pan with 1 1/2 cans dough. Arrange remaining dough over a 9x13-inch baking pan; pinch the pieces together to prevent separation when baking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour gravy mix into a small saucepan; whisk in 1 cup of cold water gradually. Cook over medium heat, stirring frequently, until gravy comes to a boil. Reduce heat and simmer for 1 minute. Allow to stand until thickened, about 4 minutes. Spread gravy over the dough; spread the hash brown mixture and cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crusts are golden, 35 to 45 minutes.\n"
            }
        ],
