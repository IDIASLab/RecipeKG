        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242355/kids-favorite-cauliflower/",
        "name": "Kids' Favorite Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2379568.jpg",
