    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254506/cedar-smoked-baby-back-ribs/",
        "name": "Cedar-Smoked Baby Back Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4564019.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2016-07-13T01:03:26.000Z",
        "description": "A slow cook and a great taste. Use with whatever ribs you might like.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT3H2M",
        "totalTime": "P0DT12H12M",
        "recipeYield": "1 rack",
        "recipeIngredient": [
            "2 tablespoons garlic powder",
            "2 tablespoons onion powder",
            "1 tablespoon fine sea salt",
            "1 tablespoon ground black pepper",
            "1 rack baby back pork ribs",
            "1 (6x18-inch) cedar plank",
            "\u00be cup barbeque sauce, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix garlic powder, onion powder, salt, and black pepper together in a small bowl. Spread over baby back ribs. Cover with plastic wrap and refrigerate, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place plank on a rimmed baking sheet and pour in enough water to cover. Let soak before grilling, about 1 hour. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill to about 500 degrees F (260 degrees C). Place plank on the grate and heat until slightly blackened, about 5 minutes. Flip plank and place ribs on top. Lower heat and cover the grill. Cook ribs, sprinkling some water on top every 30 minutes to create steam inside the grill, until tender, about 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Uncover grill and increase heat to high. Brush barbeque sauce over ribs. Cook until ribs are dark brown, about 2 minutes.\n"
            }
        ],
