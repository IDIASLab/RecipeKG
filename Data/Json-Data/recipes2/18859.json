        "mainEntityOfPage": "https://www.allrecipes.com/recipe/89194/zesty-dip-for-chips/",
        "name": "Zesty Dip for Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2206047.jpg",
