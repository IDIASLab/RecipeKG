    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213649/asparagus-with-junk/",
        "name": "Asparagus with Junk",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F398933.jpg",
            "width": 3008,
            "height": 2000
        },
        "datePublished": "2014-02-21T23:32:21.000Z",
        "description": "This dish turns asparagus into a delicious casserole! My whole family loves it! You can adjust this recipe to make a lot or a little very easily.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 bunch fresh asparagus, trimmed",
            "1 cup mayonnaise",
            "\u00bd cup bread crumbs",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan. Pour enough water into the saucepan to reach just below the bottom of the steamer. Bring the water to a boil. Add the asparagus, cover, and steam until just tender, 2 to 6 minutes depending on thickness.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the asparagus into a baking dish. Spread mayonnaise over the asparagus; sprinkle with bread crumbs and Parmesan cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the crumbs and cheese are golden brown, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Asparagus",
            "Baked"
        ],
