        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231602/shamrock-shakes/",
        "name": "Shamrock Shakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1002844.jpg",
            "width": 1066,
            "height": 1070
        },
        "datePublished": "2016-03-14T22:41:53.000Z",
        "description": "This has become a St. Patrick's Day tradition in our house. The kids love it, and so do I.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "2 large shakes",
        "recipeIngredient": [
            "2 cups vanilla ice cream",
            "1\u2009\u00bc cups milk",
            "\u00bc teaspoon mint extract, or more to taste",
            "9 drops green food coloring ",
            "2 tablespoons chocolate syrup, or to taste",
            "2 tablespoons whipped cream, or to taste",
            "\u00bd teaspoon green decorator sugar, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend ice cream, milk, mint extract, and food coloring together in a blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle chocolate syrup around inside walls of 2 tall glasses; pour shake into glass. Top with whipped cream and green decorator sugar.\n"
            }
        ],
