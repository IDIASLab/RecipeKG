        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 head lettuce, chopped",
            "1 (10 ounce) can corn, drained",
            "1 (15 ounce) can black beans, drained and rinsed",
            "1 large tomato, chopped",
            "2 green onions, chopped",
            "\u00bc cup chopped cilantro",
            "\u00bc cup diced green bell pepper",
            "1 avocado, sliced",
            "\u00bd cup shredded Cheddar cheese",
            "\u00bd cup crushed tortilla chips",
            "\u00bd cup salsa",
            "\u00bc cup ranch-style salad dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Layer lettuce, corn, black beans, tomato, green onions, cilantro, green bell pepper, avocado, Cheddar cheese, and tortilla chips, in this order, in a serving bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir ranch-style dressing and salsa together in a bowl; pour over salad.\n"
            }
