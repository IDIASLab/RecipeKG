    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229859/fried-mortadella-sandwich/",
        "name": "Fried Mortadella Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fried Mortadella Sandwich"
        },
        "datePublished": "2015-11-19T07:14:37.000Z",
        "description": "This isn't your ordinary fried bologna sandwich. Fry up authentic Italian mortadella with onions and peppers, layer it in a hoagie roll, and top with cheese, fried potatoes, and fried eggs.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 sandwiches",
        "recipeIngredient": [
            "2 tablespoons olive oil, divided",
            "4 cups diced potatoes",
            "1 onion, sliced",
            "1 cup diced pimiento peppers",
            "1 pound sliced mortadella sausage",
            "4 Italian-style hoagie buns, split lengthwise",
            "\u00bc cup spicy brown mustard",
            "8 slices Swiss cheese",
            "8 slices Asiago cheese",
            "1 tablespoon olive oil",
            "8 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 500 degrees F (260 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons olive oil in a skillet over medium heat; cook and stir potatoes until browned and crisp, about 15 minutes. Set aside on a paper towel-lined plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place onion and pimiento peppers in the hot skillet; cook and stir until onion has softened, about 5 minutes. Add mortadella and cook until warmed through and slightly browned, 3 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread each bun with 1 tablespoon spicy mustard. Layer with 2 slices Swiss cheese. Top with 1/4 of the mortadella and onion mixture and sprinkle with 1/4 of the fried potatoes. Top with 2 slices Asiago cheese. Repeat to make 4 sandwiches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sandwiches onto a baking sheet and bake in the preheated oven until cheese is melted and buns are toasted, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon olive oil in a nonstick skillet over medium heat; cook eggs, 2 to 4 at a time if necessary, until whites are set but yolks are still soft, about 2 minutes per side. Place two eggs in each sandwich.\n"
            }
        ],
