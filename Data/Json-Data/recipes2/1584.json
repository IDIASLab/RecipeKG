    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282080/lemon-oatmeal-bars/",
        "name": "Lemon Oatmeal Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1335681.jpg",
            "width": 4128,
            "height": 2322
        },
        "datePublished": "2014-08-11T02:46:25.000Z",
        "description": "The best of both worlds! If you like lemon bars and oatmeal cookies, this will satisfy both cravings at once. The hint of almond along with these other flavors, builds a light and fresh flavor that no other lemon bar has.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT3H15M",
        "recipeYield": "16 bars",
        "recipeIngredient": [
            "cooking spray",
            "1 cup brown sugar",
            "\u00bd cup unsalted butter, softened",
            "1\u2009\u2153 cups all-purpose flour",
            "1 cup rolled oats",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "1 (14 ounce) can condensed milk",
            "\u00bc cup lemon juice",
            "1 teaspoon lemon extract",
            "1 teaspoon almond extract",
            "1 teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray an 8-inch square baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar and butter in a large bowl until combined. Mix flour, oats, baking powder, and salt together in a separate bowl. Add to butter mixture and mix until combined. Press half of the oat mixture in the bottom of the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While crust is baking, mix condensed milk, lemon juice, lemon extract, and almond extract in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour lemon mixture over the baked crust and spread evenly. Top with remaining oat mixture and gently pat into lemon mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake until filling has set and top crust is golden brown, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and place on a wire rack to cool completely, about 30 minutes. Place in the refrigerator until chilled, about 2 hours. Cut into 16 bars.\n"
            }
        ],
