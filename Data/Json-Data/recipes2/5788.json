    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143016/ez-pizza-for-kids/",
        "name": "EZ Pizza for Kids",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F122946.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2016-06-01T13:01:44.000Z",
        "description": "This is a simple, quick, kid-favorite pizza meal that the kids can even help prepare!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H11M",
        "totalTime": "P0DT0H26M",
        "recipeYield": "1 pizza",
        "recipeIngredient": [
            "1 pound bulk pork sausage",
            "1 teaspoon Italian seasoning blend",
            "\u00bd teaspoon red pepper flakes, or to taste",
            "1 (8 ounce) package refrigerated crescent rolls",
            "\u00bd cup pizza sauce",
            "1 cup shredded Mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the sausage in a skillet over medium-high heat until crumbly and evenly browned. Stir in the Italian seasoning. Drain, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the crescent roll dough on a baking sheet and roll into 9x13 inch rectangle. Spoon pizza sauce over the middle of dough, and top with Mozzarella cheese, then cooked sausage. Fold two opposite sides of the dough in towards the center to cover the sausage mixture; pinch edges of dough together in a few places to seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until crust is browned, 11 to 14 minutes.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
        ],
        "recipeCuisine": [
