        "recipeIngredient": [
            "3 eggs",
            "1\u2009\u00bd cups vanilla fat-free yogurt",
            "1\u2009\u00bc cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon kosher salt",
            "\u00bd cup shortening"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat a waffle iron according to manufacturer's instructions. Beat eggs in a large mixing bowl, then add yogurt, flour, baking powder, baking soda, kosher salt, and shortening, mixing until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter onto hot waffle iron. Cook until no longer steaming, about 5 minutes.\n"
            }
