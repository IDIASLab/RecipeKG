    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254269/sorghum-kale-and-white-bean-tomato-soup/",
        "name": "Sorghum, Kale and White Bean Tomato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3679879.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-06-15T01:27:47.000Z",
        "description": "This hearty sorghum soup is the perfect meal to warm you up on a chilly fall or winter day. Packed with veggies and topped with Italian cheese, this soup is sure to be a new lunch or dinner time favorite.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd cup whole grain sorghum, uncooked",
            "3 cups low-sodium chicken broth",
            "1 tablespoon extra virgin olive oil",
            "2 large cloves garlic, finely chopped",
            "1 small-medium onion, chopped",
            "\u00bc teaspoon kosher salt",
            "\u00bc teaspoon black pepper",
            "12 ounces kale, thick stems removed, leaves chopped into 1-2-inch pieces",
            "1 teaspoon sriracha or other hot sauce",
            "2 (15 ounce) cans white cannellini beans or white kidney beans, drained and rinsed",
            "1 (14.5 ounce) can diced tomatoes in juice",
            "4 cups low-sodium chicken broth",
            "10 teaspoons Grated Pecorino Romano or Parmesan cheese for topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring 3 cups of broth to a boil, add 1/2 cup of sorghum and allow to boil gently for approximately 20-25 minutes (liquid will remain, that's fine).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat a large saucepan with lid over medium to medium/high heat. Add olive oil and saute garlic, onion, salt and pepper until the onion softens (approximately 6-8 minutes).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add kale and sriracha, cook until wilted (about 4 minutes).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take 1/2 cup of white beans and either blend in a blender/small food processor or mash well with the back of a spoon or fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add blended beans, remaining whole beans, canned tomatoes, sorghum/broth mixture and additional 4 cups low sodium chicken broth to kale mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring to a boil, cover and reduce heat to simmer for approximately 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with grated Pecorino Romano or Parmesan cheese (approximately 1 teaspoon per bowl) and serve.\n"
            }
        ],
