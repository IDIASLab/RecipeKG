    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260867/white-chocolate-peppermint-bark/",
        "name": "White Chocolate Peppermint Bark",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6355348.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2017-10-19T06:15:44.000Z",
        "description": "Even fans of non-white chocolate enjoyed! Stored in the refrigerator, the bark will keep for a week.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT0H47M",
        "recipeYield": "1 pound of bark",
        "recipeIngredient": [
            "2 (11 ounce) packages white chocolate chips (such as Ghirardelli\u00ae)",
            "12 peppermint candy canes, broken into pieces",
            "\u00bc teaspoon peppermint oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line an 11x17-inch baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt white chocolate chips in a microwave-safe glass bowl on 50% power for 3 minutes. Stir well until bowl is no longer warm. Heat for 3 minutes more; stir. Heat for 1 minute more and stir; repeat as necessary until chocolate is completely smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir candy canes and peppermint oil into the melted chocolate until well combined. Spread onto the prepared baking sheet. Smooth with a spatula. Refrigerate until set, 30 minutes to overnight. Break into pieces and store in an airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "White Chocolate"
        ],
