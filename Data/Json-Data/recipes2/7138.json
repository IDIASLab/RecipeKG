        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240137/honey-roasted-sweet-potatoes/",
        "name": "Honey Roasted Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1924503.jpg",
