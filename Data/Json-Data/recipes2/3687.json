    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283723/beef-and-bean-dip/",
        "name": "Beef and Bean Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2421673.jpg",
            "width": 1936,
            "height": 2592
        },
        "datePublished": "2015-07-25T19:18:34.000Z",
        "description": "Family loves to make burritos out of this beef and bean dip.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "3 tablespoons olive oil",
            "2 medium sweet onions (such as Vidalia\u00ae), thinly sliced",
            "2 tablespoons minced garlic",
            "2 pounds lean ground beef",
            "3 tablespoons chili powder",
            "1 (12 ounce) bottle zesty Italian-style salad dressing",
            "1 cup chunky salsa",
            "2 (20.5 ounce) cans refried black beans",
            "1 pound grated Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter and oil in a very large skillet over medium heat. Add onions and garlic and saute until translucent, about 5 minutes. Add ground beef and chili powder; cook and stir until browned and crumbly, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add salad dressing and salsa and bring to a boil. Add beans and stir until smooth. Pour into a baking dish or foil pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every 15 minutes, until hot, about 1 hour. Sprinkle cheese on top if desired and cook until melted, about 15 more minutes. Serve warm.\n"
            }
        ],
