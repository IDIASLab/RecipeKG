        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143071/spinach-caprese-salad/",
        "name": "Spinach Caprese Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F849469.jpg",
