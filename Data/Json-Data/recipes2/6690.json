        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "2 1/2 cups",
        "recipeIngredient": [
            "2 large red bell peppers, cut into chunks",
            "3 cloves garlic",
            "1 tablespoon sweet paprika",
            "1 bunch fresh cilantro",
            "1 cup olive oil",
            "1 chile pepper",
            "2 slices bread, crusts removed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend the bell peppers, garlic, paprika, cilantro, olive oil, chile pepper, and bread in a blender until liquefied.\n"
