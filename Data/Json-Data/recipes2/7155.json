        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245429/no-bake-energy-balls/",
        "name": "No Bake Energy Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3297624.jpg",
