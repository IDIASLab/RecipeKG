    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237047/swiss-ham-pie/",
        "name": "Swiss Ham Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1117086.jpg",
            "width": 1435,
            "height": 1435
        },
        "datePublished": "2015-11-24T07:00:22.000Z",
        "description": "I got this recipe from my aunt. It has been one of my favorites for as long as I can remember. It's definitely comfort food for me! Garnish cooked pie with more Swiss cheese.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pastry for a 9-inch single pie crust",
            "2 tablespoons butter",
            "2 tablespoons cornstarch",
            "\u215b teaspoon nutmeg",
            "\u215b teaspoon garlic salt",
            "ground black pepper to taste",
            "1\u2009\u00bd cups milk",
            "4 ounces shredded Swiss cheese",
            "3 eggs, slightly beaten",
            "1 cup diced ham"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Press pie crust into a 9-inch pie dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Cook and stir cornstarch, nutmeg, garlic salt, and black pepper into melted butter until cornstarch dissolves, 2 to 3 minutes. Pour milk into cornstarch mixture; cook and stir until thickened and bubbling, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir Swiss cheese into milk mixture; cook and stir until melted and cheese sauce is smooth, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 1 to 2 tablespoons cheese sauce and eggs together in a bowl until eggs are warmed; pour into cheese sauce and whisk until incorporated, 1 to 2 minutes. Stir ham into cheese sauce; pour into pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pie is set in the middle, 30 to 35 minutes. Let cool slightly before serving, 5 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Meat and Seafood",
            "Ham"
        ],
