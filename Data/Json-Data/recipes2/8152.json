        "name": "Bacon, Tomato & Triple Cheese Grilled Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3962388.jpg",
            "width": 960,
