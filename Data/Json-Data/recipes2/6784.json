        "cookTime": null,
        "totalTime": "P0DT2H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 Roma (plum) tomatoes, diced",
            "1 cup diced onion",
            "1 carrot, peeled and chopped",
            "\u00bc cup fresh cilantro leaves, or to taste",
            "3 small jalapeno peppers",
            "2 green chile peppers",
            "\u00bc large poblano chile",
            "1 lime, juiced",
            "1 tablespoon chopped garlic",
            "1 teaspoon sea salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix tomatoes and onion in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Process carrot, cilantro, jalapeno peppers, green chile peppers, poblano chile, lime juice, and garlic in a food processor to your preferred texture; add to tomato mixture and stir. Season the mixture with sea salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover bowl with plastic wrap and refrigerate at least 2 hours before serving.\n"
            }
        ],
