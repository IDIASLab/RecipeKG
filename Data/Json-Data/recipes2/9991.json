    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283052/roasted-salmon-fillets-with-lingonberry-glaze/",
        "name": "Roasted Salmon Fillets with Lingonberry Glaze",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Roasted Salmon Fillets with Lingonberry Glaze"
        },
        "datePublished": "2017-05-01T02:35:20.000Z",
        "description": "Salmon paired with a delicious tart fruit glaze.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 tablespoons salted butter, melted",
            "2 tablespoons Dijon mustard",
            "2 (6 ounce) salmon fillets",
            "salt and ground pepper to taste",
            "2 tablespoons salted butter",
            "2 stalks green onions, finely chopped",
            "2 tablespoons lingonberry jam",
            "2 tablespoons raspberry vinaigrette",
            "salt and ground pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix melted butter and Dijon in a small bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place salmon fillets on a baking sheet and brush the tops and sides with Dijon mixture. Sprinkle with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fish flakes easily with a fork, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the salmon is baking, melt butter for glaze in a skillet over medium heat. Add green onions and saute until lightly browned, about 5 minutes. Add jam and vinaigrette, then season with salt and pepper. Cook and stir until thickened, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Plate the salmon and divide the glaze between the two fillets.\n"
            }
        ],
