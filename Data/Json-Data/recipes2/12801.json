        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240404/leftover-ham-salad/",
        "name": "Leftover Ham Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1875237.jpg",
