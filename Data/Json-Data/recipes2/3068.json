    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242486/holiday-rice-pudding/",
        "name": "Holiday Rice Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Holiday Rice Pudding"
        },
        "datePublished": "2015-05-15T00:29:39.000Z",
        "description": "This is not your standard rice pudding - studded with fruits and subtly spiced, it is a great alternative to pies or cakes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups water",
            "2 Earl Grey tea bags",
            "1 tablespoon butter",
            "\u00bd teaspoon salt",
            "1 cup UNCLE BEN'S\u00ae ORIGINAL CONVERTED\u00ae Brand Rice",
            "3 cups whole milk",
            "1 cup natural (raw) sugar",
            "2 teaspoons golden rum",
            "2 tablespoons golden raisins",
            "2 tablespoons sweetened dried cranberries",
            "2 tablespoons dried pineapple pieces",
            "1 teaspoon ground cinnamon or cardamom for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water to a boil in a saucepan over high heat. Steep the tea bags until tea is strong, about 5 minutes. Discard tea bags.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a saucepan over medium heat. Stir in the rice; cook and stir until grains are coated and begin to turn golden, about 2 minutes. Stir in the tea water. Raise heat to medium-high and bring to a boil; reduce heat, cover, and cook on low until most of liquid is absorbed, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pot from heat. Stir in milk, sugar, rum, raisins, cranberries, and pineapple. Cover; return to stove and cook over low heat until mixture is thickened and silky, 35 to 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon into serving dishes and dust with cinnamon or cardamom.\n"
            }
        ],
