    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274190/fresh-peach-sorbet/",
        "name": "Fresh Peach Sorbet",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fresh Peach Sorbet"
        },
        "datePublished": "2017-12-15T09:31:01.000Z",
        "description": "If you have an abundance of fresh peaches, try this delicious peach sorbet! This recipe makes seven 1/2-pint containers.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT6H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 cups sliced peeled peaches",
            "2 cups white sugar",
            "1 cup orange juice",
            "2 tablespoons lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree peaches in a blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, orange juice, and lemon juice in a 4-quart pan over medium heat. Cook, stirring constantly, until sugar dissolves, about 5 minutes. Remove from heat and stir in pureed peaches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into a 9x13-inch pan and allow to cool to room temperature, about 30 minutes. Place in the freezer until firm but not hard, about 1 1/2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer peach mixture in small batches to a food processor. Process until light and fluffy, about 1 minute. Fill into 1/2-pint freezer containers leaving 1/2 inch head space. Freeze until firm, at least 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frozen Dessert Recipes",
            "Sorbet Recipes"
        ],
