        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20471/easy-vegan-peanut-butter-fudge/",
        "name": "Easy Vegan Peanut Butter Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1115681.jpg",
