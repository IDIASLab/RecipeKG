    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255017/sweet-potato-peach-bake/",
        "name": "Sweet Potato Peach Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3850281.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-08-11T07:24:12.000Z",
        "description": "Peaches and sweet potatoes complement each other so well in this Sweet Potato Peach Bake. This sweet and delicious casserole is perfect for the holidays, and can be dressed up with marshmallows or peach preserves.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (40 ounce) can Bruce's\u00ae Yams Cut Sweet Potatoes in Syrup, drained",
            "1 (16 ounce) can sliced peaches (no sugar added), drained, juice reserved",
            "\u00bd teaspoon ground ginger",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon pepper",
            "\u00bc cup brown sugar",
            "\u2153 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place Bruce's&reg; Yams Cut Sweet Potatoes in a bowl and cut into slightly smaller pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain the peaches (reserving 4-5 slices for the top of the casserole) and add to the yams along with 1/2 cup peach juice, ground ginger, salt and pepper; stir to mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to a 2 quart baking dish; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, stir together the brown sugar and pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle over top of casserole and place reserved peach slices in a line down the center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 35-40 minutes until hot and bubbly.\n"
            }
        ],
