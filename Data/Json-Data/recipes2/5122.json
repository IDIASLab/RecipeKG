        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237642/quinoa-greek-inspired-salad/",
        "name": "Quinoa Greek-Inspired Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9247080.jpg",
