    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256987/bread-boat-pasta-bake/",
        "name": "Bread Boat Pasta Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bread Boat Pasta Bake"
        },
        "datePublished": "2017-01-28T04:29:36.000Z",
        "description": "Easy pasta dish to share with friends and family.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H49M",
        "totalTime": "P0DT0H59M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound ground sausgae",
            "1 red onion, chopped",
            "1 tablespoon garlic pepper",
            "1 tablespoon Italian seasoning",
            "\u00bc (16 ounce) package gemelli pasta",
            "\u00bc cup butter",
            "1 pinch garlic powder, or to taste",
            "1 (1 pound) loaf Vienna bread",
            "1 cup marinara sauce, or more as needed",
            "7 ounces diced tomatoes",
            "1 cup shredded mozzarella cheese",
            "\u00bd teaspoon Italian seasoning, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir sausage, onion, garlic pepper, and 1 tablespoon Italian seasoning in the hot skillet until browned and crumbly, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook gemelli pasta in the boiling water, stirring occasionally until tender yet firm to the bite, 12 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Line a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan over medium heat; stir in garlic powder until butter is melted and garlic is fragrant, 2 to 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the top portion out of the Vienna bread, forming a boat-shape. Brush garlic butter over the opening in the bread and place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bread is toasted, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sausage mixture, marinara sauce, tomatoes, pasta, and half of the mozzarella cheese in a large bowl. Add more marinara sauce if mixture is dry, but not so much that the mixture is too wet. Fill bread \"boat\" with sausage mixture and top with the remaining mozzarella cheese and 1 teaspoon Italian seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is melted and pasta is hot, about 10 minutes.\n"
            }
        ],
