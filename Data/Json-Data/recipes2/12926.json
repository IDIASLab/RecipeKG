    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285210/cereal-cookies/",
        "name": "Cereal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4691056.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2017-12-14T20:42:46.000Z",
        "description": "Enjoy three breakfast favorites-cornflakes, rice cereal, and oats-as a sweet, crispy treat in these easy cereal cookies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "60 cookies",
        "recipeIngredient": [
            "1 cup cornflakes, lightly crushed",
            "1 cup crispy rice cereal",
            "1 cup rolled oats",
            "4 cups all-purpose flour",
            "1 tablespoon baking soda",
            "1 cup unsalted butter, softened",
            "1 cup canola oil",
            "1 cup white sugar",
            "1 cup brown sugar",
            "1 large egg",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cornflakes, rice cereal, and oats in a large bowl. Whisk flour and baking soda together in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, oil, and both sugars in a very large bowl until smooth. Add egg and vanilla and beat until smooth. Slowly mix in flour mixture. Stir in cereal mixture by hand.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough 2 inches apart onto the prepared baking sheets. Flatten balls with the bottom of a glass for even cooking and crispier cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in batches in the preheated oven until edges are golden, 10 to 12 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Quick and Easy",
