        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17144/vegetable-salsa-soup/",
        "name": "Vegetable Salsa Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8536768.jpg",
            "width": 0,
            "height": 0
