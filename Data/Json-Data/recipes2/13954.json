        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18498/pjs-fresh-corn-salad/",
        "name": "P.J.'s Fresh Corn Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F104718.jpg",
