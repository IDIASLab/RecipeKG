    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/176820/fireball-mushrooms/",
        "name": "Fireball Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fireball Mushrooms"
        },
        "datePublished": "2016-06-04T13:42:34.000Z",
        "description": "This is a spicy seafood stuffed mushroom with clams, shrimp, and lump crab. Watch out because these 'shrooms bite back!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "16 stuffed mushrooms",
        "recipeIngredient": [
            "\u00bc cup margarine",
            "\u00bd bunch celery, diced",
            "3 large onions, diced",
            "1\u2009\u00bd tablespoons minced garlic",
            "2 (6.5 ounce) cans chopped clams, drained",
            "\u00bd pound salad shrimp, chopped",
            "2 teaspoons chicken bouillon granules",
            "\u00bc teaspoon cayenne pepper",
            "\u00bc teaspoon white pepper",
            "2 cups croutons",
            "\u00bc cup seasoned bread crumbs",
            "\u00bd pound cooked lump crabmeat",
            "\u00bc cup margarine",
            "\u00bc cup dry white wine",
            "16 large fresh white mushrooms, cleaned and stems removed",
            "1 cup shredded Italian blend cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1/4 cup margarine in a large skillet, and cook and stir the celery, onion, and garlic over medium heat for about 5 minutes, until onion is translucent. Stir in the chopped clams and salad shrimp, and cook and stir about 3 more minutes, until the mixture is hot. Drain most of the liquid from the skillet, leaving about 1 tablespoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle in the chicken bouillon granules, cayenne pepper, and white pepper, and stir. Gently stir in the croutons, bread crumbs, and crab meat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 1/4 cup of margarine in the bottom of a 9x13 inch baking dish, and pour in the white wine. Mound about 1/3 cup of filling into each mushroom cap, and place them into the baking dish. Sprinkle the Italian blend cheese over the mushroom caps.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for about 20 minutes, until the cheese is bubbling and starting to brown.\n"
            }
        ],
