        "mainEntityOfPage": "https://www.allrecipes.com/recipe/131180/lisas-super-sangria/",
        "name": "Lisa's Super Sangria",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F311991.jpg",
            "width": 250,
            "height": 250
