    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/60300/carrot-gelatin-salad/",
        "name": "Carrot Gelatin Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Carrot Gelatin Salad"
        },
        "datePublished": "2018-03-21T16:32:50.000Z",
        "description": "Old-fashioned carrot salad made with lemon Jell-O and pineapple. Grandma didn't specify the amount of carrots. You probably can't use too much!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT5H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (6 ounce) package lemon flavored Jell-O\u00ae mix",
            "1 (20 ounce) can crushed pineapple, drained with juice reserved",
            "4 large carrots, shredded"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, prepare the lemon gelatin according to package directions using reserved pineapple juice in place of some of the water. Refrigerate until thickened, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the gelatin has thickened, stir in pineapple and shredded carrot. Cover and refrigerate until set, at least 4 hours.\n"
            }
        ],
