        "name": "Caramel Apple Cider with Salty-Sweet Rim",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1646222.jpg",
            "width": 3456,
            "height": 5184
