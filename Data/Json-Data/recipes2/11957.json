    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/242097/fast-food-salad/",
        "name": "Fast Food Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2222107.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2017-10-07T13:03:35.000Z",
        "description": "Want a fun new food with a new concept? Challenge the stereotype of salad having to be healthy with this dish.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "3 tablespoons vegetable oil, divided",
            "5 potatoes, thinly sliced ",
            "1 (8 ounce) package cooked and cubed ham, or to taste",
            "1 pound thick-cut maple-cured bacon",
            "1 (12 ounce) bottle barbeque sauce, or to taste",
            "1 (8 ounce) bottle ranch dressing, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons oil in a large skillet over medium heat. Cook and stir potato slices in hot oil until tender but not crispy, 10 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in a skillet over medium-high heat. Saute ham in hot oil until heated through, 3 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook bacon in a large skillet over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain and crumble bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk barbeque sauce and ranch dressing together in a bowl. Stir potatoes, beef, ham, and bacon together in a large bowl; top with barbeque sauce mixture and toss to coat.\n"
            }
        ],
