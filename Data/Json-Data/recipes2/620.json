    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234292/calgary-dinner/",
        "name": "Calgary Dinner",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3189559.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2014-01-31T23:05:39.000Z",
        "description": "A yummy hamburger filling for French bread. I got this recipe from a friend years ago and always loved it.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (12 ounce) loaf French bread",
            "1 pound lean ground beef",
            "\u00bd cup chopped celery",
            "1 (8 ounce) container reduced-fat sour cream",
            "1 (1.25 ounce) envelope dry onion soup mix",
            "1 tomato, sliced",
            "2 cups shredded mozzarella and Cheddar cheese blend"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Split French bread lengthwise and tear out the soft bread from interior of loaf to hollow out; reserve bread pieces for another use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a skillet over medium-high heat. Cook and stir beef and celery in the hot skillet until beef is browned and crumbly, 5 to 7 minutes; drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sour cream and soup mix into beef mixture; cook until heated through, about 3 minutes. Fill prepared bread with mixture; lay tomato slices over meat, sprinkle evenly with cheese blend, and place filled bread on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil until cheese is melted and bubbly, about 3 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Sandwich Recipes",
            "Beef"
        ],
