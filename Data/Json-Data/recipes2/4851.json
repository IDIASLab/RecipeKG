    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282625/creamy-oven-baked-polenta-with-pumpkin/",
        "name": "Creamy Oven-Baked Polenta with Pumpkin",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Creamy Oven-Baked Polenta with Pumpkin"
        },
        "datePublished": "2015-12-18T22:02:53.000Z",
        "description": "Easiest polenta ever! Bakes up creamy and delicious. You (ok, I) will never make polenta on the stovetop again.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "2 cups water",
            "2 cups milk",
            "1 cup stone-ground cornmeal",
            "1\u2009\u00bd teaspoons salt",
            "1 (14 ounce) can pumpkin puree",
            "1 cup shredded Jarlsberg cheese",
            "\u00bd cup grated Parmigiano-Reggiano cheese, divided",
            "\u00bc cup sour cream",
            "\u00bd teaspoon ground nutmeg",
            "\u215b teaspoon coarsely ground black pepper",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Coat a 3-quart baking dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine water, milk, cornmeal, and salt in the prepared baking dish; mix well. It's normal for the mixture to separate a bit.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven for 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and stir in pumpkin, Jarlsberg, 1/2 of the Parmigiano-Reggiano, sour cream, nutmeg, pepper, and salt. Return to the oven and bake for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and top with remaining Parmigiano-Reggiano. Turn on the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil until top is brown and bubbly, about 3 minutes. Let stand 5 minutes before serving.\n"
            }
        ],
