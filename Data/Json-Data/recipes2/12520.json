        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220523/omalins-irish-cream/",
        "name": "O'Malin's Irish Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4486446.jpg",
            "width": 2800,
            "height": 2682
        },
        "datePublished": "2017-11-09T08:51:18.000Z",
        "description": "Once you try this, you'll wonder why in the world you've been paying all that money at the liquor store for a mediocre Irish Cream when you can have world-class quality here for a fraction of the cost AND from a real Irishman.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P14DT0H10M",
        "recipeYield": "50 fluid ounces",
        "recipeIngredient": [
            "3 eggs",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (12 ounce) can evaporated milk",
            "1\u2009\u00bd cups high-quality Canadian blended whiskey, or more to taste",
            "1 teaspoon vanilla extract",
            "1 teaspoon coconut extract",
            "1 teaspoon instant coffee granules",
            "1 tablespoon chocolate syrup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine eggs, sweetened condensed milk, evaporated milk, whiskey, vanilla extract, coconut extract, instant coffee granules, and chocolate syrup in a bowl; stir just enough to break the egg yolks into the mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the contents of the bowl into a blender; blend on High for 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the blended mixture into a glass container with a tight-fitting lid. Refrigerate at least 2 weeks before serving.\n"
            }
        ],
