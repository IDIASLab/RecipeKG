        "mainEntityOfPage": "https://www.allrecipes.com/recipe/159106/chocolate-wine-balls/",
        "name": "Chocolate Wine Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8759754.jpg",
            "width": 0,
            "height": 0
