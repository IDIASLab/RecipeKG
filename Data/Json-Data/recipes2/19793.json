    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13176/french-onion-soup-ii/",
        "name": "French Onion Soup II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F488861.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This is a very rich and tasty soup!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound onions, sliced",
            "3 tablespoons vegetable oil",
            "2 (14 ounce) cans beef broth",
            "1 teaspoon Worcestershire sauce",
            "1 pinch ground black pepper",
            "6 slices French bread, toasted",
            "\u2154 cup shredded Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir onions in vegetable oil in a covered large saucepan over medium heat for 20 minutes. Stir in the beef broth, Worcestershire sauce and black pepper; heat to a boil. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread on a baking sheet; sprinkle bread with cheese. Broil 4-to-5 inches from heat for about 1 minute or until cheese is melted and golden.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Ladle soup into bowls and top with bread.\n"
            }
        ],
