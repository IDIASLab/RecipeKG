        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233530/easy-crunchy-berry-parfaits/",
        "name": "Easy Crunchy Berry Parfaits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1035420.jpg",
