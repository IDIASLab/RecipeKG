        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237395/moms-baked-salmon/",
        "name": "Mom's Baked Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9150897.jpg",
            "width": 0,
