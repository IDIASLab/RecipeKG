    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24883/beer-cheese-dip-ii/",
        "name": "Beer Cheese Dip II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F17093.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-03-21T16:18:50.000Z",
        "description": "Fashion a round loaf of your favorite type of bread into a bowl, and fill it with this smooth, cheesy beer dip. You'll go back for bite after bite!",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "1 bread bowl dip",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "2 (8 ounce) packages shredded Cheddar cheese",
            "\u00bd teaspoon garlic powder",
            "\u00bd cup beer",
            "1 (1 pound) loaf round bread"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cream cheese, Cheddar cheese, garlic powder and beer in a large bowl. Using an electric mixer, blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove and reserve top of round bread. Hollow out the loaf, reserving removed bread pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon cream cheese mixture into the hollowed loaf. Replace bread top between servings. Use the reserved removed bread pieces for dipping.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes"
        ],
