    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244206/grilled-potato-salad-with-crazy-steves-cajun-cukes/",
        "name": "Grilled Potato Salad with Crazy Steve's Cajun Cukes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2338146.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2015-06-04T20:30:25.000Z",
        "description": "This potato salad is for anyone who loves bacon and onions...a little sour and spicy. Can be served hot or cold.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd pound bacon, diced",
            "1 red onion, diced",
            "2 tablespoons kosher salt",
            "1 teaspoon ground black pepper",
            "1 teaspoon paprika",
            "1 teaspoon garlic powder",
            "1 teaspoon ground cumin",
            "\u00bc teaspoon dried rosemary",
            "\u00bc teaspoon ground thyme",
            "\u00bc teaspoon celery seed",
            "5 pounds small red potatoes",
            "1 (16 ounce) jar spicy pickles (such as Crazy Steve's\u2122 Cajun Cukes)",
            "\u00bd cup pickle juice (such as juice from Crazy Steve's\u2122 Cajun Cukes)",
            "5 tablespoons mayonnaise",
            "2 tablespoons Dijon mustard"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir bacon and red onion together in a skillet over medium heat until bacon is crisp, about 10 minutes. Remove skillet from heat and transfer bacon and onion to a plate, reserving drippings in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir salt, black pepper, paprika, garlic powder, cumin, rosemary, thyme, and celery seed into bacon drippings. Roll potatoes into seasoned bacon drippings until evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook potatoes on grill until golden brown and tender, about 10 to 15 minutes per side. Cool potatoes slightly; cut into bite-size pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix pickles, pickle juice, mayonnaise, and Dijon mustard together in a bowl until smooth. Stir bacon, onion, and potatoes into mayonnaise mixture until evenly coated.\n"
            }
        ],
