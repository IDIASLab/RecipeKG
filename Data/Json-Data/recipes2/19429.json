        "name": "Toasted Waffle Ice Cream Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F10%2F04%2F110600-toasted-waffle-ice-cream-sandwich-MFS_2x1-128-1-2000.jpg",
