    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282357/thai-red-curry-with-chicken/",
        "name": "Thai Red Curry with Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Thai Red Curry with Chicken"
        },
        "datePublished": "2017-12-12T04:45:04.000Z",
        "description": "You don't need too many special ingredients on hand to make this delicious Thai red curry meal!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups water",
            "1 cup jasmine rice",
            "2 tablespoons peanut oil, divided",
            "1 pound skinless, boneless chicken breast, cut in bite-sized pieces",
            "2 cloves garlic, minced, divided",
            "salt and ground black pepper to taste",
            "1 (1/2 inch) piece fresh ginger, minced",
            "2 tablespoons Thai red chile paste",
            "1 (14 ounce) can coconut milk",
            "1 cup chicken broth",
            "1 stalk lemongrass, bruised",
            "3 makrut lime leaves (optional)",
            "1 medium lime, juiced",
            "1 teaspoon fish sauce",
            "1 crown broccoli, cut into bite-sized pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and rice to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and water has been absorbed, about 15 minutes. Keep warm until ready to use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat 1 tablespoon oil in a large saute pan over medium-high heat. Add chicken, 1/2 of the garlic, salt, and pepper. Cook and stir until chicken is browned and the juices run clear, 8 to 10 minutes. Remove chicken to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add remaining oil to the same pan. Saute remaining garlic and ginger until fragrant, about 1 minute. Stir in chile paste. Add coconut milk and stir until blended. Simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chicken broth, lemongrass, lime leaves, lime juice, and fish sauce. Simmer, uncovered, for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chicken and broccoli, and simmer gently for 10 more minutes. Taste and season as necessary. Serve over warm rice.\n"
            }
        ],
