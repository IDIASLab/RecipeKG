    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228081/paleo-style-fire-roasted-salsa/",
        "name": "Paleo-Style Fire Roasted Salsa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5258696.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2016-06-04T14:06:54.000Z",
        "description": "Salsa with a little bit of kick, paleo-style!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 dried chipotle chili pepper",
            "6 roma tomatoes, halved lengthwise",
            "1 clove garlic",
            "\u00bc cup chopped fresh cilantro",
            "1 small white onion, quartered",
            "1 jalapeno pepper, seeded and coarsely chopped",
            "1 lime, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a small bowl with water and submerge chipotle chile pepper in the water until softened and rehydrated, 10 to 15 minutes. Remove chipotle pepper and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler. Line a baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place tomatoes on the prepared baking sheet, and broil until the skins are charred, 13 to 15 minutes. Flip and char the other side for an additional 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine charred tomatoes, garlic, cilantro, white onion, jalapeno pepper, chipotle pepper, and lime juice in a food processor; puree until evenly blended.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
