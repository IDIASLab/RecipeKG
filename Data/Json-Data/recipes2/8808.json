    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234406/ferns-tuna-casserole/",
        "name": "Fern's Tuna Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4536651.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2016-12-14T03:21:34.000Z",
        "description": "Nothing fancy here, but really great comfort food from my husband's Grandma. Good old tuna casserole with potato chips on top.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd (8 ounce) package egg noodles",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "\u00be cup whole milk",
            "1 (5 ounce) can tuna",
            "2 slices processed cheese food (such as Velveeta\u00ae)",
            "\u00bd cup crushed potato chips, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook egg noodles in the boiling water, stirring occasionally until cooked through but firm to the bite, about 5 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mushroom soup, milk, and tuna in a saucepan over medium heat; cook until well blended and warmed through, about 5 minutes. Pour mixture into a 9x13-inch baking dish; add noodles and mix well. Top noodles with cheese food and sprinkle potato chips over cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until casserole is bubbling, 30 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Tuna"
        ],
