        "mainEntityOfPage": "https://www.allrecipes.com/recipe/263702/coconut-fat-bombs/",
        "name": "Coconut Fat Bombs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8140212.jpg",
