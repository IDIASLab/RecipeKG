        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283007/braised-baby-bok-choy/",
        "name": "Braised Baby Bok Choy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8984612.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-07-05T14:22:31.000Z",
        "description": "Yummy and tender.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon extra-virgin olive oil",
            "4 large heads baby bok choy, halved lengthwise, or more to taste",
            "\u00bc cup chicken stock",
            "3 tablespoons soy sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat until hot, but not smoking. Add bok choy and cook, turning once, until just golden, about 2 minutes. Add stock and soy sauce. Cover, reduce heat to medium, and simmer until bok choy is tender, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to a serving platter, reserving liquid in the skillet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook liquid over medium-high heat until it is reduced by half, 1 to 2 minutes. Pour over bok choy and serve.\n"
            }
        ],
