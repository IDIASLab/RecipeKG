        "mainEntityOfPage": "https://www.allrecipes.com/recipe/73223/new-wife-tuna-salad/",
        "name": "New Wife Tuna Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F818338.jpg",
