        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220671/lettuce-free-salad/",
        "name": "Lettuce-Free Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1043750.jpg",
            "width": 2346,
            "height": 2346
