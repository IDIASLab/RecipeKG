        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241810/easiest-asparagus-recipe/",
        "name": "Easiest Asparagus Recipe",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2176760.jpg",
            "width": 5184,
            "height": 3888
        },
        "datePublished": "2016-06-04T14:16:00.000Z",
        "description": "I have always been good at sauteeing asparagus, but this is one of my favorite methods to create a sweet and traditional appetizer.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "2 tablespoons butter, or more as needed",
            "1 bunch asparagus, trimmed",
            "1 teaspoon honey",
            "\u215b teaspoon garlic powder",
            "\u215b teaspoon cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt 2 tablespoons butter in a skillet over medium-low heat. Cook asparagus in melted butter, stirring a few times, until tender, 6 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle honey over the asparagus and stir to coat; season with garlic powder and cayenne pepper.\n"
            }
        ],
