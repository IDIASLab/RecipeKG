    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283573/stuffed-salmon/",
        "name": "Stuffed Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2258376.jpg",
            "width": 4992,
            "height": 3744
        },
        "datePublished": "2015-05-08T02:01:27.000Z",
        "description": "Yummy stuffed salmon. Best serve with some steamed asparagus and baby carrots. Pair with Chardonnay.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 teaspoons olive oil, divided, or as needed",
            "\u00bc cup chopped onion, or more to taste",
            "\u00bd (8 ounce) package cream cheese",
            "\u00bc cup crumbled blue cheese",
            "1 cup chopped fresh spinach",
            "1 tablespoon dry bread crumbs",
            "1 clove garlic, minced, or to taste",
            "salt and ground black pepper to taste",
            "1 (8 ounce) salmon fillet",
            "1 teaspoon lemon juice, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 teaspoon oil in a skillet over medium heat; stir in onion. Cook and stir until the onion has softened and turned translucent, about 5 minutes. Remove from the heat and transfer to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese and blue cheese in a microwave-safe bowl. Heat on high for 25 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cheese mixture, spinach, bread crumbs, garlic, salt, and pepper to the onions; mix until well combined. Put stuffing in the freezer until chilled, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking dish with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut salmon into 2 portions. Slice each portion in half, leaving a connecting point at one side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat remaining oil in a skillet over medium heat. Add salmon and fry until colored a bit, about 15 seconds per side. Transfer salmon to the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Take stuffing out of the freezer and make into 2 balls. Put 1 ball into each salmon portion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until fish flakes easily with a fork, 15 to 17 minutes. Garnish with lemon juice.\n"
            }
        ],
