        "mainEntityOfPage": "https://www.allrecipes.com/recipe/39351/pina-colada-iv/",
        "name": "Pina Colada IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3473681.jpg",
