        "recipeIngredient": [
            "1\u2009\u00bd fluid ounces gin",
            "1 fluid ounce lime juice",
            "1 teaspoon simple syrup",
            "1 cup ice",
            "1 lime wheel"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine gin, lime juice, and simple syrup in a cocktail shaker. Add ice and shake until chilled. Strain into a chilled cocktail glass.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish with a lime wheel.\n"
            }
        ],
