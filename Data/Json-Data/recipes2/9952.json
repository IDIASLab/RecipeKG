    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257964/mexican-breakfast-casserole/",
        "name": "Mexican Breakfast Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6198359.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2017-04-26T03:14:16.000Z",
        "description": "A spicy spin on your average American breakfast casserole. Feel free to change anything in this recipe. This was really delicious as it is, but I'm always looking for ways to improve it! Enjoy! Top with your favorite hot sauce or even taco sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H3M",
        "totalTime": "P0DT1H23M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 slices bacon, chopped",
            "4 potatoes, shredded",
            "1 onion, chopped, or to taste",
            "1 (10 ounce) can diced tomatoes with green chile peppers",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 cup milk",
            "3 eggs, or more to taste",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a large casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a skillet and cook over medium-high heat, turning occasionally, until evenly browned, 5 to 8 minutes. Drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook and stir potatoes and onion in the skillet until golden brown, about 10 minutes. Add bacon and diced tomatoes; stir until heated through, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour potato mixture into the casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cream of chicken soup, milk, and eggs together in a bowl. Pour over potato mixture in the casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until eggs are set, about 40 minutes. Sprinkle Cheddar cheese on top. Continue baking until cheese is melted, about 5 minutes more.\n"
            }
        ],
