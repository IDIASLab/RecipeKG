        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261062/panettone-holiday-cookies/",
        "name": "Panettone Holiday Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4575573.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-10-24T01:20:13.000Z",
        "description": "Mini rounds of panettone topped with whipped, sweetened egg whites and a sprinkle of chopped nuts are baked until golden brown.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup confectioners' sugar",
            "1 egg white",
            "1 teaspoon lemon juice",
            "4 (1/2 inch thick) slices Bauducco Panettone, or as needed to cut out 12 small circles or triangles",
            "3 tablespoons chopped cashews (or hazelnuts, pistachios, or Brazilian nuts)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the confectioners' sugar with the egg whites until you get a smooth paste; add the lemon juice, mix and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the Bauducco Panettone into small triangles or circles, removing the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the frosting on one side of the pieces of Panettone, then sprinkle with bits of cashew nuts and bake at 350 degrees F for about 20 minutes or until cookies are golden.\n"
            }
        ],
