        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups rolled oats",
            "1\u2009\u00bd (14 ounce) cans coconut milk, or more to taste",
            "3 tablespoons chia seeds",
            "2 Granny Smith apples, peeled and grated",
            "\u00bd lemon, juiced",
            "2 cups sliced strawberries",
            "1 cup blueberries",
            "2 tablespoons maple syrup, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix oats, coconut milk, and chia seeds together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss apples and lemon juice together in a bowl until coated; fold into oat mixture. Stir strawberries, blueberries, and maple syrup into oat mixture. Refrigerate for 8 hours to overnight.\n"
            }
        ],
