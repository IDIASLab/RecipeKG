        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18510/frozen-no-bake-pumpkin-pie/",
        "name": "Frozen No-Bake Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060921.jpg",
            "width": 3161,
