        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75621/chipotle-mayo-relish/",
        "name": "Chipotle Mayo Relish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F822373.jpg",
