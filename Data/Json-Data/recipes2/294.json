    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285096/pear-crisp-with-cranberries-and-white-chocolate/",
        "name": "Pear Crisp with Cranberries and White Chocolate",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pear Crisp with Cranberries and White Chocolate"
        },
        "datePublished": "2013-11-28T15:38:45.000Z",
        "description": "This twist on a fall classic pear crisp can be tweaked to fit nearly any palate.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 tablespoons butterscotch schnapps",
            "2 tablespoons white sugar",
            "1 teaspoon ground cinnamon, divided",
            "\u00bc teaspoon ground nutmeg",
            "\u215b teaspoon ground ginger",
            "4 medium pears - peeled, cored, and sliced",
            "1 cup fresh cranberries",
            "\u00bd cup rolled oats",
            "\u00bd cup all-purpose flour",
            "\u00bd cup packed brown sugar",
            "\u00bd cup cold unsalted butter, cubed",
            "\u00bc teaspoon salt",
            "\u2153 cup white chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix schnapps, white sugar, 1/2 teaspoon cinnamon, nutmeg, and ginger together in a large bowl until sugar dissolves. Add pears and cranberries and toss until fruit is completely coated. Cover and refrigerate for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Lightly grease a 9-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oats, flour, brown sugar, cold butter, remaining 1/2 teaspoon cinnamon, and salt in a food processor. Pulse until mixture forms large crumbles. Fold in white chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chilled fruit mixture into the prepared baking dish, leaving approximately 3/4 inch at the top. Sprinkle crumb mixture over the fruit, covering completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until brown and bubbly, about 45 minutes.\n"
            }
        ],
