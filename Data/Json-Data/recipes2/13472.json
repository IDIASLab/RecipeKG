    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280764/braised-pork-carnitas/",
        "name": "Braised Pork Carnitas",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Braised Pork Carnitas"
        },
        "datePublished": "2018-03-05T07:45:41.000Z",
        "description": "Deliciously tender and juicy braised pulled pork carnitas can be used in fried or street tacos. Perfect for burritos and chimichangas or taco bowls.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT5H10M",
        "totalTime": "P0DT5H40M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "5 pounds pork shoulder roast ",
            "3 cloves garlic, chopped",
            "2 tablespoons kosher salt",
            "1 teaspoon ground black pepper",
            "\u00bd small yellow onion, chopped",
            "6 cups pork stock",
            "1 pinch salt and ground black pepper to taste",
            "12 each green onions, chopped",
            "24 each fresh yellow corn tortillas",
            "4 cups thinly shaved green cabbage",
            "10 ounces crumbled queso fresco",
            "2 cups fresh salsa",
            "2 each limes, cut into wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large, cast iron Dutch oven over medium-high heat. Season pork shoulder with garlic, 2 tablespoons kosher salt, and 1 teaspoon black pepper. Place pork shoulder fat-side down into the hot pot and sear until deep golden brown on all sides, 4 to 5 minutes. Add yellow onion to cook with rendered drippings for 1 minute. Slowly pour in pork stock to cover pork shoulder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring to a light boil; cover and braise until fork-tender, 4 1/2 to 5 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to a bowl and shred into carnitas, adding 1 1/2 cups of pork stock for moisture and more salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a cast iron skillet over medium-high heat. Add green onions and cook until slightly browned, 7 to 10 minutes. Transfer to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat corn tortillas in the hot skillet on both sides, 2 to 3 minutes total for each tortilla. Place braised pork into tortillas and garnish with cabbage, queso fresco, salsa, green onions, and a squeeze of lime.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Taco Recipes"
        ],
