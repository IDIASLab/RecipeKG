        "mainEntityOfPage": "https://www.allrecipes.com/recipe/140378/knock-your-socks-off-sangria/",
        "name": "Knock Your Socks Off Sangria!",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F618868.jpg",
