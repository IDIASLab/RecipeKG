        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25276/stir-fried-asparagus/",
        "name": "Stir Fried Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F969970.jpg",
