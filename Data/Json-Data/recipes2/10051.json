        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1\u2009\u00bd cups milk",
            "3 packets granular no-calorie sucralose sweetener (such as Splenda\u00ae)",
            "1 tablespoon cocoa powder",
            "4 shots fresh brewed espresso"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour milk into a large microwave-safe container; heat in microwave until warmed, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir sweetener and cocoa powder into the warmed milk to dissolve completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Froth the milk according to your espresso machine's directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir espresso into the frothed milk mixture.\n"
            }
        ],
