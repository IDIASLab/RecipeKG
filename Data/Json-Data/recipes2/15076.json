        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u2153 cup white vinegar",
            "\u00bc cup minced shallots",
            "2 teaspoons dried tarragon",
            "4 egg yolks, beaten",
            "\u00bd cup butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a microwave safe dish combine vinegar, shallots and tarragon.  Microwave on high until boiling.  Allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in egg yolks and butter.  Microwave until thickened, about 1 minute.\n"
            }
        ],
