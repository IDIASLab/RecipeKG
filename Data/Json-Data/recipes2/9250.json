    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257092/potato-hay/",
        "name": "Potato Hay",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4484958.jpg",
            "width": 3116,
            "height": 3115
        },
        "datePublished": "2017-02-08T07:47:26.000Z",
        "description": "Air-fried spiralized potatoes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 russet potatoes",
            "1 tablespoon canola oil",
            "kosher salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut potatoes into spirals using the medium grating attachment on a spiralizer, cutting the spirals with kitchen shears after 4 or 5 rotations.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak potato spirals in a bowl of water for 20 minutes. Drain and rinse well. Pat potatoes dry with paper towels, removing as much moisture as possible.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potato spirals in a large resealable plastic bag. Add oil, salt, and pepper; toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an air fryer to 360 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place half of the potato spirals in the fry basket and insert into the air fryer. Cook until golden, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Increase temperature to 390 degrees F (200 degrees C). Pull out the fry basket and toss potato spirals using tongs. Return basket to the air fryer and continue cooking, tossing occasionally, until golden brown, 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce temperature to 360 degrees F (180 degrees C) and repeat with remaining potato spirals.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato"
        ],
