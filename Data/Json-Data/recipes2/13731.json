    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17009/easy-beef-pie/",
        "name": "Easy Beef Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7649633.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-01-06T20:46:55.000Z",
        "description": "An easy to make beef and vegetable pie recipe to fill your tummy on a cold winter day.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "\u00bd pound cubed beef chuck roast",
            "1 cup red wine",
            "1 (10.5 ounce) can beef gravy",
            "1 (15 ounce) can mixed vegetables",
            "2 (9 inch)  pie crusts",
            "1 egg white"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a medium saucepan over medium heat; saute the stew meat for 10 minutes, or until well browned on all sides. Reduce heat to low and add the red wine. Cover and simmer for 15 minutes, allowing the alcohol to cook off.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cover and add the gravy and vegetables. Stir well and simmer for 10 more minutes. Pour mixture into one pastry shell. Cover with second pastry shell, sealing edges and cutting steam vents in top. Brush edges with egg white. Place pie on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Beef Pie Recipes"
        ],
