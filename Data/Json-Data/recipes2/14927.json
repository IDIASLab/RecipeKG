        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23980/tuna-delicious/",
        "name": "Tuna Delicious",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6550318.jpg",
            "width": 960,
