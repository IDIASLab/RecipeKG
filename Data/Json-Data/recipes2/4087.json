        "name": "Spinach, Bacon, and Mushroom Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5548188.jpg",
            "width": 1280,
            "height": 960
