        "mainEntityOfPage": "https://www.allrecipes.com/recipe/76798/caramel-martini/",
        "name": "Caramel Martini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F832787.jpg",
