    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20651/chutney-eggs/",
        "name": "Chutney Eggs",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chutney Eggs"
        },
        "datePublished": "2000-04-06T07:23:06.000Z",
        "description": "Here's a zesty variation on an old favorite that your guests will look forward to every time they visit. Bacon and chutney are the keys to these flavor packed eggs.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 - appetizers",
        "recipeIngredient": [
            "12 eggs",
            "\u00bc cup chutney",
            "6 slices bacon",
            "3 tablespoons mayonnaise",
            "1 tablespoon chopped almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a medium saucepan and cover with cold water. Bring water to a boil and immediately remove from heat. Cover and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool and peel. Cut in half lengthwise. Remove and reserve yolks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown; drain and crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mash egg yolks and mix with bacon, chutney, mayonnaise and almonds. Fill the egg whites with the mixture.\n"
            }
        ],
