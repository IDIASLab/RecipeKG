        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24170/mexican-hot-dip/",
        "name": "Mexican Hot Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F580402.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-03-21T16:17:26.000Z",
        "description": "Black olives take this otherwise straightforward hot dip to new heights of dipping delight! It's impossible to take just one bite.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "9 cups",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "2 (15 ounce) cans chili",
            "1 (6 ounce) can chopped black olives, drained",
            "1 (16 ounce) package shredded Cheddar cheese",
            "1 green onion, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread cream cheese on the bottom of a medium baking dish. Pour in the chili. Spread a layer of olives on the chili, and top with Cheddar cheese and green onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered in the preheated oven 30 minutes, or until bubbly and lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Hot Cheese Dip Recipes"
