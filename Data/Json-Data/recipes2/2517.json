        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese, softened",
            "1 (8 ounce) package shredded Monterey Jack cheese",
            "1 (8 ounce) package shredded Parmesan cheese",
            "\u00bd cup chopped onion",
            "\u00bd cup mayonnaise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese, Monterey Jack cheese, Parmesan cheese, onion, and mayonnaise together in a bowl; transfer to a baking dish or pie pan and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is brown and bubbling, about 20 minutes.\n"
            }
        ],
