        "name": "RICE KRISPIES&reg; Peanut Butter Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2144599.jpg",
            "width": 0,
            "height": 0
