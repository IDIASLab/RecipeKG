    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257311/dirty-chips/",
        "name": "Dirty Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4481778.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2017-03-01T08:49:22.000Z",
        "description": "This is a take on Dirty Tots that you'll find at Safeco Field in Seattle. You'll love the extra crunch from the kettle chips!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H14M",
        "totalTime": "P0DT0H29M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 slices bacon",
            "1 (8 ounce) package pepperoncini-flavored kettle-cooked potato chips",
            "6 ounces cheese curds, chopped",
            "2 tablespoons chopped pepperoncini peppers",
            "\u00bc teaspoon coarsely ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon slices in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain on paper towels; chop into smaller pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange potato chips in a single layer on a rimmed baking sheet. Sprinkle bacon and cheese curds evenly on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese curds start to melt and chips start to brown around the edges, about 4 minutes. Top with pepperoncini peppers and black pepper. Let cool. Transfer to a serving bowl.\n"
            }
        ],
