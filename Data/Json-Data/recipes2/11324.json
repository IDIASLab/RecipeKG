    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259581/valentines-cinnamon-heart-shortbread-pan-cookies/",
        "name": "Valentine's Cinnamon Heart Shortbread Pan Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Valentine's Cinnamon Heart Shortbread Pan Cookies"
        },
        "datePublished": "2017-08-12T01:52:09.000Z",
        "description": "A fast and easy Valentine's Day treat! Store cookies in a lidded container at room temperature or freeze.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "40 cookies",
        "recipeIngredient": [
            "2\u2009\u00bd cups butter",
            "1\u2009\u00bc cups confectioners' sugar",
            "1 cup heart-shaped cinnamon candies, chopped finely",
            "2\u2009\u00bd teaspoons vanilla extract",
            "\u00bc teaspoon cinnamon oil",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon baking powder",
            "5 cups all-purpose flour, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Line a jelly roll pan with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter in a large bowl with and electric mixer until fluffy. Add confectioner's sugar, cinnamon hearts, vanilla extract, cinnamon oil, baking soda, and baking powder; stir using a wooden spoon. Add flour 1 cup at a time, mixing by hand until dough sticks together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Push dough onto the prepared pan, flattening the corners to make an even layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until corners start to turn a light brown, 35 to 40 minutes. Cool on a rack for 10 minutes; lift shortbread out of the pan by holding onto the parchment paper. Cut into 2-inch squares.\n"
            }
        ],
