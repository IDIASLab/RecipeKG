        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246769/vegan-fried-rice/",
        "name": "Vegan Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3817861.jpg",
