    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244110/peppered-maple-and-thyme-turkey-brine/",
        "name": "Peppered Maple and Thyme Turkey Brine",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2296239.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-05-21T21:49:10.000Z",
        "description": "Our easy-to-prepare brine recipe will help ensure you end up with a deliciously moist and flavor-packed turkey.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H30M",
        "totalTime": "P0DT2H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 gallons cold water",
            "\u00bd cup Better Than Bouillon\u00ae Roasted Chicken Base",
            "1 cup maple syrup",
            "2 tablespoons black pepper",
            "\u00bd tablespoon granulated garlic",
            "2 tablespoons fresh thyme"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In the vessel that you will marinate the turkey in, combine all ingredients and mix well with a wire whisk, being sure that the chicken base is completely blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully place raw turkey or poultry into this vessel, being sure that the entire bird is covered with the brine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover vessel and place under proper refrigeration for 24 to 48 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to cook the turkey, carefully remove it from the brine and rinse under cold water and pat dry with a paper towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Your turkey is now ready to cook, as you normally would do.\n"
            }
        ],
