    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282678/apricot-vanilla-bean-muffins/",
        "name": "Apricot-Vanilla Bean Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apricot-Vanilla Bean Muffins"
        },
        "datePublished": "2018-01-17T08:48:16.000Z",
        "description": "Tangy apricot muffins with sweet marzipan on top.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "\u00bd cup unsalted butter",
            "1 cup white sugar",
            "1 (7 inch) vanilla bean, split lengthwise and seeds scraped",
            "2 large eggs",
            "3 cups all-purpose flour",
            "1 tablespoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1\u2009\u00bd cups buttermilk",
            "1 cup finely chopped dried apricots",
            "3 tablespoons coarse sugar",
            "2 ounces marzipan (almond paste)",
            "1 tablespoon all-purpose flour",
            "1 tablespoon unsalted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 12-cup muffin tin or line cups with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat 1/2 cup butter in a large bowl with an electric mixer until creamy. Gradually add sugar and continue beating until mixture is light and fluffy. Stir in vanilla seeds. Beat in eggs one at a time, beating well after each addition, scraping down the sides of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift 3 cups flour, baking power, baking soda, and salt together in a small bowl. Add flour mixture alternately with buttermilk to the batter, mixing just until blended. Fold in apricots.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups and sprinkle each muffin with about 1/4 teaspoon coarse sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend together marzipan, 1 tablespoon flour, and 1 tablespoon butter. Top each muffin with a dab of the mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tops spring back when lightly pressed, 25 to 30 minutes. Cool in the tin for 5 minutes. Transfer to a wire rack to cool completely.\n"
            }
        ],
