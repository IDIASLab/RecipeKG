        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26805/green-chili-and-corn-dip/",
        "name": "Green Chili and Corn Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F777118.jpg",
            "width": 2516,
            "height": 3838
