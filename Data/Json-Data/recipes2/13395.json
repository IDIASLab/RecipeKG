        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1\u2009\u00bd teaspoons onion powder",
            "1\u2009\u00bd teaspoons seasoned salt",
            "1\u2009\u00bd teaspoons garlic powder",
            "2 zucchini, thinly sliced lengthwise and 1/4-inch wide"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, onion powder, seasoned salt, and garlic powder together in a bowl; add zucchini and marinate for about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill zucchini slices on the preheated grill for about 10 minutes. Flip and grill until edges are crisp and grill marks are present, about 10 minutes more.\n"
            }
        ],
