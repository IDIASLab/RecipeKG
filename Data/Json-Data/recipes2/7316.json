        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare outdoor grill for medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 tablespoon morsels in each cone; top each with 1 tablespoon peanut butter. Divide banana slices among cones.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Loosely wrap each filled cone with aluminum foil. Place cones on side on grill and heat 5 minutes or until morsels melt, turning on other side after 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove wrapped cones from grill; let aluminum foil cool. Remove foil and top each with a serving of Reddi-wip.\n"
            }
