        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18266/cheesy-baked-cauliflower/",
        "name": "Cheesy Baked Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8116663.jpg",
