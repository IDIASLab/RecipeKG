        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/88547/candy-bar-popcorn/",
        "name": "Candy Bar Popcorn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1179437.jpg",
            "width": 2448,
