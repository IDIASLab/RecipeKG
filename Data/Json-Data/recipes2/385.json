        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235949/simple-and-delicious-kale-soup/",
        "name": "Simple and Delicious Kale Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1093788.jpg",
