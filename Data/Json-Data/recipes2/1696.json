        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228105/ham-cucumber-cups/",
        "name": "Ham Cucumber Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1117277.jpg",
            "width": 6912,
