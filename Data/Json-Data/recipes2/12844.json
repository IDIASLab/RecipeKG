        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261882/spring-sugar-cookie-bars/",
        "name": "Spring Sugar Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5443816.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2017-12-08T08:17:51.000Z",
        "description": "A sugar cookie bar crust with fruit and almonds scattered on top.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H19M",
        "totalTime": "P0DT0H39M",
        "recipeYield": "16 bars",
        "recipeIngredient": [
            "cooking spray",
            "1 (16.5 ounce) package refrigerated sugar cookie dough",
            "\u00bd cup almonds",
            "2\u2009\u00bd teaspoons apricot preserves",
            "2\u2009\u00bd teaspoons peach preserves",
            "2 teaspoons strawberry preserves",
            "\u00bd teaspoon honey",
            "\u00bd cup semisweet chocolate chips, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a 9x13-inch baking sheet lightly with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press cookie dough onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 8 to 10 minutes. Remove crust and let cool, about 5 minutes. Leave oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread almonds onto a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast in the preheated oven until almonds start to turn golden brown and become fragrant, 10 to 15 minutes. Let cool briefly and chop into desired size.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apricot preserves, peach preserves, strawberry preserves, and honey together in a bowl. Spread over the cookie dough in the pan. Spread almonds lightly over preserves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in a microwave-safe bowl in 15-second intervals, stirring after each melting, 1 to 3 minutes. Drizzle melted chocolate over almonds. Let chocolate set, about 5 minutes. Cut into bars.\n"
            }
        ],
