    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282077/gluten-free-lemon-bars/",
        "name": "Gluten-Free Lemon Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Gluten-Free Lemon Bars"
        },
        "datePublished": "2017-10-20T23:00:19.000Z",
        "description": "These gluten-free lemon bars are oh-so-delicious and have a strong, lemony, juicy taste!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "16 bars",
        "recipeIngredient": [
            "1 cup oat flour",
            "\u2153 cup agave syrup",
            "\u00bc cup coconut oil",
            "\u215b teaspoon salt",
            "\u00bd cup coconut sugar",
            "\u00bd cup lemon juice",
            "\u2153 cup oat flour",
            "3 large eggs",
            "1 large egg yolk",
            "2\u2009\u00bd tablespoons lemon zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease an 8-inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oat flour, agave, coconut oil, and salt for crust together in a bowl. Press into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the crust is baking, combine coconut sugar, lemon juice, oat flour, eggs, egg yolk, and lemon zest for filling until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from the oven and pour filling on top. Return to the oven and bake until set, 15 to 20 minutes longer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and cool on a wire rack, about 30 minutes. Cut into 16 bars.\n"
            }
        ],
