    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282836/buffalo-chicken-baked-ziti/",
        "name": "Buffalo Chicken Baked Ziti",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Buffalo Chicken Baked Ziti"
        },
        "datePublished": "2017-06-05T23:56:42.000Z",
        "description": "Creamy and cheesy Buffalo-flavored chicken baked ziti!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "12 ounces ziti pasta",
            "2 tablespoons salted butter",
            "1 tablespoon minced garlic",
            "2 tablespoons all-purpose flour",
            "2 cups heavy cream",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup Buffalo wing sauce (such as Frank's\u00ae Red Hot)",
            "1 (1 ounce) package ranch dressing mix",
            "\u00be pound cooked, shredded chicken breast meat",
            "1 cup shredded Cheddar cheese",
            "3 stalks green onions, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add ziti and cook, stirring occasionally, until tender yet firm to the bite, about 11 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the ziti is cooking, melt butter in a cast iron skillet over medium heat. Add garlic and cook until golden, about 2 minutes. Add flour and whisk to combine; cook for 1 minute. Add heavy cream and whisk until no clumps remain; bring to a simmer. Be careful not to curdle the cream--adjust temperature if necessary; simmer until thickened, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cream cheese; break up with a wooden spoon until combined and melted. Stir in Buffalo sauce and ranch seasoning until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain ziti and add to the skillet with shredded chicken; stir until coated. Top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until Cheddar is melted and golden, 15 to 20 minutes. Garnish with green onions and serve.\n"
            }
        ],
