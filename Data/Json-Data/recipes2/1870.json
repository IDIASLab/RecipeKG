        "name": "Microwave Corn-on-the-Cob in the Husk",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1325169.jpg",
            "width": 1771,
