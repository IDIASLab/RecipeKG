        "cookTime": null,
        "totalTime": "P0DT0H15M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 (5 ounce) can canned tuna   ",
            "\u00bc cup prepared basil pesto sauce",
            "6 oil-packed sun-dried tomatoes, drained and diced",
            "2 tablespoons mayonnaise",
            "2 tablespoons grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a bowl, mix the tuna, pesto, sun-dried tomatoes, mayonnaise, and Parmesan cheese. Cover, and refrigerate until ready to serve.\n"
            }
