        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00be cup dark chocolate chips",
            "6 ounces coffee-flavored low-fat yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chocolate chips in a microwave-safe glass measuring cup; heat in microwave, stirring every 10 seconds, until melted and smooth, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir yogurt into melted chocolate until icing is smooth.\n"
            }
