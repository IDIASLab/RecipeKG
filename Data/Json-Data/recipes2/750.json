    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236903/kansas-city-bbq-ribs/",
        "name": "Kansas City BBQ Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1127830.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2014-03-06T18:23:58.000Z",
        "description": "Get your napkins ready! These sumptuous ribs will be devoured by one and all. The overnight rest in the fridge lets the spice rub penetrate and really flavor these meaty ribs.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT11H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons salt",
            "1 tablespoon fennel seed",
            "3 tablespoons white sugar",
            "3 tablespoons paprika",
            "2 tablespoons chili powder",
            "4 teaspoons ground black pepper",
            "1 tablespoon onion powder",
            "1 tablespoon garlic powder",
            "6 pounds pork back ribs",
            "\u00be cup barbeque sauce",
            "3 tablespoons honey",
            "3 tablespoons cider vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grind salt and fennel seed together in a blender or coffee grinder. Stir fennel-salt mixture, sugar, paprika, chili powder, black pepper, onion powder, and garlic powder together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each pork slab into 3 pieces, about 5 ribs each. Pat ribs dry with a paper towel and rub spice mixture over each piece. Place ribs on a plate, cover with plastic wrap, and refrigerate, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow ribs to sit at room temperature for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill between 270 degrees F (132 degrees C) and 300 degrees F (149 degrees C) for indirect grilling and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill ribs on the preheated grill with the cover down, turning once, until no longer pink in the center, about 1 1/2 hours. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine barbeque sauce, honey, and cider vinegar in a small saucepan; bring to a boil. Reduce heat and simmer until sauce is thickened and smooth, about 10 minutes. Brush sauce onto ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook ribs for 15 minutes; flip, baste the other side, and cook until ribs are glazed and tender when pierced with a fork, about 15 minutes more. Let ribs stand for 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes"
        ],
