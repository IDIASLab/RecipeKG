        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245532/chocolate-peppermint-bark/",
        "name": "Chocolate Peppermint Bark",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3245096.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "2015-12-05T00:18:01.000Z",
        "description": "Chocolaty pepperminty goodness!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P0DT1H21M",
        "recipeYield": "1 sheet of bark",
        "recipeIngredient": [
            "1 cup chocolate chips",
            "\u00bd cup white chocolate chips",
            "\u00bd teaspoon vegetable oil",
            "\u00bd teaspoon peppermint extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in top of a double boiler over simmering water, stirring frequently and scraping down the sides with a rubber spatula to avoid scorching, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt white chocolate chips in top of a separate double boiler over simmering water, stirring frequently and scraping down the sides with a rubber spatula to avoid scorching, 3 to 5 minutes. Stir in vegetable oil and peppermint extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chocolate onto lined baking sheet; spread with spatula. Drizzle white chocolate over chocolate; swirl with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until bark is hardened, about 1 hour.\n"
            }
        ],
