        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 bagel, sliced in half",
            "\u00bd cup tomato sauce",
            "\u00bc cup shredded mozzarella cheese",
            "1 small onion, sliced into rings",
            "1 green bell pepper, chopped",
            "1 (8 ounce) package sliced pepperoni sausage"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spread bottom bagel half with tomato sauce; add cheese, onion, bell pepper and pepperoni. Place other half of bagel on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat toaster oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bagel in preheated toaster oven and toast about 10 minutes, or until bagel is golden brown.\n"
            }
