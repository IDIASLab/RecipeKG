        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235682/spicy-dessert-roll-ups/",
        "name": "Spicy Dessert Roll-Ups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1100681.jpg",
            "width": 3068,
