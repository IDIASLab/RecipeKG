        "name": "RAG&Uacute;&reg; No Boiling Beefy Baked Ziti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1094967.jpg",
            "width": 300,
