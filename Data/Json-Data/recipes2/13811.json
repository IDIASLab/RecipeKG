        "name": "Todd's Warm Shrimp Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4488209.jpg",
            "width": 2000,
            "height": 2000
