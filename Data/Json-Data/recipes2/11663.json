    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238198/magnificent-white-lasagna/",
        "name": "Magnificent White Lasagna",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1414735.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2017-09-28T13:01:15.000Z",
        "description": "One of my family's favorite recipes. I make this at least twice a month, if not more.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "12 lasagna noodles, or as needed",
            "2 cups shredded cooked chicken, or more to taste",
            "1 (14 ounce) can artichokes in water, drained ",
            "8 ounces shredded mozzarella cheese",
            "\u00be cup grated Parmesan cheese, divided, or to taste",
            "\u00bd cup sun-dried tomatoes packed in oil, drained and chopped",
            "2 (8 ounce) packages cream cheese, softened",
            "1 cup milk",
            "2 tablespoons dried basil, divided",
            "1 teaspoon garlic powder",
            "8 ounces mozzarella cheese, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook lasagna in the boiling water, stirring occasionally, until cooked through but firm to the bite, about 8 minutes; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix chicken, artichokes, shredded mozzarella cheese, 1/2 cup Parmesan cheese, and sun-dried tomatoes together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, milk, 1 tablespoon basil, and garlic powder together in a bowl until smooth. Mix 2/3 of the cream cheese mixture with the chicken mixture. Spread remaining cream cheese mixture into the bottom of a 9x13-inch casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread a single layer of lasagna noodles over the cream cheese mixture, top with 1/3 of the chicken mixture, and a layer of mozzarella cheese slices. Repeat layering with noodles, chicken mixture, and mozzarella slices 2 times. Top with remaining Parmesan cheese and basil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and bubbling, about 25 minutes.\n"
            }
        ],
