        "mainEntityOfPage": "https://www.allrecipes.com/recipe/64030/white-nacho-cheese-sauce/",
        "name": "White Nacho Cheese Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1056954.jpg",
