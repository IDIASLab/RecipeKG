    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238221/simple-italian-pasta-salad/",
        "name": "Simple Italian Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1203068.jpg",
            "width": 2884,
            "height": 2840
        },
        "datePublished": "2014-06-27T20:12:15.000Z",
        "description": "Perfect for barbecues or any get together. Customize with your favorite vegetables and cheeses. Serve chilled or at room temperature. You can use any assortment of diced vegetables. You can substitute mozzarella cheese for the Parmesan cheese.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (16 ounce) package rotelle pasta",
            "1 cup broccoli florets",
            "1 cup chopped carrots",
            "1 cup chopped tomatoes",
            "\u00be cup grated Parmesan cheese",
            "1 (6 ounce) can black olives, drained",
            "1 (6 ounce) package pepperoni",
            "1 cup Italian-style salad dressing (such as Olive Garden\u00ae), divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook rotelle in the boiling water, stirring occasionally, until cooked through but firm to the bite, about 12 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir rotelle, broccoli, carrots, tomatoes, Parmesan cheese, olives, and pepperoni together in a large bowl. Pour 1/2 of the dressing over the salad and toss to coat. Cover the bowl with plastic wrap and refrigerate, 8 hours to overnight. Pour remaining dressing over the salad and stir.\n"
            }
        ],
