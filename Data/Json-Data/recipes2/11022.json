    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/155982/sweet-potato-patties/",
        "name": "Sweet Potato Patties",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F892680.jpg",
            "width": 2304,
            "height": 1728
        },
        "datePublished": "2017-07-13T13:01:12.000Z",
        "description": "This recipe gives a little something different to do with sweet potatoes. My boyfriend loves these.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H6M",
        "totalTime": "P0DT0H26M",
        "recipeYield": "4 patties",
        "recipeIngredient": [
            "2 sweet potatoes, peeled and shredded",
            "1 small sweet onion, chopped",
            "\u00bc cup flaked coconut",
            "2 tablespoons flour",
            "2 tablespoons semolina",
            "3 tablespoons maple syrup",
            "2 egg, beaten",
            "1 (6 ounce) container plain yogurt",
            "2 tablespoons half and half",
            "\u00bc cup pineapple juice",
            "\u00bc teaspoon cayenne pepper",
            "\u00bc cup oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together the sweet potatoes, onion, coconut, flour, semolina, and syrup in a large bowl. Mix in the eggs.  Form the mixture into 4 patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together the yogurt, half and half, pineapple juice, and cayenne pepper in a separate bowl. Set aside until serving.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a skillet over medium-high heat. Cook the patties in the hot oil until crisp, 3 to 5 minutes per side. Transfer to plate lined with paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve patties hot with yogurt mixture on the side.\n"
            }
        ],
