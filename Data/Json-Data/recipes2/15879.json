        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26727/cheese-ball-iii/",
        "name": "Cheese Ball III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2830637.jpg",
