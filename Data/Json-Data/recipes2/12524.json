        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220534/irish-cream/",
        "name": "Irish Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4498537.jpg",
            "width": 2087,
            "height": 3024
        },
        "datePublished": "2017-11-09T09:02:21.000Z",
        "description": "This is a recipe I have made for years and everyone seems to love it.",
        "prepTime": "P0DT0H5M",
        "cookTime": null,
        "totalTime": "P0DT8H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "3 eggs",
            "1 teaspoon instant coffee granules",
            "1 teaspoon chocolate syrup (such as Hershey's\u00ae)",
            "\u00be teaspoon almond extract",
            "1 (14 ounce) can sweetened condensed milk",
            "1 cup heavy whipping cream",
            "1\u2009\u00bd cups whiskey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend eggs, coffee, chocolate syrup, and almond extract in a blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sweetened condensed milk and whipping cream into the egg mixture; blend again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour whiskey into the blender; blend until smooth. Chill overnight or 8 hours before using.\n"
            }
        ],
