    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22687/homemade-italian-sauce/",
        "name": "Homemade Italian Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F35659.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2000-05-23T19:50:46.000Z",
        "description": "Sauce like mom used to make.  Serve over your favorite pasta.  Classic red sauce with onion, mushrooms and garlic.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (16 ounce) can whole tomatoes",
            "5 cloves garlic, peeled",
            "3 tablespoons olive oil",
            "1 white onion, chopped",
            "1 pound mushrooms, sliced",
            "1 (16 ounce) can crushed tomatoes",
            "8 ounces tomato paste",
            "1 green bell pepper, chopped",
            "2 tablespoons dried oregano",
            "1 tablespoon dried basil leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a blender chop the whole tomatoes with juice and garlic until chunky.  In a medium size pot, heat oil on medium high heat. Put onion and mushroom in and saute for about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place in pot the blended tomatoes, crushed tomatoes, tomato paste, chopped green peppers, oregano and basil.  Bring to boil, then lower to medium low, cover and stir periodically.  Cook and reduce about 1 to 2 hours or to your liking.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces",
            "Tomato"
        ],
