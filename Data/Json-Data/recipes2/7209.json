    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244788/healthy-truffles/",
        "name": "Healthy Truffles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8186123.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2016-06-04T14:16:11.000Z",
        "description": "I love this recipe, it's way healthier than regular truffles and the delicious combo of peanut butter and chocolate is a total winner!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H43M",
        "recipeYield": "25 truffles",
        "recipeIngredient": [
            "\u00bd cup smooth peanut butter",
            "\u00bc cup cocoa powder",
            "\u00bc cup honey",
            "\u00bc cup coconut oil",
            "1 teaspoon butter",
            "1 teaspoon vanilla bean paste",
            "\u00bc cup unsweetened coconut flakes, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine peanut butter, cocoa powder, honey, coconut oil, butter, and vanilla bean paste in a saucepan over low heat; cook, stirring constantly, until melted and smooth, 3 to 5 minutes. Remove saucepan from heat and transfer peanut butter mixture to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate mixture, stirring every 5 minutes, until truffle mixture is firm yet sticky and soft enough to shape, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour coconut flakes into a bowl. Drop truffle mixture, about 1 teaspoon per truffle, into coconut and roll to coat. Refrigerate until firm, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Truffle Recipes"
        ],
