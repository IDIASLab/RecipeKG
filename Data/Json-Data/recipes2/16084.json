        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32349/caribbean-rum-punch/",
        "name": "Caribbean Rum Punch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F124756.jpg",
            "width": 250,
