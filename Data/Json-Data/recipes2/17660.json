        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71554/skillet-apple-chicken/",
        "name": "Skillet Apple Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4208.jpg",
