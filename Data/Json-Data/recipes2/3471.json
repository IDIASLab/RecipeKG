    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149740/worcestershire-ribs/",
        "name": "Worcestershire Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4535813.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2015-07-24T06:01:49.000Z",
        "description": "Enjoy these spicy, dark ribs!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bc pounds pork spareribs",
            "\u00bd cup tomato paste",
            "\u00bd cup Worcestershire sauce",
            "2 teaspoons hot pepper sauce (e.g. Tabasco\u2122)",
            "\u00bc cup golden syrup",
            "1 sweet red pepper, seeded and cut into thin strips",
            "1 onion, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover a roasting pan with aluminum foil. Place the ribs into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 10 minutes, turn, and cook 10 minutes more. Remove from oven, and drain off fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, stir together the tomato paste, Worcestershire sauce, hot pepper sauce, and golden syrup in a bowl. Drizzle sauce over the ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return ribs to oven, and bake 30 minutes more. Remove from oven, and spread the red pepper and onions over the ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return ribs to oven, and bake 10 minutes more.\n"
            }
        ],
