    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241053/lemon-crumb-bars/",
        "name": "Lemon Crumb Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4586760.jpg",
            "width": 1936,
            "height": 1945
        },
        "datePublished": "2015-01-24T00:55:18.000Z",
        "description": "Treat your family to these mouth-watering lemon crumb bars that are the perfect combination of sweet & tangy!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "24 - 36 bars",
        "recipeIngredient": [
            "1 (18.25 ounce) package lemon or yellow cake mix",
            "\u00bd cup butter, softened",
            "1 egg",
            "2 cups crushed saltine crackers",
            "3 egg yolks",
            "1 (14 ounce) can sweetened condensed milk",
            "\u00bd cup ReaLemon\u00ae lemon juice from concentrate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Beat cake mix, butter and 1 egg in large bowl with mixer until crumbly. Stir in saltine crumbs.  Set aside 2 cups of crumb mixture. Press remaining crumb mixture on bottom of greased 13x9-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 15 to 20 minutes or until lightly golden. Beat 3 egg yolks, condensed milk and ReaLemon&reg; with mixer or wire whisk. Spread over prepared crust. Top with reserved crumb mixture. Bake 25 minutes longer or until set and top is golden. Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate within 2 hours. Cut into bars. Store leftovers covered in refrigerator.\n"
            }
        ],
