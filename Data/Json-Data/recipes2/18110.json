        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/77426/easy-homestyle-green-beans/",
        "name": "Easy Homestyle Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3298513.jpg",
