        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "10 ounces cream cheese, softened",
            "1 cup milk, or more to taste",
            "1 cup sour cream",
            "\u00bd cup mayonnaise",
            "3 cloves garlic, pressed",
            "1 pinch ground black pepper, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese, milk, sour cream, mayonnaise, garlic, and ground black pepper together in a bowl with an electric mixer.\n"
            }
