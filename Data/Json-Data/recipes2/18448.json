        "mainEntityOfPage": "https://www.allrecipes.com/recipe/83858/spicy-ranch-dressing/",
        "name": "Spicy Ranch Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1271937.jpg",
