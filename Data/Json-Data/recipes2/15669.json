        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25695/cauliflower-cheese-soup-ii/",
        "name": "Cauliflower Cheese Soup II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4777641.jpg",
            "width": 960,
            "height": 960
