        "name": "Broiled Nectarines with Blue Cheese and Balsamic Reduction",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8359512.jpg",
            "width": 0,
