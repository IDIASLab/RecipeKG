    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257594/sloppy-joes-from-ball-park-buns/",
        "name": "Sloppy Joes from Ball Park&reg; Buns",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4498819.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-03-21T04:57:51.000Z",
        "description": "We're leveling up the traditional sloppy joe with the addition of ranch coleslaw for a symphony of messy deliciousness. Yeah, you're gonna need a bigger napkin.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 Ball Park\u00ae Tailgaters Gourmet Buns",
            "1 pound lean ground beef",
            "1 yellow onion, diced",
            "1 tablespoon chopped garlic",
            "1 small carrot, diced",
            "1 small red bell pepper, diced",
            "2 cups tomato sauce",
            "\u00bc cup ketchup",
            "1 tablespoon Worcestershire sauce",
            "1 tablespoon apple cider vinegar",
            "1 tablespoon brown sugar",
            "\u00bc teaspoon cayenne pepper",
            "salt and pepper to taste",
            "10 slices dill pickle slices",
            "1 cup cabbage slaw mix",
            "3 tablespoons ranch dressing",
            "\u00bd teaspoon apple cider vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, saute beef on medium heat until brown and cooked through.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add in onions, garlic, carrots and red bell peppers and cook for 8 to 10 minutes until vegetables are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While vegetables are cooking, combine tomato sauce, ketchup, Worcestershire sauce, vinegar, brown sugar and cayenne pepper in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the tomato sauce mixture to saute pan and simmer for 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make the ranch coleslaw, combine ranch dressing and vinegar in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in slaw mix and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast buns in oven until golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pickles, if desired, on bottom bun. Add sloppy joe mix and top with ranch coleslaw, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Ball Park Buns"
        ],
