        "name": "Dogs 'n' Beans Rice Bowl",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2317525.jpg",
            "width": 2205,
