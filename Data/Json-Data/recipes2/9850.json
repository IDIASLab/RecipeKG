    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257750/dougs-garlic-shrimp-alfredo/",
        "name": "Doug's Garlic Shrimp Alfredo",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Doug's Garlic Shrimp Alfredo"
        },
        "datePublished": "2017-04-13T02:32:43.000Z",
        "description": "A twist on the traditional shrimp Alfredo with a great tangy kick! You can also sneak some veggies into this if you have a picky eater at home! Serve with Parmesan cheese if desired.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H32M",
        "totalTime": "P0DT0H57M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "1 pint half-and-half",
            "\u00bd cup 2% milk ",
            "2 (1.6 oz) packages dry Alfredo sauce mix",
            "2 (3 ounce) packages sun-dried tomatoes, chopped",
            "1 (6 ounce) package mushrooms, chopped",
            "1 tablespoon minced garlic",
            "1 tablespoon ground black pepper",
            "\u00bd (16 ounce) box fettuccine",
            "1 pound raw shrimp, peeled and deveined ",
            "2 tablespoons lime juice, or to taste",
            "1 tablespoon chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a saucepan over medium heat. Whisk in half-and-half, milk, and Alfredo sauce mix. Bring to a boil, stirring occasionally. Reduce heat to low; stir in sun-dried tomatoes, mushrooms, and garlic. Cook and stir until flavors combine, about 10 minutes. Cover and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil. Cook fettuccine until tender yet firm to the bite, about 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss shrimp with lime juice in a large bowl. Cook on the hot grill until shrimp turn pink and start to blacken, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sauce, fettuccine, and shrimp in a large bowl; mix well. Garnish with parsley.\n"
            }
        ],
