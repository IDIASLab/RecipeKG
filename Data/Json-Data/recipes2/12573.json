    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260015/bloody-skeleton-smores/",
        "name": "Bloody Skeleton S'mores",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4553224.jpg",
            "width": 2000,
            "height": 1333
        },
        "datePublished": "2017-11-11T03:26:21.000Z",
        "description": "These spooky s'mores are easy to make and will go down as a favorite treat at your Halloween party.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT0H21M",
        "recipeYield": "6 s'mores",
        "recipeIngredient": [
            "12 honey graham crackers",
            "1 (1 ounce) tube black decorating gel (such as Wilton Icing Writer\u00ae)",
            "15 (1 ounce) squares dark chocolate (such as Hershey's\u00ae)",
            "66 miniature marshmallows",
            "6 skull-shaped candy decorations (such as Wilton\u00ae Icing Decorations)",
            "1 tablespoon red cookie icing (such as Wilton\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut 2 small triangles from the top corners of a rectangular graham cracker using sharp kitchen scissors. Cut 2 long, thin triangles from the bottom of the graham cracker to create a coffin shape. Repeat with remaining graham crackers using the first one as a template.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lay 6 graham crackers on a plate. Draw crosses and the letters \"RIP\" on the center of each coffin lid using the black decorating gel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Break 3 dark chocolate squares in half. Leave remaining 12 squares whole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lay remaining 6 graham crackers on a baking sheet. Place 2 1/2 squares of chocolate on each, leaving room on for the skull. Arrange 11 miniature marshmallows on top of the chocolate in the shape of a skeleton: 4 for arms, 5 for the body, and 2 for legs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set oven rack about 6 inches from the heat source and preheat the oven's broiler on the low setting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place graham cracker coffins under the broiler until marshmallows bring to color, 10 to 20 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place skull candies above the marshmallow skeletons. Drizzle red icing over the s'mores. Sandwich the s'mores with the decorated graham cracker coffin lids.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "Dark Chocolate"
        ],
