    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13822/apple-butter-iii/",
        "name": "Apple Butter III",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Butter III"
        },
        "datePublished": "1999-11-07T02:13:59.000Z",
        "description": "This sugarless apple butter recipe is made flavorful with dried apples, apple juice and spices. It will also last for months if refrigerated in a jar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 quarts",
        "recipeIngredient": [
            "1 pound dried, unsulfured apples",
            "\u00bd gallon sugar-free organic apple juice",
            "\u00bc teaspoon ground cloves",
            "2 teaspoons ground allspice",
            "4 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a Dutch oven, combine the dried apples, apple juice, clove, allspice and cinnamon; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring to a boil, cover and simmer for 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mixture in blender and puree until smooth. Place in jars and refrigerate.\n"
            }
        ],
