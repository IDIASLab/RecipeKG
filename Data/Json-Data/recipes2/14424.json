        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21268/crab-and-pea-salad/",
        "name": "Crab and Pea Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8236326.jpg",
