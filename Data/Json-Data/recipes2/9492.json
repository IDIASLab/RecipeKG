        "recipeIngredient": [
            "6 kiwis, peeled and diced",
            "1 small onion, diced",
            "1 jalapeno pepper, diced",
            "2 tablespoons lime juice",
            "1 tablespoon olive oil",
            "1 teaspoon honey",
            "\u00bd teaspoon cumin",
            "\u00bd teaspoon curry powder"
