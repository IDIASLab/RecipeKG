        "totalTime": "P0DT12H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "8 fresh strawberries, hulled and quartered",
            "\u00bc cup plain yogurt",
            "\u00bc cup apple juice",
            "2 tablespoons honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend strawberries, yogurt, apple juice, and honey in a blender until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour blended mixture into ice pop molds; freeze until solid, about 12 hours.\n"
            }
