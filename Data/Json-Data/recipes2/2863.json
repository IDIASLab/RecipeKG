    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279466/broiled-salmon-with-brown-sugar/",
        "name": "Broiled Salmon with Brown Sugar",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9321503.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-03-26T20:38:20.000Z",
        "description": "This is one of my favorite broiled salmon with brown sugar recipes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup brown sugar",
            "2 tablespoons low-sodium soy sauce",
            "1 tablespoon olive oil",
            "1 large red onion, cut into 1/4-inch thick wedges",
            "1 pinch kosher salt and ground black pepper to taste",
            "4 (5 ounce) skinless salmon fillets"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar and soy sauce in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a pan over medium-high heat. Saute onion, salt, and pepper in the hot oil until soft, 5 to 7 minutes. Transfer to sugar mixture and mix together. Place salmon fillets on a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook salmon in the preheated oven for 10 minutes. Turn over and drizzle with 1/2 of the sugar mixture. Continue to broil until salmon is opaque throughout and easily flaked with a fork, 5 to 10 minutes more. Serve with remaining sugar mixture.\n"
            }
        ],
