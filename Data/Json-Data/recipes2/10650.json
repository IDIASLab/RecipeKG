    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283884/spiced-tomato-jam/",
        "name": "Spiced Tomato Jam",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spiced Tomato Jam"
        },
        "datePublished": "2017-06-22T00:53:23.000Z",
        "description": "Serve this savory spiced tomato jam on your favorite burger, grilled cheese, or as an appetizer on crusty bread. Store remaining jam in the fridge for up to one week.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT13H20M",
        "recipeYield": "1 3/4 cups",
        "recipeIngredient": [
            "2 pounds tomatoes, diced",
            "1 red onion, finely chopped",
            "1 cup brown sugar",
            "1\u2009\u00bd teaspoons red pepper flakes",
            "1 teaspoon ground cumin",
            "1 teaspoon ground coriander",
            "1 teaspoon salt",
            "1 lemon, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Inspect jars for cracks and rings for rust, discarding any defective ones. Immerse in simmering water until jam is ready. Wash new, unused lids and rings in warm soapy water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, onion, brown sugar, pepper flakes, cumin, coriander, salt, and lemon juice in a saucepan. Bring to a boil over medium-high heat. Cook, stirring frequently, about 15 minutes. Reduce heat to medium and simmer until reduced and it reaches a jam-like consistency, 20 to 30 minutes. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack jam into hot, sterilized jars, filling to within 1/4 inch of the top. Run a clean knife or thin spatula around the insides of the jars to remove any air bubbles. Wipe rims with a moist paper towel to remove any residue. Top with lids and screw rings on tightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a rack in the bottom of a large stockpot and fill halfway with water. Bring to a boil and lower jars 2 inches apart into the boiling water using a holder. Pour in more boiling water to cover jars by at least 1 inch. Bring to a rolling boil, cover, and process for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the jars from the stockpot and let rest, several inches apart, for 12 to 24 hours. Press the center of each lid with a finger to ensure the lid does not move up or down. Remove the rings for storage and store in a cool, dark area.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes"
        ],
