        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23144/no-bake-pumpkin-pie-ii/",
        "name": "No Bake Pumpkin Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F132594.jpg",
