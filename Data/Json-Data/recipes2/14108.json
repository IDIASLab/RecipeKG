    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19508/shrimp-scampi/",
        "name": "Shrimp Scampi",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F734514.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2000-06-04T17:14:12.000Z",
        "description": "A simple but elegant shrimp dish. Combine it with garlic bread and a salad for a delicious dinner.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package angel hair pasta",
            "\u00bd cup butter",
            "4 cloves minced garlic",
            "1 pound shrimp, peeled and deveined",
            "1 cup dry white wine",
            "\u00bc teaspoon ground black pepper",
            "\u00be cup grated Parmesan cheese",
            "1 tablespoon chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil. Stir in pasta and return pot to boil. Cook until al dente. Drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large saucepan over medium heat. Stir in garlic and shrimp. Cook, stirring constantly, for 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in wine and pepper. Bring to a boil and cook for 30 seconds while stirring constantly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix shrimp with drained pasta in a serving bowl. Sprinkle with cheese and parsley. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Shrimp",
            "Shrimp Scampi"
        ],
