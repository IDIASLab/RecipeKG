    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256526/best-ever-fried-chicken-sandwiches/",
        "name": "Best-Ever Fried Chicken Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4341886.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2016-12-14T08:48:16.000Z",
        "description": "These chicken sandwiches are inspired by Federal Donuts' sandwich, with a sriracha-mayo sauce and twice-fried chicken.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 sandwiches",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour, divided",
            "1 egg",
            "\u00bc cup ranch dressing mix (such as Hidden Valley\u00ae)",
            "2 chicken breasts",
            "1 cup vegetable oil",
            "\u00bd cup mayonnaise",
            "\u00bc cup sriracha sauce",
            "4 potato rolls",
            "16 dill pickle chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1/2 cup flour in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg in a shallow bowl with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 cup flour in a bowl; mix in ranch dressing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Split chicken breasts to create 4 pieces. Coat each piece with plain flour, dip in egg, and coat with flour-ranch mixture. Place on a wire rack set over a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saucepan until almost boiling. Add 2 pieces chicken; fry until lightly golden, turning once, about 5 minutes. Return chicken to the wire rack. Repeat with remaining 2 pieces chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken in the preheated oven until juices run clear, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mayonnaise and sriracha sauce together in a small bowl. Spread on rolls. Place 4 pickle slices on each roll. Place 1 piece of chicken on each sandwich.\n"
            }
        ],
