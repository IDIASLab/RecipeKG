        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221072/fresh-and-crisp-cucumber-salad/",
        "name": "Fresh and Crisp Cucumber Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F967317.jpg",
