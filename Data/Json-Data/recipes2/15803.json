        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26271/hawaiian-style-pasta/",
        "name": "Hawaiian Style Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2107154.jpg",
