    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255914/pea-pesto-open-faced-sandwiches/",
        "name": "Pea Pesto Open-Faced Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4068440.jpg",
            "width": 3456,
            "height": 2304
        },
        "datePublished": "2016-10-25T02:40:25.000Z",
        "description": "Pea and basil pesto spread over toasted bread slices is topped with a fried egg--great for breakfast, lunch, or dinner.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd cup fresh basil",
            "1 cup peas, fresh or frozen (thawed)",
            "1 clove garlic, smashed",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bd cup extra virgin olive oil",
            "4 Eggland's Best Eggs, large",
            "4 slices toasted bread"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a food processor, combine basil, peas, garlic, salt, pepper, and extra virgin olive oil. Process until smooth and blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast bread in oven (350 degrees F) or toaster for about 3-4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small non-stick saute pan, over medium heat, break Eggland's Best egg onto surface, carefully.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry until transparent membrane turns to white, and yolk is cooked.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove and repeat for each egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread about one tablespoon of pesto atop each piece of toast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently lay each egg atop the pesto.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with extra salt, pepper, and extra virgin olive oil (optional).\n"
            }
        ],
