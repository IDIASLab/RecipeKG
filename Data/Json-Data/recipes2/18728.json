        "mainEntityOfPage": "https://www.allrecipes.com/recipe/87263/cheesy-french-toast/",
        "name": "Cheesy French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F881136.jpg",
