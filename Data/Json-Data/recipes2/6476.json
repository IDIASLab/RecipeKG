        "recipeIngredient": [
            "1 cup oats",
            "1 cup coconut flakes",
            "\u00bd cup honey",
            "\u00bd cup ground flax seeds",
            "\u00bd cup hemp seed hearts",
            "\u00bd cup chocolate chips",
            "\u00bd cup almond butter",
            "\u00bd teaspoon vanilla extract"
