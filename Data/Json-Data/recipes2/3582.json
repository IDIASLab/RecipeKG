        "name": "Creamy Pesto Chicken and Bow Ties",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F979067.jpg",
            "width": 250,
