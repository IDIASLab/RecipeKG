    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257032/maple-almond-energy-bites/",
        "name": "Maple Almond Energy Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4473970.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-01-31T05:59:43.000Z",
        "description": "Oats, almond butter, maple syrup, and almonds make quick and delicious energy bites for days when you're on the go.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "2\u2009\u00bc cups Quaker\u00ae Oats (Quick or Old Fashioned, uncooked)",
            "3 tablespoons almond flour or oat flour",
            "\u00bc cup almond butter",
            "3 tablespoons maple syrup",
            "2 tablespoons apple juice",
            "2 tablespoons water",
            "1 tablespoon sliced almonds, chopped, toasted if desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place oats and almond flour in large bowl; stir to blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add combined almond butter and syrup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir until ingredients are well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add apple juice and stir to combine ingredients, adding water as needed for shaping consistency. If desired, add salt to taste preference.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into 24 (about 1-inch diameter) balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread chopped almonds on waxed paper in single layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently press top of each ball into almonds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate, covered, until chilled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Store leftovers in refrigerator, covered.\n"
            }
        ],
