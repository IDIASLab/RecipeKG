        "mainEntityOfPage": "https://www.allrecipes.com/recipe/149345/crunchy-french-toast-sticks/",
        "name": "Crunchy French Toast Sticks",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F229523.jpg",
