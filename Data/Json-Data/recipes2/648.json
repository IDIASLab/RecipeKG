    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282495/moms-broccoli-cheese-soup/",
        "name": "Mom's Broccoli Cheese Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mom's Broccoli Cheese Soup"
        },
        "datePublished": "2014-02-10T20:24:06.000Z",
        "description": "Mom's broccoli cheese soup is my favorite! My mother has been making this soup for years. It is the only way I can get my son to eat broccoli!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 (14.5 ounce) cans chicken broth",
            "1 head broccoli, chopped",
            "1 head broccoli, finely chopped",
            "\u00bd cup shredded carrots",
            "6 tablespoons salted butter, cubed",
            "1 medium onion, thinly sliced",
            "6 tablespoons all-purpose flour, or as needed",
            "2 cups milk",
            "\u00bd (8 ounce) package processed cheese food (such as Velveeta\u00ae), cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring chicken broth to a simmer in a large pot. Add both heads of broccoli and carrots to the simmering stock; simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt butter in a large skillet over medium heat. Add onion and saute until softened, about 5 minutes. Stir in flour, 1 tablespoon at a time. Cook and stir until a golden roux forms, about 2 minutes. Slowly add milk, 1/2 cup at a time, stirring continuously. Bring to a boil and cook until a thickened gravy forms, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly pour onion gravy into the broccoli mixture, stirring continuously. Bring to a simmer. Stir in processed cheese food. Simmer, stirring often, until cheese melts, about 5 minutes. Do not let the soup come to a boil or the cheese could separate.\n"
            }
        ],
