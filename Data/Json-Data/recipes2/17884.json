        "mainEntityOfPage": "https://www.allrecipes.com/recipe/74054/spinach-cantaloupe-salad-with-mint/",
        "name": "Spinach Cantaloupe Salad with Mint",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F687691.jpg",
            "width": 4288,
            "height": 2848
