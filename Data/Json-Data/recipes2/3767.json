        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232414/the-girls-chicken-sandwiches/",
        "name": "The Girls' Chicken Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1010178.jpg",
