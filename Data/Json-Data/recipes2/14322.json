        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20753/incredible-punch/",
        "name": "Incredible Punch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F16286.jpg",
