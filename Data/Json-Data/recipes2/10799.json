    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/237507/audreys-taco-sauce/",
        "name": "Audrey's Taco Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1132381.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2017-06-22T13:02:32.000Z",
        "description": "A great, quick, and easy taco sauce! Excellent spooned onto fish or chicken tacos, or added into taco salads or rice bowls. Serve chilled.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "\u00bd cup reduced-fat sour cream, or more to taste",
            "\u00bd cup reduced-fat mayonnaise",
            "\u00bd lime, or more to taste, juiced",
            "\u00bd teaspoon ground cumin",
            "\u00bd teaspoon ground coriander",
            "\u00bd teaspoon dried dill weed",
            "\u00bd teaspoon dried oregano",
            "\u00bc teaspoon chipotle chile powder",
            "1 jalapeno pepper, seeded and minced",
            "1 tablespoon fresh cilantro, chopped",
            "\u00bc teaspoon minced garlic"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk sour cream and mayonnaise together in a bowl until smooth; add lime juice and whisk until again smooth and pourable, though still creamy, adding lime juice or sour cream if the mixture needs to be thinner or thicker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cumin, coriander, dill, oregano, and chipotle chile powder through the sour cream mixture. Fold jalapeno pepper, cilantro, and garlic through the mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the bowl with plastic wrap and refrigerate until the seasonings disseminate throughout the sauce, at least 1 hour.\n"
            }
        ],
