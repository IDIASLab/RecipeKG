        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16536/perfect-porterhouse-steak/",
        "name": "Perfect Porterhouse Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3571410.jpg",
            "width": 2529,
            "height": 2529
