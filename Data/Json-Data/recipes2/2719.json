    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241584/fiesta-chicken-burritos/",
        "name": "Fiesta Chicken Burritos",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2140245.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2015-03-05T18:52:13.000Z",
        "description": "Saucy, spicy chicken burritos are easy to assemble and quick to bake.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H28M",
        "totalTime": "P0DT0H38M",
        "recipeYield": "4 burritos",
        "recipeIngredient": [
            "1 pound skinless boneless chicken breasts",
            "2 teaspoons Gay Lea Butter",
            "1 pinch Pinch salt",
            "1 pinch Pinch pepper",
            "2 cups shredded Ivanhoe Old Cheddar",
            "\u2154 cup Gay Lea Sour Cream",
            "1 cup diced green pepper",
            "\u2153 cup salsa",
            "\u00bd cup sliced black olives",
            "1 jalapeno pepper, seeded and minced",
            "2 tablespoons chopped fresh cilantro",
            "4 large flour tortillas"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut chicken into small 1-inch (2.5cm) cubes. In skillet, melt butter over medium high heat and cook chicken for about 8 minutes or until no longer pink inside. Salt and pepper to taste. Pour into large bowl and stir in 1/2 cup (375mL) of the cheese, sour cream, green pepper, salsa, olives, jalapeno, and cilantro.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture among tortillas: Fold and roll to enclose filling completely; place seam side down in small baking dish. Sprinkle with remaining cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in centre of oven for about 20 minutes or until cheese is melted and tortillas are crispy.\n"
            }
        ],
