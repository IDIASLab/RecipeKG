    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16626/hurricanes-chicken-casserole/",
        "name": "Hurricane's Chicken Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F688804.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-17T08:03:44.000Z",
        "description": "Fried onion rings top this creamy chicken, pea and carrot casserole; bread stuffing flavors the bottom. Are you ready for a 'Hurricane' in your kitchen?!",
        "prepTime": "P0DT0H40M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "1 (6 ounce) package herb-seasoned dry bread stuffing mix",
            "1 (16 ounce) package chicken tenderloins",
            "1 (10.75 ounce) can  condensed cream of celery soup",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (15 ounce) can peas, drained",
            "1 (16 ounce) can carrots, drained",
            "1 (2.8 ounce) can French fried onions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare stuffing according to package directions; line bottom of a 9x13 inch baking dish with prepared stuffing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare chicken tenders according to package directions, then cut into bite size pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine cream of celery soup, cream of chicken soup, peas, carrots and chicken pieces. Mix well and spread mixture on top of stuffing, then sprinkle with fried onions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 15 to 20 minutes, or until heated through.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Casserole Recipes"
        ],
