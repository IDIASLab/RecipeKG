    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284703/grilled-chicken-thighs/",
        "name": "Grilled Chicken Thighs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5044726.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2018-01-24T03:47:56.000Z",
        "description": "My family's favorite chicken recipe. Easy to prepare and easy to grill.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "4 pounds boneless, skinless chicken thighs",
            "\u00be cup soy sauce",
            "\u00be cup orange juice",
            "\u2153 cup olive oil",
            "\u2153 cup ketchup",
            "\u2153 cup chopped fresh parsley",
            "3 cloves garlic, minced",
            "3 teaspoons lemon juice",
            "1\u2009\u00bd teaspoons dried oregano",
            "1\u2009\u00bd teaspoons ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rinse chicken thighs and pat dry between paper towels. Transfer to a 1-gallon freezer bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine soy sauce, orange juice, olive oil, ketchup, parsley, garlic, lemon juice, oregano, and pepper in a large bowl until well combined. Pour marinade over chicken thighs in the freezer bag. Squeeze out excess air, seal the bag, and marinate in the refrigerator for at least 1 hour, up to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken thighs from the marinade and shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill chicken in a single layer until no longer pink in the center, 5 to 7 minutes per side. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
