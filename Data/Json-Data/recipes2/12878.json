        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238541/white-chocolate-bugles/",
        "name": "White Chocolate Bugles&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1348567.jpg",
