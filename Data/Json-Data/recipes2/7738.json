        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254892/turkey-stuffing-soup/",
        "name": "Turkey Stuffing Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7551049.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2016-08-05T04:06:57.000Z",
        "description": "This is a Thanksgiving tradition in our family. I've never seen a similar recipe anywhere else!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H25M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "3 ribs celery, sliced",
            "3 carrots, thinly sliced",
            "1 large onion, chopped",
            "3 cups chopped cooked turkey",
            "2 cups leftover stuffing",
            "2 large tomatoes, peeled and chopped",
            "3 sprigs fresh parsley, minced",
            "6 cups turkey stock",
            "1\u2009\u00bd cups egg noodles"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a stockpot over low heat. Add celery, carrots, onion; cook and stir until tender, 8 to 10 minutes. Stir in turkey, stuffing, tomatoes, and parsley. Pour in turkey stock. Bring soup to a boil; reduce heat and simmer, partially covered, until flavors combine, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook egg noodles in the boiling water, stirring occasionally until tender yet firm to the bite, 7 to 9 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop hot noodles into deep serving bowls; ladle soup on top.\n"
            }
        ],
