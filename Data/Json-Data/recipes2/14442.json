    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21349/chocolate-picnic-cake/",
        "name": "Chocolate Picnic Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F34440.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2000-02-12T15:44:00.000Z",
        "description": "Cake travels well. No cooler needed. When out of oven... it is all done!  Enjoy and God Bless!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (5 ounce) package non-instant chocolate pudding mix",
            "1 (18.25 ounce) package chocolate cake mix",
            "1 cup semisweet chocolate chips",
            "1\u2009\u00bd cups chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour a 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, prepare the pudding according to the instructions on the box.  Remove from heat.  Mix cake mix into the hot pudding and stir until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared pan, and sprinkle with the chocolate chips and walnuts.  Bake in the preheated oven for 25 minutes, or until a toothpick inserted into the center of the cake comes out clean. Allow to cool before serving.\n"
            }
        ],
