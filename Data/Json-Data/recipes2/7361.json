    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254325/smoky-baked-chicken/",
        "name": "Smoky Baked Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3697323.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2016-06-22T23:52:34.000Z",
        "description": "This dish is like Alice Springs Chicken on steroids. Mixing everything together gives it a much better, juicier flavor. I like to pair it with steamed veggies.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H18M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "4 skinless, boneless chicken breasts",
            "2 teaspoons mesquite seasoning",
            "\u00bd (8 ounce) package shredded Mexican cheese blend",
            "\u2153 cup real bacon bits",
            "\u2153 cup sliced green onions",
            "\u00bc cup mayonnaise",
            "1\u2009\u00bd teaspoons chipotle-flavored hot sauce (such as Tabasco\u00ae Chipotle Pepper Sauce)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Line a baking dish with aluminum foil; brush with olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season chicken breasts with mesquite seasoning and place in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cheese blend, bacon bits, green onions, mayonnaise, and chipotle-flavored hot sauce together in a small bowl. Spread mixture evenly over chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink in the center and an instant-read thermometer inserted into the center reads at least 165 degrees F (74 degrees C), about 16 minutes. Turn off oven; leave chicken inside for 2 minutes. Turn on broiler; broil chicken on low until topping is golden, 2 to 3 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
