        "recipeYield": "2 servings",
        "recipeIngredient": [
            "8 ounces Gevalia\u00ae Cold Brew Concentrate - Vanilla, divided",
            "2 ounces sweetened condensed milk",
            "2 ounces half-and-half",
            "\u215b teaspoon ground cardamom",
            "\u215b teaspoon ground cinnamon",
            "ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour sweetened condensed milk and half-and-half into small measuring cup or pitcher. Add cardamom and cinnamon. Whisk until smooth and blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ice cubes in mugs. Pour 4 ounces cold brew concentrate into each mug. Pour half of spice mixture into each glass slowly over the back of a spoon.\n"
            }
        ],
