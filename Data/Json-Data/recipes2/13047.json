    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261969/amys-curried-ground-turkey-and-lentil-soup/",
        "name": "Amy's Curried Ground Turkey and Lentil Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F889560.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2018-01-05T06:35:03.000Z",
        "description": "The meal trifecta is possible - cheap, lean, and delicious! This ground turkey-lentil soup is gluten-free, incredibly fast and easy to make, and a huge crowd pleaser. Stir in jalapenos to taste.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H11M",
        "totalTime": "P0DT0H26M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon coconut oil",
            "1 red bell pepper, cut into bite-size pieces ",
            "1 small white onion, chopped",
            "1 pound lean ground turkey",
            "1 tablespoon cracked black pepper, divided",
            "1 teaspoon roasted garlic, divided",
            "2 (14.5 ounce) cans curried lentil soup (such as Amy's Organic Lentil Soup)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat. Add red bell pepper and onion; cook and stir until small brown spots begin to appear on the vegetables, about 6 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a separate skillet over medium-high heat. Cook and stir turkey in the hot skillet until browned and crumbly, about 5 minutes. Stir in half the black pepper and half the roasted garlic until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour both cans of soup into a 2-quart pot over medium-high heat. Stir occasionally until it starts to bubble, about 5 minutes. Reduce heat to low. Add the cooked vegetables and turkey; add the remaining black pepper and garlic. Stir thoroughly.\n"
            }
        ],
