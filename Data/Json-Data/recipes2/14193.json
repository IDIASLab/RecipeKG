        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20161/fast-food-biscuits/",
        "name": "Fast Food Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F671645.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-07-08T13:38:57.000Z",
        "description": "These easy biscuits are perfect for those times when you crave fast food but don't want to leave the house!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 biscuits",
        "recipeIngredient": [
            "\u00bc cup lemon-lime flavored carbonated beverage",
            "\u2153 cup buttermilk",
            "2 cups baking mix",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour soda and buttermilk into a large bowl. Stir in baking mixing until dough pulls together. Knead dough in bowl until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Flatten dough and cut into 6 rounds. Place biscuits on pan and brush with melted butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until golden brown, about 18 to 20 minutes. Let cool 10 minutes before serving.\n"
            }
        ],
