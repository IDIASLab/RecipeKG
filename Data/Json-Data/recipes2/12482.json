    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283490/cold-tortellini-salad/",
        "name": "Cold Tortellini Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cold Tortellini Salad"
        },
        "datePublished": "2017-11-08T03:25:46.000Z",
        "description": "This is my most requested dish to bring to a potluck. It is not overly spicy and vegetarians just love this. You can add more cheese and seasonings if desired. This salad has been passed around the world and is always well received.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package fresh cheese tortellini",
            "1 (14 ounce) can artichoke hearts, drained",
            "1 head broccoli, cut into bite-sized florets",
            "1 head cauliflower, cut into bite-sized florets",
            "1 (8 ounce) package shredded mozzarella cheese",
            "\u00bc cup grated Parmesan cheese",
            "\u00bc cup quartered green olives",
            "\u00bc cup quartered black olives",
            "1 teaspoon Italian seasoning",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon cracked black pepper",
            "1 (16 ounce) bottle Italian dressing, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with lightly salted water and bring to a rolling boil; stir in tortellini and return to a boil. Cook uncovered, stirring occasionally, until the tortellini float to the top and the filling is hot, 6 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, slice each artichoke heart lengthwise into 8 pieces; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain tortellini and transfer to a large bowl. Cool to room temperature, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add broccoli, cauliflower, mozzarella, Parmesan, green and black olives, Italian seasoning, salt, and pepper; mix until ingredients are evenly distributed. Stir in Italian dressing. Cover and refrigerate, 8 hours to overnight.\n"
            }
        ],
