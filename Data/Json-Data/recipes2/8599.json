    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284152/savory-chicken-and-mushroom-pie/",
        "name": "Savory Chicken and Mushroom Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Savory Chicken and Mushroom Pie"
        },
        "datePublished": "2016-11-21T05:36:29.000Z",
        "description": "I revised an old-school chicken pie recipe of my mother's after substituting cream cheese for the milk called in the original. Creamy, and satisfying comfort food at its best.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "1 large onion, chopped",
            "1 cup sliced mushrooms",
            "\u00be cup chopped celery",
            "\u2153 cup all-purpose flour",
            "1 teaspoon poultry seasoning",
            "\u00bc teaspoon dried savory",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1\u2009\u00bd cups chicken broth",
            "1 (8 ounce) package cream cheese, softened",
            "2\u2009\u00bd cups chopped cooked chicken",
            "1 cup frozen mixed vegetables",
            "\u00bd (17.5 ounce) package frozen puff pastry, thawed",
            "1 large egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large saucepan over medium heat. Add onion, mushrooms, and celery; cook until vegetables are softened, 4 to 5 minutes. Stir in flour, poultry seasoning, savory, salt, and pepper. Add broth and cream cheese all at once. Cook and stir until thick and bubbly, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in chicken and frozen vegetables. Pour into a 2-quart baking dish. Place puff pastry sheet over the chicken mixture and brush with beaten egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven until puff pastry is golden brown and chicken mixture is bubbly, 20 to 25 minutes. Remove from the oven and let cool for 20 minutes before serving.\n"
            }
        ],
