    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256270/chocolate-marshmallow-fondant/",
        "name": "Chocolate Marshmallow Fondant",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5669880.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2016-11-29T07:39:09.000Z",
        "description": "A chocolate version of the very popular marshmallow fondant.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT8H22M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (16 ounce) package miniature marshmallows",
            "2 tablespoons light corn syrup, or more to taste",
            "1 teaspoon coffee-flavored extract",
            "\u00bd cup chocolate chips, or more to taste",
            "4 cups confectioners' sugar, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt marshmallows in a large microwave-safe bowl in 30-second intervals, stirring after each melting, 1 to 2 minutes. Mix in light corn syrup and coffee-flavored extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in a microwave-safe bowl in 30-second intervals, stirring after each melting, 1 to 2 minutes. Fold into the marshmallow mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir confectioners' sugar into the chocolate-marshmallow mixture, 1 cup at a time, until a thick, stringy dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dust a flat work surface with confectioners' sugar; turn dough out and knead until smooth and no longer sticky. Wrap tightly in plastic wrap. Let fondant rest at room temperature, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dust a flat work surface with confectioners' sugar and roll out fondant to desired size.\n"
            }
        ],
