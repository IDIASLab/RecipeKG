    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/57676/a-southern-fried-chicken/",
        "name": "A Southern Fried Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2735695.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2018-03-21T16:32:08.000Z",
        "description": "A cut-up chicken dredged in buttermilk and seasoned flour, then deep-fried to perfection.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 tablespoon seasoned salt",
            "1 tablespoon garlic powder",
            "1 tablespoon onion powder",
            "1 tablespoon coarse ground black pepper",
            "2 eggs",
            "4 cups buttermilk",
            "1 cup barbeque sauce",
            "2 tablespoons Worcestershire sauce",
            "1 tablespoon steak sauce",
            "1 (3 pound) whole chicken, cut into pieces",
            "2 cups oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large shallow dish, mix together flour, seasoned salt, garlic powder, onion powder, and black pepper. In a separate bowl, beat eggs, then whisk in buttermilk, barbeque sauce, Worcestershire sauce, and steak sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dredge chicken in milk mixture, then in seasoned flour, alternating in each at least twice. Heat oil in a large, deep-sided skillet to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook chicken in hot oil until golden brown on both sides, about 10 minutes each side.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Fried Chicken Recipes"
        ],
