    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282067/flaky-buttermilk-biscuits/",
        "name": "Flaky Buttermilk Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3774600.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2016-07-20T23:30:15.000Z",
        "description": "The best buttery and flaky buttermilk biscuits you'll ever have.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "16 biscuits",
        "recipeIngredient": [
            "4 cups Wheat flour, white, all-purpose, enriched, bleached",
            "4 teaspoons baking powder",
            "3 teaspoons white sugar",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup cold salted butter, cubed",
            "1\u2009\u00bd cups buttermilk",
            "2 tablespoons buttermilk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, baking powder, sugar, baking soda, and salt together in a large bowl. Cut butter in with a pastry blender or 2 knives until it resembles bread crumbs. Gently stir in 1 1/2 cups buttermilk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put biscuit dough onto a floured surface and sprinkle a bit of extra flour on top. Cut out 16 biscuits with a biscuit or cookie cutter and place on the prepared baking sheet. Brush tops with remaining buttermilk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown on top, about 10 minutes.\n"
            }
        ],
