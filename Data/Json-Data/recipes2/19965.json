    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14329/dilly-potato-salad/",
        "name": "Dilly Potato Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9239265.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-08-11T20:52:34.000Z",
        "description": "A new twist on the traditional potato salad.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds red potatoes, diced",
            "6 hard-cooked eggs, chopped",
            "1 cup nonfat plain yogurt",
            "3 tablespoons fat-free mayonnaise",
            "\u00bc cup chopped green onions",
            "1 teaspoon prepared Dijon-style mustard",
            "1 teaspoon dried dill weed",
            "1 teaspoon garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil.  Add potatoes; cook until tender but still firm, about 15 minutes. Drain, and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the potatoes and chopped eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together the yogurt, mayonnaise, green onions, mustard, dill weed and garlic powder. Pour dressing  over potatoes and eggs; toss and chill until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Potato Salad Recipes",
            "Red Potato Salad Recipes"
        ],
