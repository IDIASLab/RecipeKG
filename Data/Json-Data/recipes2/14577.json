        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22658/mashed-potatoes-with-horseradish/",
        "name": "Mashed Potatoes with Horseradish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4528183.jpg",
            "width": 1500,
