        "recipeIngredient": [
            "1 (46 fluid ounce) can pineapple juice",
            "1 (750 milliliter) bottle light rum",
            "1 (15 ounce) can cream of coconut",
            "1 (12 fluid ounce) can frozen pink lemonade concentrate",
            "1 (2 liter) bottle lemon-lime soda (such as 7-Up\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix pineapple juice, light rum, cream of coconut, and pink lemonade concentrate together in a large freezer-safe container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze pineapple mixture until icy and slushy, 4 to 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill a glass halfway with slushy mixture and top off with lemon-lime soda to serve.\n"
            }
