        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19386/cold-tropical-macaroni-salad/",
        "name": "Cold Tropical Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1224887.jpg",
