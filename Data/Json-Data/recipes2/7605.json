        "name": "Asian Deviled Egg Pretzel Crisps&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3682820.jpg",
            "width": 960,
