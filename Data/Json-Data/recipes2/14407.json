        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21175/hawaiian-sandwiches/",
        "name": "Hawaiian Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9404478.jpg",
