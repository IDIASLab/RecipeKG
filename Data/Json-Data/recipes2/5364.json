        "recipeIngredient": [
            "4 fresh mint leaves",
            "4 fresh raspberries",
            "\u00bd fluid ounce simple syrup",
            "\u00bd fluid ounce lime juice",
            "ice",
            "1 fluid ounce raspberry-flavored vodka",
            "1 fluid ounce silver rum",
            "2 fluid ounces club soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Muddle mint leaves, raspberries, simple syrup, and lime juice together in a shaker. Fill the shaker with ice and add vodka and rum; cover and shake until chilled. Pour entire contents of shaker into a Collins glass and top with club soda.\n"
            }
        ],
