        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/75661/asparagus-and-water-chestnuts/",
        "name": "Asparagus and Water Chestnuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F440750.jpg",
