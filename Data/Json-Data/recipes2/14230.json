        "recipeIngredient": [
            "1 teaspoon ground ginger",
            "2 tablespoons crushed garlic",
            "2 teaspoons ground turmeric",
            "\u00bc cup yogurt",
            "1 (3 pound) whole chicken, cut into pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine ginger, garlic, turmeric, salt, and yogurt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Marinate the chicken pieces in the mixture for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil grate. Grill chicken until juices run clear.\n"
            }
