        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222550/blue-hawaiian-cocktail/",
        "name": "Blue Hawaiian Cocktail",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2267923.jpg",
            "width": 1421,
