    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283633/apple-cinnamon-glazed-angel-food-cake/",
        "name": "Apple-Cinnamon Glazed Angel Food Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple-Cinnamon Glazed Angel Food Cake"
        },
        "datePublished": "2014-09-06T19:13:59.000Z",
        "description": "Great, light angel food cake with an apple-cinnamon glaze.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT2H55M",
        "recipeYield": "1 10-inch cake",
        "recipeIngredient": [
            "1\u2009\u00bd cups egg whites",
            "1\u2009\u00bd teaspoons cream of tartar",
            "\u00bc teaspoon salt",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon almond extract",
            "1\u2009\u00bd cups confectioners' sugar",
            "1 cup cake flour",
            "\u2153 cup unsalted butter",
            "2 cups confectioners' sugar",
            "\u00bd teaspoon ground cinnamon",
            "3 tablespoons apple cider, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a rack at the lowest position in the oven and preheat to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare cake: Beat egg whites, cream of tartar, and salt in a glass, metal, or ceramic mixing bowl until soft peaks form. Add sugar, 2 tablespoons at a time, beating well after each addition. Continue to beat until smooth and glossy and stiff peaks form. Mix in vanilla and almond extracts on low speed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine confectioners' sugar and cake flour in a bowl; gently fold into the egg mixture. Pour into an ungreased, 10-inch fluted tube pan such as Bundt&reg;.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top crust is golden brown and the cracks feel dry, 35 to 40 minutes. Remove from the oven and immediately invert cake pan; let cool completely, about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Loosen the sides of the cake from the pan and remove onto a serving plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare glaze: melt butter in a small saucepan. Stir in confectioners' sugar and cinnamon. Slowly add apple cider until glaze is thin enough to drizzle.\n"
            }
        ],
