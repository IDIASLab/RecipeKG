    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255136/dark-chocolate-covered-berries-almonds-and-pretzel-crisps/",
        "name": "Dark Chocolate-Covered Berries, Almonds, and Pretzel Crisps&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3901062.jpg",
            "width": 5472,
            "height": 3648
        },
        "datePublished": "2016-09-01T01:22:31.000Z",
        "description": "Salty Snack Factory&reg; Original Pretzel Crisps&reg;, sweet berries, crunchy almonds, and dark chocolate come together for a quick snack or dessert.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H40M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "4 (4 ounce) bars 70% dark chocolate, chopped",
            "1 tablespoon coconut oil",
            "1 teaspoon vanilla extract",
            "1 (6 ounce) container blueberries, stemmed",
            "\u00bd cup lightly roasted whole almonds",
            "12 thin pretzel crackers (such as Snack Factory\u00ae Pretzel Crisps\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with a silicone baking liner or waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine dark chocolate, coconut oil, and vanilla extract in top of a double boiler over simmering water. Stir frequently, scraping down the sides with a rubber spatula, until melted, about 5 minutes. Remove from heat and cool to room temperature, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop 2 tablespoons of the melted chocolate mixture into a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir blueberries and almonds into the remaining chocolate mixture. Spread on the prepared baking sheet in an even layer. Press pretzel crackers in gently. Drizzle reserved 2 tablespoons chocolate over crackers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze until firm, 15 to 20 minutes. Break gently into pieces before serving.\n"
            }
        ],
