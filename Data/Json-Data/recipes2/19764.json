    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12910/mullakinvowles-soup/",
        "name": "MullaKinVowles Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1076790.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1999-01-20T22:23:52.000Z",
        "description": "Curried chicken, rice, broccoli, and cheese soup -  perfect for a cold winter day!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "6 - 8 servings",
        "recipeIngredient": [
            "2 cups chicken broth",
            "2 cups milk",
            "3\u2009\u00bd tablespoons all-purpose flour",
            "\u00bd cup skinless, boneless chicken breast halves - chopped",
            "1 teaspoon curry powder",
            "2 cups cooked white rice",
            "1 stalk celery, finely chopped",
            "1 small onion, finely chopped",
            "\u00bd cup cooked chopped broccoli",
            "\u00bd cup cooked cauliflower",
            "\u00bd cup cooked, sliced mushrooms",
            "\u00be cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, heat chicken broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place milk in a jar with lid, and add flour; shake until well blended.  Add milk/flour mixture to chicken broth; stir until boiling.   Stir in curry powder, and reduce heat to medium low.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chopped chicken breast, onion, and celery into skillet.  Cook at medium heat until chicken is cooked and vegetables are soft; add  to soup.  Stir rice and optional vegetables into the soup.  Simmer for 30 minutes, stirring.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in grated cheese before serving.\n"
            }
        ],
