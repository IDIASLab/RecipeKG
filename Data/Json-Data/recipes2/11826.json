    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241748/easy-bubble-pizza/",
        "name": "Easy Bubble Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2222058.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2017-10-07T13:01:34.000Z",
        "description": "This throw together dish has been a lifesaver; working full-time and taking care of my disabled fiance doesn't leave much time for cooking. He loves this, I love it, and the kids love it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 bubble pizza",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) can refrigerated biscuit dough, separated and cut into quarters",
            "1 pound ground beef",
            "1 (16 ounce) jar tomato-based pasta sauce",
            "\u00bd pound shredded mozzarella cheese",
            "\u00bc pound shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray a 9x13-inch baking dish with cooking spray; place biscuit dough pieces into the prepared baking dish to cover the base of the dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease, leaving meat in the skillet. Add pasta sauce to beef and stir to combine. Pour sauce mixture over biscuit pieces to cover completely. Sprinkle mozzarella cheese and Cheddar cheese over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is bubbling and biscuits are cooked through, 30 to 35 minutes.\n"
            }
        ],
