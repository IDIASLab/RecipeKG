        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222105/healthier-taco-seasoning-i/",
        "name": "Healthier Taco Seasoning I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F922865.jpg",
