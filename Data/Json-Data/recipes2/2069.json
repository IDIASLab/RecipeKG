        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239339/mock-butterfinger/",
        "name": "Mock Butterfinger&reg;",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1474257.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2014-11-11T21:50:09.000Z",
        "description": "These are a fast, simple, and tasty treat.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT2H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "\u00bd cup peanut butter, or as needed",
            "48 thin wheat crackers (such as Wheat Thins\u00ae)",
            "\u00bd cup chocolate chips",
            "2 tablespoons butter, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 2 to 3 teaspoons peanut butter onto each of 24 crackers; top each with another cracker to create a \"sandwich.\"\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange \"sandwiches\" on the prepared baking sheet. Chill in freezer until peanut butter is firm, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat chocolate and butter together in a microwave-safe glass or ceramic bowl in 20-second intervals in a microwave oven, stirring frequently, until melted and smooth, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip each \"sandwich\" in the melted chocolate mixture to coat and arrange onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill coated sandwiches in freezer until chocolate is set, about 1 hour.\n"
            }
        ],
