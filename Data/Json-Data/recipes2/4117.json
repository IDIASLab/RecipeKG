    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245329/oreo-apple-hand-pies/",
        "name": "OREO & Apple Hand Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2763722.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2015-10-30T19:50:34.000Z",
        "description": "These fun-to-make individual OREO & Apple Hand Pies are the perfect twist to the traditional holiday dessert!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H18M",
        "totalTime": "P0DT1H52M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "1 (14.1 ounce) package ready-to-use refrigerated pie crusts",
            "2 small Granny Smith apples",
            "1 egg",
            "1 tablespoon water",
            "11 OREO Cookies, divided",
            "3 tablespoons powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 375 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unroll 1 pie crust on work surface; use 4-inch cutter to cut into 9 rounds, re-rolling scraps as necessary. Repeat with remaining pastry crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Core apples; cut crosswise into a total of 9 rings. Whisk egg and water until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 9 pastry rounds in single layer on parchment-covered baking sheet; brush edges with egg. Top each with 1 each apple ring and cookie. Cover with remaining pastry rounds; press edges together to seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crush remaining cookies finely; sprinkle over pies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 18 to 22 min. or until golden brown. Cool completely. Dust with sugar just before serving.\n"
            }
        ],
