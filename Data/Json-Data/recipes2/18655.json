        "mainEntityOfPage": "https://www.allrecipes.com/recipe/86746/rice-casserole/",
        "name": "Rice Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F28560.jpg",
