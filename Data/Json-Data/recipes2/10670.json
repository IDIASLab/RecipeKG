    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258276/salmon-and-avocado-salad/",
        "name": "Salmon and Avocado Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Salmon and Avocado Salad"
        },
        "datePublished": "2017-06-22T13:01:20.000Z",
        "description": "This is a quick, fresh, light salad that makes a nice lunch or a light summer dinner.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 (6 ounce) salmon fillet",
            "2 avocados - peeled, pitted, and cubed",
            "1 cup grape tomatoes, halved",
            "\u00bd red bell pepper, diced",
            "3 green onions, sliced",
            "1 lime, juiced",
            "1 teaspoon extra-virgin olive oil",
            "1 pinch kosher salt and ground black pepper to taste",
            "1 pinch chili powder, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper; place salmon fillet on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake salmon in the preheated oven until easily flaked with a fork, about 15 minutes. Let cool, about 10 minutes. Flake into bite-sized pieces with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cooled salmon, avocados, grape tomatoes, red bell pepper, and green onions in a large bowl. Drizzle lime juice and olive oil on top. Add kosher salt, black pepper, and chili powder; toss gently distribute evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill salad until flavors combine, 2 hours to overnight.\n"
            }
        ],
