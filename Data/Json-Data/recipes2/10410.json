        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221291/fiesta-guacamole/",
        "name": "Fiesta Guacamole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F829280.jpg",
