        "mainEntityOfPage": "https://www.allrecipes.com/recipe/50527/sweet-orange-chicken-ii/",
        "name": "Sweet Orange Chicken II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1470807.jpg",
