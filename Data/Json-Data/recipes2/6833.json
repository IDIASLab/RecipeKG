        "totalTime": "P0DT0H15M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "\u00bd teaspoon sunflower oil",
            "1 onion, chopped",
            "2 cloves garlic, crushed",
            "\u00bd teaspoon mild curry powder",
            "1 cup plain yogurt",
            "2 tablespoons mango chutney",
            "2 tablespoons chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat. Cook and stir onion and garlic in hot oil until tender, about 5 minutes. Stir curry powder into the onion mixture; cook 1 minute more. Remove from heat to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend onion mixture, yogurt, chutney, and parsley in a food processor and blend until smooth.\n"
            }
        ],
