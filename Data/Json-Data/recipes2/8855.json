    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256582/mint-maple-walnut-rocky-road/",
        "name": "Mint-Maple Walnut Rocky Road",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mint-Maple Walnut Rocky Road"
        },
        "datePublished": "2016-12-21T04:54:17.000Z",
        "description": "Delicious rocky road fudge recipe inherited from my grandfather.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H17M",
        "totalTime": "P0DT1H32M",
        "recipeYield": "4 pounds",
        "recipeIngredient": [
            "2 (8 ounce) packages finely chopped walnuts, or more to taste",
            "3 (16 ounce) packages semisweet chocolate chips",
            "\u00bd cup unsalted butter, melted",
            "2 tablespoons unsalted butter, melted",
            "1 tablespoon maple-flavored extract",
            "\u00bd (16 ounce) package marshmallows",
            "1 teaspoon peppermint extract, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spread walnuts on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast walnuts until fragrant, about 6 minutes. Stir and continue toasting until browned, about 9 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chocolate in a microwave-safe bowl. Heat in the microwave in 20-second intervals, stirring after each interval, until mostly melted, about 2 minutes. Stir in 1/2 cup plus 2 tablespoons butter and maple-flavored extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease an 8x14-inch baking pan. Pour in half of the chocolate mixture. Add marshmallows and half of the walnuts; press into the chocolate using a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix peppermint extract into remaining chocolate mixture. Scrape into the baking pan and press in remaining walnuts. Press layers together using a spatula. Cool fudge to room temperature, 1 to 2 hours.\n"
            }
        ],
