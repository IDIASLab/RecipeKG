        "recipeIngredient": [
            "1 pound Brussels sprouts",
            "3 tablespoons extra-virgin olive oil, or as needed",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove and discard the hard stems from the Brussels sprouts. Halve the sprouts and place on a baking sheet. Drizzle with olive oil and mix until coated. Season with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until soft, 15 to 20 minutes.\n"
            }
