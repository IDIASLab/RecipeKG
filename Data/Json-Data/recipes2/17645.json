        "mainEntityOfPage": "https://www.allrecipes.com/recipe/71315/grilled-pineapple-slices/",
        "name": "Grilled Pineapple Slices",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F653586.jpg",
