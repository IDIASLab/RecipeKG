    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279876/junes-layered-rhubarb-bars/",
        "name": "June's Layered Rhubarb Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8125519.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-24T18:49:05.000Z",
        "description": "Delicious layers of rhubarb, Cool Whip&reg; with marshmallows and vanilla pudding, on top of a graham cracker crust.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "24 bars",
        "recipeIngredient": [
            "2 cups crushed graham crackers, divided",
            "\u00be cup unsalted butter, melted",
            "4 cups chopped rhubarb",
            "1\u2009\u00bd cups white sugar",
            "3 tablespoons cornstarch",
            "2 tablespoons water",
            "3 drops red food coloring, or as desired",
            "1 (8 ounce) tub frozen whipped topping (such as Cool Whip\u00ae), thawed",
            "1\u2009\u00bd cups miniature marshmallows",
            "1 (3.4 ounce) package instant vanilla pudding mix",
            "1\u2009\u00be cups milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle 3/4 of the crushed graham crackers in the bottom of a 9x13-inch pan; reserve the other 1/4 for topping. Add melted butter over top and press into the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set and browned, about 7 minutes. Remove from the oven and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix rhubarb, sugar, cornstarch, and water together in a pot. Cook over medium heat until thick, 5 to 7 minutes. Add red food coloring. Put mixture on top of cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix frozen whipped topping and marshmallows together in a bowl; spread on top of rhubarb layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vanilla pudding with milk in a bowl. Put on top of marshmallow layer. Sprinkle with reserved 1/4 graham crackers. Cool in the refrigerator for about 3 hours.\n"
            }
        ],
