        "name": "Candy-Coated Milk Chocolate Pieces Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F61560.jpg",
            "width": 250,
            "height": 250
