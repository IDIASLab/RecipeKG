        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9811/potato-chip-cookies-i/",
        "name": "Potato Chip Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1954033.jpg",
            "width": 5152,
            "height": 3864
        },
        "datePublished": "1997-07-29T16:17:52.000Z",
        "description": "I haven't made these cookies, but I have eaten them and they were delicious.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 pound butter, softened",
            "1\u2009\u00bd cups white sugar",
            "2 teaspoons vanilla extract",
            "3\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups crushed potato chips",
            "2 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Lightly grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the butter, sugar, eggs and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in the flour and potato chips and stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoon onto cookie sheets about 2 inches apart.  Bake for 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
