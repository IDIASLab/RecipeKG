    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220965/garlic-baked-ham/",
        "name": "Garlic Baked Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1080532.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-03-05T05:08:44.000Z",
        "description": "Garlic gives this baked ham a great flavor.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H5M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "1 whole ham",
        "recipeIngredient": [
            "1 (8 pound) ham",
            "20 cloves garlic, peeled",
            "10 whole cloves",
            "1 (6 ounce) can pineapple juice",
            "2 tablespoons brown sugar",
            "2 teaspoons dry mustard powder",
            "\u00bd teaspoon ground ginger"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse ham and pat dry with paper towels; arrange ham cut-side down in a shallow roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make 20 incisions with the point of a knife all over the ham; stuff one garlic clove into each incision.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press whole cloves into the ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover ham with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 1 hour and 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While ham is baking, mix pineapple juice, brown sugar, dry mustard, and ginger in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove foil from ham. Pour pineapple juice glaze over ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return ham to oven uncovered and continue baking until glaze thickens, about 45 minutes more. Baste ham every 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Allow ham to rest 5 to 10 minutes. Remove garlic and whole cloves before slicing.\n"
            }
        ],
