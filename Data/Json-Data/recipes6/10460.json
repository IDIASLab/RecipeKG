        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6918/portuguese-fried-bread/",
        "name": "Portuguese Fried Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F234808.jpg",
