        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11178/snowflake-cookies/",
        "name": "Snowflake Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F941102.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-12-12T22:17:10.000Z",
        "description": "This is my mom's Christmas cookie recipe that the kids and I have made for years. Great fun to make and decorate.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "3\u2009\u00be cups all-purpose flour",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream the shortening with the sugar. Add the eggs and vanilla and mix well. Sift the flour and salt into the creamed mixture and stir in gradually. Chill dough for at least one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the chilled dough out to 1/8 inch thick and cut into desired shapes. Bake on an ungreased baking sheet for 6 to 8 minutes. Allow cookies to cool before decorating.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cut-Out Cookie Recipes"
        ],
