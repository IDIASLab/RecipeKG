        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279872/whipped-peanut-butter-iced-coffee/",
        "name": "Whipped Peanut Butter Iced Coffee",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7989681.jpg",
            "width": 0,
            "height": 0
