    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267119/crispy-vegan-gingersnaps/",
        "name": "Crispy Vegan Gingersnaps",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Crispy Vegan Gingersnaps"
        },
        "datePublished": "2020-06-18T00:35:43.000Z",
        "description": "These vegan cookies are crispy, spicy, and perfect with a hot drink in autumn and winter.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "\u00be cup almond flour",
            "\u00bc cup coconut flour",
            "1 tablespoon ground ginger",
            "2 teaspoons ground cinnamon",
            "\u00bd teaspoon ground allspice",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon salt",
            "\u00be cup almond butter",
            "\u00bd cup honey",
            "\u00bc cup melted coconut oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine almond flour, coconut flour, ginger, cinnamon, allspice, baking soda, nutmeg, and salt in a bowl. Whisk almond butter, honey, and melted coconut oil together in a separate bowl. Pour wet ingredients into dry and mix together until a dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into balls, then press to flatten onto the prepared baking sheet using the palm of the hand, the back of a spoon, or a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cookies are browned but still soft, 15 to 18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cookies from oven and let cool 5 minutes. Transfer to a wire rack to cool completely; store in an airtight container to keep cookies crispy.\n"
            }
        ],
