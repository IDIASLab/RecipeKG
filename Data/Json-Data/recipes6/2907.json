    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259786/gluten-free-raisin-oatmeal-cookies/",
        "name": "Gluten-Free Raisin Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4571313.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "2020-03-05T05:16:45.000Z",
        "description": "Created for a friend, a crispy cookie full of flavor that satisfies both gluten-free and non-gluten-free friends/family. Enjoy!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H18M",
        "totalTime": "P0DT1H53M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup pecan halves or pieces",
            "1\u2009\u2153 cups light brown sugar",
            "\u00bd cup unsalted butter, at room temperature ",
            "\u00bd cup shortening",
            "2 eggs, at room temperature",
            "2 teaspoons vanilla extract",
            "1\u2009\u00bd cups gluten-free all-purpose baking flour",
            "1 (.25 ounce) package unflavored gelatin",
            "2 teaspoons unflavored gelatin",
            "1 teaspoon baking powder",
            "1 teaspoon kosher salt",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground ginger",
            "3 cups gluten-free oats",
            "1 cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast pecans in the preheated oven until fragrant, about 5 minutes. Let cool for 5 minutes. Chop coarsely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, butter, and shortening in a stand mixer fitted with the paddle attachment; beat until light and fluffy. Beat in eggs one at a time. Beat in vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift gluten-free flour, 1 packet plus 2 teaspoons gelatin, baking powder, salt, cinnamon, and ginger together in a bowl. Mix into the brown sugar mixture on low speed. Mix pecans, oats, and raisins into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap dough in plastic wrap and chill in the refrigerator for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop spoonfuls of dough 2 inches apart onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the the preheated oven until edges are golden, about 13 minutes. Cool on the baking sheets for 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes",
            "Oatmeal Raisin Cookie Recipes"
        ],
