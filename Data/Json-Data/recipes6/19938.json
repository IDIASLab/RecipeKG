        "recipeIngredient": [
            "1\u2009\u00bc cups fresh lime juice",
            "1 cup Triple Sec or other orange liqueur",
            "1 cup silver tequila",
            "3 tablespoons fresh lemon juice",
            "3 cups ice cubes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the lime juice, triple sec, tequila, lemon juice, and 2 cups ice in a blender; blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 1 cup ice cubes in a cocktail shaker; add desired amount of margarita; cover and shake well; serve.\n"
            }
