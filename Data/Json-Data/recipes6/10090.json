    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280413/mixed-berry-pretzel-salad/",
        "name": "Mixed Berry Pretzel Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mixed Berry Pretzel Salad"
        },
        "datePublished": "2020-06-12T10:46:07.000Z",
        "description": "This mixed frozen berry salad is a delicious, fruity dessert that is sweet and salty at the same time. Be careful to follow the instructions exactly, or it will not come out looking nice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 cups unsalted pretzels, crushed",
            "\u00be cup melted margarine",
            "3 tablespoons white sugar",
            "1 cup white sugar",
            "1 (8 ounce) package cream cheese, softened",
            "1 (8 ounce) container frozen whipped topping (such as Cool Whip\u00ae), thawed",
            "1 (3 ounce) package strawberry-flavored gelatin (such as Jell-O\u00ae)",
            "1 (3 ounce) package raspberry-flavored gelatin mix (such as Jell-O\u00ae)",
            "2 cups boiling water",
            "2 cups mixed frozen berries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix crushed pretzels, margarine, and 3 tablespoons sugar together in a bowl. Press into a 9x9-inch baking dish in a flat layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 8 minutes. Remove and let cool thoroughly, 20 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream 1 cup sugar with cream cheese in a bowl; fold in whipped topping. Spread on top of cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix strawberry and raspberry gelatin mixes with boiling water until thoroughly dissolved. Stir in frozen berries until distributed. Pour over whipped topping mixture. Chill until set, about 2 hours.\n"
            }
        ],
