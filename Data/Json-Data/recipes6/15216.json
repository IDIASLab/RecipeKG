        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18929/chocolate-strawberry-banana-milkshake/",
        "name": "Chocolate Strawberry Banana Milkshake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F881071.jpg",
            "width": 1920,
