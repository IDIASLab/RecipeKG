        "recipeIngredient": [
            "1\u2009\u00bd cups finely ground graham cracker crumbs",
            "\u2153 cup white sugar",
            "6 tablespoons butter, melted",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs, sugar, melted butter or margarine, and cinnamon until well blended . Press mixture into an 8 or 9 inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 375 degrees F (190 degrees C) for 7 minutes.  Cool.  If recipe calls for unbaked pie shell, just chill for about 1 hour.\n"
            }
        ],
