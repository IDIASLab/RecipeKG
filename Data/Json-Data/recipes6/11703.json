        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9395/kinderpunsch/",
        "name": "Kinderpunsch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F59840.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-12T20:44:09.000Z",
        "description": "Kinderpunsch is an alcohol-free hot punch, served mainly in Christmas markets in Germany. Substitute grape juice for apple if you'd rather.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 1/2 quarts",
        "recipeIngredient": [
            "1 cup water",
            "\u2154 cup white sugar",
            "2 pinches ground cloves",
            "1 cinnamon stick",
            "3 cups apple juice",
            "1 lemon, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a 3 quart saucepan, bring water, sugar, cloves and cinnamon to boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in juice and heat through.  Do not boil. Take spices out and pour over lemon slices in serving bowl. Serve warm.\n"
            }
        ],
