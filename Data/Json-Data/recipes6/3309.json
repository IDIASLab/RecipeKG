    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267144/easy-blueberry-crumble/",
        "name": "Easy Blueberry Crumble",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5650832.jpg",
            "width": 1279,
            "height": 960
        },
        "datePublished": "2020-03-05T05:18:16.000Z",
        "description": "A delicious crumble best made with frozen blueberries is unmatched when served warm with vanilla ice cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "6\u2009\u00bd cups frozen blueberries",
            "\u2153 cup brown sugar",
            "1 teaspoon lemon zest",
            "6 tablespoons unsalted butter, room temperature and cut into chunks",
            "\u00be cup all-purpose flour",
            "1\u2009\u00bc cups brown sugar",
            "\u00bc teaspoon salt",
            "\u00bd cup rolled oats",
            "\u00bd teaspoon ground cinnamon",
            "1 pinch ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine blueberries, brown sugar, and lemon zest in a large bowl. Pour filling into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter, flour, brown sugar, oats, salt, cinnamon, and nutmeg in a bowl and rub between your fingers to make crumbles. Sprinkle topping evenly over blueberries. .\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crumble is lightly browned and bubbly, 40 to 45 minutes. Serve warm.\n"
            }
        ],
