    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244988/mini-pecan-pie/",
        "name": "Mini Pecan Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mini Pecan Pie"
        },
        "datePublished": "2020-03-05T05:13:34.000Z",
        "description": "Great dessert or snack if you're not on a diet.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 dozen pies",
        "recipeIngredient": [
            "\u00bd (8 ounce) package cream cheese, softened",
            "\u00bd cup margarine, softened",
            "1 cup self-rising flour",
            "\u00be cup packed brown sugar",
            "1 egg",
            "1 tablespoon ground cinnamon",
            "1 teaspoon vanilla extract",
            "\u00be cup chopped pecans",
            "3 (1 ounce) squares semi-sweet baking chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and margarine together in a bowl using an electric mixer until smooth and creamy. Slowly add flour and continually mix until dough is smooth. Refrigerate mixture for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar, egg, cinnamon, and vanilla extract together in a bowl until smooth; add pecans and stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 24 mini muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon dough, about 1 tablespoon per pie, into each muffin cup. Press dough into the bottom and up the sides of each cup, creating a pie shell. Fill each shell with pecan mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pies are set, about 30 minutes. Cool pies on waxed paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in a small saucepan over low heat, stirring constantly. Dip a fork into melted chocolate and drizzle over pies.\n"
            }
        ],
