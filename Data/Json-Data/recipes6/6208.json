    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279192/sous-vide-cod/",
        "name": "Sous Vide Cod",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7782594.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-14T09:57:47.000Z",
        "description": "Never eat another piece of overcooked cod again! This sous vide method produces cod that is flaky and moist and there's very little hands-on time.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 (4-ounce) cod fillets",
        "recipeIngredient": [
            "2 (4 ounce) cod loins",
            "1 pinch salt and freshly ground black pepper",
            "1 lemon",
            "2 teaspoons butter",
            "1 shallot, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fill a large pot with water. Attach an immersion circulator to the pot. Set the temperature to 130 degrees F (54 degrees C) and the timer to 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season cod lightly with salt and pepper. Zest 1/2 of the lemon over each piece. Slice lemon. Place butter and 2 lemon slices over the lemon zest. Place 1/2 of the shallot slices in the bottom of a resealable bag and place 1 cod loin on top. Repeat with second piece of cod and remaining shallot slices. Close the bags, leaving 1 small corner open.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lower the bags into the water and submerge until all the air has been expelled. Close the corners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wait until timer goes off and remove the bags. Carefully transfer cod onto 2 plates. Drizzle 1 tablespoon of cooking liquid from the bag over each cod loin. Garnish with extra lemon slices.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish"
        ],
