    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25335/vegan-fajitas/",
        "name": "Vegan Fajitas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2185904.jpg",
            "width": 1920,
            "height": 1278
        },
        "datePublished": "2020-06-19T03:06:11.000Z",
        "description": "This is a wonderful meatless version of traditional fajitas! It can be prepared in advance, or right away.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "\u00bc cup red wine vinegar",
            "1 teaspoon dried oregano",
            "1 teaspoon chili powder",
            "garlic salt to taste",
            "salt and pepper to taste",
            "1 teaspoon white sugar",
            "2 small zucchini, julienned",
            "2 medium small yellow squash, julienned",
            "1 large onion, sliced",
            "1 green bell pepper, cut into thin strips",
            "1 red bell pepper, cut into thin strips",
            "2 tablespoons olive oil",
            "1 (8.75 ounce) can whole kernel corn, drained",
            "1 (15 ounce) can black beans, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine olive oil, vinegar, oregano, chili powder, garlic salt, salt, pepper and sugar.  To the marinade add the zucchini, yellow squash, onion, green pepper and red pepper.  Marinate vegetables in the refrigerator for at least 30 minutes, but not more than 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium-high heat.  Drain the vegetables and saute until tender, about 10 to 15 minutes.  Stir in the corn and beans; increase the heat to high for 5 minutes, to brown vegetables.\n"
            }
        ],
        "recipeCategory": [
