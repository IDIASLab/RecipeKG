    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279786/low-carb-cauliflower-and-leek-soup/",
        "name": "Low-Carb Cauliflower and Leek Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Low-Carb Cauliflower and Leek Soup"
        },
        "datePublished": "2020-04-25T00:31:18.000Z",
        "description": "A low-carb, satisfying soup, perfect for fall!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 head cauliflower, chopped into small florets",
            "6 cloves garlic, peeled, or more to taste",
            "4 tablespoons olive oil",
            "1 teaspoon salt, or more to taste",
            "\u00bd teaspoon ground black pepper, or more to taste",
            "\u00bd cup salted butter",
            "2 leeks, chopped, or more to taste",
            "1 small onion, chopped",
            "1 (32 ounce) carton chicken broth",
            "2 cups heavy whipping cream",
            "\u00bc cup shredded Cheddar cheese, or to taste",
            "2 tablespoons bacon bits, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cauliflower florets, garlic cloves, olive oil, 1 teaspoon salt, and 1/2 teaspoon pepper in a 1-gallon zip-top bag. Seal bag and shake until florets are evenly coated. Empty bag onto a rimmed baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven, stirring a couple of times to ensure even cooking, for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a heavy-bottomed stockpot over medium heat. Add leeks and onion and season with salt and pepper. Cook until vegetables are softened, about 5 minutes. Add chicken broth and roasted cauliflower and garlic. Cook until cauliflower is tender, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add whipping cream and puree soup with an immersion blender until smooth. Cook until heated through, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve topped with Cheddar cheese and bacon bits.\n"
            }
        ],
