    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262460/instant-pot-easy-vegan-tomato-and-basil-soup/",
        "name": "Instant Pot&reg; Easy Vegan Tomato and Basil Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5075705.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2020-03-10T02:53:16.000Z",
        "description": "Make this creamy and delicious tomato soup in your Instant Pot&reg; truly in an instant! San Marzano tomatoes, garlic, and onions create perfection in this quick and easy vegan comfort food recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "1 onion, chopped",
            "2 cloves garlic",
            "2 (14.5 ounce) cans San Marzano whole tomatoes",
            "2 cups vegetable broth",
            "1 tablespoon chopped fresh basil",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon Italian seasoning"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Turn on a multi-functional pressure cooker (such as Instant Pot&reg;) and select Saute function. Heat olive oil and add onion; cook until translucent, about 7 minutes. Add garlic and cook for 1 minute more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine tomatoes, vegetable broth, basil, salt, ground black pepper, and Italian seasoning in the pot. Close and lock the lid. Select high pressure according to manufacturer's instructions; set timer for 5 minutes. Allow 10 to 15 minutes for pressure to build.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Release pressure carefully using the quick-release method according to manufacturer's instructions, about 5 minutes. Unlock and remove lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend with an immersion blender until creamy.\n"
            }
        ],
