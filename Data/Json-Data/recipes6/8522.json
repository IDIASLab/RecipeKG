    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7742/apple-dump-cake/",
        "name": "Apple Dump Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Dump Cake"
        },
        "datePublished": "1998-05-28T15:56:21.000Z",
        "description": "Use this recipe combining apple pie filling and a cinnamon-apple muffin mix for an easy and delicious apple-flavored dump cake.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9x13 inch pan",
        "recipeIngredient": [
            "1 (21 ounce) can apple pie filling",
            "1 (18.25 ounce) package apples and cinnamon muffin mix",
            "\u00bd cup butter",
            "\u00bd teaspoon ground cinnamon, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease one 9x13 inch baking pan\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apple pie filling with ground cinnamon to taste. Pour mixture into pan. Sprinkle apples and cinnamon mixture over top of apples.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan melt the butter or margarine and pour evenly over the top of the cake. Sprinkle with additional cinnamon if desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 20 minutes or until golden brown. Serve hot with ice cream!\n"
            }
        ],
