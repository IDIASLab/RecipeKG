    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246169/spamaroni-and-cheese/",
        "name": "Spamaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3370952.jpg",
            "width": 2542,
            "height": 2538
        },
        "datePublished": "2020-04-09T06:50:17.000Z",
        "description": "Easy, economical comfort food, combining childhood favorites. My kids loved this when they were growing up.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray (such as Pam\u00ae)",
            "1 (14 ounce) package deluxe macaroni and cheese dinner mix (such as Kraft\u00ae)",
            "1 (12 ounce) can luncheon meat (such as Spam\u00ae), cut into 1/4-inch cubes",
            "\u00bc cup milk",
            "\u00bd cup Italian-style bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray a 9x13-inch casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook macaroni in the boiling water, stirring occasionally until tender yet firm to the bite, about 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place macaroni, cheese, cubed luncheon meat, and milk in a large bowl; mix well to combine. Turn out into casserole dish. Sprinkle bread crumbs on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbly, about 15 minutes.\n"
            }
        ],
