    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241545/christmas-crispix-mix/",
        "name": "Christmas Crispix&reg; Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6108040.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-03-05T05:13:04.000Z",
        "description": "This is an excellent treat that is great to have on hand around Christmas.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 (12 ounce) package crispy corn and rice cereal (such as Crispix\u00ae)",
            "3 cups pecan halves",
            "3 cups whole almonds",
            "2 cups brown sugar",
            "1 cup corn syrup",
            "\u00bd cup butter",
            "1\u2009\u00bd teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C). Lightly butter a large roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix corn and rice cereal, pecans, and almonds together in the prepared roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, corn syrup, and butter in a saucepan; bring to a boil. Heat to 240 degrees F (115 degrees C) or until a small amount of syrup dropped into cold water forms a soft ball that flattens when removed from the water and placed on a flat surface. Remove saucepan from heat and stir vanilla extract into syrup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle syrup over cereal mixture and toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cereal mixture in the preheated oven for 30 minutes; stir and continue baking for 30 minutes more. Spread cereal mixture in 1 layer onto 2 lightly buttered baking sheets, separating any large chunks with 2 forks; allow to cool.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Party Mix Recipes"
        ],
