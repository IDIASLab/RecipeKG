        "name": "Quick and Fast Vegetarian Vegetable Soup in a Hurry",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3788065.jpg",
            "width": 2012,
            "height": 2018
