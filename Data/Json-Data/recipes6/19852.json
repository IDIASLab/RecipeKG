    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/70278/oysters-n-bacon/",
        "name": "Oysters 'n' Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F965658.jpg",
            "width": 1197,
            "height": 1200
        },
        "datePublished": "2020-06-19T03:10:40.000Z",
        "description": "A great hot appetizer to serve for company. Oysters wrapped in bacon, and baked in a garlicky sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 ounces sliced bacon, cut in half",
            "4 teaspoons white sugar",
            "\u00bc cup dark soy sauce",
            "2 cloves garlic, chopped",
            "1 (8 ounce) can oysters, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet over medium-high heat. Cook until shrunken, but not crisp. Remove to paper towels to drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a shallow baking dish, whisk together the sugar, soy sauce and garlic with a fork. Wrap each oyster with a piece of bacon, and secure with toothpicks. Place wrapped oysters into the dish with the sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 minutes in the preheated oven, until the sauce has thickened and bacon is nicely crisped at the edges.\n"
            }
        ],
