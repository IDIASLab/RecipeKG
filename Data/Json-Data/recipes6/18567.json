        "mainEntityOfPage": "https://www.allrecipes.com/recipe/41455/peas-with-mushrooms/",
        "name": "Peas with Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F147007.jpg",
