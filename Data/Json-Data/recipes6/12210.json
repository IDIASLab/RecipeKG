    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10218/stir-and-drop-sugar-cookies-i/",
        "name": "Stir and Drop Sugar Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F471627.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-12-29T07:23:58.000Z",
        "description": "Soft, tasty, quick and easy cookies. Great for spur-of-the-moment baking since you don't need to soften butter.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 eggs",
            "\u2154 cup corn oil",
            "2 teaspoons vanilla extract",
            "1 teaspoon grated lemon zest",
            "\u00be cup white sugar",
            "2 cups sifted all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir oil into beaten eggs. Add lemon rind and vanilla. Blend in sugar until mixture thickens.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder and salt; stir into wet mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoons about 2 inches apart on UNGREASED cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip the bottom of a glass in oil then in sugar. Flatten each cookie by gently pressing with bottom of glass. Dip in sugar before stamping each cookie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until the edges are golden brown, 8 to 10 minutes. Remove immediately from cookie sheet and cool on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
