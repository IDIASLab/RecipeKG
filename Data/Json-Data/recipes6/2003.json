        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241352/dark-chocolate-hot-cocoa/",
        "name": "Dark Chocolate Hot Cocoa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2087944.jpg",
            "width": 1471,
