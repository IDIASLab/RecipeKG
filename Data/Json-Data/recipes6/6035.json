    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273977/julies-sensational-smoky-corn-chowder/",
        "name": "Julie's Sensational Smoky Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Julie's Sensational Smoky Corn Chowder"
        },
        "datePublished": "2020-03-10T06:53:10.000Z",
        "description": "This hearty chowder uses pantry staples to come together quickly. It is spicy, smoky, creamy, and out-of-this-world delicious! A perfect meal for a chilly evening. Try it tonight!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "1 medium onion, chopped",
            "1 (4 ounce) can fire-roasted diced green chilies, with their juice",
            "2 cloves garlic, minced",
            "1 teaspoon smoked paprika",
            "\u00bd teaspoon crushed red pepper flakes",
            "2 (14.5 ounce) cans whole new potatoes - rinsed, drained, and diced",
            "2 (10 ounce) packages frozen corn, thawed",
            "4 cups vegetable broth",
            "1 cup half-and-half",
            "\u00bc cup tortilla chips, or to taste",
            "1 tablespoon grated Cheddar cheese, or to taste",
            "1 tablespoon sour cream, or to taste",
            "1 tablespoon sliced green onions, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil over medium heat in a stockpot. Add onion and cook, stirring occasionally, until tender but not browned, about 5 minutes. Add chilies, garlic, paprika, and red pepper flakes; cook and stir for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir potatoes, corn, vegetable broth, and half-and-half into mixture. Bring to a boil; reduce heat to low and let simmer until vegetables are tender, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend soup using an immersion blender to desired consistency, leaving some chunks of potato and corn. Top with tortilla chips, Cheddar cheese, sour cream, and green onions.\n"
            }
        ],
