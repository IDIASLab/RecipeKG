    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273858/matcha-mousse/",
        "name": "Matcha Mousse",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6645115.jpg",
            "width": 1108,
            "height": 1477
        },
        "datePublished": "2020-03-05T05:19:50.000Z",
        "description": "Light matcha mousse, quick and easy.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon unflavored gelatin",
            "4 tablespoons hot water",
            "\u00bc cup brown sugar",
            "2 egg yolks",
            "1 cup milk",
            "2 tablespoons hot water",
            "1 teaspoon green tea powder (matcha)",
            "1 cup heavy cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve gelatin in 4 tablespoons hot water in a bowl. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar and egg yolks together in a medium bowl. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat milk in a saucepan over low heat, about 5 minutes. Stir in gelatin mixture until dissolved, making sure not to overheat milk. Remove from heat. Pour milk mixture into egg mixture gradually, stirring well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 2 tablespoons hot water and green tea powder together in a bowl. Set bowl of gelatin mixture on top of a bowl filled with ice water; stir matcha paste into mixture as it cools.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip heavy cream in a bowl using an electric mixer until firm but not yet forming peaks, about 3 minutes. Add to matcha mixture; stir well until fully incorporated, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mousse evenly into 8 serving glasses. Chill in the refrigerator at least 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Mousse Recipes"
        ],
