    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255363/vegan-yogi-oat-cookies/",
        "name": "Vegan Yogi Oat Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Yogi Oat Cookies"
        },
        "datePublished": "2020-03-05T05:15:37.000Z",
        "description": "Packed with nutrition and flavor, these vegan cookies make a perfect after-yoga treat!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "3 cups rolled oats",
            "1 tablespoon rolled oats",
            "\u00be cup whole wheat flour",
            "1 tablespoon whole wheat flour",
            "\u00be cup firmly packed brown sugar",
            "\u00bd cup milled flax seeds",
            "\u00bd cup coarsely chopped almonds",
            "\u00bd cup golden raisins",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon baking powder",
            "\u215b teaspoon ground nutmeg",
            "\u00be cup canola oil",
            "2 tablespoons canola oil",
            "\u00be cup water, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3 cups plus 1 tablespoon oats, 3/4 cup plus 1 tablespoon whole wheat flour, brown sugar, flax seeds, almonds, raisins, cinnamon, ginger, baking powder, and nutmeg together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 3/4 cup plus 2 tablespoons canola oil into the oat mixture. Add water; stir until dough is firm. Let rest about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough about 4 inches apart onto the prepared baking sheet. Flatten with a fork to a 3-inch diameter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden around the edges, 10 to 13 minutes. Cool for 5 minutes before transferring to a wire rack to cool completely.\n"
            }
        ],
