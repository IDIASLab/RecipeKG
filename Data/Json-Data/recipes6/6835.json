        "name": "Dad's Kentucky Home Fries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2345019.jpg",
            "width": 2333,
            "height": 2333
