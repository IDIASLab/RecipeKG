    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255463/seeded-flatbread/",
        "name": "Seeded Flatbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Seeded Flatbread"
        },
        "datePublished": "2020-03-05T05:15:39.000Z",
        "description": "We love the wonderful nutty flavor of this bread and wanted to replace the sandwich rounds I was still buying. When sliced, they're the perfect size for a tuna sandwich or for toasting with butter and jam. Store in a plastic bag.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "16 flatbreads",
        "recipeIngredient": [
            "1 cup bread flour",
            "1 cup spelt flour",
            "\u00bc cup brown sugar",
            "2 tablespoons sesame seeds",
            "1 tablespoon quinoa",
            "1 tablespoon sunflower seeds",
            "1 tablespoon millet",
            "1 tablespoon oats",
            "1 tablespoon flax seeds",
            "\u00be cup warm milk",
            "1 tablespoon olive oil",
            "2 teaspoons bread machine yeast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix bread flour, spelt flour, brown sugar, sesame seeds, quinoa, sunflower seeds, millet, oats, and flax seeds together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk and oil in the bread pan of a bread machine; add flour mixture. Make a well in the center of the flour mixture; pour in yeast. Knead dough according to manufacturer's instructions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn out dough on a floured work surface. Punch down and separate into 16 pieces. Flatten into 5-inch diameter flatbreads.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place flatbreads on 2 baking sheets; cover with plastic wrap. Let sit in a warm place until risen, about 1 hour. Remove plastic wrap.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 15 minutes. Cool slightly; slice in half.\n"
            }
        ],
