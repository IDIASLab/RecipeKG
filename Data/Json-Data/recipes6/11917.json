    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9760/mocha-java-bars/",
        "name": "Mocha Java Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F351351.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "For best results, use a high quality Dutch cocoa and gourmet white chocolate.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "15 bars",
        "recipeIngredient": [
            "2 teaspoons instant coffee granules",
            "2 tablespoons strong brewed coffee",
            "2 tablespoons milk",
            "\u00bc cup butter, softened",
            "\u00bc cup cocoa powder",
            "2\u2009\u00bd cups confectioners' sugar",
            "6 ounces white chocolate",
            "\u00bd cup butter",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "1 egg",
            "2 cups graham cracker crumbs",
            "\u00bd cup finely chopped walnuts",
            "\u00be cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "To make Pastry:  Combine 1/2 cup butter, white sugar, vanilla and the egg in a double boiler over low heat.  Stir until thick. Add the graham cracker crumbs, coconut and nuts and mix well. Pour into baking pan and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To prepare Filling:  In medium-sized bowl, dissolve the instant coffee in the hot coffee.   Mix in powdered sugar, cocoa, 1/4 cup of butter and milk.  Spread filling over pastry and chill for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To prepare Topping:  Melt white chocolate in double boiler over low heat.  Spread over filling and cut into squares.  Store in refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "No-Bake Cookie Recipes"
