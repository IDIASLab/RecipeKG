        "totalTime": "P0DT0H55M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups non-fat cottage cheese",
            "\u2154 cup shredded Swiss cheese",
            "2 eggs, beaten",
            "1 onion, chopped",
            "\u00bd cup soft bread crumbs",
            "\u2153 cup frozen chopped spinach, thawed "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine cottage cheese, Swiss cheese, eggs, onion, bread crumbs and spinach.  Transfer to an 8x8 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 minutes.  Let stand 10 minutes before cutting into squares.\n"
            }
        ],
