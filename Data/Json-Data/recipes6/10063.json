        "totalTime": "P0DT0H5M",
        "recipeYield": "1 cocktail",
        "recipeIngredient": [
            "1\u2009\u00bd ounces Tito's Handmade Vodka",
            "8 fresh mint leaves, or more to taste",
            "2 teaspoons sugar",
            "\u00be ounce fresh lime juice",
            "\u00bd cup ice, or as needed",
            "1\u2009\u00bd ounces sparkling water",
            "1 sprig fresh mint for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Muddle mint leaves in a shaker.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add Tito's Handmade Vodka, sugar, fresh lime juice, and ice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shake well and pour into a rocks glass.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with sparkling water and garnish with a mint sprig and lime slice.\n"
            }
        ],
