        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7035/basic-fruit-bread-recipe/",
        "name": "Basic Fruit Bread Recipe",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F535516.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-12-09T19:36:37.000Z",
        "description": "Add any fruits, vegetables, nuts and spices that you desire.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 8 1/2x4 1/2 inch loaf",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup white sugar",
            "\u00bd cup vegetable oil",
            "2 eggs",
            "1 cup shredded apple",
            "\u00be cup chopped walnuts",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease one 4 1/2 x 8 1/2 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking powder, soda, salt, sugar, oil, eggs, apple, walnuts, and vanilla only until dry ingredients are moistened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in greased 4 1/2 x 8 1/2 inch loaf pan at 350 degrees F (175 degrees C) for 35 to 40 minutes.\n"
            }
        ],
