    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18501/rhubarb-pie-iv/",
        "name": "Rhubarb Pie IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4530374.jpg",
            "width": 4288,
            "height": 2848
        },
        "datePublished": "2000-06-11T21:30:33.000Z",
        "description": "This is the easiest rhubarb pie recipe that I ever tried. Hardly any work at all. Enjoy!!!!!!!!!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "4 cups chopped rhubarb",
            "\u00be cup all-purpose flour",
            "1\u2009\u00bc cups white sugar",
            "2 (9 inch)  unbaked pie crusts",
            "1 egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour rhubarb into the prepared pie crust. Combine flour and sugar; sprinkle over the rhubarb in the crust. Cover with top crust making sure to cut 4 steam slots into the top of it. Brush the top with egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 to 45 minutes.\n"
            }
        ],
