        "recipeIngredient": [
            "\u00bd cup brown sugar",
            "\u00bc cup heavy cream",
            "3 tablespoons butter",
            "1 pinch salt",
            "1\u2009\u2154 cups confectioners' sugar",
            "1 teaspoon maple extract",
            "\u00bc teaspoon vanilla extract"
