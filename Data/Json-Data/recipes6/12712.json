        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10959/crunch-drops/",
        "name": "Crunch Drops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2303774.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1998-06-09T13:15:47.000Z",
        "description": "Crunchy cookies with a lot of flavor.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup shortening",
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2 cups quick cooking oats",
            "2 cups crisp rice cereal",
            "1 cup shredded coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix, flour, salt and soda together.  Cream shortening until fluffy; add both kinds of sugar gradually, beat until smooth and creamy.  Add eggs, one at a time, beating well after each one.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in vanilla and flour mixture; add cereal, oats, and coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form into small balls and place on greased cookie sheets and bake at 350 degrees F  (175 degrees C) for 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
