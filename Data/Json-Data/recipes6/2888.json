    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259225/chocolate-covered-caramel-corn/",
        "name": "Chocolate-Covered Caramel Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5236356.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "2020-03-05T05:16:38.000Z",
        "description": "Chocolate-covered caramel corn. Disposable roasting pans work great for mixing. Store in airtight containers or resealable plastic bags.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H21M",
        "totalTime": "P0DT1H41M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "4 (3.25 ounce) packages butter-flavored microwave popcorn",
            "2 cups dry-roasted peanuts",
            "2 cups brown sugar",
            "1 cup margarine",
            "\u00bd cup light corn syrup",
            "1 teaspoon salt",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon baking soda",
            "1 (14 ounce) bag chocolate almond bark, broken into pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place 1 bag popcorn in the microwave; cook on high until popping begins to slow down, about 2 1/2 minutes. Repeat with remaining bags. Open bags carefully; divide popcorn between 2 shallow baking dishes. Mix in peanuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, margarine, corn syrup, and salt in a saucepan over medium heat. Bring to a boil, stirring enough to blend. Simmer, stirring constantly, for 5 minutes. Remove from heat; mix in vanilla extract and baking soda until mixture is light and foamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour brown sugar mixture over the popcorn-peanut mixture; stir to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every 25 minutes, until a cooled piece of popcorn is crunchy, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line a flat work surface with waxed paper. Pour popcorn onto the waxed paper. Break up any large clusters.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate almond bark in a microwave-safe glass or ceramic bowl in 15-second intervals, stirring after each melting, 1 to 3 minutes. Pour into a resealable plastic bag; snip off 1 corner. Drizzle melted chocolate over popcorn. Let popcorn cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Popcorn Candy Recipes"
        ],
