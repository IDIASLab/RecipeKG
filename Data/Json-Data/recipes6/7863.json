    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273262/30-minute-pork-fried-rice/",
        "name": "30-Minute Pork Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "30-Minute Pork Fried Rice"
        },
        "datePublished": "2020-04-15T22:41:31.000Z",
        "description": "Easy 30-minute pork fried rice. Perfect for a busy weeknight meal.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 cups basmati rice",
            "8 cups water",
            "\u00bc cup reduced-sodium soy sauce",
            "1 tablespoon chili garlic sauce",
            "1\u2009\u00bd teaspoons hoisin sauce",
            "2 tablespoons peanut oil, divided",
            "1 pound pork shoulder, cut into 1/4-inch cubes",
            "1 large egg, lightly beaten",
            "1\u2009\u00bd cups chopped red bell pepper",
            "1 cup frozen shelled edamame",
            "\u2154 cup thinly sliced green onions"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water and rice to a boil in a large pot. Reduce heat to medium-low, cover, and simmer until rice is tender and water has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine soy sauce, chili garlic sauce, and hoisin in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a wok or large skillet over high heat. Add 1 tablespoon peanut oil; swirl to coat. Add pork in a single layer; cook, without stirring, for 2 minutes. Stir-fry until pork is no longer pink in the center and juices run clear, about 4 minutes. Transfer pork to soy sauce mixture; toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour egg into the pan; cook, without stirring, until set, about 45 seconds. Cut egg into bite-sized pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add remaining 1 tablespoon oil to pan; swirl to coat. Add bell pepper, edamame, and 1/2 cup green onions; stir-fry 1 minute. Add cooked rice; stir-fry 2 minutes. Add pork and soy sauce mixture; cook, stirring constantly, until heated through, about 1 minute. Top rice with egg and remaining green onions.\n"
            }
        ],
