    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266000/yummy-peanut-butter-apple-muffins/",
        "name": "Yummy Peanut Butter Apple Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1640019.jpg",
            "width": 4320,
            "height": 3240
        },
        "datePublished": "2020-03-05T05:18:00.000Z",
        "description": "My son had the idea for these one fall Saturday morning after we had gone apple picking. They are delicious! Great for school lunches, too.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 dozen muffins",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 cup white whole wheat flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 tablespoons unsalted butter",
            "\u00be cup natural creamy peanut butter",
            "\u2154 cup brown sugar",
            "\u00be cup milk, or more as needed",
            "1 egg, beaten",
            "1 teaspoon vanilla extract",
            "1 cup peeled and diced apple",
            "1 cup peanut butter chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flours, baking powder, baking soda, and salt in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan. Stir in peanut butter and brown sugar. Remove from heat and add milk, egg, and vanilla extract. Add butter mixture to the flour mixture, mixing until just blended. Add more milk, 1 tablespoon at a time, if the batter seems dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add apple and peanut butter chips, stirring gently to distribute. Divide the batter evenly among the muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 20 minutes.\n"
            }
        ],
