    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/55107/fried-cauliflower/",
        "name": "Fried Cauliflower",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1492830.jpg",
            "width": 1506,
            "height": 1506
        },
        "datePublished": "2020-06-19T03:09:30.000Z",
        "description": "Tender bits of cauliflower are battered and deep-fried. Great for appetizers.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 head cauliflower, broken into small florets",
            "1 egg",
            "2 teaspoons milk",
            "1 cup cracker crumbs",
            "4 cups oil for deep frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cauliflower into a pan of lightly salted boiling water. Cook for 5 to 8 minutes, or until tender. Remove from heat, drain and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer to 365 degrees F (180 degrees C). In a small bowl, whisk together the egg and milk with a fork. Place cracker crumbs in a shallow dish. Dip cauliflower in the egg mixture then roll in cracker crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry coated cauliflower in the hot oil until golden brown. Drain on paper towels.\n"
            }
        ],
