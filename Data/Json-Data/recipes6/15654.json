        "cookTime": null,
        "totalTime": "P0DT0H3M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 cup crushed ice",
            "1 fluid ounce amaretto liqueur",
            "1 fluid ounce vodka",
            "8 fluid ounces pineapple juice",
            "1 teaspoon grenadine syrup",
            "1 slice fresh pineapple",
            "1 maraschino cherry"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a blender combine ice, amaretto, vodka, pineapple juice and grenadine. Blend well and pour into a tall chilled glass. garnish with an umbrella, pineapple slice and cherry.\n"
            }
