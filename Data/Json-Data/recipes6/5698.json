    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222054/kellys-black-bean-salad/",
        "name": "Kelly's Black Bean Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8028734.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-10T06:51:55.000Z",
        "description": "My sister Kelly makes this AMAZING salad anytime we have a family get-together. It is loved by everyone in the family. The Mexican flavor is is unbeatable! Keeps in the fridge for up to a week!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 cup white rice",
            "2 cups water",
            "1 (14.5 ounce) can diced tomatoes, drained",
            "\u2153 cup red wine vinegar",
            "3 cloves garlic, minced",
            "1 tablespoon ground cumin",
            "4 teaspoons chili powder",
            "1\u2009\u00bd teaspoons salt",
            "1 teaspoon oregano",
            "ground black pepper to taste",
            "\u00be cup canola oil",
            "3 (15.5 ounce) cans black beans, drained and rinsed",
            "\u00bd (10 ounce) box frozen corn, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring rice and water to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and the liquid has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix tomatoes, vinegar, garlic, cumin, chili powder, salt, oregano, and black pepper together in a bowl. Whisk oil into tomato mixture to make a dressing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir rice, black beans, and corn together in a bowl; toss with dressing until well-coated.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Beans",
            "Black Bean Salad Recipes"
        ],
