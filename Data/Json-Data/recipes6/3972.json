    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276124/pork-tenderloin-on-the-grill/",
        "name": "Pork Tenderloin on the Grill",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pork Tenderloin on the Grill"
        },
        "datePublished": "2020-03-05T05:20:40.000Z",
        "description": "This is the easiest and tastiest pork tenderloin recipe ever! Every time one of our friends is grilling, they call to ask for this simple and delicious marinade.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup honey",
            "\u00bc cup soy sauce",
            "\u00bc cup oyster sauce",
            "3 tablespoons packed brown sugar",
            "2 tablespoons minced garlic",
            "1 tablespoon minced fresh ginger root",
            "1 tablespoon ketchup",
            "\u00bc teaspoon onion powder",
            "\u00bc teaspoon cayenne pepper",
            "\u00bc teaspoon ground cinnamon",
            "2 (3/4 pound) pork tenderloins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine honey, soy sauce, oyster sauce, brown sugar, garlic, ginger, ketchup, onion powder, cayenne, and cinnamon in a large, strong, zip-top bag. Close the bag and squish to combine. Add pork to marinade and let chill in the refrigerator for at least 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate. Remove pork from bag and reserve marinade for basting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook pork on the preheated grill for 10 minutes, basting with reserved marinade and turning every 3 minutes. Discard excess marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue to grill pork, turning every 3 minutes, until an instant-read thermometer inserted into the centers reads at least 145 degrees F (63 degrees C), about 10 minutes more. Let pork stand for 5 minutes before slicing.\n"
            }
        ],
