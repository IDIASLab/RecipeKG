    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11717/spaghetti-with-tomato-and-sausage-sauce/",
        "name": "Spaghetti with Tomato and Sausage Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F269948.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-02-07T20:39:55.000Z",
        "description": "The longer you cook the sauce, the better it tastes!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT3H30M",
        "totalTime": "P0DT3H45M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 pound beef sausage",
            "1 onion, minced",
            "2 cups fresh sliced mushrooms",
            "\u00bc cup olive oil",
            "2 (6 ounce) cans tomato paste",
            "1 (46 fluid ounce) can tomato juice",
            "1 (16 ounce) can crushed tomatoes",
            "1 cup Burgundy wine",
            "1\u2009\u00bd tablespoons dried oregano",
            "1 tablespoon dried basil",
            "2 tablespoons dried parsley",
            "1 tablespoon minced garlic",
            "2 tablespoons garlic salt",
            "\u00bd cup white sugar",
            "2 pounds spaghetti"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Cook sausage for 30 minutes.  Cut into bite sized pieces, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a Dutch oven, saute onion and mushrooms in olive oil until tender.  Remove with slotted spoon, and set aside.  Stir into Dutch oven: tomato paste, tomato juice, Italian tomatoes, and wine.  Stir until smooth.  Mix in oregano, basil, parsley, garlic, garlic salt, and sugar.  Return sausage and  onion and mushroom saute to sauce.  Bring to a boil.  Reduce heat, and simmer for at least 3 hours.  Cover pot if sauce becomes too thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook pasta according to package directions.  Drain.  Serve sauce over pasta.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
            "Spaghetti"
        ],
