    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/261961/turkey-quinoa-baked-burgers/",
        "name": "Turkey Quinoa Baked Burgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4997060.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "2020-03-05T05:17:17.000Z",
        "description": "The most moist, spicy, and flavorful turkey burger you will ever have. The quinoa gives the ground turkey the texture of a beef burger! Serve on a bun and use whatever favorite burger topping you like!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup water",
            "\u00bd cup quinoa",
            "1 tablespoon olive oil",
            "1 onion, finely chopped",
            "5 cloves garlic, finely chopped",
            "\u00bd cup Worcestershire sauce, divided",
            "2 eggs",
            "2 tablespoons barbeque sauce (such as Sweet Baby Ray's\u00ae)",
            "2 tablespoons hot pepper sauce",
            "1 tablespoon seasoned salt (such as LAWRY'S\u00ae)",
            "2 teaspoons ground black pepper",
            "2 pounds lean ground turkey",
            "3 tablespoons brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a glass baking dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water and quinoa to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until quinoa is tender, 15 to 20 minutes. Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat. Cook and stir onion until translucent, about 5 minutes. Add garlic; cook until fragrant, about 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cooked quinoa, onion mixture, 1/4 cup Worcestershire sauce, eggs, barbeque sauce, hot sauce, seasoned salt, and black pepper in a large bowl. Whisk together. Add ground turkey and mix well with hands. Form patties and place in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining 1/4 cup Worcestershire sauce and brown sugar together in a bowl to make a paste. Spoon on top of the patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake patties in the preheated oven until no longer pink in the center, 35 to 45 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
