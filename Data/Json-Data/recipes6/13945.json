        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14491/all-purpose-rub-for-meat/",
        "name": "All-Purpose Rub for Meat",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8974093.jpg",
            "width": 0,
            "height": 0
