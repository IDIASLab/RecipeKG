        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9634/shortbread-cookies-i/",
        "name": "Shortbread Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F19623.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These are always a hit at our church bazaar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "2 cups butter, softened",
            "1\u2009\u00be cups white sugar",
            "6 eggs",
            "1 tablespoon caraway seed",
            "8 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream the butter, and sugar.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, beat eggs and add to the butter mixture.  Stir in the caraway seeds and flour.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out the dough on a floured surface to 1/4 inch thickness.  Cut into 1 inch squares (or other shapes) and place at least 1 inch apart on cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 12 to 15 minutes...watch closely...until lightly colored.  Cool on wire racks.\n"
            }
        ],
