    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9757/hermits-i/",
        "name": "Hermits I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F13741.jpg",
            "width": 800,
            "height": 534
        },
        "datePublished": "1998-02-27T20:42:41.000Z",
        "description": "These can be made into either drop cookies or bar cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1\u2009\u00bd cups white sugar",
            "1 teaspoon ground allspice",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground cloves",
            "3 eggs",
            "3\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 cup raisins",
            "\u00bd cup molasses",
            "\u00bd teaspoon salt",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (160 degrees C).  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl cream together butter, sugar and spices.  Beat in the eggs one at a time. Sift in the flour, baking soda and salt and stir until well blended.  Stir in molasses and raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make bar cookies:  Spread mixture on cookie sheet and bake for 25 minutes.  Cut into bars while warm and let cool on wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make drop cookies:  Preheat oven to 375 degrees F (190 degrees C). Drop batter about 2 inches apart on greased cookie sheet and bake 12 to 15 minutes.  Let cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Molasses Cookie Recipes"
        ],
