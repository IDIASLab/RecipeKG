    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240340/pumpkin-oatmeal-cookies/",
        "name": "Pumpkin Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2879789.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2020-03-05T05:12:46.000Z",
        "description": "These soft and moist pumpkin spiced oatmeal cookies are delicious! Perfect for when you're in the mood for pumpkin and there's no pie around.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 (15 ounce) can pumpkin puree",
            "1 cup honey",
            "\u00be cup shortening",
            "\u00bc cup water",
            "1 egg",
            "1 teaspoon vanilla extract",
            "3 cups oats",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bc cup brown sugar",
            "2 teaspoons ground cinnamon",
            "1\u2009\u00bd teaspoons ground ginger",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground nutmeg",
            "\u215b teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat pumpkin, honey, shortening, water, egg, and vanilla extract together in a large bowl until creamy. Whisk oats, flour, brown sugar, cinnamon, ginger, baking soda, baking powder, salt, nutmeg, and cloves together in another bowl. Add oat mixture to pumpkin mixture; stir until dough is well-blended. Drop dough by rounded teaspoonfuls onto prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and set, 12 to 15 minutes.\n"
            }
        ],
