    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275148/easy-instant-pot-corn-chowder/",
        "name": "Easy Instant Pot&reg; Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6890836.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-10T06:53:11.000Z",
        "description": "This corn chowder cooks in an Instant Pot&reg; in under 15 minutes. Starting the pot on Saute mode ensures it will come to pressure faster.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 medium yellow onion, chopped",
            "1 tablespoon butter",
            "2 cups frozen corn",
            "2 cups chicken broth",
            "1 (12 fluid ounce) can evaporated milk",
            "\u00bd cup instant potato flakes",
            "\u00bd teaspoon dried thyme",
            "\u00bd teaspoon minced garlic",
            "\u00bd teaspoon dried chives",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Turn on a multi-functional pressure cooker (such as Instant Pot&reg;) and select Saute function. Add onion and butter to the hot pot; cook and stir until onion is lightly browned, about 5 minutes. Add corn and chicken broth. Turn off Saute function.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Close and lock the lid. Select high pressure according to manufacturer's instructions; set timer for 5 minutes. Allow 10 to 15 minutes for pressure to build.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Release pressure carefully using the quick-release method according to manufacturer's instructions, about 5 minutes. Unlock and remove the lid. Carefully remove lid and add evaporated milk, potato flakes, thyme, garlic, chives, salt, and pepper; stir to combine. Serve immediately.\n"
            }
        ],
