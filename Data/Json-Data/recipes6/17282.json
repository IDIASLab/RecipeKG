    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25514/easy-marinated-mushrooms/",
        "name": "Easy Marinated Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F465588.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:06:13.000Z",
        "description": "Slow-cooked marinated mushrooms are a delicious, easy appetizer, and always a favorite!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT8H0M",
        "totalTime": "P0DT8H15M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2 cups soy sauce",
            "2 cups water",
            "1 cup butter",
            "2 cups white sugar",
            "4 (8 ounce) packages fresh mushrooms, stems removed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan over low heat, mix soy sauce, water and butter. Stir until the butter has melted, then gradually mix in the sugar until it is completely dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mushrooms in a slow cooker set to low, and cover with the soy sauce mixture. Cook 8 to 10 hours, stirring approximately every hour. Chill in the refrigerator until serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Mushrooms"
        ],
