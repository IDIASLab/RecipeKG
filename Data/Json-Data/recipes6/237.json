        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222119/healthier-sloppy-joes-ii/",
        "name": "Healthier Sloppy Joes II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F876321.jpg",
