    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10968/macaroons-ii/",
        "name": "Macaroons II",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Macaroons II"
        },
        "datePublished": "1998-06-17T15:20:46.000Z",
        "description": "These cookies can be piped out using a cookie press or pastry bag, shaped into mounds or little finger logs.  Nice Christmas cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "2 egg whites",
            "1 tablespoon cornstarch",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "1 cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).  Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, beat the egg whites until stiff but not dry.  Fold in cornstarch.  Transfer to the top of a double boiler and set over low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in sugar and cook for about 3 to 4 minutes, until edges of mixture begin to pull away from pan.  Remove from heat and stir in vanilla extract.  Stir in coconut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by spoonfuls 1 1/2 inches apart on the prepared baking sheets.  Bake for 20 to 25 minutes until golden brown and firm to the touch.  Cool on the pans on wire racks.\n"
            }
        ],
