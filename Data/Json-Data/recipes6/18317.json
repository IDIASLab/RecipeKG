    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/35210/grannys-ice-cream/",
        "name": "Granny's Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F359059.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:07:31.000Z",
        "description": "This is my Grandmother's recipe for ice cream. It's yummy, doesn't require an ice cream maker, very simple and can be made into any flavor of ice cream on the planet.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT2H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 egg whites",
            "\u2154 cup sour cream",
            "\u00bd cup white sugar",
            "vanilla"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal mixing bowl, beat egg whites until foamy. Gradually add white sugar and vanilla, continuing to beat until soft peaks form. Carefully fold in sour cream until well blended. Fold in any additional flavorings or goodies at this time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the mixture into the freezer and stir every 30 minutes or so until frozen. This can be made without stirring, but the texture will be icy rather than creamy. If you have an ice cream maker, you can put the ice cream mixture in the maker, and follow the manufacturer's instructions.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frozen Dessert Recipes",
            "Ice Cream"
        ],
