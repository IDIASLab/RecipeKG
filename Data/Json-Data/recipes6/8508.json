    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279884/instant-pot-salsa-verde/",
        "name": "Instant Pot&reg; Salsa Verde",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8187401.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-25T03:20:42.000Z",
        "description": "Salsa verde made easy with the help of your multi-functional pressure cooker. Bottled sauce will never be the same again.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1\u2009\u00bc pounds tomatillos, husked and quartered",
            "2 large jalapenos, halved lengthwise",
            "1 medium onion, peeled and quartered",
            "\u00bc cup water",
            "2 teaspoons minced garlic",
            "\u00bc cup firmly packed fresh cilantro",
            "1 tablespoon lime juice",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine tomatillos, jalapenos, onion, water, and garlic in the inner pot of a multi-functional pressure cooker (such as Instant Pot&reg;). Close and lock the lid. Select high pressure according to manufacturer's instructions; set timer for 10 minutes. Allow 10 to 15 minutes for pressure to build.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Release pressure carefully using the quick-release method according to manufacturer's instructions, about 5 minutes. Unlock and remove the lid. Let cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in cilantro, lime juice, and salt. Blend until smooth using an immersion blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer mixture to 2 pint-sized canning jars with lids. Refrigerate until cool and thickened.\n"
            }
        ],
