    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19179/mrs-schallers-sugar-cookies/",
        "name": "Mrs. Schaller's Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F966485.jpg",
            "width": 3261,
            "height": 3261
        },
        "datePublished": "2000-06-17T03:16:13.000Z",
        "description": "This recipe was given to me by the dear ladies from my church and has been a hit with my friends and family every Christmas. Frost with any icing you like.",
        "prepTime": "P0DT1H0M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT9H10M",
        "recipeYield": "72 cookies",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup butter",
            "2 teaspoons vanilla extract",
            "2 eggs",
            "6 cups all-purpose flour",
            "2 teaspoons baking powder",
            "2 teaspoons baking soda",
            "1 cup buttermilk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, cream sugar, butter and vanilla until light colored. Beat in eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, sift together flour and baking powder. In a separate small bowl, dissolve the baking soda in the buttermilk. Stir the egg mixture and buttermilk mixture into the dry incrementally. The dough should be stiff. Refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out on a lightly floured surface to 1/4 inch thick. Cut with cookie cutters and arrange on cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degrees F (175 degrees C) oven for 8 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
