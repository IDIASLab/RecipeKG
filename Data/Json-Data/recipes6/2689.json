        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "1 piece",
        "recipeIngredient": [
            "1\u2009\u00bd teaspoons cream cheese",
            "1 teaspoon brown sugar",
            "1 slice applewood-smoked bacon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Line a rimmed baking sheet with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese and brown sugar together in a small bowl; spread over bacon slice. Roll up and transfer to the baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bacon is browned and crisp, 10 to 15 minutes.\n"
            }
        ],
