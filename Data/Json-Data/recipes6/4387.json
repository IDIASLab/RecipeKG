    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24376/stovetop-saffron-rice/",
        "name": "Stovetop Saffron Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5196827.jpg",
            "width": 1524,
            "height": 1524
        },
        "datePublished": "2020-03-10T02:50:28.000Z",
        "description": "Takes a little time but well worth the wait!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2 cups uncooked long-grain rice",
            "\u00be teaspoon crushed saffron threads",
            "4 tablespoons butter",
            "6 whole cardamom seeds",
            "4 whole cloves",
            "3 cinnamon sticks",
            "1 onion, chopped",
            "3 cups boiling vegetable broth",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cover rice with cold water and set aside to soak for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak saffron threads in 2 tablespoons boiling water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large saucepan over medium heat; add cardamom, cloves and cinnamon and fry 2 minutes, stirring occasionally.  Stir in onion and saute, stirring occasionally, until golden brown.  Stir in the rice, reduce heat to low and simmer for 5 minutes, stirring constantly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in the boiling broth and stir in the salt and saffron.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook until rice is cooked and all liquid is absorbed, about 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Rice Side Dish Recipes"
        ],
