    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22241/bundt-dutch-apple-cake/",
        "name": "Bundt Dutch Apple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F601897.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-03T05:03:35.000Z",
        "description": "This is a very delicious variation for Dutch Apple Cake. Simple to make and perfect for the fall holiday season.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "1 10-inch Bundt pan",
        "recipeIngredient": [
            "3 apples - peeled, cored and cut into thin wedges",
            "5 tablespoons white sugar",
            "2 teaspoons ground cinnamon",
            "3 cups all-purpose flour",
            "3 teaspoons baking powder",
            "2 cups white sugar",
            "4 eggs",
            "1 cup vegetable oil",
            "2\u2009\u00bd teaspoons vanilla extract",
            "\u2154 cup fresh orange juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour a 10 inch Bundt pan.  In a medium bowl, combine the apples, 5 tablespoons sugar and cinnamon.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together the flour and baking powder; set aside. In a large bowl, mix together the eggs and sugar.  Stir in the oil, vanilla and orange juice, mixing until blended.  Make a well in the center of the dry ingredients and pour in the orange juice mixture.  Stir just until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer batter and apple mixture in the prepared pan, beginning and ending with the batter. Bake in the preheated oven for 70 minutes, or until a toothpick inserted into the center of the cake comes out clean. Allow to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
