        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15854/rhubarb-cream-pie/",
        "name": "Rhubarb Cream Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F431906.jpg",
