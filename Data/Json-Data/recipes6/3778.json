    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274437/cinnamon-raisin-oat-triangles/",
        "name": "Cinnamon Raisin Oat Triangles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cinnamon Raisin Oat Triangles"
        },
        "datePublished": "2020-03-05T05:20:03.000Z",
        "description": "A great lunch box treat. I created it about 15 years ago when my kids were in elementary school.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "18 triangles",
        "recipeIngredient": [
            "2 cups regular oats",
            "1 cup unbleached all-purpose flour",
            "\u00be cup brown sugar",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon baking soda",
            "\u00be cup melted butter",
            "1 cup raisins",
            "\u00bc cup orange marmalade",
            "\u00bd cup finely chopped pecans",
            "\u00be cup powdered sugar",
            "2 tablespoons milk",
            "1 tablespoon melted butter",
            "\u00bc teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x9-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix oats, flour, brown sugar, cinnamon, and baking soda together in a medium bowl until combined. Stir in melted butter until evenly mixed. Remove 1 cup of the mixture and set aside. Pat remaining mixture into the bottom of the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup reserved crumb mixture, raisins, marmalade, and pecans in a bowl. Sprinkle over the mixture in the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine powdered sugar, milk, butter, and cinnamon for icing in a bowl. Stir until well blended and smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove bars from the oven and carefully cut into 3-inch squares while still warm, then cut diagonally to make a triangle. Drizzle with icing.\n"
            }
        ],
