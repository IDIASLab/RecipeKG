    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10861/pohany/",
        "name": "Pohany",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pohany"
        },
        "datePublished": "1998-05-06T04:48:39.000Z",
        "description": "My Grandma made these cookies for us.   We make an 'imitation' of these by cutting flour tortillas into strips and deep-frying and then sprinkling with confectioners' sugar.  To make the cookies or tortillas fancier my Grandma used a  serrated knife to give a jagged edge.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "2 eggs",
            "\u00bc cup white sugar",
            "1\u2009\u00bd cups all-purpose flour",
            "\u215b teaspoon salt",
            "\u00bd teaspoon lemon extract",
            "1 cup vegetable oil for frying",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat eggs until fluffy, add remaining ingredients and mix well.  Knead until smooth and elastic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll thinly on floured board. Cut into strips 1 x 5 inches.  Cut a 1 inch slit in the center of each strip and pull one end thru.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry in 375 degrees F (190 degrees C) oil until golden brown. Sprinkle with confectioners' sugar.\n"
            }
        ],
