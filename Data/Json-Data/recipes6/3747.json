    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274202/oatmeal-fudge-squares/",
        "name": "Oatmeal Fudge Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6843912.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-03-05T05:19:57.000Z",
        "description": "These oatmeal fudge squares are always a hit at every gathering. People will be asking for the recipe. Don't expect them to last very long!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 fudge squares",
        "recipeIngredient": [
            "\u00be cup butter, softened",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "3 cups quick-cooking oats",
            "2\u2009\u00bd cups all-purpose baking mix (such as Bisquick\u00ae)",
            "2 cups brown sugar",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (12 ounce) package milk chocolate chips",
            "2 tablespoons margarine",
            "2 teaspoons vanilla extract",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees). Spray a rimmed cookie sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix butter, eggs, and vanilla extract together in a large bowl with an electric mixer. Add oats, baking mix, and brown sugar; mix to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir condensed milk, chocolate chips, margarine, vanilla, and salt in a saucepan over low heat until chocolate chips have melted, 6 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread 3/4 of the oat mixture onto the prepared baking sheet. Pour the chocolate mixture over the top. Place small pieces of the remaining oat mixture randomly over the chocolate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, 15 to 20 minutes. Let cool before cutting into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes"
        ],
