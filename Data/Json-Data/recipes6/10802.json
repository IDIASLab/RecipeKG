    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7387/pineapple-sponge-cake/",
        "name": "Pineapple Sponge Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F709976.jpg",
            "width": 1016,
            "height": 1016
        },
        "datePublished": "1998-04-20T16:28:06.000Z",
        "description": "This cake is great for any occasion even birthdays and special occasions.  This is one of my mother's favorite special occasion cakes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -10 inch tube cake",
        "recipeIngredient": [
            "1\u2009\u00bd cups cake flour",
            "1 teaspoon baking powder",
            "6 eggs",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups white sugar",
            "1 tablespoon lemon juice",
            "\u00bd cup unsweetened pineapple juice",
            "2 cups whipped cream",
            "1 fresh pineapple, peeled and cored",
            "12 maraschino cherries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the cake flour and baking powder together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate the eggs, put the egg yolks into a large bowl and the egg whites into a small bowl. With an electric mixer beat the egg yolks until thick and fluffy, about 2 minutes. Gradually beat in 3/4 cup of the white sugar. Add the lemon juice and pineapple juice. Add the sifted cake flour mixture gradually while continually beating. Beat only until just blended (about 1 1/2 minutes).\n"
            },
            {
                "@type": "HowToStep",
                "text": "With an electric mixer beat the egg whites with the salt until moist peaks form (about 1 minute). Gradually beat in 3/4 cup of the white sugar (about 50 seconds).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold the beaten egg whites into the batter and mix only until blended. Pour batter into an ungreased 10 inch tube pan. With a knife or spatula gently cut through batter going round in a circular motion six times to release any air bubbles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for about 1 hour or until golden brown and firm to the touch. Invert pan on rack to cool. Once cake is cool loosen sides with a spatula and remove from pan. Frost cake with whipped cream and garnish with wedges of pineapple and maraschino cherries.\n"
            }
        ],
