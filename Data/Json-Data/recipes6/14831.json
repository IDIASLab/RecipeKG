    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17519/spicy-pumpkin-pie-i/",
        "name": "Spicy Pumpkin Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Spicy Pumpkin Pie I"
        },
        "datePublished": "2000-02-02T17:40:29.000Z",
        "description": "Mom's recipe. Best in the world. Just the right amount of cinnamon, ginger, and cloves. Best when served with whipped cream.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "2 - 9 inch pies",
        "recipeIngredient": [
            "2 (9 inch)  unbaked pie crusts",
            "2 tablespoons butter, melted",
            "1 (29 ounce) can pumpkin puree",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground ginger",
            "\u00bc teaspoon ground cloves",
            "\u00bd teaspoon salt",
            "2 eggs",
            "2 tablespoons all-purpose flour",
            "\u00bd cup brown sugar",
            "\u00bd cup white sugar",
            "1 cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together butter or margarine, pumpkin, cinnamon, ginger, cloves, and salt. In a separate bowl, beat eggs until foamy. Mix flour, brown sugar, white sugar, and milk into eggs. Blend egg mixture into pumpkin mixture. Pour half of mixture into each pastry lined pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 minutes. Reduce heat to 375 degrees F (190 degrees C) and bake an additional 45 minutes, until toothpick inserted in center comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pumpkin Pie Recipes"
        ],
