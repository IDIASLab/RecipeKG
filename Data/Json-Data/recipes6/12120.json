        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10058/butterscotch-potato-chip-cookies/",
        "name": "Butterscotch Potato Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7061309.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-11-03T20:56:05.000Z",
        "description": "Crunchy, sweet,  drop cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "1 cup butter",
            "2 eggs",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "1\u2009\u2153 cups butterscotch chips",
            "2 cups crushed potato chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together sugars and butter or margarine.  Beat in eggs, add flour and baking soda, mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in butterscotch chips and potato chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by tsp. on cookie sheet and bake for 8 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
