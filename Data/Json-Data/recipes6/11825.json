    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9613/snowy-pecan-cookies/",
        "name": "Snowy Pecan Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F768649.jpg",
            "width": 720,
            "height": 480
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "You will need to refrigerate the dough for these cookies overnight.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "4 (1 ounce) squares unsweetened chocolate",
            "\u00bd cup vegetable oil",
            "4 eggs",
            "2 teaspoons vanilla extract",
            "2 cups all-purpose flour",
            "2 cups white sugar",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "1 cup chopped pecans",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt chocolate squares and cooking oil in a saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cool and pour into mixing bowl.  Add sugar, eggs and vanilla, mixing until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients together and gradually add to chocolate/egg mixture, stirring well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in nuts, cover and place in refrigerator overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees and grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into small balls, about an inch in diameter.  Roll in powdered sugar and place about 2 inches apart on cookie sheet .\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 minutes.\n"
            }
        ],
