    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23810/grandmas-banana-bread/",
        "name": "Grandma's Banana Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F837796.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:05:08.000Z",
        "description": "A super moist banana bread which uses six bananas!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "3 cups sugar",
            "1 cup margarine, softened",
            "1 teaspoon vanilla extract",
            "4 eggs",
            "3\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking soda",
            "\u00bc teaspoon salt",
            "1 cup buttermilk",
            "6 very ripe bananas, mashed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Lightly grease a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the sugar and margarine until light and fluffy. Stir in the eggs one at a time, beating well with each addition, then stir in the vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, sift together flour, baking soda and salt. Blend this mixture into the egg mixture, alternately with the buttermilk; stir just to combine. Fold in the mashed bananas; mixing just enough to evenly combine. Pour batter into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until a toothpick inserted into center of the loaf comes our clean, about 60 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
