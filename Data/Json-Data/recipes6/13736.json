    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13694/apricot-salad/",
        "name": "Apricot Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7954724.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-07-12T17:54:38.000Z",
        "description": "This is a great salad for the holidays due to the very festive orange color.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (6 ounce) package apricot flavored Jell-O\u00ae mix",
            "\u2154 cup white sugar",
            "\u2154 cup water",
            "2 (4 ounce) jars apricot baby food",
            "1 (20 ounce) can crushed pineapple with juice",
            "1 (8 ounce) package cream cheese, softened",
            "1 (14 ounce) can sweetened condensed milk",
            "\u2153 cup chopped pecans",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, combine gelatin, sugar and water. Bring to a boil and mix until dissolved. Remove from heat, add baby food and pineapple with juice. Stir, set aside to cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine the cream cheese and sweetened condensed milk, mix together until smooth. Pour into fruit mixture and whisk together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in pecans and pour into a 9x13 inch pan. Refrigerate until chilled. Top with thawed whipped topping if desired. Sprinkle with pecans to decorate and cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes",
            "Jell-O&reg; Salad"
        ],
