        "mainEntityOfPage": "https://www.allrecipes.com/recipe/260910/rocky-road-salad/",
        "name": "Rocky Road Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F959896.jpg",
            "width": 1536,
            "height": 1533
