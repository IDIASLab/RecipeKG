    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241951/shrimp-and-corn-chowder/",
        "name": "Shrimp and Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9054441.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-10T06:52:24.000Z",
        "description": "Shrimp and corn chowder will warm you up!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "2 leeks (white and pale green parts only), chopped",
            "salt and ground black pepper to taste",
            "2 tablespoons all-purpose flour",
            "3 cups half-and-half",
            "1 pound potatoes, peeled and chopped",
            "1 (8 ounce) bottle clam juice",
            "1 pound cooked shrimp",
            "2 (8 ounce) bags frozen corn",
            "2 tablespoons fresh lemon juice",
            "2 tablespoons chopped fresh chives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a Dutch oven over medium heat. Stir leeks into the butter and season with salt and black pepper; cook and stir until the leeks are tender, 4 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour into the leek mixture until dissolved completely and the mixture begins to thicken; add half-and-half, potatoes, and clam juice. Bring the mixture to a boil, reduce heat to medium-low, and cook at a simmer for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir shrimp and corn into the potato mixture; continue cooking until the potatoes are completely tender, about 10 minutes more. Remove pot from heat, add lemon juice and chives, and stir.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chowders",
            "Corn Chowder Recipes"
        ],
