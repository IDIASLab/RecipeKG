    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222614/creamy-twice-baked-potatoes/",
        "name": "Creamy Twice-Baked Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7847789.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-28T23:21:24.000Z",
        "description": "These easy twice baked potatoes get their creamy texture from a rich cream cheese filling. Double the recipe if you're having guests over or want leftovers.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 baking potatoes",
            "2 tablespoons butter, softened",
            "1 tablespoon milk",
            "\u00bc teaspoon salt",
            "1 (3 ounce) package cream cheese, cubed",
            "2 tablespoons sour cream",
            "1 pinch paprika, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Scrub and dry potatoes, then prick several times with a fork and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until potatoes are easily pierced with a fork, 50 minutes to 1 hour. Remove from oven and cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut a thin slice off the top of each potato; scoop out pulp and place in a small bowl. Beat potato flesh, butter, milk, and salt with an electric mixer in a large bowl until smooth and fluffy. Fold cream cheese and sour cream into potatoes; spoon mixture into potato shells. Sprinkle with paprika and place on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until heated through and tops are golden brown, 20 to 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Twice Baked Potato Recipes"
        ],
