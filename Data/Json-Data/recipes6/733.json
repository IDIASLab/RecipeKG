    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230980/my-favorite-pot-roast/",
        "name": "My Favorite Pot Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1553071.jpg",
            "width": 3018,
            "height": 3018
        },
        "datePublished": "2020-03-05T05:10:09.000Z",
        "description": "This is the oven version that is the original I got from my mother. It is unbelievably delicious.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT4H10M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "1 (4 pound) beef pot roast",
            "1 tablespoon brown sugar",
            "1 tablespoon salt",
            "1 tablespoon dry mustard",
            "\u00bc teaspoon black pepper",
            "3 large potatoes, quartered",
            "3 carrots, peeled and cut into 2-inch pieces",
            "1 onion, sliced",
            "\u2153 cup vinegar",
            "\u2153 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a Dutch oven over medium heat. Cook beef in Dutch oven until browned on all sides, 2 to 3 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, salt, mustard, and black pepper together in a small bowl; sprinkle evenly over the roast. Arrange potatoes, carrots, and onion in the Dutch oven around the roast. Pour vinegar and water over the vegetables.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a cover on the Dutch oven and put in preheated oven. Cook until the roast is hot in the center, and just turning from pink to grey, about 4 hours. An instant-read thermometer inserted into the center should read 150 degrees F (65 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Beef",
            "Pot Roast Recipes"
        ],
