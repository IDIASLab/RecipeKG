        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22736/easy-southern-biscuits/",
        "name": "Easy Southern Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8073633.jpg",
            "width": 0,
            "height": 0
