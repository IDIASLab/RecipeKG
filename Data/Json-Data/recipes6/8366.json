    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258394/mushroom-rice-turnovers/",
        "name": "Mushroom Rice Turnovers",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mushroom Rice Turnovers"
        },
        "datePublished": "2020-04-24T19:11:14.000Z",
        "description": "My coworker gave me this recipe years ago and it's been one of my favorites ever since.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "16 turnovers",
        "recipeIngredient": [
            "1 tablespoon butter",
            "1 pound sliced fresh mushrooms",
            "5 scallions, sliced",
            "1 cup cooked brown rice",
            "\u00bd cup sour cream",
            "4 teaspoons dried dill weed",
            "1 teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1 (17.5 ounce) package frozen puff pastry, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a large skillet over medium heat. Add mushrooms and scallions; cook, stirring occasionally, until softened, about 10 minutes. Transfer to a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir brown rice, sour cream, dill, salt, and pepper into the mushroom mixture in the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unfold 1 puff pastry sheet onto an ungreased baking sheet. Spread mushroom mixture evenly on top, leaving a 1-inch border on all sides. Lay the second puff pastry sheet on top. Seal edges by pressing together with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pastry is puffed up and golden brown on top, about 30 minutes. Cool slightly before serving, about 10 minutes.\n"
            }
        ],
