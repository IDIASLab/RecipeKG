    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/47355/mustard-pickles/",
        "name": "Mustard Pickles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1326567.jpg",
            "width": 2919,
            "height": 2292
        },
        "datePublished": "2020-06-19T03:08:47.000Z",
        "description": "An old fashioned recipe that is different from most pickles. Cucumbers and onions are pickled in a mustard sauce made with turmeric and celery seed.",
        "prepTime": "P0DT0H45M",
        "cookTime": null,
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 - 1 quart jars",
        "recipeIngredient": [
            "8 large cucumbers, sliced",
            "4 cups sliced onion",
            "2 tablespoons pickling salt",
            "2 cups white sugar",
            "2 tablespoons all-purpose flour",
            "2 cups white vinegar",
            "1 tablespoon ground dried turmeric",
            "1 tablespoon dry mustard powder",
            "\u00bd teaspoon celery seed",
            "2 quarts cold water, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the sliced cucumbers and onions into a large bowl and sprinkle the salt over the top. Fill the bowl with enough water to cover the cucumbers. Let stand for 8 to 10 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain and rinse the cucumbers and onion with fresh water. Sterilize 4 (1 quart) jars and lids.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 6 quart pot, stir together the sugar and flour. Whisk in the vinegar, turmeric, mustard powder and celery seed to make a smooth paste. Add the cucumbers and onions and fill with enough water to cover. Stir, and bring to a boil. Cook until thickened, about 10 minutes, stirring frequently.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into hot sterilized jars filling to within 1/2 inch of the top. Seal with lids and rings. Process for 10 minutes in a simmering water bath. Refrigerate any jars that fail to seal properly.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Pickled"
        ],
