        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8342/rhubarb-upside-down-cake-i/",
        "name": "Rhubarb Upside Down Cake I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1132419.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "1999-06-23T00:26:42.000Z",
        "description": "This easy recipe uses cake mix to make a delicious upside-down cake with rhubarb and marshmallows.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "6 cups chopped rhubarb",
            "1 cup white sugar",
            "3 cups miniature marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chopped rhubarb on the bottom of greased 9x13 inch pan. Sprinkle sugar evenly over rhubarb. Sprinkle marshmallows over rhubarb and sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare cake mix according to instructions on package. Pour over rhubarb and marshmallows in pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 1 hour. Cool in pan for 10 minutes, then invert onto serving dish so that rhubarb is on top.\n"
            }
        ],
