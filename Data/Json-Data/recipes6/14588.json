        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16776/fresh-strawberry-pie-iii/",
        "name": "Fresh Strawberry Pie III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F613611.jpg",
