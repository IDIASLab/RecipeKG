        "cookTime": null,
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) pie crust, baked",
            "1 cup white sugar",
            "3 tablespoons strawberry flavored Jell-O\u00ae mix",
            "2 tablespoons cornstarch",
            "\u00bc teaspoon salt",
            "1 cup boiling water",
            "2 pints strawberries, cleaned and stemmed",
            "2 cups whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine sugar, gelatin, cornstarch, and salt in a medium saucepan. Stir in boiling water. Boil mixture for 3 minutes over high heat, stirring constantly. Cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange whole strawberries in pastry shell. Pour gelatin mixture over berries. Chill before serving. Top with whipped cream.\n"
            }
        ],
