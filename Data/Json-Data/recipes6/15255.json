    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19027/creme-brulee-i/",
        "name": "Creme Brulee I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2785578.jpg",
            "width": 4272,
            "height": 2848
        },
        "datePublished": "2000-06-11T05:49:23.000Z",
        "description": "Stovetop creme brulee with a brown sugar top, served over fruit -- use raspberries, or substitute your favorite seasonal fruit.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 - 9 inch round",
        "recipeIngredient": [
            "4 egg yolks",
            "3 tablespoons white sugar",
            "2 cups heavy cream",
            "\u2153 cup brown sugar",
            "4 cups raspberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat egg yolks in medium bowl with electric mixer on high speed, 5 minutes or until thick and light yellow.  Gradually beat in sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat cream in medium saucepan over medium heat until just hot to the touch.  A little at a time, so the eggs do not curdle, stir half of the cream into the egg yolk mixture.  Then stir all the egg yolk and cream mixture into the remaining cream in the saucepan.  Cook, stirring continuously, over low heat, 5 to 8 minutes, until mixture thickens; do not boil.  Pour custard into 9 inch pie plate, cover and refrigerate 2 to 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Before serving, sprinkle with brown sugar and place under preheated oven broiler, or use a kitchen torch to melt sugar.  Serve with berries.\n"
            }
        ],
