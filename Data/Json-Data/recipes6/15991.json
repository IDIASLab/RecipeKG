        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22522/sweet-potato-pie-ix/",
        "name": "Sweet Potato Pie IX",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7399477.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-10-01T00:05:19.000Z",
        "description": "A tasty pie made from sweet potatoes, and always an interesting addition at Thanksgiving or at anytime of year!!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "2 9-inch pies",
        "recipeIngredient": [
            "3 large sweet potatoes",
            "\u00bd cup butter, softened",
            "1 tablespoon vanilla extract",
            "2 cups sugar, or more to taste",
            "\u00bd teaspoon ground nutmeg",
            "4 eggs, beaten",
            "\u00be cup evaporated milk",
            "2 (9 inch)  unbaked pie shells"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of water to a boil.  Add sweet potatoes and cook until tender but still firm, about 30 minutes. Drain, cool, peel and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sweet potatoes, butter, sugar, vanilla and nutmeg in a large bowl. Whisk together the eggs and milk in a small bowl, and blend into the sweet potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pie shells and bake in preheated oven for 60 minutes, or until done.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Sweet Potato Pie Recipes"
        ],
