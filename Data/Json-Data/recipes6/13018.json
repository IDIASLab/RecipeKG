    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11436/choc-oat-pb-bars/",
        "name": "Choc-Oat-PB Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F857162.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-07-30T00:53:14.000Z",
        "description": "These are like the no-bake cookies, but much better as a bar!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "14 bars",
        "recipeIngredient": [
            "2 cups white sugar",
            "\u00bc cup cocoa powder",
            "\u00bd cup evaporated milk",
            "\u00bd cup butter, melted",
            "2\u2009\u00bd cups rolled oats",
            "\u00bd cup peanut butter",
            "2 tablespoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray a 9x9 inch baking pan with non-aerosol cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, cocoa, milk and butter in a saucepan. Bring mixture to a boil. Stop stirring after the mixture starts boiling; allow it to boil for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove mixture from heat and stir in oats, peanut butter and vanilla; mix well. Spread mixture into the prepared pan. Cool and store in refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "No-Bake Cookie Recipes"
