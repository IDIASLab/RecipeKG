        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (15 ounce) can black beans",
            "1 (15 ounce) can kidney beans, drained",
            "1 (15 ounce) can butter beans",
            "1 (15 ounce) can lima beans, drained",
            "1 cup packed brown sugar",
            "1 cup chopped onion",
            "\u00bd cup fresh tomato sauce",
            "\u00bd cup chopped celery",
            "3 tablespoons white wine vinegar",
            "1 teaspoon mustard powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir black beans, kidney beans, butter beans, lima beans, brown sugar, onion, tomato sauce, celery, vinegar, and mustard powder together in a four-quart casserole dish; cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 1 hour.\n"
            }
        ],
