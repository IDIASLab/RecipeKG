    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19081/sayuris-three-sugar-pound-cake/",
        "name": "Sayuri's Three-Sugar Pound Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sayuri's Three-Sugar Pound Cake"
        },
        "datePublished": "2000-04-03T02:17:44.000Z",
        "description": "My personal pound cake recipe that I developed over the years. This pound cake is very good and, as the name suggests, uses three different kinds of sugar.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u2154 cup milk",
            "3 teaspoons vanilla extract",
            "\u00be cup butter, softened",
            "\u00bd cup white sugar",
            "\u00bc cup confectioners' sugar",
            "\u00bc cup brown sugar",
            "2 egg whites",
            "1 egg yolk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease and flour a 9x13 inch pan. Sift together the flour, baking powder and baking soda; set aside. In a measuring cup combine the milk and vanilla; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and white, confectioners' and brown sugars until light and fluffy. Beat in the egg whites, then the yolk. Beat in the flour mixture alternately with the milk, mixing just until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter evenly into prepared pan. Bake for 50 to 60 minutes in the preheated oven, until a toothpick inserted in the center of the cake comes out clean. Cool in the pan for 10 minutes before inverting onto a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Sheet Cake Recipes"
        ],
