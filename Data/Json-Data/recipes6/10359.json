        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1 cup self-rising flour",
            "3 tablespoons white sugar",
            "3 tablespoons mayonnaise",
            "\u00bd cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).  Grease muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour and sugar together.  Stir milk and mayonnaise.  Fill muffin cups 1/2 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 15 minutes.  Serve warm.\n"
            }
        ],
