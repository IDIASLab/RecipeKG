    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15311/chewy-crispy-coconut-cookies/",
        "name": "Chewy Crispy Coconut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F364291.jpg",
            "width": 3056,
            "height": 2292
        },
        "datePublished": "1999-12-21T18:13:36.000Z",
        "description": "Crispy, chewy, coconut, oatmeal, cornflake cookies. Wow are these good!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "\u00bd cup packed brown sugar",
            "\u00bd cup white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 cup crushed cornflakes cereal",
            "1 cup rolled oats",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon baking powder",
            "1\u2009\u2153 cups flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter, brown sugar and white sugar until smooth. Stir in the egg and vanilla. Sift together the flour, baking soda, salt and baking powder; stir into the creamed mixture. Add the oatmeal, crushed cereal and coconut and mix until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by teaspoonfuls onto a cookie sheet. Cookies should be about 2 inches apart. Bake for 10 to 12 minutes in the preheated oven. Cookies should be light brown at the edges and on the bottom. Remove from baking sheets to cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes"
        ],
        "recipeCuisine": [],
        "author": [
