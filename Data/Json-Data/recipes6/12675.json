        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10910/cocoa-quickies/",
        "name": "Cocoa Quickies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F190927.jpg",
