    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279419/lightened-up-macaroni-and-cheese/",
        "name": "Lightened-Up Macaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7847616.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-02T03:16:01.000Z",
        "description": "Who doesn't love macaroni and cheese? This lightened-up version is sure to please those who want to indulge without the extra calories!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "6 ounces whole wheat elbow macaroni",
            "1 (5 ounce) can evaporated skim milk, divided",
            "1 teaspoon tapioca starch",
            "5 ounces low-fat sharp Cheddar cheese, grated",
            "2 teaspoons Dijon mustard, or more to taste",
            "\u00bd teaspoon Worcestershire sauce",
            "1 pinch cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Coat an 8x8-inch baking dish with cooking spray and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook whole wheat macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir 1 tablespoon evaporated milk and tapioca together in a saucepan. Whisk in remaining milk. Stir in Cheddar cheese, mustard, Worcestershire sauce, and pepper. Bring to a simmer and stir until the sauce thickens a bit and cheese is completely melted. Drain pasta and stir into the sauce. Spoon into the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook under the broiler until top browns, 2 to 3 minutes. Remove from oven and allow to cool for 5 minutes before serving.\n"
            }
        ],
