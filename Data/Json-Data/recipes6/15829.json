    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21410/twice-fried-fish/",
        "name": "Twice Fried Fish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2526652.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2000-08-03T16:48:50.000Z",
        "description": "Slightly spicy and aromatic, this fried fish is always my husband's favorite choice!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "5 filets",
        "recipeIngredient": [
            "1 tablespoon hoisin sauce",
            "1 tablespoon canned tomato sauce",
            "1 tablespoon dry sherry",
            "1 teaspoon ground black pepper",
            "2 tablespoons dark soy sauce",
            "1 teaspoon white sugar",
            "1 teaspoon salt",
            "1\u2009\u00bd pounds cod fillets",
            "3 tablespoons vegetable oil",
            "1 tablespoon lard",
            "1 teaspoon cornstarch",
            "4 tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, mix together hoisin sauce, tomato sauce, sherry, pepper, soy sauce, sugar and salt. Set aside for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rub cod filets with 1 tablespoon of oil. Heat a large skillet and add the remaining 2 tablespoons of oil. Fry fish for 2 minutes on both sides, drain on paper towels, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Discard oil from skillet, and add 1 tablespoon of lard. Melt over medium heat and add soy sauce mixture, stirring well. Dissolve the cornstarch in the water and pour into the skillet, stirring constantly. Cook until thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return fish to the skillet and cook again for 1 minute on each side.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish"
        ],
