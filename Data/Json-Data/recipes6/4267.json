    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278878/flourless-chocolate-cookies/",
        "name": "Flourless Chocolate Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Flourless Chocolate Cookies"
        },
        "datePublished": "2020-03-07T10:31:56.000Z",
        "description": "These cookies have a delicate crispy and chewy texture and are packed with chocolate flavor!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "20 cookies",
        "recipeIngredient": [
            "4 egg whites",
            "2 teaspoons vanilla extract",
            "3 cups powdered sugar",
            "\u00bd cup unsweetened cocoa powder",
            "3 tablespoons unsweetened cocoa powder",
            "2 cups chopped pecans",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites with an electric mixer on medium-low speed until well combined and foamy, but not stiff, about 2 minutes. Beat in vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir powdered sugar, 1/2 cup plus 3 tablespoons cocoa powder, pecans, and salt together in a medium bowl. Add to the beaten egg whites and stir with a spatula until just combined; the batter will look similar to brownie batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop mounds of the batter onto the baking sheets using a 2-tablespoon cookie scoop. Space the mounds several inches apart, so there are 6 on each sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until puffed, shiny, and cracked, 13 to 17 minutes. Cool on the baking sheets for 3 to 5 minutes. Gently peel the cookies off the parchment paper and move to a cooling rack. Repeat with remaining batter once the baking sheets have cooled.\n"
            }
        ],
