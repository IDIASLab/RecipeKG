    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275439/spinach-ricotta-quiche/",
        "name": "Spinach-Ricotta Quiche",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6947995.jpg",
            "width": 3258,
            "height": 3258
        },
        "datePublished": "2020-04-25T00:12:23.000Z",
        "description": "This savory quiche is perfect for breakfast, brunch, lunch, or served with a side salad for a light dinner. It's light and fluffy with a creamy texture, and bursting with delicious flavors!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9 1/2-inch quiche",
        "recipeIngredient": [
            "1 tablespoon butter",
            "\u2153 cup finely chopped red onion",
            "1 (8 ounce) package fresh spinach",
            "\u00bd (14 ounce) package pastry for 9-inch double-crust pie",
            "4 large eggs",
            "\u00be cup whole-milk ricotta cheese",
            "\u00be cup heavy cream",
            "\u2153 cup grated Parmigiano-Reggiano cheese  ",
            "1 tablespoon chopped fresh basil",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a skillet over medium heat; add onion and cook for 1 minute. Add spinach and cook, stirring occasionally, until starting to wilt, about 1 minute. Cover skillet and cook for 1 minute more, allowing condensation to build inside the skillet. Remove from heat, uncover, and stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Press pie pastry into a 9 1/2-inch deep-dish pie pan. Prick bottom and sides with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven for 10 minutes. Remove from oven and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine eggs, ricotta cheese, cream, Parmigiano Reggiano cheese, basil, salt, and pepper in a blender or food processor. Blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread spinach and onion mixture evenly over the crust. Pour egg mixture on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until center of quiche is set and the top is lightly browned, about 40 minutes. Allow to stand for 10 minutes before serving.\n"
            }
        ],
