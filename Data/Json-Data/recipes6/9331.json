        "mainEntityOfPage": "https://www.allrecipes.com/recipe/194425/creamy-cheesy-scrambled-eggs-with-basil/",
        "name": "Creamy Cheesy Scrambled Eggs with Basil",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F664766.jpg",
            "width": 3072,
            "height": 2304
