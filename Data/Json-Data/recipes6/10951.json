    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7594/white-velvet-cake-i/",
        "name": "White Velvet Cake I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1008536.jpg",
            "width": 1243,
            "height": 1243
        },
        "datePublished": "1998-06-01T16:32:13.000Z",
        "description": "Use fresh coconut to achieve this velvety soft white cake that is a great dessert cake for picnics, potlucks, or family gatherings.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 9 inch layers",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "1\u2009\u00bd cups white sugar",
            "2\u2009\u00bd cups all-purpose flour",
            "3 teaspoons baking powder",
            "1 teaspoon salt",
            "1 cup milk",
            "3 egg whites",
            "1 teaspoon vanilla extract",
            "1 tablespoon all-purpose flour",
            "1 cup milk",
            "1 cup coconut milk",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "1 fresh coconut",
            "2 teaspoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, sift 2 1/2 cups flour, baking powder, and salt together and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend shortening and 1 1/2 cups sugar.  Mix in the flour mixture.  Add 3/4 cup milk; beat for 2 minutes.  Add egg whites, 1 teaspoon vanilla and 1/4 cup milk.  Beat well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease and flour two 9 inch round pans.  Pour the batter into the pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 35 minutes.  Cool cake layers on a wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the meat from the coconut and shred it.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting:  Moisten 1 tablespoon flour with a small amount of water to make a paste.  Combine 1 cup milk, coconut milk, 1 cup sugar, 1 teaspoon vanilla, and shredded coconut in a saucepan.  Mix in flour paste, and cook for 10 minutes.  Cool the frosting, and spread over cake.\n"
            }
        ],
