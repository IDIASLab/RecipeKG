    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9098/corn-souffle/",
        "name": "Corn Souffle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3032361.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "I make this every year, and my family always looks forward to having it.  It's one of those recipes that looks like you've spent a lot of time on, but it's so easy to make. Originally submitted to ThanksgivingRecipe.com.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (15.25 ounce) cans whole kernel corn",
            "2 (14.75 ounce) cans cream-style corn",
            "4 tablespoons white sugar",
            "4 tablespoons all-purpose flour",
            "4 tablespoons milk",
            "4 eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the corn, creamed corn, sugar, flour, milk and eggs. Mix well. Pour the mixture into a 2-quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 to 1 1/2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Corn"
        ],
