        "recipeIngredient": [
            "\u00bd cup raisins",
            "\u00bd cup mandarin oranges",
            "1 cup pitted prunes",
            "\u00be cup white sugar",
            "water to cover",
            "1 (15 ounce) can sliced peaches, chopped",
            "1 (15 ounce) can pineapple chunks",
            "\u00be cup brandy"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan combine raisins, mandarin oranges, prunes, sugar and water to cover.  Simmer over medium heat until syrupy, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in peaches, pineapple and brandy.  Store in refrigerator.\n"
            }
