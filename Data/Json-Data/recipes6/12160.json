        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10128/mississippi-mud-cookies/",
        "name": "Mississippi Mud Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F452703.jpg",
