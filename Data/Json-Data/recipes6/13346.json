    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12270/cranberry-pie-i/",
        "name": "Cranberry Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cranberry Pie I"
        },
        "datePublished": "1998-05-09T01:41:06.000Z",
        "description": "What would be Thanksgiving without cranberries?  Here is a lovely pie to go with that celebration.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT2H30M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 recipe pastry for a 9 inch double crust pie",
            "3 cups cranberries",
            "1 cup raisins",
            "2 tablespoons all-purpose flour",
            "1\u2009\u00bc cups white sugar",
            "\u00bd cup water",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). Line a 9 inch pie pan with pastry, and save remaining pastry for the top. Refrigerate both while you make the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chop cranberries coarsely. Combine with raisins, flour, sugar, water, and vanilla. Place filling in chilled pie shell. Cover with top crust, slash top in several places, and crimp edges together securely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven) for 10 minutes. Reduce oven temperature to 350 degrees F (175 degrees C), and continue baking for 30 minutes. Cool before serving.\n"
            }
        ],
