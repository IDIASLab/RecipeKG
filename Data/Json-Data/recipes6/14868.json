    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17629/australian-pavlova/",
        "name": "Australian Pavlova",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F621219.jpg",
            "width": 1084,
            "height": 1021
        },
        "datePublished": "2000-02-01T21:51:42.000Z",
        "description": "A large meringue cake that can be topped with whipped cream and fruit. This is an Australian specialty.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 9 to 10 inch round",
        "recipeIngredient": [
            "4 egg whites",
            "1 cup white sugar",
            "1 pinch salt",
            "1 tablespoon malt vinegar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 150 degrees F (65 degrees C). Line a baking sheet with parchment paper. Trace around a 9 or 10 inch round pan onto the paper to make a guide for spreading the meringue.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal mixing bowl, beat egg whites until foamy. Gradually add sugar and salt. When mixture is stiff and sugar and salt appear to be dissolved, add the vinegar and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread mixture onto parchment paper, using the circular guide.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 1 hour.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Pavlova Recipes"
        ],
