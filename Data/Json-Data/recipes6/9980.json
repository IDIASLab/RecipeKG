    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280484/strawberry-chocolate-chip-muffins/",
        "name": "Strawberry Chocolate Chip Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Strawberry Chocolate Chip Muffins"
        },
        "datePublished": "2017-12-30T06:28:43.000Z",
        "description": "These strawberry chocolate chip muffins are yummy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1 cup frozen strawberries, roughly chopped",
            "2 tablespoons all-purpose flour",
            "2 cups all-purpose flour",
            "1 cup white sugar",
            "2 teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon salt",
            "\u2154 cup buttermilk",
            "\u00bd cup vegetable oil",
            "1 large egg",
            "2\u2009\u00bd tablespoons plain yogurt",
            "1 teaspoon vanilla extract",
            "\u00be cup chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease or line a 12-cup muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix strawberries and 2 tablespoons flour together in a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 cups flour, sugar, baking powder, cinnamon, and salt in a separate bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine buttermilk, oil, egg, yogurt, and vanilla extract in a large bowl. Pour in dry ingredients and mix until just incorporated. Fold in strawberries and chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the prepared muffin cups, filling each 2/3 to 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of a muffin comes out clean, about 20 minutes.\n"
            }
        ],
