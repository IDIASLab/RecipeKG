    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21506/autumn-apple-salad/",
        "name": "Autumn Apple Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1118473.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-09-21T22:45:07.000Z",
        "description": "This sparkling salad is creamy and slightly tart. Nuts and celery add texture. Not only do adults love it, so do the kids! Cut the finished salad into squares and serve on crisp leaves of lettuce. Adjust the amount of nuts, if desired.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (20 ounce) can crushed pineapple with juice",
            "\u2154 cup white sugar",
            "1 (3 ounce) package lemon flavored Jell-O\u00ae mix",
            "1 (8 ounce) package cream cheese, softened",
            "1 cup diced, unpeeled apples",
            "1 cup chopped nuts",
            "1 cup chopped celery",
            "1 cup frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan over medium heat, boil the pineapple and sugar 3 minutes. Mix in the lemon gelatin mix, stirring until dissolved. Stir in the cream cheese. Remove from heat and allow to cool 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently fold the apples, nuts, celery and whipped topping into the mixture. Transfer to a 9x9 inch square pan. Refrigerate 4 hours, or until firm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes",
            "Jell-O&reg; Salad"
        ],
