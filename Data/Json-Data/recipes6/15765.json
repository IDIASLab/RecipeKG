    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21053/haitian-french-toast/",
        "name": "Haitian French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F77605.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-06-13T00:22:23.000Z",
        "description": "A yummy and rich French toast perfect for a celebration brunch. I first served this for New Year's Day brunch with a mixed grill of meats and a bottle of good champagne.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT4H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 French baguette",
            "1 cup orange juice",
            "\u00bd cup heavy whipping cream",
            "2 eggs",
            "1 teaspoon ground cinnamon",
            "\u00bc cup white sugar",
            "1 dash ground nutmeg",
            "3 tablespoons butter",
            "2 tablespoons confectioners' sugar for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut loaf ends off and reserve for another use. Cut remaining loaf into 1 1/2 inch slices; let stand 4 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 9x13 inch baking pan, combine orange juice, cream, eggs ground cinnamon and white sugar. Place bread slices in pan and turn until liquid is absorbed, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, melt butter over medium heat. Add bread slices and cook until browned on both sides, about 5 minutes. Dust generously with confectioners' sugar and serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "French Toast Recipes"
        ],
