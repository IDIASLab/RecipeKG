    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11141/cookie-press-butter-cookies/",
        "name": "Cookie Press Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27265.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "1998-12-01T22:03:38.000Z",
        "description": "This is an old family recipe that makes a very delicious cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups unsalted butter, softened",
            "1 cup white sugar",
            "2 egg yolks",
            "2 tablespoons water",
            "3\u2009\u00be cups all-purpose flour",
            "1 pinch salt",
            "1 tablespoon vanilla extract",
            "2 egg whites",
            "2 tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together the butter and sugar until light and fluffy. Mix in the egg yolks, 2 tablespoons water, flour, salt and vanilla. Chill the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill the cookie press and shape the cookies on an ungreased baking sheet. Brush the top of each cookie with the egg whites mixed with 2 tablespoons of the water. Decorate as desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) until the cookies are golden brown on top, about 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Butter Cookie Recipes",
