        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20289/fancy-pancakes/",
        "name": "Fancy Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F834350.jpg",
