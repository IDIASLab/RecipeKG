    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273478/baked-buffalo-chicken-potato-bowl-for-two/",
        "name": "Baked Buffalo Chicken Potato Bowl for Two",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Baked Buffalo Chicken Potato Bowl for Two"
        },
        "datePublished": "2020-03-20T06:50:32.000Z",
        "description": "Quick and easy Buffalo chicken served on top of a baked potato, corn, and topped with cheese in a bowl.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 baking potatoes",
            "2 skinless, boneless chicken breasts",
            "\u00bd cup Buffalo sauce, or to taste",
            "1 (15.25 ounce) can whole kernel corn, drained",
            "3 tablespoons butter",
            "2 tablespoons garlic powder",
            "\u215b cup shredded Cheddar cheese, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium-high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prick each potato several times with a fork. Microwave potatoes for 5 minutes, turn, and continue to cook in the microwave until tender, about 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill chicken on the preheated grill until no longer pink in the center and the juices run clear, about 10 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Let cool 10 minutes; cut into strips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place grilled chicken strips into a zip-top bag. Pour Buffalo sauce over chicken; seal bag and shake until evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut open potatoes and place in a large, microwave-safe bowl. Place grilled chicken on top of potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour corn into a microwave-safe bowl; add butter and garlic powder and stir to combine. Microwave corn mixture until warm, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour corn on top of chicken and top with Cheddar cheese. Microwave once more until cheese is melted, about 1 minute.\n"
            }
        ],
