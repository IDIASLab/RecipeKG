        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10489/stir-and-drop-sugar-cookies-ii/",
        "name": "Stir and Drop Sugar Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1110796.jpg",
            "width": 3264,
            "height": 1836
        },
        "datePublished": "1998-02-11T19:01:03.000Z",
        "description": "Delicate brown sugar cookies.  If you use self-rising flour, omit baking powder and salt.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "3 egg whites",
            "\u2154 cup vegetable oil",
            "2 teaspoons vanilla extract",
            "1 teaspoon lemon zest",
            "\u00be cup white sugar",
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites with fork.  Stir in vegetable oil, vanilla, and lemon rind.  Blend in sugar until mixture thickens.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Measure flour by dipping method or by sifting.  Stir together flour, baking powder, and salt; blend into egg mixture. Drop dough by teaspoonfuls about 2 inches apart on ungreased baking sheet. Flatten with oiled bottom of glass dipped in sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 8 to 10 minutes or until a delicate brown.  Remove from baking sheet immediately.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
