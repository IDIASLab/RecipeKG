    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/266753/bake-sale-golden-peanut-butter-bars/",
        "name": "Bake Sale Golden Peanut Butter Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F798700.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T05:18:10.000Z",
        "description": "If you like peanut butter and need a good recipe to bring to a bake sale, this is what you need. Store covered at room temperature.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "30 bars",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00be cup firmly packed light brown sugar",
            "1 egg, beaten",
            "\u00bd cup cold butter",
            "1 cup finely chopped peanuts",
            "1 (14 ounce) can sweetened condensed milk",
            "\u00bd cup peanut butter",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, brown sugar, and egg in a large bowl. Cut in butter until mixture appears crumbly. Stir in chopped peanuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set 2 cups of the peanut crumble aside. Press remaining mixture into a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven until lightly browned, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat condensed milk, peanut butter, and vanilla extract together in a large bowl. Spread over the prepared crust. Top with reserved peanut crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly browned, about 25 minutes. Cool before cutting into bars.\n"
            }
        ],
