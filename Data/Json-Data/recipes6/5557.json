    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/76117/baked-creamed-corn-ii/",
        "name": "Baked Creamed Corn II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4812028.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2020-03-10T06:50:41.000Z",
        "description": "This is a great creamed corn recipe that is not too sweet and is baked in the oven to give it a firmer consistency.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 (8 ounce) package whipped cream cheese",
            "1 cube chicken bouillon",
            "1 (10 ounce) package frozen corn kernels, thawed",
            "\u00bc teaspoon white pepper",
            "\u215b teaspoon ground nutmeg",
            "salt to taste",
            "\u00bd cup heavy cream",
            "\u00bd cup shredded mozzarella cheese",
            "\u00bd cup crushed buttery round crackers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly grease an 8x8 inch casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat, melt the butter and cream cheese, and dissolve the chicken bouillon. Mix in the corn. Season with pepper, nutmeg, and salt. Stir in the cream. Transfer the mixture to the prepared casserole dish. Sprinkle with cheese, and top with crackers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 20 minutes in the preheated oven, or until lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Corn Casserole Recipes"
        ],
