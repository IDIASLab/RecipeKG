    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15695/sea-foam-candy/",
        "name": "Sea Foam Candy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1053922.jpg",
            "width": 3264,
            "height": 1840
        },
        "datePublished": "1999-12-24T23:09:08.000Z",
        "description": "A beige, delicately flavored, smooth and creamy candy.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "50 pieces",
        "recipeIngredient": [
            "1\u2009\u00be cups packed light brown sugar",
            "\u00be cup white sugar",
            "\u00bd cup hot water",
            "\u00bc cup light corn syrup",
            "\u00bc teaspoon salt",
            "2 egg whites",
            "1 teaspoon vanilla extract",
            "1 teaspoon pecan flavored extract",
            "\u00bc cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine light brown and white sugars, water, corn syrup, and salt in a heavy 2 quart saucepan.  Cook covered until mixture boils rapidly.  Remove lid, place candy thermometer in pan, and cook without stirring to 260 degrees F (127 degrees C).  Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until stiff.  Pour hot syrup in thin stream over egg whites, beating constantly with electric mixer on high speed. Add vanilla and pecan flavoring.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue beating until soft peaks form and candy starts to lose its gloss.  Stir in pecans and drop by spoonfuls onto waxed paper. When candy has set, store in airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Nut Candy Recipes"
        ],
