        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259210/buffalo-and-ranch-pumpkin-seeds/",
        "name": "Buffalo and Ranch Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4542750.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-03-20T06:50:25.000Z",
        "description": "My own personal creation. All the spice of buffalo wings with a hint of ranch flavor, coupled with the crunch of toasted pumpkin seeds.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (8 ounce) bottle buffalo wing sauce",
            "1 (1 ounce) package ranch dressing mix",
            "1\u2009\u00bd tablespoons lemon pepper",
            "seasoned salt",
            "4 cups pumpkin seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Line baking sheet with wax paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine buffalo sauce, ranch seasoning, lemon pepper, and salt in a small bowl. Toss pumpkin seeds in mixture until evenly coated. Spread on the prepared sheet in an even layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, stirring every 15 minutes, until seeds are golden brown and crunchy, about 45 minutes.\n"
            }
        ],
