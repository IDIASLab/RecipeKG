    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25207/blueberry-gems/",
        "name": "Blueberry Gems",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3559190.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:06:07.000Z",
        "description": "Wonderful blueberry muffins with a twist - orange twist! Serve warm with butter.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H18M",
        "totalTime": "P0DT0H33M",
        "recipeYield": "3 dozen mini-muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bc cup white sugar",
            "1\u2009\u00bd teaspoons baking powder",
            "\u215b teaspoon salt",
            "2 egg whites",
            "\u2154 cup orange juice",
            "2 tablespoons vegetable oil",
            "1\u2009\u00bd tablespoons vanilla extract",
            "1\u2009\u00bd cups fresh or frozen blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly coat muffin pans with nonstick cooking spray, set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, stir together flour, sugar, baking powder, and salt.  Make a well in the center of flour mixture, set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, stir together egg whites, orange juice, oil and vanilla.  Add egg white mixture all at once to the flour mixture; stir just until moistened.  Fold in blueberries.  Spoon into prepared muffin cups, filling each about 2/3 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 to 18 minutes, or until golden and toothpick inserted into centers comes out clean.  Cool slightly before removing from pans. Serve warm with butter.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
