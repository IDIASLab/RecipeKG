    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11442/special-cereal-bars-ii/",
        "name": "Special Cereal Bars II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1097278.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-06-16T21:53:46.000Z",
        "description": "A coco-nutty, peanut buttery, chocolately and crunchy bar-cookie!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "16 to 20 bars",
        "recipeIngredient": [
            "4 cups high protein crisp rice and wheat cereal",
            "1 cup shredded coconut",
            "1\u2009\u00bd cups creamy peanut butter",
            "1 cup peanuts",
            "1 cup white sugar",
            "1 cup white corn syrup",
            "2 cups butterscotch chips",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cereal and coconut in a medium size bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine peanut butter and peanuts in a large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat the sugar and corn syrup over a medium heat and stir constantly until the sugar is dissolved. Pour this mixture into the bowl with the peanut butter; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cereal mixture into the liquid mixture; mix well. Press the mixture into an ungreased 10x15 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butterscotch and chocolate chips over a medium-low heat; mix well. Spread the mixture over the pan of cereal mixture. Let cool before cutting into bars.\n"
            }
        ],
