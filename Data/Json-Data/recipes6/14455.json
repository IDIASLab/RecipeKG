    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16171/sweet-carrot-casserole/",
        "name": "Sweet Carrot Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27783.jpg",
            "width": 2000,
            "height": 2847
        },
        "datePublished": "2000-01-15T22:48:12.000Z",
        "description": "Carrots, spiced sweetly! This recipe is a little heavier than a souffle, but equally as scrumptious and rich!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups sliced carrots",
            "1 tablespoon butter",
            "3 eggs",
            "1 cup white sugar",
            "\u2153 cup milk",
            "\u00bd teaspoon salt",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground nutmeg",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease a 1 quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place carrots and enough water to cover them in a small saucepan. Bring to a boil, and cook until carrots are very tender. Drain. Using a blender or food processor, puree carrots to a smooth consistency.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the carrots and butter. Beat in the eggs, sugar, milk, salt, cinnamon, nutmeg,  and vanilla. Spread mixture into the prepared casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 to 30 minutes in the preheated oven, or until set.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Carrots"
        ],
