    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7234/grandma-emmas-spice-loaf/",
        "name": "Grandma Emma's Spice Loaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6212950.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "1999-10-01T02:45:39.000Z",
        "description": "A quick bread, moist and full of raisins and nuts. A family favorite for the winter holidays.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "1 cup hot, brewed coffee",
            "\u00bd cup raisins",
            "\u00bd cup shortening",
            "1 cup white sugar",
            "2 eggs",
            "2 tablespoons molasses",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon ground ginger",
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak raisins in coffee until plump. Do not drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the shortening, sugar and eggs. Blend in molasses and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl mix together the cinnamon, nutmeg, cloves, ginger, flour, baking soda, and salt. Make a well in the center of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the liquid ingredients to the well in the dry ingredients. Stir in coffee with raisins and nuts. Pour batter into a greased 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degree F(175 degrees C) oven for one hour.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
