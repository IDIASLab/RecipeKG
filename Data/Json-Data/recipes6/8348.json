    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279740/potato-soup-with-ground-beef/",
        "name": "Potato Soup with Ground Beef",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Potato Soup with Ground Beef"
        },
        "datePublished": "2020-04-24T18:33:18.000Z",
        "description": "Warm and hearty comfort food. My family of 9, (yes, 9) loves this potato soup with ground beef. One of the few meals the entire family can agree upon and fits within our food allergies. Great on a cold day.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ground beef",
            "2 tablespoons all-purpose flour",
            "2\u2009\u00bd pounds Yukon Gold potatoes, peeled and chopped",
            "\u00bc cup unsalted butter",
            "1 bunch celery, chopped",
            "1 medium onion, chopped",
            "1 pint heavy cream",
            "1 cup whole milk",
            "1 cup water",
            "1 pinch fennel seeds, or to taste",
            "salt and ground white pepper to taste",
            "\u00bc cup shredded Cheddar cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook and stir ground beef in a pot over medium-high heat until slightly browned, about 5 minutes. Stir in flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While meat is browning, rinse starch from potatoes and keep in water until needed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove browned meat to a plate, discarding fat. Place butter in the pot along with celery and onion. Cook over medium-high heat until onion is soft and translucent, 5 to 7 minutes. Add potatoes, heavy cream, milk, and water. Cook at a low simmer until all liquid is slightly thick and potatoes are soft, 45 minutes to 1 hour. Season with fennel seeds, salt, and pepper. Top with Cheddar cheese.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Potato Soup Recipes"
        ],
