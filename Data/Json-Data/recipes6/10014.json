    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280488/rhubarb-cheesecake-squares/",
        "name": "Rhubarb Cheesecake Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1133500.jpg",
            "width": 2736,
            "height": 2736
        },
        "datePublished": "2020-06-10T08:35:31.000Z",
        "description": "Since this rhubarb cheesecake squares recipe is always requested, it is too scrumptious not to share with many!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "16 squares",
        "recipeIngredient": [
            "1  serving cooking spray",
            "2 cups all-purpose flour",
            "1 cup old-fashioned oats",
            "1 cup brown sugar",
            "\u215b teaspoon ground cinnamon",
            "\u00bd cup cold unsalted butter",
            "2 (8 ounce) packages cream cheese, softened",
            "2 cups white sugar",
            "2 large eggs",
            "1 teaspoon vanilla extract",
            "1 teaspoon vanilla butter and nut flavoring",
            "\u215b teaspoon ground cinnamon",
            "2 cups thinly sliced rhubarb"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 9x13-inch baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, oats, brown sugar, and cinnamon in a bowl. Cut in butter until coarse and crumbly. Press into the bottom of the prepared pan to form a crust, reserving about 1/3 cup for topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cream cheese, sugar, eggs, vanilla extract, vanilla butter and nut flavoring, and cinnamon together in a bowl until smooth. Stir in rhubarb. Pour filling on top of crust. Sprinkle with the reserved crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick or knife inserted into the center comes out clean, 45 to 50 minutes. Remove from the oven and let cool completely before cutting into squares.\n"
            }
        ],
