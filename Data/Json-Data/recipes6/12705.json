    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10949/pumpkin-cookies-v/",
        "name": "Pumpkin Cookies V",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F270183.jpg",
            "width": 1796,
            "height": 1797
        },
        "datePublished": "1998-01-04T21:39:43.000Z",
        "description": "Soft, cake-like cookies with caramel icing.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups shortening",
            "2 cups white sugar",
            "2 cups canned pumpkin",
            "2 eggs",
            "2 teaspoons baking soda",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "1 teaspoon salt",
            "4 cups all-purpose flour",
            "6 tablespoons butter",
            "8 tablespoons milk",
            "2 cups confectioners' sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1 cup packed brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream shortening, white sugar and pumpkin. Add eggs and mix well.  Sift together the baking soda, ground cinnamon, salt and flour.  Add to pumpkin mixture and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop from spoon to cookie sheet.  Bake 10 minutes at 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting:  Cook butter, milk, and brown sugar until dissolved.  Cool and add confectioners' sugar and vanilla.  Spread over warm cookies.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
