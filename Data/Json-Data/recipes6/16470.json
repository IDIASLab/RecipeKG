    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24091/cranberry-nut-pie/",
        "name": "Cranberry Nut Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2110688.jpg",
            "width": 1960,
            "height": 1960
        },
        "datePublished": "2020-06-19T03:05:17.000Z",
        "description": "A wonderfully delicious, quick dessert which is perfect for the holidays and throughout the year as well! Buy your cranberries in the fall and freeze several packages to use at a later date. Serve warm with a scoop of ice cream on top.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bc cups fresh or frozen cranberries",
            "\u00bc cup brown sugar",
            "\u00bc cup chopped walnuts",
            "1 egg",
            "\u00bd cup white sugar",
            "\u00bd cup all-purpose flour",
            "\u2153 cup melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter a 9 inch pie plate and layer cranberries on the bottom. Sprinkle with brown sugar and chopped walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat egg until thick. Gradually add sugar, beating until thoroughly blended. Stir in flour and melted butter. Blend well and pour over cranberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 45 minutes, or until golden brown. Serve warm.\n"
            }
        ],
