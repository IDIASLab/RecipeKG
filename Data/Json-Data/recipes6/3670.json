    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273432/pear-galette-with-mascarpone/",
        "name": "Pear Galette with Mascarpone",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pear Galette with Mascarpone"
        },
        "datePublished": "2020-03-05T05:19:41.000Z",
        "description": "This puffed pastry pear galette dessert is topped with a layer of cannoli filling and lightly spiced pears. Serve warm or cold for a delicious fall favourite!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 11-inch galette",
        "recipeIngredient": [
            "1 (16 ounce) container ricotta cheese, strained",
            "1 (8 ounce) container mascarpone cheese",
            "\u2154 cup confectioners' sugar",
            "\u00bd teaspoon vanilla extract",
            "8 pears - peeled, cored, and sliced into wedges",
            "2 tablespoons cornstarch",
            "1 tablespoon lemon juice",
            "1 tablespoon brown sugar",
            "\u00bc teaspoon ground cloves",
            "\u00bc teaspoon ground cinnamon",
            "1 frozen puff pastry sheet, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ricotta cheese, mascarpone cheese, confectioners' sugar, and vanilla extract in a bowl. Place in the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pears, cornstarch, lemon juice, brown sugar, cloves, and cinnamon into a bowl; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place thawed puff pastry sheet into the bottom of an 11-inch tart pan. Spoon cheese mixture onto pastry and spread evenly. Arrange spiced pear mixture on top of the cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 45 minutes.\n"
            }
        ],
