    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15095/coffee-liqueur-brownies/",
        "name": "Coffee Liqueur Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F319478.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-08T15:18:53.000Z",
        "description": "Coffee lovers will delight in this heavenly brownie recipe. The best part about this recipe is that it only dirties one dish!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 8x8 inch pan",
        "recipeIngredient": [
            "1 cup semisweet chocolate chips",
            "\u00bc cup butter",
            "1 tablespoon instant coffee powder ",
            "1 egg",
            "1 egg yolk",
            "\u2154 cup all-purpose flour",
            "\u00bd cup white sugar",
            "\u215b teaspoon baking soda",
            "1 tablespoon coffee-flavored liqueur",
            "1 teaspoon vanilla extract",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In the 8x8 inch baking pan, melt the butter and chocolate chips on the stove top, over medium heat, stirring constantly. Remove from the heat, and stir in the coffee, egg and yolk. Sift together the flour, sugar, and baking soda, stir into the chocolate mixture. Then stir in the vanilla and coffee liqueur.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 to 25 minutes in the preheated oven. Cool slightly and dust with confectioners' sugar. Cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Chocolate Brownie Recipes"
