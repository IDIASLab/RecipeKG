    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15746/banana-oatmeal-bread/",
        "name": "Banana Oatmeal Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2429679.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This is an old family recipe, and it is very moist!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 9x5-inch loaf",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "1 cup white sugar",
            "2 eggs, beaten",
            "\u00bd teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 cup quick cooking oats",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc cup milk",
            "\u00bd teaspoon ground cinnamon",
            "1\u2009\u00bd cups mashed bananas",
            "\u00bd cup chopped raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5-inch loaf pan and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the shortening and sugar. Add eggs and vanilla; beat until fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together the flour, oatmeal, baking soda, salt and cinnamon. Add dry ingredients alternately with bananas and milk. Mix until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in raisins and pour into prepared pan. Bake for 50 to 60 minutes; remove from oven and cover for 5 minutes.\n"
            }
        ],
