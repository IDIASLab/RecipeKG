    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9430/jeans-fudge/",
        "name": "Jean's Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F209213.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-07-12T18:18:06.000Z",
        "description": "Jean is very dear friend that gave me this recipe when we worked together, she was a mentor, friend and colleague. The marshmallows make this recipe fail-proof easy. This is smooth and perfect every time.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1.25 pounds fudge",
        "recipeIngredient": [
            "2\u2009\u00bc cups white sugar",
            "\u2154 cup sweetened condensed milk",
            "\u00bc cup butter",
            "16 large marshmallows",
            "1 teaspoon vanilla extract",
            "\u00be cup chopped walnuts",
            "1 cup semi-sweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter a 7 x 11 inch pan. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 3 quart saucepan, mix together sugar, condensed milk, butter or margarine, and marshmallows. Cook over low heat and bring to a boil.  Boil 5-7 minutes.  Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add vanilla, nuts, and chocolate chips. Stir until chips are melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into prepared pan. Cool. Cut.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Fudge Recipes",
            "Chocolate"
        ],
