    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258484/grillable-vegan-burger/",
        "name": "Grillable Vegan Burger",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9080546.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-05-27T23:51:57.000Z",
        "description": "This vegan burger recipe can be made ahead of time and frozen for later, or quickly whipped up for a barbeque when needed. It's absolutely delicious!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 burgers",
        "recipeIngredient": [
            "2 cups raw walnuts",
            "2 tablespoons olive oil",
            "1 onion, minced",
            "salt and ground black pepper to taste",
            "3 cups rinsed and drained canned black beans",
            "2 cups cooked rice",
            "\u00be cup vegan barbeque sauce",
            "\u2154 cup cornflake crumbs",
            "2 tablespoons brown sugar",
            "2 tablespoons chile powder",
            "2 tablespoons ground paprika",
            "2 tablespoons ground cumin"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Toast walnuts in a skillet over medium heat, stirring frequently until lightly browned and fragrant, 5 to 7 minutes. Crush into fine pieces using a mortar and pestle or food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in the same skillet over medium heat. Cook and stir onion until lightly browned, about 5 minutes. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash black beans in a large bowl using a fork. Add walnuts, onion, rice, barbeque sauce, cornflake crumbs, brown sugar, chile powder, paprika, and cumin. Season with salt and pepper; stir until mixture is well combined and holds together. Shape into 10 burgers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate. Place burgers on aluminum foil and grill until browned, 5 to 8 minutes per side.\n"
            }
        ],
