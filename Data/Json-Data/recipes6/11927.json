        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9776/easy-butter-cookies-i/",
        "name": "Easy Butter Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2218680.jpg",
            "width": 2340,
            "height": 4160
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Light and buttery, these cookies will only take about 15 minutes to prepare.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups sifted all-purpose flour",
            "\u00bd cup cornstarch",
            "\u00bd cup white sugar",
            "1 cup unsalted butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together all ingredients until well-blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll 2-inch sized pieces of dough into balls and  place on cookie sheet.  Flatten each with the back of  a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 13 to 15 minutes until light brown.  Let cool on wire rack.\n"
            }
        ],
