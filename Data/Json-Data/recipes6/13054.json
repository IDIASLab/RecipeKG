    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11494/rum-glazed-raisin-bars/",
        "name": "Rum Glazed Raisin Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2337136.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "1999-10-25T04:38:05.000Z",
        "description": "An eye-catching treat. Great for special occasions or after school snacks.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "32 bars",
        "recipeIngredient": [
            "2 tablespoons rum",
            "2 tablespoons butter",
            "\u00bd cup raisins",
            "6 tablespoons white sugar",
            "\u00bd cup butter",
            "1 egg",
            "1 cup all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc cup milk",
            "1 tablespoon rum",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). In a small bowl, pour the rum over the raisins and set aside. Grease a 9x9 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream the sugar and butter. Add the egg and mix until fluffy. Sift together the flour and baking powder. Add this alternately to the egg mixture with the milk. Mix until blended, then fold in raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the batter evenly in the greased pan and bake in preheated oven for 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the raisin bars are baking, in a smaller bowl, stir together the 2 tablespoons of butter with the confectioners' sugar. Add the remaining rum and stir until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the bars come out of the oven, let them cool slightly, then spread the icing evenly over the top. Cut into 32 bars.\n"
            }
        ],
