        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9855/special-k-cookies/",
        "name": "Special K Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7166544.jpg",
