        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264356/vegan-rhubarb-bran-muffins/",
        "name": "Vegan Rhubarb Bran Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1028366.jpg",
            "width": 3264,
            "height": 1840
        },
        "datePublished": "2020-03-05T05:17:41.000Z",
        "description": "Delicious and moist rhubarb muffins that are also vegan!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 cup chopped rhubarb",
            "\u00be cup brown sugar",
            "\u00be cup wheat bran",
            "2 teaspoons baking powder",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "1 cup unsweetened applesauce",
            "\u00bd cup vegetable oil",
            "2 tablespoons molasses",
            "1\u2009\u00bd teaspoons apple cider vinegar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a muffin pan with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, rhubarb, brown sugar, wheat bran, baking powder, cinnamon, and nutmeg in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix applesauce, oil, molasses, vinegar, and vanilla extract together in a separate bowl. Pour over flour mixture. Slowly mix until a thick batter forms. Divide batter among the muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 15 to 20 minutes.\n"
            }
        ],
