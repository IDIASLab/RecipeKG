        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10814/mace-cookies/",
        "name": "Mace Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mace Cookies"
        },
        "datePublished": "1998-04-16T23:31:59.000Z",
        "description": "Rolled cookies made with ground mace.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bc teaspoon ground mace",
            "\u00be cup shortening",
            "2 cups white sugar",
            "2 eggs",
            "\u00bc cup milk",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1 teaspoon lemon zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder and mace. Set aside. Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large bowl, cream shortening and sugar together. Beat in eggs, then beat in the milk and vanilla extract. Beat in lemon zest. Gradually blend in dry ingredients.  Cover and chill for at least four hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out dough on floured surface to 1/8 inch thickness.  Use a 2 inch round cookie cutter and cut into rounds. Place 1 inch apart on prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10-12 minutes or until lightly colored. Cool on cookie racks.\n"
            }
        ],
