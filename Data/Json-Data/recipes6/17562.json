    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25849/eggnog-blend/",
        "name": "Eggnog Blend",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1071937.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:06:33.000Z",
        "description": "This homemade eggnog, spiked with rum and Scotch, will fill you with glee. Perfect for the holiday season!",
        "prepTime": "P0DT8H10M",
        "cookTime": null,
        "totalTime": "P0DT8H10M",
        "recipeYield": "8 (6 ounce) servings",
        "recipeIngredient": [
            "4 cups half-and-half",
            "1 cup white sugar",
            "11 egg yolks, beaten",
            "1 teaspoon vanilla extract",
            "2 cups heavy cream",
            "\u2154 cup dark rum",
            "\u00bd cup Scotch whiskey",
            "1 pint vanilla ice cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan over low heat, stir together the half-and-half and sugar. Whisk in egg yolks and cook until light custard forms; do not boil. Remove from heat and cool, then place in refrigerator and chill overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Into the chilled mixture, stir in vanilla, cream, dark rum, scotch, and vanilla ice cream. Serve cold.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Eggnog Recipes"
