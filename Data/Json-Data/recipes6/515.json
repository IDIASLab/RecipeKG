        "recipeYield": "1 1/4 cup",
        "recipeIngredient": [
            "\u00bd cup brown sugar",
            "\u00bd cup paprika",
            "1 tablespoon ground black pepper",
            "1 tablespoon salt",
            "1 tablespoon chili powder",
            "1 tablespoon garlic powder",
            "1 tablespoon onion powder",
            "1 teaspoon cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, paprika, black pepper, salt, chili powder, garlic powder, onion powder, and cayenne pepper in a bowl. Store in an air-tight container or keep in the freezer in a sealable plastic bag.\n"
            }
