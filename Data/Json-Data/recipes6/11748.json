    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9472/coffee-liqueur-i/",
        "name": "Coffee Liqueur I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2889562.jpg",
            "width": 1931,
            "height": 1934
        },
        "datePublished": "1999-11-28T00:19:21.000Z",
        "description": "Rich and yummy!  This liqueur needs to be made 1 month ahead of giving or drinking!  Makes great gifts when poured into separate smaller bottles and tied with ribbon!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "2 quarts",
        "recipeIngredient": [
            "1 (2 ounce) jar instant espresso coffee powder",
            "3\u2009\u00bd cups white sugar",
            "2 cups boiling water",
            "1 pint vodka",
            "1 whole vanilla bean"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix coffee and sugar. Add boiling water and stir until dissolved.  Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add Vodka and pour into two 26 ounce empty bottles. Split vanilla into 4 pieces and add to bottles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Close bottles tightly and keep in a cool dark place for one month.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Liqueur Recipes"
        ],
