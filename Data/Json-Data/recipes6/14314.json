    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15832/pineapple-pie-iv/",
        "name": "Pineapple Pie IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F613107.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-24T13:17:23.000Z",
        "description": "You can use either fresh or canned pineapple for this pie.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "2 (9 inch)  pie shell",
            "2 eggs, beaten",
            "1\u2009\u2153 cups white sugar",
            "1 tablespoon lemon juice",
            "2 cups fresh pineapple - peeled, cored and chopped",
            "1 tablespoon butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (225 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat the eggs until foamy. Mix in sugar, lemon juice and chopped pineapple. Pour into pie shell and dot the top with butter. Cover with top crust, crimp edges to seal, and cut a few slits for steam vents.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 10 minutes, then reduce oven temperature to 350 degrees F (175 degrees C) and bake 35 minutes more, until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies",
            "Pineapple Pie"
