    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9713/fudge-chunk-brownies/",
        "name": "Fudge Chunk Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7697203.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "There's nothing like a fudgy brownie with a cold glass of milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups chopped pecans",
            "2 (1 ounce) squares unsweetened chocolate",
            "\u00bd cup unsalted butter",
            "1 pinch salt",
            "\u2154 cup all-purpose flour",
            "2 eggs",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "4 (1 ounce) squares semisweet chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease an 8 inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle pecans on cookie sheet and toast in  oven until lightly colored.  Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In saucepan over low heat, melt unsweetened chocolate and butter, stirring occasionally.  Let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, beat the eggs, sugar and  vanilla. Fold in the melted chocolate. Sift together the flour, and salt and add to mixture, stirring  lightly. Chop the semisweet chocolate into coarse chunks and add to mixture, along with pecans.  Mix until just blended.  Pour batter into pan and smooth top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake about 25 minutes.  Let cool before cutting into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Chocolate Brownie Recipes"
        ],
