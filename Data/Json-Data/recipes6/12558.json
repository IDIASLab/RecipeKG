    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10744/ginger-snaps-ii/",
        "name": "Ginger Snaps II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F255683.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-03-31T13:39:12.000Z",
        "description": "These cookies are no fail and taste so good!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00be cup shortening",
            "1 cup white sugar",
            "\u00bc cup light molasses",
            "1 egg",
            "2 cups all-purpose flour",
            "1 teaspoon ground ginger",
            "1 teaspoon ground cinnamon",
            "2 teaspoons baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together shortening and sugar, add egg and beat until fluffy.  Stir in molasses and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift all dry ingredients-twice, then add to molasses mixture and mix well\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a cookie scoop or spoon scoop out spoonfuls of dough and form into round balls.  Roll tops of balls  in sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the round balls on a cookie sheet, these cookies flatten as they bake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for exactly 9 minutes in a preheated 350 degree (175 degree C) oven. Remove from baking sheet and cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Gingersnap Recipes"
        ],
