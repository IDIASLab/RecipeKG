    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255373/baek-kimchi-korean-white-non-spicy-kimchi/",
        "name": "Baek Kimchi (Korean White Non-Spicy Kimchi)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5108762.jpg",
            "width": 1330,
            "height": 960
        },
        "datePublished": "2020-03-05T05:15:38.000Z",
        "description": "We thought venturing into other kimchi domains would be a wise decision, especially with the warmer weather. This type of kimchi is perfect for those who can't handle spicy foods and is milder in flavor. Enjoy with rice and other meals.",
        "prepTime": "P0DT1H0M",
        "cookTime": null,
        "totalTime": "P1DT13H10M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00bd cup coarse salt",
            "1 napa cabbage, cut into chunks",
            "2 jujube (Chinese dates), or more to taste",
            "5\u2009\u00bd cups water, divided",
            "3 tablespoons salted fermented shrimp (saewujeot)",
            "2 scallions, cut into 1-inch pieces",
            "6 cloves garlic, sliced",
            "2 tablespoons sliced fresh ginger",
            "\u00bd Korean radish, cut into matchsticks",
            "5 chestnuts, cut into thin strips",
            "\u00bd pear, cut into matchsticks",
            "2 carrots, cut into matchsticks ",
            "1 red chile pepper, seeded and thinly sliced, or more to taste",
            "1 green chile pepper, seeded and thinly sliced, or more to taste",
            "4 teaspoons brown sugar",
            "2 teaspoons salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve 1/2 cup coarse salt in a large bowl of water. Submerge cabbage in the salted water and let soak, 12 to 24 hours. Drain. Rinse well and drain, squeezing out excess water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Soak jujube in a small bowl of water for 10 to 15 minutes. Drain, pit, and cut into thin slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup water, salted shrimp, scallions, garlic, and ginger in a food processor; puree until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour puree into a large bowl; add jujube, radish, chestnuts, pear, carrots, red chile pepper, green chile pepper, brown sugar, and 2 teaspoon salt. Stir in remaining 5 cups water. Mix in cabbage until well coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack cabbage mixture into an airtight jar. Pour in any liquid left in the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Seal and let sit at room temperature until starting to ferment, 1 to 2 days. Transfer to the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
