    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7917/chocolate-web-cake/",
        "name": "Chocolate Web Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4570799.jpg",
            "width": 3008,
            "height": 2000
        },
        "datePublished": "1998-08-06T23:10:21.000Z",
        "description": "If you like chocolate, make this cake recipe of a chocolate cake with chocolate filling. You'll love it.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -4 layer 9 inch round cake",
        "recipeIngredient": [
            "1\u2009\u2153 cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon salt",
            "\u00bc teaspoon baking soda",
            "1\u2009\u00bd cups white sugar",
            "\u00bd cup shortening",
            "1\u2009\u00bc cups evaporated milk",
            "2 eggs",
            "2 (1 ounce) squares unsweetened chocolate, melted",
            "1\u2009\u2153 cups shortening",
            "1\u2009\u2153 cups white sugar",
            "\u00be cup evaporated milk",
            "2 teaspoons vanilla extract",
            "2 (1 ounce) squares unsweetened chocolate, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F(175 degrees C). Grease two 9 inch round cake pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder, salt, baking soda and 1 1/2 cups of the white sugar together in a large mixing bowl. Add 1/2 cup of the shortening and 1-1/4 cup of the evaporated milk. Beat at medium speed with an electric mixer for 2 minutes. Beat in the eggs and beat for 2 minutes longer. Spread the batter evenly into the prepared pans. Drizzle 1 square of the melted chocolate in a spiral on top of each cake. Feather lines with a knife to form a web pattern.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 40 minutes or until a toothpick inserted in the center comes out clean. Let cakes cool in pans for 10 minutes then remove from pans and let cakes cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Filling: Combine the 2 squares unsweetened  melted chocolate, 1 1/3 cups shortening, 1 cup white sugar, 3/4 cup evaporated milk and the vanilla together and beat with an electric mixer until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Assemble Cake: Cut each cooled cake layer in half horizontally. Spread 1/4 of the filling between each layer making a 4 layer cake with a web design on top. Frost sides with the remaining filling.\n"
            }
        ],
