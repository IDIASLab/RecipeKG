        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222030/shellis-lemon-pepper-catfish/",
        "name": "Shelli's Lemon Pepper Catfish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5711171.jpg",
            "width": 3024,
