    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24019/pear-bread-ii/",
        "name": "Pear Bread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2294769.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-06-19T03:05:14.000Z",
        "description": "The delicate flavor of fresh pears and pecans dance enchantingly in this lovely summer loaf. Freezes well if double wrapped. Delicious and easy.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "2 - 8x4 inch loaves",
        "recipeIngredient": [
            "1 cup vegetable oil",
            "2 cups granulated sugar",
            "3 eggs",
            "2\u2009\u00bd cups pears - peeled, cored and chopped",
            "1 cup chopped pecans",
            "2 teaspoons vanilla extract",
            "3 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "1 teaspoon salt",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease two 8x4 inch loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl combine oil, sugar and eggs, beat well. Stir in pears, pecans and vanilla. In another bowl, combine flour, baking soda, baking powder, salt, cinnamon and nutmeg. Stir dry ingredients into the pear mixture; mix well. Pour batter into prepared loaf pans\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 60 minutes, until a toothpick inserted into center of a loaf comes out clean. Allow loaves to cool in pans for 10 minutes before moving to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
