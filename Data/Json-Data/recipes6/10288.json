    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6672/strawberry-bread-i/",
        "name": "Strawberry Bread I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F347834.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-05-07T19:22:52.000Z",
        "description": "A bread you MUST try!  This is one you'll want to keep on hand.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9x5 inch pan or 2 small loaves",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "2 cups white sugar",
            "1 teaspoon ground cinnamon",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "4 eggs",
            "1\u2009\u00bc cups vegetable oil",
            "1 cup chopped pecans",
            "1 (10 ounce) package <a href=\"https://www.allrecipes.com/article/how-to-freeze-strawberries/\" aria-label=\"frozen strawberries\">frozen strawberries</a>, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease and flour a 9 x 5 inch pan well.  Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, sift together sugar, flour, cinnamon, salt, and baking soda.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a smaller bowl, beat the eggs and oil.  Stir in pecans and strawberries.  Add egg mixture to the sifted ingredients, and stir until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 hour, or until tester inserted in the center comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
