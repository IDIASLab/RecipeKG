    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10939/apricot-explosions/",
        "name": "Apricot Explosions",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apricot Explosions"
        },
        "datePublished": "1997-12-12T20:07:53.000Z",
        "description": "I love apricots and there are not many cookies with apricots in them. This one rocks!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup chopped walnuts",
            "3\u2009\u00bd cups all-purpose flour",
            "\u00be cup white sugar",
            "2 teaspoons baking powder",
            "2 eggs",
            "1 cup shortening",
            "1 cup apricot preserves",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl, combine sugar, eggs, and shortening together by hand. Add flour and baking powder; mix well. Divide dough in half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread half of dough in 10 x 15 inch pan. Spread preserves over dough. Sprinkle walnuts on top and then sprinkle cinnamon on top\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grate second half of dough on top (crumble). Press down gently. Bake for 1/2 hour at 350 degrees F (175 degrees C). Cut into squares when cool.\n"
            }
        ],
