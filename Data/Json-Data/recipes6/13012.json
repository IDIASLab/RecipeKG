    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11424/coffee-chocolate-chip-cookies/",
        "name": "Coffee Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F315350.jpg",
            "width": 3456,
            "height": 2592
        },
        "datePublished": "1999-07-11T20:54:32.000Z",
        "description": "These cookies are a step above any you have ever tasted.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00be cup blanched slivered almonds",
            "\u2153 cup instant coffee granules",
            "2 tablespoons hot water",
            "2\u2009\u2154 cups all-purpose flour",
            "\u00be teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup butter, softened",
            "1\u2009\u00bc cups white sugar",
            "1 teaspoon vanilla extract",
            "2 eggs",
            "2 cups semi-sweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast almonds in oven for 10 minutes or until brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve coffee in 2 tablespoons hot water.  In a separate small mixing bowl mix flour, baking soda and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar in an electric mixer at a medium speed until fluffy. Pour coffee, vanilla and eggs into the butter mixture; mix well. Reduce mixer's speed to low and slowly pour flour mixture into the butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold chocolate chips and almonds into the dough. Drop dough by teaspoonfuls onto an ungreased cookie sheet, 3 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
