    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8128/beet-surprise-cake/",
        "name": "Beet Surprise Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F777864.jpg",
            "width": 3872,
            "height": 2592
        },
        "datePublished": "1999-01-19T03:23:15.000Z",
        "description": "Made with fresh red beets. It is delicious, nice and moist, a recipe that has been in the family a long time. You would not believe how good it is until you make it.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 - 9x13-inch cake",
        "recipeIngredient": [
            "2 teaspoons lemon juice",
            "1 cup beets, boiled and grated",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon salt",
            "2 teaspoons baking soda",
            "2 cups white sugar",
            "\u00bd cup unsweetened cocoa powder",
            "1\u2009\u00bc cups melted butter",
            "4 eggs, beaten",
            "2 tablespoons honey",
            "\u00bd cup milk",
            "2 teaspoons vanilla extract",
            "3 tablespoons cream",
            "3 ounces cream cheese",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "1 pound confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease one 9x13 inch pan. Preheat oven to 350 degrees F (175 degrees C). Sprinkle lemon juice over beets, set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, 1 teaspoon salt, soda, white sugar, cocoa, melted butter, eggs, honey, milk, and 2 teaspoons vanilla in a large mixing bowl. Stir in beets. Beat 2 minutes at medium speed of mixer. Pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) about 40 minutes. Cool and frost.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make frosting: combine cream and cream cheese, 1 teaspoon vanilla, and pinch of salt. Add confectioners' sugar, beat until fluffy. Spread on cake. Add nuts if desired.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Sheet Cake Recipes"
        ],
