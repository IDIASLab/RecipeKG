    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279853/low-carb-cheesy-chicken-casserole/",
        "name": "Low-Carb Cheesy Chicken Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Low-Carb Cheesy Chicken Casserole"
        },
        "datePublished": "2020-04-23T18:03:43.000Z",
        "description": "A quick and easy, low-carb chicken casserole recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 tablespoons margarine",
            "2 tablespoons minced garlic",
            "5 skinless, boneless chicken breasts, chopped",
            "1 pinch garlic powder, or to taste",
            "1 pinch dried basil, or to taste",
            "1 pinch dried oregano, or to taste",
            "salt and ground black pepper to taste",
            "1 (16 ounce) container ricotta cheese",
            "2 cups shredded mozzarella cheese, divided",
            "1 cup shredded Parmesan cheese",
            "\u00bd cup milk",
            "1 large egg",
            "1 (28 ounce) jar vodka marinara sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine in a roasting pan with minced garlic in the preheated oven. Add chicken, garlic powder, basil, oregano, salt, and pepper; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink, 20 to 25 minutes. Remove from the oven but leave the oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ricotta, 1 cup mozzarella cheese, Parmesan cheese, milk, and egg together in a bowl. Spread evenly over the top of the chicken. Pour vodka marinara sauce over top and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake for 20 minutes. Sprinkle remaining mozzarella cheese on top. Continue to bake until melted, about 10 minutes.\n"
            }
        ],
