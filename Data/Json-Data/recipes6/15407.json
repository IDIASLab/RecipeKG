    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19434/brandy-alexander-pie/",
        "name": "Brandy Alexander Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F876000.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2000-05-14T14:19:08.000Z",
        "description": "Try this cognac- and creme de cacao-flavored pie that is great for company. Be sure to make it ahead of time so it has time to chill.",
        "prepTime": "P0DT1H0M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (9 inch) prepared graham cracker crust",
            "1 (.25 ounce) package unflavored gelatin",
            "\u00bd cup cold water",
            "\u2154 cup white sugar",
            "\u215b teaspoon salt",
            "\u00bc cup cognac",
            "3 egg yolks",
            "\u00bc cup creme de cacao",
            "3 egg whites",
            "2 cups whipping cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Bake crust in oven for 10 minutes; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, allow gelatin to soften in cold water. Add 1/3 cup sugar, salt, and beaten egg yolks; blend together. Heat over low heat stirring until gelatin dissolves and mixture coats spoon. Do not bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, stir in cognac and creme de cacao. Chill until mixture thickens.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, beat egg whites until stiff; while beating, slowly add rest of sugar. Fold egg white and sugar mixture into the thickened filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip the heavy cream and gently fold half of it into the cognac mixture. Pour into graham cracker crust; cover and refrigerate for an hour. Garnish with remaining whipped cream and chocolate curls.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes",
            "Chiffon Pie Recipes"
        ],
