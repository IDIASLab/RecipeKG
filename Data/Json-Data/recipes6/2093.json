    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241994/baked-peaches-n-cream/",
        "name": "Baked Peaches 'n Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2241796.jpg",
            "width": 2115,
            "height": 2109
        },
        "datePublished": "2020-03-05T05:13:12.000Z",
        "description": "These baked peaches taste like pie, minus the guilt of eating one! Of course all pie is even better with ice cream.Perfect way to make those summer peaches disappear.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 teaspoons brown sugar",
            "2 tablespoons butter, cut into 8 pieces",
            "1 pinch ground cinnamon, or more to taste",
            "4 ripe peaches, halved and pitted ",
            "4 scoops vanilla ice cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange brown sugar, 1 teaspoon per peach, in a 9x13-inch baking dish. Top each brown sugar mound with a piece of butter and a sprinkle of cinnamon. Place a peach half, cut-side down, on top of brown sugar-butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until peaches are soft, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Plate 2 warm peaches per serving and top with 1 scoop vanilla ice cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Peach Dessert Recipes"
        ],
