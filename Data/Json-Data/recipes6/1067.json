        "recipeIngredient": [
            "\u00bc cup chili powder",
            "1\u2009\u00bd tablespoons ground cumin",
            "4 teaspoons onion powder",
            "4 teaspoons Hungarian paprika",
            "2 teaspoons garlic powder",
            "2 teaspoons salt, or more to taste ",
            "1 teaspoon dried oregano",
            "1\u2009\u00bd teaspoons brown sugar",
            "1 teaspoon cayenne pepper",
            "1 teaspoon mustard powder",
            "\u00bd teaspoon ground coriander",
            "\u00bc teaspoon ground black pepper",
            "1 pinch ground cinnamon",
            "2 bay leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix chili powder, cumin, onion powder, paprika, garlic powder, salt, oregano, brown sugar, cayenne pepper, mustard powder, coriander, black pepper, cinnamon, and bay leaves in a bowl. Store seasoning mix in an airtight container.\n"
            }
