    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24845/apple-style-omelets/",
        "name": "Apple Style Omelets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7981.jpg",
            "width": 2032,
            "height": 1524
        },
        "datePublished": "2020-06-19T03:05:43.000Z",
        "description": "You'll be surprised how wonderful sauteed apples and eggs taste together! Serve warm with apple butter or maple syrup.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "\u00bc cup butter",
            "3 large apples - peeled, cored and diced",
            "6 eggs, lightly beaten",
            "1 pinch salt",
            "1 pinch black pepper",
            "\u00bc cup heavy cream",
            "1 teaspoon white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet or frying pan, melt butter over medium heat. Add apples and saute until tender but not completely cooked.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a lightly oiled omelet pan or medium sized skillet. Whisk together eggs, cream, salt, pepper and sugar. Pour egg mixture into omelet pan. When bottom of omelet has begun to set, place half of the apple mixture on top of the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When bottom of omelet is completely cooked, fold over omelet, cover and cook through. Serve warm with remaining apples on top or on the side.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs",
            "Omelet Recipes"
