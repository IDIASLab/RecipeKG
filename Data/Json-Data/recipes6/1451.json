        "recipeIngredient": [
            "1 pound ground beef",
            "1 cup ketchup (such as Heinz\u00ae)",
            "\u00bd (12 ounce) bottle chili sauce (such as Heinz\u00ae)",
            "\u00bc cup chopped onion",
            "\u00bc cup chopped green bell pepper",
            "\u00bc cup brown sugar",
            "\u00bd teaspoon mustard powder",
            "hamburger buns"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until browned and crumbly, 5 to 7 minutes; drain and discard grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ketchup, chili sauce, onion, green bell pepper, brown sugar, and mustard powder into ground beef; bring to a boil. Reduce heat and simmer until flavors have blended, about 15 minutes; serve on hamburger buns.\n"
            }
