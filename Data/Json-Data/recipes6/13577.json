        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12631/marshmallow-apple-pie/",
        "name": "Marshmallow Apple Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5886361.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "1999-07-29T15:19:03.000Z",
        "description": "I remember walking into my grandmothers house and the wonderful smell of this pie. It is really very good.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "6 Granny Smith apples",
            "1 tablespoon butter",
            "1 pinch salt",
            "\u00bd cup white sugar",
            "\u00bd teaspoon ground nutmeg",
            "14 large marshmallows",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees F). Line pie pan with plain pastry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel, remove core and slice apples. Place in pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, salt and nutmeg.  Sprinkle over apples. Dot with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 425 degrees F (220 degrees C) for 30 minutes, or until crust is brown and the apples are tender.  Remove from oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the apples with halved marshmallows.  Brown in slow oven 325 degrees F (165 degrees C).  Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
