        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17020/japanese-fruit-pie/",
        "name": "Japanese Fruit Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7976430.jpg",
