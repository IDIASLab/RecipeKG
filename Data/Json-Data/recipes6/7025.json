    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279471/broiled-salmon-with-lemon-dill-sauce/",
        "name": "Broiled Salmon with Lemon-Dill Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Broiled Salmon with Lemon-Dill Sauce"
        },
        "datePublished": "2020-04-02T20:47:08.000Z",
        "description": "This easy-to-make broiled salmon recipe is good for even non-fish eaters.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 salmon steaks",
        "recipeIngredient": [
            "1 teaspoon ground black pepper",
            "4 (6 ounce) salmon steaks",
            "2 tablespoons mayonnaise",
            "2 tablespoons lemon juice",
            "1 teaspoon Dijon mustard",
            "\u00be teaspoon dried dill weed",
            "4 lemon wedges"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler. Line a baking pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle pepper on both sides of salmon steaks. Place on the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil salmon under the preheated broiler until it flakes easily with a fork, 3 to 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While salmon is cooking, stir mayonnaise, lemon juice, Dijon, and dill together in a small bowl until a smooth sauce is achieved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Portion salmon steaks on a plate, spoon sauce over top, and garnish with lemon wedges.\n"
            }
        ],
