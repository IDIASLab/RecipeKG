    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15176/nannys-spaghetti-sauce/",
        "name": "Nanny's Spaghetti Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4931747.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1999-11-21T06:31:41.000Z",
        "description": "This is my Sicilian great-grandmother's recipe. I've never tasted better. Serve with meatballs and Italian sausage; it's also terrific as a marinara sauce.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (28 ounce) can crushed tomatoes",
            "2 (8 ounce) cans tomato sauce",
            "1 (6 ounce) can tomato paste",
            "3 cloves garlic, minced",
            "1 tablespoon white sugar",
            "2 tablespoons red wine vinegar",
            "2 teaspoons dried oregano",
            "1 pinch crushed red pepper flakes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet combine the crushed tomatoes, tomato paste, tomato sauce, garlic, sugar, vinegar, oregano and red pepper flakes. Stir all together and simmer over low heat for at least 30 minutes (for the best flavor). Stir frequently to prevent burning.\n"
            }
        ],
