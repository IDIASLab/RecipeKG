        "name": "Weeknight Slow Cooker Chuck Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1258072.jpg",
            "width": 1961,
            "height": 1961
