    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254542/open-face-toasted-caprese/",
        "name": "Open-Face Toasted Caprese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4180260.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2016-07-06T07:32:14.000Z",
        "description": "Toasted crusty bread topped with sliced mozzarella cheese, fresh basil, and grilled avocado slices makes a delicious summertime lunch.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bd cup balsamic vinegar",
            "1 teaspoon brown sugar",
            "1 loaf crusty sourdough or ciabatta bread, sliced",
            "Olive oil, to drizzle over bread slices and avocados",
            "1 clove garlic",
            "salt and pepper to taste",
            "2 avocados - peeled, pitted and sliced",
            "1 (8 ounce) package Stella\u00ae fresh mozzarella, sliced",
            "Fresh basil leaves",
            "4 heirloom tomatoes, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Add vinegar and brown sugar, if using, to a small saucepan and bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce to a very low simmer and cook for 10-15 minutes, until liquid reduces by about half and is slightly syrupy. Remove from heat, pour vinegar in a bowl or glass to be used later, and set aside to cool and thicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees  F (230 degrees  C) or grill to high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the slices of bread on a baking sheet and drizzle with olive oil on both sides. Rub each side of bread with the halved garlic clove. Sprinkle with salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle avocado with olive oil, salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread slices and avocado slices in oven or grill both until lightly charred, about 2 to 3 minutes per side. Remove from the oven or grill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice Stella fresh mozzarella and divide equally among the sandwiches. Place on top toast, then add fresh basil to taste. Add grilled avocado slices and tomato slices. Drizzle everything with olive oil, balsamic drizzle and salt and pepper to taste.\n"
            }
        ],
