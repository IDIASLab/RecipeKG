    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12489/margarita-party-pie/",
        "name": "Margarita Party Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3192.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "1998-11-28T15:49:05.000Z",
        "description": "This frozen pie is great for summer barbecues! Very tasty! Frozen strawberries may be used instead of fresh.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT4H30M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups crushed pretzels",
            "\u00bc cup white sugar",
            "\u2154 cup butter, melted",
            "1 (14 ounce) can sweetened condensed milk",
            "\u00bc cup fresh lime juice",
            "\u00bc cup tequila",
            "4 tablespoons orange liqueur ",
            "1 cup sliced fresh strawberries",
            "2 drops red food coloring",
            "2 drops yellow food coloring",
            "2 cups whipped cream, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine crushed pretzels, sugar and butter. Mix well and press onto the bottom and sides of a 9 inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine sweetened condensed milk, lime juice, tequila and orange liqueur. Pour half of the mixture into another bowl. Add strawberries and a few drops of red food coloring to one half. To other half, add only a drop or two of yellow food coloring. Fold one cup of whipped cream into each half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon into crust, alternating colors. Freeze for 4 hours or overnight.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes"
        ],
