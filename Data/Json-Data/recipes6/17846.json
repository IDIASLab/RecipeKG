    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26334/miss-hatties-apple-pie/",
        "name": "Miss Hattie's Apple Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7370957.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:06:57.000Z",
        "description": "A deep-dish double crust apple pie using Granny Smith apples. Tart and tasty.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 - 9 inch deep-dish pie",
        "recipeIngredient": [
            "2 (9 inch)  unbaked pie crusts",
            "12 unpeeled Granny Smith apples, cored and quartered",
            "1 cup white sugar",
            "2 teaspoons ground cinnamon",
            "1\u2009\u00bd teaspoons ground cloves",
            "1 teaspoon freshly grated nutmeg",
            "3 tablespoons butter",
            "3 tablespoons melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shred the apples into a large bowl. Stir in sugar, cinnamon, cloves and nutmeg. Mix well and place in bottom crust. Dot the top with 3 tablespoons butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a cutting board, place rolled out (or packaged) pie crust. With a leaf or apple cookie cutter, cut shapes from the pie crust. Position the cutouts all over the top of the pie, leaving small spaces between cutouts for the vent holes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 minutes. Remove pie from oven and brush with 3 tablespoons melted butter. Return to oven and bake an additional 15 minutes, or until golden brown. Place a piece of aluminum foil under the pan in case it boils over.\n"
            }
        ],
