        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16045/matzo-brei/",
        "name": "Matzo Brei",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1120494.jpg",
            "width": 1689,
            "height": 1703
        },
        "datePublished": "1999-10-27T20:44:52.000Z",
        "description": "Matzo and egg fried together. This traditional dish is great anytime, but everyone really looks forward to Matzo Brei during Passover!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 matzo cracker",
            "1 egg",
            "salt and pepper to taste",
            "1 teaspoon vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Take the matzo and break into small pieces into a bowl. Cover with hot water for a minute. Then squeeze out the water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, beat one egg with salt and pepper to taste and add to matzo. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat frying pan with a little oil. Pour mixture into the pan. Brown one side and turn over.\n"
            }
        ],
