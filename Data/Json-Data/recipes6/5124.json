    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/259738/black-bean-pumpkin-soup/",
        "name": "Black Bean Pumpkin Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Black Bean Pumpkin Soup"
        },
        "datePublished": "2020-03-10T02:53:05.000Z",
        "description": "The black beans and the pumpkin are so delicious together you won't believe it. And with all the spices used, each bite is a flavor party in your mouth! Garnish with baked pumpkin seeds.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "\u2153 cup chopped red onion",
            "2 cloves garlic, minced",
            "1 tablespoon ground cumin",
            "1 teaspoon kosher salt",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground black pepper",
            "1 pinch ground allspice",
            "4 cups vegetable broth, divided",
            "2 (15 ounce) cans black beans, rinsed and drained",
            "1 (14.5 ounce) can diced tomatoes",
            "1 (16 ounce) can pumpkin puree",
            "3 tablespoons balsamic vinegar, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine olive oil, red onion, garlic, cumin, salt, cinnamon, black pepper, and allspice in a large pot over medium-low heat; cook and stir until onion and garlic are browned, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 cups vegetable broth, black beans, and tomatoes in a blender; puree until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour remaining 2 cups vegetable broth, pureed black bean mixture, and pumpkin puree into the pot. Simmer soup, uncovered, until thick, 40 to 45 minutes. Stir in balsamic vinegar before serving.\n"
            }
        ],
