        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18348/lap-salad/",
        "name": "Lap Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F648209.jpg",
            "width": 250,
            "height": 250
