        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12295/green-tomato-pie-ii/",
        "name": "Green Tomato Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F907175.jpg",
