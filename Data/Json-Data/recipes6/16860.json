    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24860/cranberry-grape-salad/",
        "name": "Cranberry Grape Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cranberry Grape Salad"
        },
        "datePublished": "2020-06-19T03:05:44.000Z",
        "description": "Four types of fruit in a creamy salad mixture will be certain to liven up your holiday table! Green grapes may be used in place of red -- or use both!",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "2 tablespoons frozen whipped topping, thawed",
            "1 cup finely chopped walnuts",
            "1 cup cranberries",
            "1 tablespoon white sugar",
            "2\u2009\u00bd cups heavy cream, whipped",
            "1 cup diced mandarin oranges",
            "1 (8 ounce) can pineapple tidbits, drained",
            "1 cup halved seedless red grapes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together cream cheese, thawed frozen whipped topping and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Thoroughly blend cranberries in a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cranberries into cream cheese mixture. Gently fold in sugar and heavy cream. Stir in mandarin oranges, pineapple chunks and grapes. Chill in the refrigerator approximately 1 hour before serving.\n"
            }
        ],
