        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280055/applesauce-chocolate-chip-cookies/",
        "name": "Applesauce Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Applesauce Chocolate Chip Cookies"
        },
        "datePublished": "2020-05-28T03:35:27.000Z",
        "description": "Use this low-fat, dairy-free cookie dough in endless sweet creations.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup unsweetened applesauce",
            "1 cup packed brown sugar",
            "\u00bc cup white sugar",
            "1 egg",
            "1 egg yolk",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir applesauce, brown sugar, and white sugar together in a large bowl until sugars dissolve. Stir in egg and egg yolk until well blended. Add flour, baking soda, and salt; mix into a smooth dough. Fold in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a disher scoop to drop uniform amounts of dough onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are dry and bottoms are golden brown, about 12 minutes. Transfer to a wire rack to cool.\n"
            }
        ],
