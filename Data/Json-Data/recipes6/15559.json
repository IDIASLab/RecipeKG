        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20219/whiskey-slush/",
        "name": "Whiskey Slush",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F393412.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-07-23T01:14:42.000Z",
        "description": "Nice for large gatherings, or just to have on hand in warm weather.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "1 (12 fluid ounce) can frozen lemonade concentrate",
            "1 (6 ounce) can frozen orange juice concentrate",
            "2 cups strong brewed black tea",
            "1 cup white sugar",
            "2 cups whiskey",
            "7 cups water",
            "1 liter ginger ale soda",
            "2 lemons - cut into wedges, for garnish"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large plastic container combine lemonade concentrate, orange juice concentrate, tea, sugar, whiskey and water. Mix well and freeze for 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop into a tall glass, 3/4 full, and pour ginger ale to fill the glass. Stir and garnish with lemon wedges.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Slushie Recipes"
        ],
