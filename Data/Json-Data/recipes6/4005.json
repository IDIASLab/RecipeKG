    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276340/baked-vegan-rice-pudding/",
        "name": "Baked Vegan Rice Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Baked Vegan Rice Pudding"
        },
        "datePublished": "2020-03-05T05:20:44.000Z",
        "description": "This vegan rice pudding is creamy and delicious served warm or cold, or with fruit.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups water",
            "1 cup Arborio rice",
            "1 tablespoon grapeseed oil",
            "33 fluid ounces vanilla-flavored soy milk",
            "\u00bd cup brown sugar",
            "\u00bc teaspoon sea salt",
            "2 cinnamon sticks",
            "2 whole cloves",
            "\u00bd teaspoon vanilla bean paste",
            "\u00bc teaspoon ground cinnamon, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring water, rice, and grapeseed oil to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and water has been absorbed, about 15 minutes. Let stand for 5 to 10 minutes; fluff with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk soy milk, brown sugar, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour rice into a casserole dish; pour soy milk mixture on top. Add cinnamon sticks, cloves, and vanilla. Cover.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, 30 to 45 minutes. Remove cinnamon sticks and cloves. Dust with ground cinnamon.\n"
            }
        ],
