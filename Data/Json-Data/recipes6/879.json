    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232065/ham-and-mac-bake/",
        "name": "Ham and Mac Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2934300.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T05:10:28.000Z",
        "description": "This is a great recipe for that leftover Easter/Christmas ham.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 cup elbow macaroni",
            "\u00bc cup butter",
            "\u00bc cup all-purpose flour",
            "2 tablespoons brown sugar",
            "2 tablespoons brown mustard",
            "\u00bc teaspoon salt",
            "1 pinch ground black pepper to taste",
            "2 cups milk",
            "2 cups cubed fully cooked ham",
            "2 cups peeled and sliced Granny Smith apples",
            "1\u2009\u00bd cups soft bread crumbs",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 1/4 cup butter in a large saucepan over medium heat; whisk flour, brown sugar, mustard, salt, and black pepper into butter. Pour in milk and bring to a simmer; reduce heat to low and whisk until sauce is thick and bubbly, about 5 minutes. Stir macaroni, ham, and apple slices into sauce. Spread mixture into a 2-quart casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine soft bread crumbs with 2 tablespoons melted butter in a bowl; sprinkle crumbs over casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until casserole is bubbly and crumb topping is browned, about 35 minutes.\n"
            }
        ],
