        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10410/divinity-cookies/",
        "name": "Divinity Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6142348.jpg",
            "width": 960,
