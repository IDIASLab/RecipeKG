        "mainEntityOfPage": "https://www.allrecipes.com/recipe/50497/brookville-hotel-cream-style-corn/",
        "name": "Brookville Hotel Cream-Style Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F618953.jpg",
            "width": 250,
