    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279024/grilled-patty-melts/",
        "name": "Grilled Patty Melts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F612847.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-09T23:44:01.000Z",
        "description": "This is a grilled version of the classic patty melt sandwich on rye bread. Simple and delicious!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "3 patty melts",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 pinch salt and ground black pepper to taste",
            "2 tablespoons butter",
            "1 onion, sliced",
            "6 slices dark rye bread",
            "3 slices American cheese",
            "3 slices Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pat out three 1/3-pound hamburger patties, pressing each patty down with your index and middle fingers to get a nice, flat burger instead of a ball. Season patties on both sides with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on the preheated grill until no longer pink in the centers, 10 to 12 minutes, flipping halfway. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, melt butter in a saucepan over low heat. Increase heat to medium-low, add onion, and saute until tender, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 3 slices of bread on the grill. Top each with a slice of American cheese, a cooked patty, sauteed onions, a slice of Swiss cheese, and the remaining bread. Grill sandwiches until cheese melts, about 1 minute per side.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes",
            "Hamburgers"
        ],
