    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/50947/fudgy-wudgy/",
        "name": "Fudgy Wudgy",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fudgy Wudgy"
        },
        "datePublished": "2020-06-19T03:09:06.000Z",
        "description": "Soft spongy fudge chocolate cake melting into a sinful oblivion... a definite crowd pleaser!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 - 9 inch cake",
        "recipeIngredient": [
            "10 ounces semisweet chocolate, chopped",
            "5 tablespoons finely ground espresso beans",
            "1\u2009\u00bd cups sugar",
            "1 cup unsalted butter, softened",
            "6 egg yolks",
            "1 cup all-purpose flour",
            "1 teaspoon baking soda",
            "1 pinch salt",
            "1 pinch ground cinnamon",
            "6 egg whites",
            "3 tablespoons confectioners' sugar for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 9 inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the chocolate and ground espresso in a metal bowl, and set the bowl over a pan of simmering water. Stir occasionally until melted and smooth. Remove from heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and sugar until light and fluffy. Beat in the egg yolks one at a time, mixing well after each one. Sift together the flour, baking soda, salt and cinnamon; stir into the butter mixture.  Stir in the chocolate and espresso mixture. In a large clean glass or metal bowl, whip the egg whites until stiff peaks form. Fold in about 1/3 of the egg whites, blending well to lighten the mixture, then fold in the remaining egg whites gently. Spread the batter evenly in the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 60 to 70 minutes in the preheated oven, or until the top appears dry and cracked. A toothpick inserted into the center should come out clean. Cool cake in the pan over a wire rack. Dust with confectioners' sugar before serving, if desired.\n"
            }
        ],
