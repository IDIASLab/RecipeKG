    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12387/fried-fruit-pies/",
        "name": "Fried Fruit Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7284800.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-08-15T08:32:43.000Z",
        "description": "Apricot filled, fried pastry.",
        "prepTime": "P0DT1H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "12 fruit pastries",
        "recipeIngredient": [
            "12 ounces dried apricots",
            "water to cover",
            "\u00bd cup white sugar",
            "1 tablespoon butter",
            "\u00bd teaspoon ground cinnamon",
            "1 (15 ounce) package frozen prepared pie crusts, thawed",
            "\u00bd cup vegetable oil for frying",
            "1 tablespoon confectioners' sugar, for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place apricots in a saucepan, and add water to cover. Bring to a boil. Reduce heat to low and simmer for 30 minutes, or until tender. Drain, reserving 5 tablespoons liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place apricots and reserved liquid in a food processor container fitted with steel blade. Process until smooth. Add sugar, butter, and cinnamon; process until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out one crust on lightly floured surface and cut it into six 4 inch squares. Place 1 tablespoon apricot filling in the center of  each square. Fold each square in half diagonally to form a triangle. Press a fork dipped in flour onto the edges to seal, and poke tops to permit steam to escape. Repeat with remaining crust and apricot mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet on medium-high heat. Cook triangles, a few at a time, 2 to 3 minutes on each side; fry until the crust is golden brown. Drain on paper towels. Sprinkle with confectioners' sugar.\n"
            }
        ],
