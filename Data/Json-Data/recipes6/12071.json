        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9982/oatmeal-crispies-ii/",
        "name": "Oatmeal Crispies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1234002.jpg",
            "width": 1704,
