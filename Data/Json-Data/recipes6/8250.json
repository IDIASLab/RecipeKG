        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268187/slow-cooker-chicken-lettuce-wraps/",
        "name": "Slow Cooker Chicken Lettuce Wraps",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5764780.jpg",
            "width": 1536,
