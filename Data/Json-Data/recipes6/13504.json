        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12523/amazing-coconut-pie/",
        "name": "Amazing Coconut Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8295237.jpg",
