        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/235395/apple-pie-crescents/",
        "name": "Apple Pie Crescents",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8014795.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-03-05T05:11:25.000Z",
        "description": "All the flavor of apple pie without the fuss!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 small Granny Smith apple, peeled, cored, cut into 8 (1/2-inch) slices",
            "3 tablespoons butter, melted",
            "1 (8 ounce) can Pillsbury\u00ae refrigerated crescent dinner rolls",
            "\u2153 cup packed brown sugar",
            "1 teaspoon apple pie spice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 375 degrees F. Line cookie sheet with cooking parchment paper. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small bowl, toss apple slices in melted butter; set aside. Separate dough into 8 triangles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small bowl, mix brown sugar and apple pie spice. Divide mixture evenly among triangles. Top each triangle with apple slice. Starting at long side of each triangle, wrap dough around apple. Place on cookie sheet. Brush with remaining butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 10 to 12 minutes or until crescents are deep golden brown and apples are softened. Cool 5 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Pillsbury&reg; Crescents"
        ],
