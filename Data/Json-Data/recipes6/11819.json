        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9607/snickerdoodle-bars/",
        "name": "Snickerdoodle Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4648162.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "I substitute these bars for the cookies to take to picnics.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "16 1 inch squares",
        "recipeIngredient": [
            "\u2154 cup shortening",
            "1 cup white sugar",
            "1 cup all-purpose flour",
            "4 eggs",
            "1 teaspoon baking powder",
            "1 pinch salt",
            "2 teaspoons ground cinnamon",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Grease 13 x 9 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt shortening over low heat.  Add eggs and sugar, mixing well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, mix together flour, baking powder and salt until well blended.  Add to the egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into baking pan.  Sprinkle thickly with cinnamon and chopped nuts.  Bake until light brown, about 10 to 15 minutes.  Cut into squares and remove from pan immediately.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Snickerdoodle Recipes"
