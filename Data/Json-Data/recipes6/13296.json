    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12198/peaches-n-cream-pie/",
        "name": "Peaches 'n Cream Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F21494.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "1998-06-02T06:07:05.000Z",
        "description": "A wonderfully, easy pie to make that is good warm or cold, and makes its own crust!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "\u00be cup all-purpose flour",
            "1 (3 ounce) package non-instant vanilla pudding mix",
            "3 tablespoons butter",
            "1 egg",
            "\u00bd cup milk",
            "2\u2009\u00bd cups canned sliced peaches, syrup reserved",
            "1 (8 ounce) package cream cheese",
            "\u00bd cup white sugar",
            "1 tablespoon white sugar",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Drain peaches and reserve the syrup; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, pudding mix, margarine, egg, and milk; beat well.  Pour into a greased 8 or 9 inch pie pan.  Arrange drained peaches on top of mixture, just to edges.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together softened cream cheese, sugar, and 3 tablespoons reserved peach syrup.  Spoon mixture carefully on top of peaches, just to edges.  Sprinkle sugar and cinnamon on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 35 minutes.  Do not over bake.\n"
            }
        ],
