    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10357/moms-rolled-sugar-cookies/",
        "name": "Mom's Rolled Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F511681.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-11-28T05:46:11.000Z",
        "description": "These cookies are great rolled out or you can put them through the bar cookie disc of a cookie press to make thin cookies.  (Using a cookie press is much faster than rolling them out.)",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "2 cups white sugar",
            "3 eggs",
            "4\u2009\u00bd cups sifted all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon cream of tartar",
            "\u00bc teaspoon salt",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter and sugar; add eggs.  Sift dry ingredients 3 times; combine thoroughly with egg mixture.  Add vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll in waxed paper and store in refrigerator to chill thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out dough very thin on well-floured board; cut with cookie cutter.  Bake on greased cookie sheets for 6 - 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
