        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18196/lemon-sponge-pie-ii/",
        "name": "Lemon Sponge Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon Sponge Pie II"
        },
        "datePublished": "1999-03-07T13:13:43.000Z",
        "description": "Simple, light, and fluffy! Perfect for picnics and potlucks!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell",
            "1 cup white sugar",
            "2\u2009\u00bd tablespoons fresh lemon juice",
            "1 tablespoon grated lemon zest",
            "1 tablespoon butter, melted",
            "2 egg yolks, beaten",
            "3 tablespoons all-purpose flour",
            "1 cup milk",
            "2 egg whites"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine sugar, lemon rind, and lemon juice. Mix well, then stir in melted butter or margarine and egg yolks. Mix in flour. Slowly pour in milk while stirring. Mix until all ingredients are thoroughly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium glass or metal bowl, beat egg whites until soft peaks form. Fold into milk mixture. Pour mixture into pastry shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 25 to 30 minutes, until golden brown on top.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies",
            "Lemon Pies"
