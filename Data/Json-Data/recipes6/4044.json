        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276836/fig-and-rosemary-cranberry-sauce/",
        "name": "Fig and Rosemary Cranberry Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1724743.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-03-05T05:20:53.000Z",
        "description": "If you love figs and rosemary, then this sauce is for you!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup water",
            "1 cup dark brown sugar",
            "2 teaspoons maple extract",
            "1 (12 ounce) package fresh cranberries",
            "10 dried Mission figs, chopped",
            "1 (5 inch) fresh rosemary sprig"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water, brown sugar, and maple extract to a boil in a non-reactive saucepan. Add cranberries and figs. Cook over medium heat, stirring frequently, until slightly thickened, about 5 minutes. Add rosemary and cook until sauce is thickened, about 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully remove rosemary sprig and discard. Remove pot from the burner and let cool, about 10 minutes. Pour sauce into a bowl and refrigerate until ready to serve.\n"
            }
        ],
