        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12267/sour-cream-raisin-pie-i/",
        "name": "Sour Cream Raisin Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1838120.jpg",
            "width": 2448,
