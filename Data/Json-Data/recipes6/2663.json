    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256084/fruity-holiday-ham/",
        "name": "Fruity Holiday Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fruity Holiday Ham"
        },
        "datePublished": "2020-03-05T05:15:52.000Z",
        "description": "A delicious twist on the traditional holiday ham we grew up with. Great leftovers for sandwiches or to warm up with the casseroles and sweet potatoes for lunch the next day as well.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup syrup from canned mandarin oranges, oranges reserved",
            "1 cup apple juice",
            "\u00bd cup light brown sugar",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground cloves",
            "1 (4 pound) precooked spiral-cut ham",
            "2 (10 ounce) jars maraschino cherries",
            "1 (20 ounce) can crushed pineapple"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place ham in a deep roasting dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix syrup, apple juice, brown sugar, cinnamon, and cloves together in a small bowl. Pour over ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat ham with maraschino cherries and crushed pineapple.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake ham in the preheated oven, basting every 30 minutes with juices, until an instant-read thermometer inserted into the center reads 140 degrees F (60 degrees C), about 1 hour 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish ham with reserved mandarin oranges.\n"
            }
        ],
