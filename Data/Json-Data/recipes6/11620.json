        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9139/bavarian-ice-cream-pie/",
        "name": "Bavarian Ice Cream Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F78222.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This pie is a delicate vanilla delight!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P1DT1H55M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd (.25 ounce) packages unflavored gelatin",
            "\u00bd cup cold water",
            "\u00bd cup white sugar",
            "1 tablespoon white sugar",
            "2\u2009\u00bc tablespoons cornstarch",
            "2 eggs, lightly beaten",
            "1\u2009\u00bd cups milk",
            "\u00be cup premium vanilla ice cream",
            "1 teaspoon vanilla extract",
            "\u00be cup heavy cream",
            "1 9-inch vanilla cookie crumb crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle gelatin over cold water and set aside to soften. Whisk together 1/2 cup plus 1 tablespoon sugar and cornstarch in a small bowl. Whisk in the eggs and add the milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook in double boiler over simmering water until custard thickens and coats the back of a spoon. Remove from heat. Stir in gelatin until the gelatin melts. Add ice cream while custard is hot. Cool slightly and stir in vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the custard is room temperature, whip the cream until medium-stiff peaks form. Fold whipped cream into the custard. Pour filling into pie crust and refrigerate for several hours or overnight, until set.\n"
            }
        ],
