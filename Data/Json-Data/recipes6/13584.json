    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12641/strawberry-glazed-pie/",
        "name": "Strawberry Glazed Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3448829.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-04-17T16:08:09.000Z",
        "description": "This is a restaurant style strawberry glazed pie. Fresh berries are tossed with a glaze, and then chilled until serving time. This is also good made with peaches, using peach gelatin instead of strawberry.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "6 cups sliced fresh strawberries",
            "\u2154 cup white sugar",
            "1 cup water",
            "4 tablespoons strawberry flavored Jell-O\u00ae",
            "4 tablespoons cornstarch",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together the sugar, water, gelatin and cornstarch in a medium large saucepan. Boil for one minute. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in the sliced strawberries, toss until well coated. Pour into the graham cracker crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill until well set. Serve topped with whipped cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "No-Bake Pie Recipes"
        ],
