        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32556/fuzzy-navel-slush/",
        "name": "Fuzzy Navel Slush",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F840193.jpg",
            "width": 1434,
