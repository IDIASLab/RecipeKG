    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238966/spicy-quinoa-bean-and-pepper-salad/",
        "name": "Spicy Quinoa, Bean, and Pepper Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2079915.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "2020-03-10T06:52:18.000Z",
        "description": "Incredibly tasty dish that even non-quinoa lovers will love! Add other veggies and peppers to taste. This can be served either hot or cold.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 teaspoon chile oil",
            "1 onion, chopped",
            "3 cloves garlic, chopped",
            "1 large red bell pepper, chopped",
            "\u00be cup red quinoa",
            "1\u2009\u00bd cups vegetable broth",
            "\u00bd teaspoon chipotle pepper powder, or more to taste",
            "\u00bc teaspoon cayenne pepper",
            "1 (15 ounce) can black beans, rinsed and drained",
            "1 cup frozen corn kernels"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a saucepan over medium heat. Cook and stir onion and garlic in hot oil until lightly browned, 5 to 7 minutes; add chopped red pepper and continue to cook and stir until slightly tender, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir quinoa with the onion mixture. Pour vegetable broth into the saucepan; season with chipotle pepper powder and cayenne pepper. Bring the broth to a boil. Cover the saucepan, reduce heat to low, and simmer until the quinoa is tender, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir black beans and corn into the quinoa mixture; simmer until the beans and corn are heated through, about 5 minutes more.\n"
            }
        ],
