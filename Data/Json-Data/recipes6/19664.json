        "mainEntityOfPage": "https://www.allrecipes.com/recipe/65442/moms-cucumber-salad/",
        "name": "Mom's Cucumber Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F242200.jpg",
