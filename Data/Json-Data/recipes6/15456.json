        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19792/fudge-sauce/",
        "name": "Fudge Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F963836.jpg",
            "width": 1021,
            "height": 1021
        },
        "datePublished": "1999-12-09T01:10:53.000Z",
        "description": "Keep in a jar in the fridge, and heat in the microwave by the spoonful to pour over ice cream, or cake and ice cream, or whatever!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 cup white sugar",
            "3 tablespoons unsweetened cocoa powder",
            "\u00bd teaspoon salt",
            "1 teaspoon ground cinnamon",
            "\u00be cup evaporated milk",
            "1 teaspoon vanilla extract",
            "2 tablespoons butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, combine sugar, cocoa, salt and cinnamon. Stir to mix and to eliminate lumps.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in evaporated milk and bring to a boil over medium-high heat; boil for 2 minutes, remove from heat and stir in vanilla and butter.\n"
            }
        ],
