        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244666/oatmeal-carrot-craisin-cookies/",
        "name": "Oatmeal Carrot Craisin&reg; Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4459827.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-03-05T05:13:28.000Z",
        "description": "A good recipe for carrots leftover from juicing.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H32M",
        "recipeYield": "30 cookies",
        "recipeIngredient": [
            "cooking spray",
            "2 cups all-purpose flour",
            "1 cup oats",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon ground cinnamon",
            "3 cups grated carrots",
            "1 cup sweetened dried cranberries (such as Craisins\u00ae)",
            "1 cup dark brown sugar",
            "1 cup light brown sugar",
            "2 eggs",
            "\u00bd cup butter, softened",
            "\u00bd cup corn oil",
            "1 tablespoon maple syrup",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, oats, baking powder, baking soda, and ground cinnamon together in a bowl; stir in carrots and dried cranberries. Add dark brown sugar, light brown sugar, eggs, butter, oil, maple syrup, vanilla extract, and salt to the carrot mixture and stir until dough is well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough about 2 inches apart onto prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until edges are brown, 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
