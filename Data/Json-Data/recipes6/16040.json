    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22769/key-lime-pie-x/",
        "name": "Key Lime Pie X",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F314223.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-14T00:49:36.000Z",
        "description": "This is the traditional Floridian pie, except made with less fat. It is delicious!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups cornflakes cereal crumbs",
            "\u00bc cup white sugar",
            "\u2153 cup fat free margarine spread",
            "1 (14 ounce) can sweetened condensed milk",
            "4 tablespoons fresh lime juice",
            "3 tablespoons fresh lemon juice",
            "3 drops yellow food coloring",
            "4 egg whites",
            "6 tablespoons white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix cornflake crumbs, 1/4 cup sugar and melted margarine until well blended . Press mixture into a 9 inch pie plate. Refrigerate until set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix sweetened condensed milk, lime juice, lemon juice and yellow food coloring until creamy. Pour into crust and refrigerate until set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C). In a large glass or metal mixing bowl, beat egg whites until foamy. Gradually add 6 tablespoons white sugar, continuing to beat until stiff peaks form. Spread meringue over pie, covering completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 5 minutes, or until  meringue is golden brown.\n"
            }
        ],
