    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7730/caramel-raisin-cheesecake/",
        "name": "Caramel Raisin Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Caramel Raisin Cheesecake"
        },
        "datePublished": "1998-06-03T23:16:31.000Z",
        "description": "This recipe blends the good tastes of gingersnap cookies, caramel or butterscotch ice cream topping and cream cheese!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9 inch springform pan",
        "recipeIngredient": [
            "2\u2009\u00bd cups raisins",
            "\u00bd cup water",
            "1\u2009\u00bd cups gingersnap cookie crumbs",
            "\u215d cup white sugar",
            "\u2153 cup butter",
            "3 (8 ounce) packages cream cheese",
            "2 teaspoons vanilla extract",
            "3 eggs",
            "\u00bd cup caramel ice cream topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine raisins and water in a microwave safe bowl, cover and microwave on high for 3 minutes. Let stand for 20 minutes then drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter or margarine. Combine cookie crumbs, 1/4 cup of the white sugar, and the melted butter or margarine. Press  the cookie crumb mixture into the bottom of one 9 inch springform pan. Set aside 1 cup of the raisins for the topping. Sprinkle the remaining raisins evenly over the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl beat the cream cheese until light. Gradually beat in the remaining 2/3 cup of white sugar. Beat in the eggs and the vanilla. Pour cream cheese mixture over raisins and crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 55 minutes. Turn off oven and leave oven door ajar for 1 hour. Remove cake from oven and let cool completely. Cover and refrigerate for 4 hours or overnight. To serve, combine remaining raisins and caramel or butterscotch topping. Spread over the top of the cake. Makes 12 servings.\n"
            }
        ],
