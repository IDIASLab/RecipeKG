    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9586/spritz/",
        "name": "Spritz",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F750685.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "A cookie press is needed to make these cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "9 dozen",
        "recipeIngredient": [
            "3\u2009\u00be cups all-purpose flour",
            "\u00be cup white sugar",
            "1\u2009\u2153 cups butter, softened",
            "\u00bc cup orange juice",
            "1 egg yolk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large bowl, mix all ingredients at medium speed until well mixed.  Cover and refrigerate for about one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F.  Use part of dough at a time, keeping the rest refrigerated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a cookie press fitted with a bar-plate tip, press dough in long strips, about one inch apart down the length of the cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "With a sharp knife, cut each strip into 2 1/2 inch pieces.  Leave pieces in place.  Bake 8 minutes or until light golden.  Do not to overbake.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and immediately cut again between cookies to separate any cookies which are still joined together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Move cookies to wire rack, and allow cookies to cool completely.  Store tightly covered.\n"
            }
        ],
