    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26205/pecan-puffs-ii/",
        "name": "Pecan Puffs II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1127518.jpg",
            "width": 2621,
            "height": 2621
        },
        "datePublished": "2020-06-19T03:06:53.000Z",
        "description": "These are similar to Russian Tea Cakes but use cake flour instead of regular. They literally melt in you mouth!!!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "2 tablespoons white sugar",
            "1 teaspoon vanilla extract",
            "1 cup sifted cake flour",
            "1 cup pecan halves",
            "\u00bd cup confectioners' sugar for rolling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together butter and sugar until smooth. Beat in the vanilla. Grind pecans in a food processor or blender; toss with cake flour. Stir the pecan mixture into the creamed mixture until well blended. Roll the dough into tablespoon balls, then place them 1 inch apart onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 to 20 minutes in the preheated oven, or until lightly browned. Roll warm cookies in confectioners' sugar. When they are cooled, roll them again.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
