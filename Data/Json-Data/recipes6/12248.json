    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10277/pineapple-cookies-ii/",
        "name": "Pineapple Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8024654.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-01-08T15:23:56.000Z",
        "description": "This has been one of my husbands favorites for years.   Enjoy!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1\u2009\u00bd cups white sugar",
            "1 egg",
            "1 (8 ounce) can crushed pineapple with juice",
            "3\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground nutmeg",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix shortening, sugar and egg thoroughly.  Stir in pineapple.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, baking soda, salt, nutmeg, and nuts. Add to pineapple mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill at least one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls on lightly greased cookie sheet, about 2 inches apart.   Bake for about 8 to 10 minutes, or until no imprint is made when touched lightly.  Variations:  Add one cup of coconut (omit nutmeg) or add one cup of raisins, if desired\n"
            }
        ],
        "recipeCategory": [
