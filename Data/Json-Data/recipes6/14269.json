        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15736/simple-microwave-peanut-brittle/",
        "name": "Simple Microwave Peanut Brittle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F568959.jpg",
