    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274865/slow-cooker-pork-rib-tips/",
        "name": "Slow Cooker Pork Rib Tips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6856447.jpg",
            "width": 2808,
            "height": 3502
        },
        "datePublished": "2020-03-05T05:20:16.000Z",
        "description": "Cooking your rib tips in a slow cooker will give you tender, falling-off-the-bone ribs. Then finish them off under the broiler or throw them on the grill for a few minutes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT4H10M",
        "totalTime": "P0DT4H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons brown sugar",
            "1 tablespoon chili powder",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 teaspoon garlic powder",
            "1 teaspoon onion powder",
            "1 pound pork rip tips",
            "1 (18 ounce) bottle BBQ sauce, divided",
            "1 tablespoon liquid smoke flavoring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, chili powder, salt, pepper, garlic powder, and onion powder in a small bowl. Rub each rib with dry rub mixture until thoroughly coated. Let rest for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix BBQ sauce and liquid smoke until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ribs in the slow cooker and pour BBQ sauce mixture evenly over ribs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until ribs are tender, 4 to 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove ribs from the slow cooker and baste with remaining BBQ sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil ribs in the preheated oven for 3 to 5 minutes on each side.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Pork Rib Recipes"
