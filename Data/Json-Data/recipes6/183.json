        "recipeIngredient": [
            "\u00bc cup vegetable oil",
            "1 tablespoon chopped garlic",
            "1 tablespoon chopped shallot",
            "2 cups apple cider",
            "1\u2009\u00bd cups chicken stock",
            "1 cup golden raisins",
            "\u00bd cup brown sugar",
            "\u00bd orange, peeled and segmented",
            "4 sprigs fresh thyme",
            "1 bay leaf",
            "4 whole cloves",
            "1 pinch kosher salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a saucepan over medium heat. Cook and stir garlic and shallot in hot oil until fragrant, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir apple cider, chicken stock, raisins, brown sugar, orange segments, thyme sprigs, bay leaf, and cloves into garlic and shallot. Bring to a simmer and cook until sauce is reduced by half, stirring often, 15 to 20 minutes. Season with kosher salt.\n"
            }
