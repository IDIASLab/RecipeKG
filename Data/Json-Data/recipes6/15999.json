    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22565/sweet-potato-bake/",
        "name": "Sweet Potato Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4368161.jpg",
            "width": 3200,
            "height": 2400
        },
        "datePublished": "1999-12-07T02:49:27.000Z",
        "description": "I have used this recipe in my family for 15 years now and they love it! Enjoy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 9x13-inch dish",
        "recipeIngredient": [
            "2 (29 ounce) cans sweet potatoes in light syrup, drained",
            "\u00bd cup white sugar",
            "\u00bd cup butter, melted",
            "2 eggs, beaten",
            "1 teaspoon vanilla extract",
            "\u2153 cup milk",
            "\u2153 cup butter, melted",
            "1 cup brown sugar",
            "\u00bd cup all-purpose flour",
            "1 cup pecan halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash sweet potatoes. Stir in sugar, 1/2 cup melted butter, eggs, vanilla and milk until smooth.  Pour into 9x13 inch baking dish and spread evenly. In a separate bowl, combine 1/3 cup melted butter, brown sugar and flour.  Stir in pecans. Spread topping evenly over potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven 25 minutes, until top is golden and potatoes are hot and bubbly.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
