    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/37662/germantown-cookies/",
        "name": "Germantown Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3668043.jpg",
            "width": 2272,
            "height": 1696
        },
        "datePublished": "2020-06-19T03:07:45.000Z",
        "description": "Almost a macaroon type cookie: These make great Fund-Raiser Cookies. They sell very fast!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H27M",
        "recipeYield": "40 cookies",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "1 cup brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 teaspoon orange zest",
            "1 cup coconut",
            "1 cup rolled oats",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter, white sugar and brown sugar until smooth. Beat in the eggs one at a time, then stir in the vanilla. Combine the flour, baking powder, baking soda and salt; stir into the butter mixture. Finally, mix in the orange zest, coconut, oats and pecans. Drop by tablespoonfuls onto cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 12 minutes in the preheated oven, or until the edges begin to brown slightly. Allow cookies to cool on the baking sheet for 2 to 3 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
