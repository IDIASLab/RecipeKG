    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10863/chocolate-chip-crispy-cookies/",
        "name": "Chocolate Chip Crispy Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F675056.jpg",
            "width": 640,
            "height": 480
        },
        "datePublished": "1998-04-28T00:39:10.000Z",
        "description": "A twist on chocolate chip cookies!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 1/2 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bd cup margarine, softened",
            "1 cup white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "2 cups crisp rice cereal",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In small mixing bowl, combine flour, baking soda and salt.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl, beat together margarine and sugar until light and fluffy.  Add egg and vanilla.  Beat well.  Add flour mixture, mixing until combined.  Stir in Rice Krispies cereal and chocolate chips .\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by level tablespoons onto cookie sheets coated with vegetable spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C)  about 12 minutes or until lightly browned.  Remove immediately from cookie sheets and cool on wire racks.  Store in airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
