        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12077/chocolate-chip-pie-i/",
        "name": "Chocolate Chip Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4705327.jpg",
            "width": 5312,
            "height": 2988
        },
        "datePublished": "1999-02-08T08:34:40.000Z",
        "description": "A very rich pie made with chocolate chips and walnuts.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 nine inch pie",
        "recipeIngredient": [
            "2 eggs",
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "1 teaspoon vanilla extract",
            "1 cup butter",
            "\u00bd cup all-purpose flour",
            "1 cup semisweet chocolate chips",
            "1 cup chopped walnuts",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs until frothy, add sugars and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter or margarine, and add to sugar and egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in walnuts and chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pie pan and bake for 1 hour.  Serve warm with whipped cream or ice cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Chocolate Pie Recipes"
        ],
