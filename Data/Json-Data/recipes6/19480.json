        "mainEntityOfPage": "https://www.allrecipes.com/recipe/59524/cucumber-peanut-salad/",
        "name": "Cucumber Peanut Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F448654.jpg",
            "width": 250,
            "height": 250
