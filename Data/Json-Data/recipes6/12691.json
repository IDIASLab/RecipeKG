        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10932/beckys-sugary-sugar-cookies/",
        "name": "Becky's Sugary Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F674304.jpg",
