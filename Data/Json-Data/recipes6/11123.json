    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7842/lemon-apricot-cake/",
        "name": "Lemon Apricot Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7991832.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-07-16T04:56:58.000Z",
        "description": "This cake recipe turns lemon cake mix and apricot nectar into a light dessert that can also double as a breakfast cake.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -10 inch tube or bundt cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package lemon cake mix",
            "\u2153 cup white sugar",
            "\u00be cup vegetable oil",
            "1 cup apricot nectar",
            "4 eggs",
            "2 cups confectioners' sugar",
            "3 tablespoons lemon juice",
            "3 drops vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease one 10 inch tube or bundt pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the cake mix, white sugar, 3/4 cup vegetable oil, and apricot nectar together. Beat in the eggs one at a time, mixing well after each addition. Pour the batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 1 hour.  Let cake cool in pan for 10 minute then invert onto a serving dish and pour glaze over cake while it is still warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Glaze: Combine the confectioners' sugar, lemon juice and the 3 drops of oil, mixing until smooth. Use immediately to pour over still warm cake.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
