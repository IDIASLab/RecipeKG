    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275005/buffalo-chicken-naan-pizza/",
        "name": "Buffalo Chicken Naan Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6948012.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-20T06:50:34.000Z",
        "description": "Cajun-spiced grilled chicken, spicy Buffalo sauce, and a creamy cheese sauce on garlic naan bread makes a delicious, easy pizza meal.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 naan bread pizzas",
        "recipeIngredient": [
            "2 large chicken breasts",
            "1 tablespoon Cajun seasoning, or to taste",
            "1 (8 ounce) package cream cheese",
            "\u2154 cup sour cream",
            "\u00bc cup half-and-half",
            "2 tablespoons powdered Parmesan cheese",
            "1 tablespoon chopped fresh rosemary",
            "1 teaspoon garlic, minced",
            "crushed black pepper to taste",
            "4 garlic naan breads",
            "1 (4 ounce) package crumbled feta cheese, or to taste",
            "\u00bd cup Buffalo wing sauce (such as Frank's\u00ae)",
            "\u2153 cup melted butter",
            "1\u2009\u00bd tablespoons tomato paste",
            "1 teaspoon smoked paprika",
            "\u00bd teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season chicken with Cajun seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill chicken on the preheated grill until no longer pink inside and juices run clear, 5 to 10 minutes per side. Remove from grill, leaving grill on. Let chicken rest for 3 to 4 minutes, then cut into small pieces and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, sour cream, half-and-half, Parmesan cheese, rosemary, garlic, and black pepper in a mixing bowl. Spread white sauce evenly onto naan, then layer chicken and feta cheese on top. Refrigerate any leftover white sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Buffalo sauce, melted butter, tomato paste, smoked paprika, and nutmeg in a bowl. Drizzle Buffalo sauce on top of naan pizzas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat pizzas on the hot grill until warmed through, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer pizzas to the preheated broiler and finish cooking until golden brown on top, about 1 minute more.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
