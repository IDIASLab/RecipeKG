    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143473/easy-as-chicken-pot-pie/",
        "name": "Easy As Chicken Pot Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F423187.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-10T06:51:26.000Z",
        "description": "This is the easiest chicken pot pie you will ever make from scratch. It uses stuff you have on hand and is great for leftover chicken. It's pretty, too! Substitute whatever frozen vegetables you have on hand.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 sheet frozen puff pastry, thawed",
            "1\u2009\u2153 cups frozen peas and carrots",
            "\u2154 cup frozen corn kernels",
            "2 tablespoons butter",
            "\u00bc cup all-purpose flour",
            "2 cups milk",
            "1 teaspoon crumbled dried thyme, or to taste",
            "2 cups diced cooked chicken",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a pizza cutter to slice the puff pastry into 8 equal-sized strips. Weave the strips into a square lattice and place on a greased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until it just starts to brown, 10  to 15 minutes. Remove from oven and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the frozen peas, carrots, and corn in a microwave-safe bowl and cook in the microwave until cooked but still firm, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a skillet over medium heat. Stir in the flour and cook for 1 minute.  Pour in the milk and stir while cooking until the mixture is smooth. Add the thyme and continue to cook and stir until the mixture thickens. Stir in the cooked vegetables and the chicken. Season with salt and pepper.  Cook until heated through, stirring frequently, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the chicken mixture into a 2-quart baking dish. Lay the pastry lattice on top. Bake in the preheated oven until the crust is golden brown, 10 to 15 minutes.\n"
            }
        ],
