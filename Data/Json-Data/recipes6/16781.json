        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24614/coleslaw-dressing/",
        "name": "Coleslaw Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F265320.jpg",
