    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275193/slow-cooked-pork-tenderloin/",
        "name": "Slow-Cooked Pork Tenderloin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6984188.jpg",
            "width": 1122,
            "height": 1122
        },
        "datePublished": "2020-03-05T05:20:22.000Z",
        "description": "Most tender, flavorful pork you will ever have! The slow cooking and overnight marinade is worth the time and the sauce is amazing!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT3H15M",
        "totalTime": "P1DT3H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup olive oil",
            "\u00bd cup balsamic vinegar",
            "3 cloves garlic, peeled",
            "3 tablespoons brown sugar",
            "2 tablespoons chopped fresh thyme",
            "1 teaspoon dry mustard",
            "1 teaspoon paprika",
            "1 (4 pound) pork tenderloin",
            "\u00bc cup dry sherry",
            "2 tablespoons cornstarch",
            "2 tablespoons water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place olive oil, balsamic vinegar, garlic, brown sugar, thyme, dry mustard, and paprika into a food processor and blend. Pour marinade into a zip-top bag, add pork tenderloin, and marinate in the refrigerator for 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C). Transfer pork and marinade into a Dutch oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pork from Dutch oven and set aside on a plate, reserving drippings in the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat drippings in the Dutch oven over medium heat on the stovetop. Add sherry and deglaze the pan, making sure to scrape up any stuck-on pieces with a wooden spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cornstarch and water in a separate bowl until completely mixed; add to sauce in the pot. Bring to a simmer and cook until thickened, about 15 minutes. Slice pork tenderloin and serve with sauce on the side or poured on top.\n"
            }
        ],
