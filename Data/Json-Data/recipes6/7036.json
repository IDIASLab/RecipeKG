    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279461/old-bay-fried-shrimp/",
        "name": "Old Bay&reg; Fried Shrimp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7961861.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-02T22:52:02.000Z",
        "description": "This is an old recipe used by many of the seafood restaurants in Florida for Old Bay&reg; fried shrimp. Excellent for fish, shrimp, scallops!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound large shrimp, peeled and deveined",
            "3 large eggs, or more as needed",
            "2 cups milk, or more as needed",
            "peanut oil for frying",
            "2 cups all-purpose flour",
            "2 cups yellow cornmeal",
            "\u00bd cup seafood seasoning (such as <a href=\"https://www.allrecipes.com/article/what-is-old-bay-seasoning/\" aria-label=\"Old Bay\">Old Bay</a>\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place each shrimp on a flat work surface and butterfly by slicing horizontally through the middle, being careful not to cut all the way through to the other side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk eggs in a bowl; mix in milk. Place shrimp in milk mixture and soak for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, cornmeal, and seafood seasoning together and pour into a zip-top bag. Place 5 to 6 shrimp in the bag and shake well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop shrimp in the hot oil and fry until golden brown and opaque in the centers, about 5 minutes. Remove and place on paper towels to soak out oil. Repeat with remaining shrimp.\n"
            }
        ],
