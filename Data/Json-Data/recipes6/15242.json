        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18994/blender-chocolate-mousse/",
        "name": "Blender Chocolate Mousse",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3049264.jpg",
