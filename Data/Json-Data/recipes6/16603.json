    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24279/christmas-fudge/",
        "name": "Christmas Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7496459.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:05:26.000Z",
        "description": "White chocolate fudge with green & red swirls!  Great to take to parties or the neighbors!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "30 squares",
        "recipeIngredient": [
            "3 cups white sugar",
            "\u2154 cup heavy cream",
            "\u00be cup butter",
            "1 (7 ounce) jar marshmallow creme",
            "1 (11 ounce) package white chocolate chips",
            "red food coloring, as desired",
            "green food coloring, as desired"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan combine sugar, cream and butter.  Heat to between 234 and 240 degrees F (112 to 116 degrees C), or until a small amount of syrup dropped into cold water forms a soft ball that flattens when removed from the water and placed on a flat surface.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in marshmallow creme and white chocolate chips; mix well.  Reserve 2 separate 1/2 cups of mixture and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To one of the reserved 1/2 cups mixture add red food coloring until desired shade of red is achieved.  To the other 1/2 cup of mixture add green food coloring until desired shade of green is achieved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour white mixture into prepared pan.  Pour dots of both the red and green mixtures across the top.  Using a toothpick or knife, swirl colors throughout the white.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill for 2 hours and cut into 2 inch squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Fudge Recipes",
            "Chocolate"
        ],
