    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10951/ginger-crinkles/",
        "name": "Ginger Crinkles",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1084606.jpg",
            "width": 4288,
            "height": 3216
        },
        "datePublished": "1998-06-09T18:27:38.000Z",
        "description": "This is a recipe from my mother. Our family has been making them for 50 years.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u2154 cup vegetable oil",
            "1 cup white sugar",
            "1 egg",
            "\u00bc cup molasses",
            "2 cups all-purpose flour",
            "2 teaspoons baking soda",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bc cup white sugar",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix oil and sugar thoroughly with electric mixer.  Add egg and mix well.  Pour in molasses.  Sift and add dry ingredients until incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll teaspoonful of dough into ball, drop into sugar to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on ungreased cookie sheet.   Bake at 350 degrees F (175 degrees C) for 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Spice Cookie Recipes"
        ],
