    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280445/air-fryer-stuffed-apples/",
        "name": "Air Fryer Stuffed Apples",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8151377.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T01:56:27.000Z",
        "description": "An apple stuffed with raisins and walnuts and air fried. The top is crunchy while the inside is soft, juicy, and creamy. The pinch of nutmeg adds an additional layer of flavor.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 serving",
        "recipeIngredient": [
            "1 apple",
            "2 tablespoons raisins",
            "2 tablespoons chopped walnuts",
            "\u00bd tablespoon unsalted butter, melted",
            "1 teaspoon white sugar",
            "\u00bd teaspoon vanilla extract",
            "\u00bd teaspoon ground cinnamon",
            "\u215b teaspoon ground nutmeg",
            "\u00bc cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the air fryer to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut about 1/2 inch around the top of the apple and spoon out some of the flesh, leaving at least a 1/4-inch border.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix raisins, walnuts, butter, sugar, vanilla, cinnamon, and nutmeg until well combined. Spoon into the cavity of the apple. Place apple in a small oven-safe dish and pour in water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place dish in the air fryer basket and cook until the apple is fork-tender, 20 to 25 minutes.\n"
            }
        ],
