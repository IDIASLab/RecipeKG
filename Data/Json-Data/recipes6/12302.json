        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10360/sour-cream-sugar-cookies-i/",
        "name": "Sour Cream Sugar Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3189238.jpg",
            "width": 4272,
            "height": 2848
        },
        "datePublished": "1997-12-08T04:10:21.000Z",
        "description": "Soft sugar cookies  =)",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup butter",
            "1 teaspoon vanilla extract",
            "2 eggs",
            "1 cup sour cream",
            "5 cups all-purpose flour",
            "\u00bd teaspoon salt",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "1 (8 ounce) package cream cheese",
            "2 tablespoons evaporated milk",
            "1 teaspoon vanilla extract",
            "\u215b teaspoon salt",
            "4 cups confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together 1 1/2 cups white sugar, butter or margarine, 1 teaspoon vanilla, and eggs.  Add sour cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together 5 cups flour, 1/2 teaspoon salt, baking powder and baking soda.  Gradually add to the egg-sugar mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and chill dough for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out on lightly floured surface to 1/4 inch thick.  Cut out cookies.  Bake on lightly greased cookie sheet for 10 - 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting:   In small bowl, with mixer at medium speed, beat together the softened cream cheese and evaporated milk until smooth.  Add 1 teaspoon vanilla, 1/8 teaspoon salt and confectioners' sugar.  Beat until well blended.  Color as desired and frost cooled cookies.\n"
            }
        ],
