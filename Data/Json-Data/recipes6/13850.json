    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14165/taffy-apple-salad-i/",
        "name": "Taffy Apple Salad I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970304.jpg",
            "width": 962,
            "height": 960
        },
        "datePublished": "1999-07-25T13:02:35.000Z",
        "description": "Perfect salad for a summer picnic! This is a light dessert which all will enjoy. I recommend making this the day it will be served, as it sometimes gets runny if made the night before.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 tablespoon all-purpose flour",
            "\u00bd cup white sugar",
            "1 egg yolk",
            "2 tablespoons cider vinegar",
            "1 (8 ounce) can crushed pineapple, drained with juice reserved",
            "4 large apples, cored and diced with peel",
            "2 cups unsalted peanuts",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan combine the flour, sugar, egg yolk, vinegar and reserved pineapple juice. Cook over medium heat until thick. Remove from heat and chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "in a large bowl combine the pineapple, apples, peanuts and chilled cooked mixture. Fold in whipped topping and chill for at least 1 hour before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes"
        ],
