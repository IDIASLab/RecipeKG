    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10986/peanut-butter-mini-candy-coated-chocolates-cookies/",
        "name": "Peanut Butter Mini Candy-Coated Chocolates Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8111187.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-07-06T04:04:08.000Z",
        "description": "A little peanut butter with your chocolate.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00be cup butter, softened",
            "\u00be cup peanut butter",
            "1\u2009\u00bc cups packed brown sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1\u2009\u00be cups mini candy-coated chocolates"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream butter or margarine, peanut butter, and sugar until light and fluffy.  Beat in egg and vanilla.  Combine flour, baking soda, and salt; blend into creamed mixture. Stir in 1 1/3 cups mini candy-coated chocolates.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1 1/4 inch balls.  Place about 2 inches apart on ungreased cookie sheets.  Gently flatten to about 1/2 inch thickness.  Place 7 to 8 of the remaining candies on each cookie; press in lightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 10 to 12 minutes, or until edges are light brown.  Do not over bake!  Cool about 1 minute on cookie sheets; transfer cookies to wire racks to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
