    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234525/habanero-bbq-sauce/",
        "name": "Habanero BBQ Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6988790.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-05T05:11:06.000Z",
        "description": "Sweet, tangy, and spicy...Great addition to ribs, chicken, or pulled pork (ESPECIALLY pulled pork sandwiches.)",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "4 cups cider vinegar",
            "4 habanero peppers, minced (wear gloves)",
            "3 tablespoons minced onion",
            "2 cloves minced garlic",
            "1 tablespoon red pepper flakes",
            "1 tablespoon cayenne pepper",
            "2 teaspoons dry mustard",
            "2 teaspoons white pepper",
            "1 teaspoon seasoned salt",
            "2 cups dark brown sugar",
            "\u00bd cup ketchup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring vinegar to a boil in a saucepan; add habanero peppers, onion, garlic, red pepper flakes, cayenne pepper, mustard, white pepper, and seasoned salt. Allow mixture to return to a boil, then stir in brown sugar and ketchup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Boil sauce for 15 minutes, stirring constantly. Reduce heat to medium-low and simmer, stirring occasionally, until thickened, 30 to 45 minutes. Cool and serve.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "BBQ Sauce Recipes"
        ],
