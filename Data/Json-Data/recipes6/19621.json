    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/64317/boysenberry-cashew-cheesecake-ice-cream/",
        "name": "Boysenberry Cashew Cheesecake Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Boysenberry Cashew Cheesecake Ice Cream"
        },
        "datePublished": "2020-06-19T03:10:15.000Z",
        "description": "I love ice cream and I love cheesecake. Here is a way to have the best of both worlds.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 1/2 quarts",
        "recipeIngredient": [
            "1 cup graham cracker crumbs",
            "3 tablespoons sugar",
            "4 tablespoons butter or margarine, melted",
            "2 cups buttermilk",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (8 ounce) package cream cheese",
            "\u215b cup lemon juice",
            "1 tablespoon lemon zest",
            "1 pinch salt",
            "\u00bc cup coarsely chopped cashews",
            "\u00bc cup boysenberry preserves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). In a small bowl, combine graham cracker crumbs, sugar, and melted butter. Press mixture evenly into the bottom of a pie pan. Bake for 8 minutes, and remove from oven. When cool, break into small pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine buttermilk, sweetened condensed milk, cream cheese, lemon juice, zest, and salt in a blender or food processor. Process until smooth. Pour into an ice cream maker, and freeze according to the manufacturer's instructions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in cashews and pieces of graham cracker crust. Gently fold in preserves to create a ribbon effect. Transfer to the freezer for 1 hour to harden.\n"
            }
        ],
