    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275631/pumpkin-oatmeal-bars/",
        "name": "Pumpkin-Oatmeal Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin-Oatmeal Bars"
        },
        "datePublished": "2020-03-05T05:20:31.000Z",
        "description": "Pumpkin sweets are popular at the holidays but really are yummy all year round! Try these pumpkin-oatmeal bars for a delicious mix between pumpkin bread and chewy oatmeal bars!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "20 bars",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) can pumpkin puree",
            "1 cup brown sugar",
            "2 eggs",
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "3 cups rolled oats",
            "1 cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Coat a 9x11-inch pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine pumpkin puree, brown sugar, and eggs in a large bowl; beat with an electric mixer until smooth and creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together flour, baking soda, cinnamon, nutmeg, and cloves in a separate bowl. Slowly add flour mixture to pumpkin mixture until combined. Stir in oats and raisins. Spread batter evenly in the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a fork inserted into the center comes out clean, 20 to 25 minutes. Allow to cool completely, then slice into 20 bars.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
