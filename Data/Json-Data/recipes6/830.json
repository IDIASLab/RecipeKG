    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231703/apple-berry-brown-sugar-ham/",
        "name": "Apple Berry Brown Sugar Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Berry Brown Sugar Ham"
        },
        "datePublished": "2020-03-05T05:10:22.000Z",
        "description": "With a glaze made of brown sugar, cherry cola, strawberry jam, and apple brandy, this oven-roasted ham is fruity and sweet.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H35M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "1 15-pound ham",
        "recipeIngredient": [
            "1 (15 pound) fully cooked bone-in ham",
            "1 (2 liter) bottle cherry cola, divided",
            "1 teaspoon ground allspice",
            "\u00bd teaspoon salt",
            "\u00be cup strawberry jam",
            "\u00bc cup brown sugar",
            "2 tablespoons apple brandy"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Trim excess fat from ham, leaving 1/4-inch layer of fat. Score fat in a diamond pattern using a sharp knife. Place ham on a rack in a large roasting pan and pour cola in pan, reserving about 1/4 cup for the glaze. Season ham with allspice and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven, basting every 15 minutes with pan juices, for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine strawberry jam, brown sugar, and apple brandy in a saucepan over low heat; cook and stir until sugar is dissolved, about 5 minutes. Remove pan from heat and stir reserved cherry cola into the glaze; brush onto ham to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return ham to oven to continue cooking, brushing with the glaze 2 to 3 times more as it cooks, until an instant-read thermometer inserted into the center reads 140 degrees F (60 degrees C), 30 to 45 minutes more. Tent ham loosely with aluminum foil and set aside for 30 minutes before carving.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Ham",
            "Whole"
        ],
