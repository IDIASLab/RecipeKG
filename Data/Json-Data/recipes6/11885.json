    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9714/turtles-brownies/",
        "name": "Turtles&reg; Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F660197.jpg",
            "width": 4272,
            "height": 2848
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These brownies have pecans and caramel, much like the candies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "2 cups chopped pecans",
            "2 cups white sugar",
            "1 cup unsalted butter",
            "4 eggs",
            "1 cup unsweetened cocoa powder",
            "1 cup all-purpose flour",
            "2 teaspoons vanilla extract",
            "24 individually wrapped caramels, unwrapped",
            "42 pecan halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the sugar, butter, eggs and cocoa until just blended. Slowly add flour and one teaspoon of the  vanilla.  Mix until just blended.  Spread batter in pan and top with chopped pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake about 25 minutes. Let cool on wire rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In saucepan over low heat, melt caramels, stirring until smooth.  Remove from heat and stir in  remaining vanilla.  Drizzle over brownies in parallel rows and press pecan halves into the caramel in neat rows. Let set for at least 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Chocolate Brownie Recipes"
        ],
