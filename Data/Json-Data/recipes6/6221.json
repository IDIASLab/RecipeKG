    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279285/ranch-zucchini-chips/",
        "name": "Ranch Zucchini Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7815834.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-16T03:12:00.000Z",
        "description": "Dill pickle potato chips meet ranch-flavored potato chips but made with zucchini instead of potato and baked instead of fried.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 medium zucchini, sliced",
            "1 pinch salt to taste",
            "2 teaspoons extra-virgin olive oil",
            "1 tablespoon ranch dressing mix (such as Hidden Valley Ranch\u00ae)",
            "1 teaspoon freeze-dried dill",
            "1  serving cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice the zucchini using a mandoline into 1/16-inch-thick rounds. Spread out on a clean work surface and sprinkle with salt. Let sit for 10 minutes. Remove as much moisture from the zucchini slices as you can with a paper towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 220 degrees F (105 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine zucchini chips and olive oil in a large bowl and stir to coat. Season with ranch powder and dill and gently stir to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray 2 large baking sheets with nonstick cooking spray. Arrange zucchini slices on the sheets in one layer, making sure none are overlapping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 40 minutes. Carefully flip the slices and continue baking for 30 minutes, or until desired crispiness is reached. Transfer to a paper towel-lined plate.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Zucchini Appetizer Recipes"
        ],
