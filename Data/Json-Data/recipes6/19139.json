    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/53172/lavender-tea-bread/",
        "name": "Lavender Tea Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F373882.jpg",
            "width": 4288,
            "height": 2848
        },
        "datePublished": "2020-06-19T03:09:20.000Z",
        "description": "I make this tea bread when the  lavender is in bloom. Try it with fresh strawberry slices on top.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "\u00be cup milk",
            "3 tablespoons finely chopped fresh lavender",
            "6 tablespoons butter, softened",
            "1 cup white sugar",
            "2 eggs",
            "2 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease and flour a 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the milk and lavender in a small saucepan over medium heat. Heat to a simmer, then remove from heat, and allow to cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the butter and sugar until smooth. Beat in the egg until the mixture is light and fluffy. Combine the flour, baking powder, and salt; stir into the creamed mixture alternately with the milk and lavender until just blended. Pour into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 50 minutes in the preheated oven, or until a wooden pick inserted into the crown of the loaf comes out clean. Cool in the pan on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
