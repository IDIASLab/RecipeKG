        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12673/mock-apple-pie-ii/",
        "name": "Mock Apple Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4868894.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "1999-09-04T19:09:27.000Z",
        "description": "Love apple pie, but hate apples? Your prayers have been answered!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1\u2009\u00bd cups water",
            "1 tablespoon butter",
            "1\u2009\u00bd teaspoons cream of tartar",
            "\u00bd teaspoon ground cinnamon",
            "16 double-wide soda crackers",
            "1 (9 inch) pie shell"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine sugar, water, butter, cream of tartar and cinnamon. Bring to a boil, and continue to boil for 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Break 16 (double) soda crackers into pie shell. Pour hot syrup over.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add top crust, seal and flute edges. Bake at 425 degrees F (220 degrees C) for 30 to 35 minutes.\n"
            }
        ],
