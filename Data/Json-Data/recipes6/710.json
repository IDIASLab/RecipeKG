    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230745/oven-barbecued-chicken-and-potatoes/",
        "name": "Oven Barbecued Chicken and Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1000229.jpg",
            "width": 3320,
            "height": 3320
        },
        "datePublished": "2020-03-05T05:10:06.000Z",
        "description": "Easy barbecue chicken and potato dish. I prepare this on Sunday morning, and it is ready when we get home from church. It is hearty, and we never have leftovers.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT2H5M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 pounds skinless, boneless chicken breast halves",
            "6 potatoes, cut into bite-sized chunks",
            "1 onion, chopped",
            "\u00bd cup ketchup",
            "6 tablespoons brown sugar",
            "\u00bc cup Worcestershire sauce",
            "\u00bc cup white vinegar",
            "\u00bc cup margarine",
            "2 tablespoons lemon juice",
            "2 teaspoons dry mustard",
            "2 teaspoons salt",
            "2 teaspoons paprika",
            "2 teaspoons chili powder",
            "1 pinch red pepper flakes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange chicken breasts, potatoes, and onion in a large baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ketchup, brown sugar, Worcestershire sauce, vinegar, margarine, lemon juice, dry mustard, salt, paprika, chili powder, and red pepper flakes in a saucepan. Bring sauce to a boil, stirring constantly to dissolve brown sugar and salt. Pour the sauce over chicken and vegetables. Cover dish with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink, the juices run clear, and the sauce has thickened, about 2 hours. An instant-read meat thermometer inserted into the center of a breast should read at least 160 degrees F (70 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
