    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7528/blueberry-dump-cake/",
        "name": "Blueberry Dump Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F97043.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "By definition, dump cakes are simple and fuss-free to make. Try this recipe using fresh blueberries and cake mix for a dessert everyone will love.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9 x 13 inch pan",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "4 cups fresh blueberries",
            "\u00bd cup white sugar",
            "\u00bd cup butter, melted",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix berries, sugar, and cinnamon in the bottom of a 9 x 13 inch pan. Cover berries with dry cake mix.  Pour butter over cake mix, do not stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 minutes, or until light brown.  Serve warm or cold. top with ice cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Blueberry Dessert Recipes"
        ],
