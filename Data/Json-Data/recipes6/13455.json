    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12439/jimmy-carter-dessert/",
        "name": "Jimmy Carter Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4575453.jpg",
            "width": 2304,
            "height": 2304
        },
        "datePublished": "1998-10-11T00:59:27.000Z",
        "description": "A fitting tribute for the 39th president of the U.S.A.  This pie is larger than life, and is topped with the peanut farmer's favorite nut.  Serves enough for a Habitat for Humanity work crew.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 x 13 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups graham cracker crumbs",
            "\u00bc cup white sugar",
            "\u2153 cup butter, melted",
            "1 (8 ounce) package cream cheese",
            "\u2153 cup peanut butter",
            "2 (8 ounce) containers frozen whipped topping, thawed",
            "1 cup confectioners' sugar",
            "1 (3.4 ounce) package instant vanilla pudding mix",
            "1 (3.9 ounce) package instant chocolate pudding mix",
            "3 cups milk",
            "\u00bc cup chopped peanuts",
            "\u215b cup grated semisweet chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine graham cracker crumbs, sugar, and melted butter or margarine.  Press firmly into the bottom of a 13 x 9 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the cream cheese and peanut butter together until smooth.  Stir in 2 cups of the whipped topping and the confectioners' sugar.  Pour mixture over crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the pudding mixes and milk together, and pour over peanut butter layer.  Spread remaining whipped topping on top.  Sprinkle with chopped peanuts and shaved chocolate.  Chill.\n"
            }
        ],
