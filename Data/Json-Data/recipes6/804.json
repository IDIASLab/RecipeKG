    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231562/no-chocolate-blondie-brownies/",
        "name": "No-Chocolate Blondie Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F973599.jpg",
            "width": 2986,
            "height": 2986
        },
        "datePublished": "2020-03-05T05:10:18.000Z",
        "description": "This quick and easy no-chocolate brownie recipe takes 15 minutes to prepare and 20 minutes to bake.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "16 2-inch square brownies",
        "recipeIngredient": [
            "\u00be cup butter",
            "1 tablespoon milk",
            "1 cup packed brown sugar",
            "1 egg, beaten",
            "1 cup self-rising flour",
            "\u00bd cup chopped walnuts",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour an 8-inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter and milk in a large saucepan over low heat; cook, stirring occasionally, until butter melts, 3 to 5 minutes. Remove saucepan from heat. Stir brown sugar and egg into the butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour into the butter mixture; stir. Add walnuts and vanilla extract; stir. Pour the mixture into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 20 to 25 minutes. Cool in the pans for 10 minutes before removing to cool completely on a wire rack. Cut into 2-inch squares to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Blondie Recipes"
        ],
