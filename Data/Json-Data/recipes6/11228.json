    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7988/chocolate-chip-apple-cake/",
        "name": "Chocolate Chip Apple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7851288.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-06-15T19:59:27.000Z",
        "description": "Apples add moistness to the spicy chocolate batter and chocolate chips further enhance the chocolate flavor.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9 or 10 inch tube cake",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 cups white sugar",
            "3 eggs",
            "\u00bd cup water",
            "1 tablespoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "2 tablespoons unsweetened cocoa powder",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground nutmeg",
            "2 cups apples - peeled, cored and finely diced",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease and flour one 9 or 10 inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl cream the butter or margarine with the sugar. Beat in the eggs then add the water and the vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour, cocoa, baking soda, ground cinnamon and ground nutmeg together. Beat this mixture into the creamed mixture. Stir in the chopped apples and the semisweet chocolate chips. Pour the batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 1 hour and 15 minutes or until the cake tests done when a toothpick is inserted near the center. Transfer to a rack to cool. Makes about 16 servings.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
