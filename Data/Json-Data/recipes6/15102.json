    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18229/affy-tapple-salad/",
        "name": "Affy Tapple Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Affy Tapple Salad"
        },
        "datePublished": "2000-06-04T19:07:56.000Z",
        "description": "A sweet, crunchy, almost candy-like apple salad.  For added flavor, use 2 green and 2 red apples!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 tablespoon all-purpose flour",
            "\u00bd cup white sugar",
            "2 tablespoons instant tapioca",
            "2 tablespoons cider vinegar",
            "1 (8 ounce) can crushed pineapple, drained with juice reserved",
            "4 apples, cored and chopped",
            "2 cups unsalted peanuts",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, stir together the flour, sugar, tapioca, vinegar, and the reserved pineapple juice.  Cook over medium heat, stirring constantly, until thick.  Remove from heat and chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the pineapple, apples, peanuts, and chilled tapioca mixture.  Fold in the whipped topping and chill for at least 1 hour before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes"
        ],
