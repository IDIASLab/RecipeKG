        "name": "Potato and Corn Chowder (Freezer Dump Meal)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5842325.jpg",
            "width": 3024,
