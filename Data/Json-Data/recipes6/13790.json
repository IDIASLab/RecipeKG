    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13835/pineapple-pecan-pie/",
        "name": "Pineapple Pecan Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F874674.jpg",
            "width": 3264,
            "height": 2983
        },
        "datePublished": "1999-11-11T20:02:27.000Z",
        "description": "This is a very unusual pie that blends fruit and nuts in a very tasty combination!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9 inch pie",
        "recipeIngredient": [
            "1 (9 inch) unbaked pie crust",
            "1 (8 ounce) package cream cheese, softened",
            "1 egg, beaten",
            "\u00bd cup white sugar",
            "\u00bc teaspoon salt",
            "1 teaspoon vanilla extract",
            "1 (8 ounce) can crushed pineapple, drained",
            "1\u2009\u00bd cups chopped pecans",
            "3 eggs, beaten",
            "1 cup light corn syrup",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend together the cream cheese, 1 egg, 1/2 cup sugar, salt and vanilla. Gently stir in pineapple, pour into pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle pecans over cream cheese mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend together the 3 eggs, corn syrup, 1/4 cup sugar and vanilla. Pour over pecan layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on lower rack of oven and bake for 40 to 45 minutes or until center is firm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pecan Pie Recipes"
        ],
