        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "4 tablespoons unsweetened cocoa powder",
            "2 cups white sugar",
            "\u00bd cup milk",
            "\u215b teaspoon salt",
            "3 cups quick cooking oats",
            "\u00bd cup crunchy peanut butter",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix butter or margarine, cocoa, sugar, milk, and salt together in saucepan.  Boil for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in oatmeal, peanut butter and vanilla.  Drop by tablespoonfuls onto waxed paper.  Allow to cool and harden.\n"
            }
        ],
