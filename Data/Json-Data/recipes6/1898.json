    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240446/vanilla-almond-spiced-french-toast/",
        "name": "Vanilla-Almond Spiced French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1947544.jpg",
            "width": 2181,
            "height": 2908
        },
        "datePublished": "2020-03-05T05:12:48.000Z",
        "description": "A must for those who like their French toast sweet and super flavorful!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "8 slices",
        "recipeIngredient": [
            "2 cups whole milk",
            "2 tablespoons brown sugar",
            "4 large eggs",
            "1 teaspoon double-strength imitation vanilla extract (such as Goodman's Route 66)",
            "\u00bd teaspoon almond extract (such as Watkins\u00ae pure almond extract)",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u215b teaspoon ground allspice",
            "8 thick slices slightly stale Texas toast"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix whole milk and brown sugar in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs, vanilla extract, almond extract, cinnamon, nutmeg, and allspice together in a separate bowl until the eggs are well beaten; add to the milk mixture and stir to dissolve the brown sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a non-stick skillet or griddle over medium-low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set one slice of Texas toast into the milk mixture; let soak until moistened, about 10 seconds per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the dipped toast in the preheated skillet until bottom is golden brown, 3 to 5 minutes; flip toast and continue to cook until the other side is browned, 3 to 5 minutes more. Repeat dipping and cooking with remaining bread slices.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "French Toast Recipes"
        ],
