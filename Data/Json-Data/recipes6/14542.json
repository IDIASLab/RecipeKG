    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16508/vegan-cheesecake/",
        "name": "Vegan Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F647890.jpg",
            "width": 1936,
            "height": 2592
        },
        "datePublished": "2000-03-16T23:37:17.000Z",
        "description": "This sweetened tofu recipe is baked in a graham cracker pie crust and served chilled. Try drizzling with a sweet berry sauce for extra color and flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (12 ounce) package soft tofu",
            "\u00bd cup soy milk",
            "\u00bd cup white sugar",
            "1 tablespoon vanilla extract",
            "\u00bc cup maple syrup",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a blender, combine the tofu, soy milk, sugar, vanilla extract and maple  syrup. Blend until smooth and pour into pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 minutes. Remove from oven and allow to cool; refrigerate until chilled.\n"
            }
        ],
