        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268259/pumpkin-butter-cookies/",
        "name": "Pumpkin Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5817049.jpg",
            "width": 2419,
            "height": 2005
        },
        "datePublished": "2020-03-05T05:18:34.000Z",
        "description": "Delicious and moist pumpkin butter cookies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "32 cookies",
        "recipeIngredient": [
            "1 cup packed light brown sugar",
            "\u00bc cup butter, softened",
            "\u00bd cup quick cooking oats",
            "\u00bd cup pumpkin butter",
            "1 egg",
            "1 cup all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon pumpkin pie spice",
            "\u00bd teaspoon ground cinnamon",
            "2 tablespoons milk",
            "\u00bd cup chopped pecans",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream brown sugar and butter together in a mixing bowl. Beat in oats, pumpkin butter, and egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking soda, baking powder, salt, pumpkin pie spice, and cinnamon in a separate bowl. Gradually add flour mixture to sugar mixture, alternating with milk. Mix until well blended. Stir in pecans and raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop cookie dough by teaspoonfuls onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set and golden, about 15 minutes. Transfer cookies to wire racks to cool.\n"
            }
        ],
