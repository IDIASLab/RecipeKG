    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9842/lemon-bars-i/",
        "name": "Lemon Bars I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3279512.jpg",
            "width": 1437,
            "height": 960
        },
        "datePublished": "1997-08-03T18:36:47.000Z",
        "description": "Lemon bars on a shortbread base.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "16 squares",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "\u00bc cup confectioners' sugar",
            "\u00bc cup butter",
            "1 cup white sugar",
            "2 tablespoons all-purpose flour",
            "\u00bd teaspoon baking powder",
            "2 eggs",
            "3 tablespoons lemon juice",
            "1 tablespoon lemon zest",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make Bottom Layer:  Mix one cup flour and 1/4 cup confectioners sugar.  Melt the butter and stir into flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press flat and even into an 8x8 inch baking dish.  Bake for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While baking, make the top layer:  Mix 1 cup sugar, 2 tablespoons flour, and the baking powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs and add to mixture, stirring well.   Add lemon juice and rind, mix again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour over bottom layer; Bake at 350 for 25 minutes.  Cool a little, cut into squares while warm; dust with confectioners sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
