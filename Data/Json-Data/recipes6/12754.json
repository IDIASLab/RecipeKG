    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11026/macadamia-nut-chocolate-chip-cookies/",
        "name": "Macadamia Nut Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F172027.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-07-20T13:00:10.000Z",
        "description": "Drop cookies with macadamia nuts and chocolate chips!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "\u2153 cup packed dark brown sugar",
            "\u2153 cup white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u215b cups sifted all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup macadamia nuts, chopped",
            "1\u2009\u00bc cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease 2 large cookie sheets with vegetable shortening.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter and sugars together in a large bowl. Beat in the egg and vanilla extract until well blended. Sift together the flour, baking soda, and salt; gradually blend into the batter.  Stir in the chopped macadamia nuts and chocolate chips. Drop by rounded teaspoonfuls onto the cookie sheets, about 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 10 to 12 minutes, or until the cookies are golden brown. Remove from the oven, and transfer the cookies to cooling racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Nut Cookie Recipes",
            "Macadamia Nut"
        ],
