    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24274/creamy-cranberry-salad/",
        "name": "Creamy Cranberry Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7327877.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:05:26.000Z",
        "description": "Not your average cranberry salad. Many fruit flavors are included, which give a zing to it.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT12H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (12 ounce) package fresh cranberries",
            "1 cup white sugar",
            "1 (6 ounce) package red Jell-O\u00ae mix",
            "1 (8 ounce) can crushed pineapple, drained",
            "\u00be cup orange juice",
            "1 apple with peel, grated",
            "1 cup chopped pecans",
            "1 cup whipping cream",
            "1 (8 ounce) package cream cheese, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Process cranberries in blender or food processor; pour sugar over cranberries and stir to dissolve.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring 3/4 cup of water to a boil; stir in gelatin and let cool.  Stir cooled gelatin into cranberries, and add pineapple, orange juice, apple and pecans.  Mix well, cover and refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whip cream and beat in cream cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a glass serving dish, layer half of the cranberry mixture, then the whipped cream mixture, then the remaining cranberry mixture.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Fruit Salad Recipes",
            "Cranberry Salad Recipes"
        ],
