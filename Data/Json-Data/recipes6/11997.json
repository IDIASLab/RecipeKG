    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9869/peanut-butter-cornflake-crunch-bars/",
        "name": "Peanut Butter Cornflake Crunch Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F198562.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-08-07T00:55:31.000Z",
        "description": "A very simple to make bar cookie that kids love.  Sort of like a peanut butter rice krispie treat, except made with cornflakes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup light corn syrup",
            "1 cup white sugar",
            "1 cup smooth peanut butter",
            "10 cups cornflakes cereal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook syrup and sugar until mixture comes to a boil.  Stir constantly; do not overcook.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, add peanut butter, and stir until smooth.  Pour mixture over cornflakes in a large bowl.  Mix until cereal is thoroughly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread in a buttered 9 x 13 inch pan and press down lightly with buttered hands.  When cool, cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
