        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10583/sugar-cookies-v/",
        "name": "Sugar Cookies V",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F542330.jpg",
            "width": 2745,
            "height": 2687
        },
        "datePublished": "1998-02-22T16:20:11.000Z",
        "description": "Quick, easy recipe...make extras and freeze some.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 - 9 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup confectioners' sugar",
            "1 cup butter",
            "1 cup shortening",
            "2 eggs",
            "4 cups all-purpose flour",
            "1 teaspoon cream of tartar",
            "1 teaspoon baking soda",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together sugars, shortening and butter.  Mix in the eggs and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour, cream of tartar and baking soda.  Stir in vanilla and salt and stir until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into walnut sized balls.  Flatten with glass bottom dipped in sugar.  Bake for 10 minutes.\n"
            }
        ],
