    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272158/keto-lemon-bars/",
        "name": "Keto Lemon Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6505110.jpg",
            "width": 1240,
            "height": 1550
        },
        "datePublished": "2020-04-08T21:02:12.000Z",
        "description": "These low-carb, keto-friendly lemon bars are made with almond flour and erythritol sweetener (such as Swerve&reg;). Top with a dusting of confectioners' sugar if you like.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "18 bars",
        "recipeIngredient": [
            "2\u2009\u00bd cups low-calorie natural sweetener (such as Swerve\u00ae)",
            "8 tablespoons butter, melted",
            "\u00bd teaspoon almond extract",
            "1\u2009\u00bd cups low-calorie natural sweetener (such as Swerve \u00ae)",
            "\u00be cup almond flour",
            "4 eggs",
            "3 lemons, juiced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend 2 1/2 cups sweetener, melted butter, and almond extract together in a bowl. Press crust mixture into an ungreased 9x13-inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake crust in the preheated oven for 15 to 20 minutes. Remove from oven and leave oven on. Cool crust for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 1 1/2 cups sweetener and almond flour together in a bowl. Whisk in eggs and lemon juice. Pour over the cooled crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake bars in the hot oven for 20 minutes. Cut into 18 bars.\n"
            }
        ],
