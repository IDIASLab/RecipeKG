    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25838/jens-almond-cardamom-cookies/",
        "name": "Jen's Almond Cardamom Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F858191.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-06-19T03:06:33.000Z",
        "description": "This moist cookie has delicious a almond taste with a kick of cardamom - dreamy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u215b teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground cardamom",
            "\u2154 cup unsalted butter, softened",
            "1 cup white sugar",
            "1 (3 ounce) package cream cheese, softened",
            "3 tablespoons almond paste",
            "1 egg",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon lemon zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease cookie sheets. Sift together the flour, baking powder, baking soda, salt and cardamom. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the butter, sugar, cream cheese and almond paste until smooth. Beat in the egg, vanilla and lemon zest. Gradually blend in the dry ingredients until well mixed. Drop by spoonfuls onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10 minutes in the preheated oven. Allow cookies to cool on baking sheet for 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Almond Dessert Recipes"
        ],
