    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239311/barbaras-best-meatloaf/",
        "name": "Barbara's Best Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1615682.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "2020-03-05T05:12:24.000Z",
        "description": "This is one of those dishes that anytime I make it, I get a request for the recipe. It's an old family favorite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 meatloaf",
        "recipeIngredient": [
            "1 cup milk",
            "\u2154 cup dry bread crumbs",
            "1\u2009\u00bd pounds ground beef",
            "1 egg, beaten",
            "\u00bc cup diced onion",
            "1 teaspoon salt",
            "ground black pepper to taste",
            "\u00bc cup ketchup",
            "3 tablespoons brown sugar",
            "1 teaspoon ground mustard",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour milk over breadcrumbs in a large bowl; soak until crumbs are softened, at least 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ground beef, egg, onion, salt, and black pepper with the bread crumbs and milk. Shape the beef mixture into a loaf and transfer to a baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir ketchup, brown sugar, mustard, and nutmeg together in a bowl until smooth; spread over the loaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until no longer pink in the center, about 1 hour. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Beef Meatloaf Recipes"
        ],
