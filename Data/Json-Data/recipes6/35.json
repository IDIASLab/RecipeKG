    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218899/zucchini-gingerbread/",
        "name": "Zucchini Gingerbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F696247.jpg",
            "width": 2856,
            "height": 2142
        },
        "datePublished": "2020-03-05T05:08:17.000Z",
        "description": "A moist spicy zucchini bread perfect for the transition from summer to fall.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "cooking spray",
            "1 cup brown sugar",
            "\u00bc cup molasses",
            "\u00bc cup vegetable oil",
            "\u00be cup vanilla yogurt",
            "3 eggs",
            "2 teaspoons ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "1 teaspoon baking soda",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon salt",
            "2\u2009\u00bd cups shredded zucchini",
            "3 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 325 degrees F (165 degrees C). Spray 2 9x5-inch loaf pans with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together the brown sugar, molasses, vegetable oil, vanilla yogurt, and eggs until thoroughly combined. Stir in the cinnamon, ginger, nutmeg, cloves, baking soda, baking powder, and salt. Gently mix in the shredded zucchini, and stir in the flour. Pour the batter into the prepared loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the loaves are browned and springy to the touch, 50 to 60 minutes. A toothpick inserted into the center of a loaf should come out clean. Allow to cool in pans for about 10 minutes before removing to finish cooling on racks.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Zucchini Bread Recipes"
        ],
