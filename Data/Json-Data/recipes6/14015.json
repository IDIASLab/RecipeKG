        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15086/chocolate-cereal-puffs/",
        "name": "Chocolate Cereal Puffs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6333137.jpg",
            "width": 3084,
            "height": 3084
        },
        "datePublished": "1999-12-19T21:11:30.000Z",
        "description": "Anyone who loves peanut butter and easy cookie recipes will love this!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup light corn syrup",
            "1 teaspoon vanilla extract",
            "1 cup peanut butter",
            "4 cups chocolate puff cereal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the sugar, corn syrup and vanilla in a medium saucepan. Bring to a boil over medium high heat, stirring occasionally, boil for 1 to2 minutes. Remove from heat and stir in the peanut butter.  Add cereal, mix until cereal is evenly coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop cookies by spoonfuls onto greased foil or waxed paper. Let sit at room temperature until cool and set. This recipe can also be made in the microwave, if watched carefully.\n"
            }
        ],
