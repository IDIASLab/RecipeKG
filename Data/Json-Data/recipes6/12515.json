        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10686/scottish-lace-cookies/",
        "name": "Scottish Lace Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Scottish Lace Cookies"
        },
        "datePublished": "1998-03-13T00:22:44.000Z",
        "description": "An old-fashioned cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 3 dozen",
        "recipeIngredient": [
            "3 tablespoons shortening",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "1\u2009\u00bd teaspoons almond extract",
            "3 cups rolled oats",
            "1\u2009\u00bd teaspoons salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together shortening and sugar.  Beat in the eggs, then the almond extract.  Gradually blend in oats and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by spoonfuls 2 inches apart on cookie sheets.  Flatten cookies with the bottom of a glass dipped in flour. Bake 12-15 minutes until golden colored.  Transfer to wire racks for cooling.\n"
            }
        ],
