        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23656/german-sweet-chocolate-pie/",
        "name": "German Sweet Chocolate Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5991279.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "2020-06-19T03:05:02.000Z",
        "description": "This Pie is EASY to make!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT5H0M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (4 ounce) bar German sweet chocolate, chopped",
            "\u2153 cup milk",
            "4 ounces cream cheese, softened",
            "2 tablespoons white sugar",
            "1 (12 ounce) container frozen whipped topping, thawed",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Microwave chocolate and 2 tablespoons of the milk in large microwave-safe bowl on HIGH 1 1/2 to 2 minutes or until chocolate is almost melted, stirring halfway through heating time. Stir until chocolate is melted and mixture is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in cream cheese, sugar, and remaining milk with wire whisk until blended.  Refrigerate 10 minutes.  Gently fold in 3 1/2 cups of the whipped topping until no streaks remain.  Spoon into crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze pie 4 hours. Before cutting pie, let stand at room temperature 15 minutes. Serve with remaining whipped topping.  Store pie in freezer.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies"
        ],
