    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268608/vegan-pumpkin-bisque/",
        "name": "Vegan Pumpkin Bisque",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5868030.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2018-10-11T02:56:42.000Z",
        "description": "Fast, easy, and delicious. All the flavor of the heavy cream classic but dairy free. A splash of balsamic vinegar and a few toasted almonds crank this soup into high gourmet gear.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "\u00bd cup chopped onion",
            "1 (15 ounce) can pumpkin puree",
            "2 cups vegetable broth",
            "1 cup Original Unsweetened Almond Breeze Almondmilk",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground pepper",
            "\u00bc teaspoon ground cinnamon",
            "4 teaspoons sliced, toasted almonds",
            "4 teaspoons balsamic vinegar",
            "4 teaspoons chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a saucepan over medium-high heat. Add the onion and cook, stirring, until softened, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the pumpkin and vegetable broth. Remove from heat. Transfer to a blender and process 1 minute or until smooth. Return mixture to pan. Return pan to heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in almondmilk, salt, pepper, and cinnamon. Cook, stirring occasionally for 3 minutes, or until soup is heated through. Divide among 4 bowls and top each with 1 teaspoon almonds, 1 teaspoon vinegar, and 1 teaspoon parsley.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Almond Breeze"
        ],
