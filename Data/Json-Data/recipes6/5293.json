    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/268510/easy-minestrone-soup/",
        "name": "Easy Minestrone Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5920696.jpg",
            "width": 2427,
            "height": 2297
        },
        "datePublished": "2020-03-10T02:53:38.000Z",
        "description": "Use leftover marinara for a head start on this yummy, veggie-packed soup. It's perfect with a sandwich on a chilly day, or with a crusty garlic bread and simple salad on a busy night.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon extra-virgin olive oil",
            "1 cup diced celery",
            "1 cup sliced carrots",
            "\u2154 cup diced onion",
            "3 cloves garlic, minced",
            "4 cups vegetable broth",
            "1\u2009\u00be cups jarred marinara sauce",
            "\u00bc cup tomato paste",
            "1 teaspoon Italian seasoning",
            "2 cups small shell pasta",
            "2 cups quartered and sliced zucchini",
            "1 (14.5 ounce) can great northern beans, rinsed and drained",
            "1 cup stemmed and thinly sliced kale",
            "salt and ground black pepper to taste",
            "2 tablespoons grated Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large pot or Dutch oven over medium-high heat until shimmering. Add celery, carrots, and onion. Cook, stirring frequently, until onion begins to turn translucent, about 4 minutes. Add garlic and stir until fragrant, about 30 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add vegetable broth, marinara sauce, tomato paste, and Italian seasoning. Stir to combine. Bring to a boil. Reduce heat to medium-low and simmer, uncovered, until carrots are soft, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add pasta and zucchini. Cook until pasta is tender, about 10 minutes. Stir in beans, kale, salt, and pepper. Remove from heat and allow soup to cool slightly. Spoon into individual bowls and garnish with Parmesan cheese.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Squash Soup Recipes",
            "Zucchini Soup Recipes"
