    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72046/raspberry-peach-crumble/",
        "name": "Raspberry Peach Crumble",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8286134.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:10:49.000Z",
        "description": "This is a showstopper, perfect for summer. Serve with vanilla ice cream to make it a perfect dessert. Use frozen fruit, if the fresh is not in season.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pint fresh raspberries",
            "3 fresh peaches, pitted and chopped",
            "2 tablespoons lemon juice",
            "\u2153 cup white sugar",
            "1 pinch cinnamon",
            "1 cup rolled oats",
            "\u00bd cup unsalted butter",
            "\u00bc cup brown sugar",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract",
            "1 teaspoon salt",
            "1 pinch cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease 6 small ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a bowl, mix the raspberries, peaches, lemon juice, 1/3 cup white sugar, and 1 pinch cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, mix the oats, butter, brown sugar, 1/4 cup white sugar, vanilla, salt, and 1 pinch cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill the prepared ramekins with equal amounts of the raspberry and peach mixture, and top with equal amounts of the oats mixture. Arrange the ramekins on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 35 minutes in the preheated oven, until crisp and golden brown. Cool 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Raspberry Dessert Recipes"
        ],
