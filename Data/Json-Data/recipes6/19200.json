        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/54083/brazilian-lemonade/",
        "name": "Brazilian Lemonade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F14940.jpg",
