    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18086/melt-in-your-mouth-pumpkin-bread/",
        "name": "Melt In Your Mouth Pumpkin Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7174540.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-05-21T22:44:55.000Z",
        "description": "Think of this as pumpkin pie in sweet bread form. You'll love it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "2 - 9x5 inch loaves",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "2 cups white sugar",
            "2 (3.5 ounce) packages instant coconut cream pudding mix",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 teaspoon ground cinnamon",
            "1\u2009\u00bc cups vegetable oil",
            "5 eggs",
            "2 cups canned pumpkin puree"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Lightly grease two 9x5 inch loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine flour, sugar, pudding mix, baking soda, salt and cinnamon. Stir in the oil, eggs and pumpkin until just blended.. Stir the flour mixture into the egg mixture. Pour batter into prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 hour, or until a toothpick inserted into center of the loaf comes out clean. Remove from oven and let stand 15 minutes, then remove from pans and cool on wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
