    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278994/banana-cheesecake-pie/",
        "name": "Banana Cheesecake Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Banana Cheesecake Pie"
        },
        "datePublished": "2020-03-13T04:00:58.000Z",
        "description": "Banana cheesecake pie, a new twist on the old Southern favorite.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT14H20M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups vanilla wafer crumbs",
            "\u2153 cup unsalted butter, softened",
            "1 (8 ounce) package cream cheese, softened",
            "3 cups cold milk",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (5 ounce) package instant vanilla pudding mix",
            "1 (8 ounce) container frozen French vanilla-flavored whipped topping, thawed",
            "2 bananas, sliced",
            "\u00bc cup vanilla wafer crumbs",
            "12 vanilla wafer cookies, or more to taste",
            "\u00bc cup vanilla wafer crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix wafer crumbs and butter for crust in a bowl until thoroughly combined. Press into a 9-inch pie plate and chill 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese for filling in a large bowl with an electric mixer until fluffy. Beat in cold milk, condensed milk, pudding mix, and 1/2 of the whipped topping until smooth. Fold in remaining whipped topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chilled crust from the refrigerator and layer the bottom with 1/2 of the banana slices. Add 1/2 of the filling and spread with a spoon or cake spatula until smooth and bananas are covered. Sprinkle with 1/4 cup wafer crumbs. Place remaining banana slices over top and spread remaining filling over top to cover.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Border the cheesecake all around the edges with wafer cookies and sprinkle 1/4 cup wafer crumbs in the center. Chill for 6 hours before serving.\n"
            }
        ],
