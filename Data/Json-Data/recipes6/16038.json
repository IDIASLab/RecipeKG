    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22762/pineapple-cobbler/",
        "name": "Pineapple Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F964369.jpg",
            "width": 1134,
            "height": 1134
        },
        "datePublished": "2000-08-28T13:48:09.000Z",
        "description": "A cobbler like a pineapple upside down cake.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 cup self-rising flour",
            "1 cup white sugar",
            "1 cup milk",
            "1 (15 ounce) can pineapple chunks, drained",
            "\u00bd cup brown sugar",
            "1 (4 ounce) jar maraschino cherries, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). In a 9x13 inch baking dish, melt the butter in the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together flour, white sugar, and milk until smooth. Pour batter into the prepared dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, toss pineapple with brown sugar until fruit is well coated. Drop pineapple mixture by spoonfuls into the batter. Sprinkle with cherries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 30 minutes in the preheated oven, until golden.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Pineapple Dessert Recipes"
        ],
