    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9610/lots-o-cookies/",
        "name": "Lots o' Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6742419.jpg",
            "width": 1458,
            "height": 2592
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Makes about 7 dozen cookies",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "7 dozen",
        "recipeIngredient": [
            "2 cups packed brown sugar",
            "2 cups white sugar",
            "\u00be cup butter, softened",
            "1 cup shortening",
            "4 eggs",
            "2 teaspoons vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups whole wheat flour",
            "2 teaspoons salt",
            "2 teaspoons baking soda",
            "3 cups rolled oats",
            "1\u2009\u00bd cups semisweet chocolate chips",
            "1\u2009\u00bd cups chopped walnuts",
            "2 cups flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream sugars, butter and shortening.  Beat in eggs and vanilla until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in sifted flours, salt and baking soda and stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop in teaspoon size balls on greased baking sheet about 2 inches apart.   Bake 8 minutes or until edges are brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
