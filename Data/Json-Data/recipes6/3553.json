        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270631/chuck-roast-chili/",
        "name": "Chuck Roast Chili",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chuck Roast Chili"
        },
        "datePublished": "2020-03-05T05:19:15.000Z",
        "description": "This is a hearty and delicious chili recipe that is just perfect for football games and cookouts.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H15M",
        "totalTime": "P0DT2H35M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (2 pound) chuck roast, or more to taste",
            "2 tablespoons olive oil, divided",
            "1 teaspoon apple cider vinegar",
            "1 large onion, diced",
            "2 green bell peppers, diced",
            "2 cups chicken stock",
            "1 cup water",
            "2 (14.5 ounce) cans diced tomatoes",
            "1 (15 ounce) can black beans",
            "1 (15 ounce) can light red kidney beans",
            "1 (15 ounce) can dark red kidney beans",
            "3 tablespoons brown sugar",
            "2 tablespoons tomato paste",
            "2 tablespoons chili powder",
            "1 tablespoon barbecue sauce",
            "1 tablespoon ketchup",
            "2 teaspoons minced garlic",
            "1 teaspoon ground cumin",
            "\u00bd tablespoon hot sauce (such as Valentina\u00ae)",
            "2 teaspoons mustard",
            "2 teaspoons Worcestershire sauce",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Remove fat and ligaments from chuck roast and cut into 1/2-inch cubes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 1 tablespoon oil in a large pot over medium heat. Add cubed roast. Cook and stir until browned, about 5 minutes. Drain and discard grease. Transfer meat to a bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pot to medium heat. Add remaining oil and vinegar, scraping up any bits at the bottom of the pot. Add onion and bell peppers and cook until onion is translucent, 5 to 10 minutes. Add stock and water. Add diced tomatoes, light kidney beans, dark kidney beans, brown sugar, tomato paste, chili powder, barbecue sauce, ketchup, garlic, cumin, hot sauce, mustard, Worcestershire sauce, and salt. Bring chili to a simmer, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover pot and simmer 2 to 4 hours.\n"
            }
        ],
