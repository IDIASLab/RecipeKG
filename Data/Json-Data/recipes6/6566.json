    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279248/grandmas-homemade-beef-stew/",
        "name": "Grandma's Homemade Beef Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grandma's Homemade Beef Stew"
        },
        "datePublished": "2020-03-23T19:06:59.000Z",
        "description": "Just like grandma used to make. Very little work goes into this homemade beef stew but it was always made with love. Serve with saltine crackers or yeast rolls.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H5M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds cubed <a href=\"https://www.allrecipes.com/recipe/14685/slow-cooker-beef-stew-i/\" aria-label=\"beef stew\">beef stew</a> meat, trimmed",
            "6 cups water",
            "1 large yellow onion, cut into large chunks",
            "salt and ground black pepper to taste",
            "6 large carrots, cut into 1/2-inch pieces",
            "5 red potatoes, cut into small chunks",
            "5 stalks celery, cut into 1/2-inch pieces",
            "1 (15 ounce) can green beans, drained",
            "1 (15 ounce) can corn, drained",
            "1 (15 ounce) can sweet peas, drained",
            "1 (14.5 ounce) can crushed tomatoes",
            "1 (6.5 ounce) can tomato sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place stew meat, water, onion, salt, and pepper in a large stockpot. Cook over medium heat until meat is fork-tender, about 45 minutes. Do not drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add carrots and potatoes to the stew, stir, and cook over medium heat for 20 minutes. Stir in celery, green beans, corn, peas, crushed tomatoes, and tomato sauce. Reduce heat to medium-low and let cook for about 1 hour.\n"
            }
        ],
