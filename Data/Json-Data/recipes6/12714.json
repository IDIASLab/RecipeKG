    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10961/biscotti-cookies/",
        "name": "Biscotti Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F766847.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-06-15T14:45:33.000Z",
        "description": "A different way of making biscotti -- in the form of rolled cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd cup white sugar",
            "\u00bd teaspoon baking powder",
            "6 tablespoons shortening",
            "4 eggs",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees F).  Grease 2 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine flour, sugar, and baking powder.  Cut in the vegetable shortening  until mixture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat the eggs until thick and light-colored.  Gradually beat the eggs into the flour mixture.  Fold in the walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a floured surface, roll out the dough 1/4 inch thick.  Using a cookie cutter, cut into desired shapes and place 1 1/2 inches apart on baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 12 to 14 minutes, until lightly colored.  Transfer to wire racks to cool.\n"
            }
        ],
