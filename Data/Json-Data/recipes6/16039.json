    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22764/buttermilk-biscuits-iii/",
        "name": "Buttermilk Biscuits III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F933926.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-18T01:03:58.000Z",
        "description": "This is a taste alike recipe for buttermilk biscuits from a Southern fast food chain.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "16 biscuits",
        "recipeIngredient": [
            "4 cups self-rising flour",
            "1 teaspoon white sugar",
            "1 teaspoon baking powder",
            "\u2154 cup shortening",
            "2 cups buttermilk",
            "2 tablespoons buttermilk, for brushing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, stir together flour, sugar and baking powder. Cut in shortening until mixture resembles coarse crumbs. Stir in 2 cups buttermilk, mixing just enough for dough to come together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out until 1 inch thick. Cut into rounds with a biscuit cutter. Place biscuits on prepared baking pan. Brush biscuits with 2 tablespoons buttermilk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 minutes, until golden.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Biscuits"
        ],
