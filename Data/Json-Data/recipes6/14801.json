        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17451/vidas-famous-buttermilk-pie/",
        "name": "Vida's Famous Buttermilk Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F932876.jpg",
            "width": 250,
            "height": 250
