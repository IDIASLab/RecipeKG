    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274205/cauliflower-chowder/",
        "name": "Cauliflower Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6792678.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2020-03-10T02:53:53.000Z",
        "description": "A real creamy cauliflower chowder that warms you to your bones. When your children are not fond of eating vegetables, this is a good way to have them enjoy their veggies!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon margarine",
            "2 stalks celery, chopped",
            "1 small onion, chopped",
            "28 ounces vegetable broth",
            "1\u2009\u00bd cups water",
            "1 head cauliflower, finely chopped",
            "1 large potato, peeled and chopped",
            "1 (16 ounce) jar roasted red peppers, drained and chopped",
            "1 cup evaporated milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt margarine over medium heat in a Dutch oven. Add celery and onion and cook, stirring occasionally, for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable broth and water into the Dutch oven and bring to a boil. Add cauliflower and potato; return to a boil. Reduce heat to low and let simmer until potato is tender, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir red peppers and evaporated milk into the simmering mixture and heat for 2 minutes more. Pour soup carefully into an electric blender or food processor and puree until smooth. Return soup to the Dutch oven and bring to a slow boil, stirring constantly, until thoroughly heated, about 5 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chowders"
        ],
