        "cookTime": null,
        "totalTime": "P0DT0H10M",
        "recipeYield": "100 servings",
        "recipeIngredient": [
            "2 cups orange-flavored drink mix (e.g. Tang)",
            "2 cups white sugar",
            "\u00bc cup instant tea powder",
            "\u00be cup lemon-flavored instant tea powder",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine orange drink mix, sugar, tea powder, cinnamon and cloves. Mix well and store in an airtight container.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, put 3 teaspoons of mix in a mug. Stir in 1 cup boiling water. Adjust to taste.\n"
            }
        ],
