    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280028/cauliflower-potato-salad/",
        "name": "Cauliflower \"Potato\" Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cauliflower \"Potato\" Salad"
        },
        "datePublished": "2020-05-06T08:13:34.000Z",
        "description": "Loaded with bacon, pickles, and dill, this cauliflower salad is better than potato salad.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large head cauliflower, cut into bite-sized pieces",
            "6 slices bacon",
            "2 hard-boiled eggs, diced",
            "\u00bd cup shredded Cheddar cheese",
            "\u2153 cup mayonnaise",
            "\u00bc cup chopped red onion",
            "1 tablespoon chopped fresh dill",
            "1 tablespoon prepared yellow mustard",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan and fill with water to just below the bottom of the steamer. Bring water to a boil. Add cauliflower, cover, and steam until tender-crisp, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cauliflower into a large mixing bowl and place in the refrigerator to chill for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain on paper towels and crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add bacon, eggs, Cheddar cheese, mayonnaise, onion, dill, mustard, salt, and pepper to the bowl of cauliflower and stir well to combine. Serve immediately.\n"
            }
        ],
