    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267849/roasted-sweet-potato-casserole-with-pecan-crumble/",
        "name": "Roasted Sweet Potato Casserole with Pecan Crumble",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4587843.jpg",
            "width": 4305,
            "height": 2421
        },
        "datePublished": "2020-03-05T05:18:27.000Z",
        "description": "This roasted sweet potato casserole gets the special treatment with a spiced pecan crumble topping.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H55M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "3 medium sweet potatoes, peeled and cubed",
            "2 tablespoons extra-virgin olive oil",
            "\u2153 cup packed dark brown sugar",
            "\u2153 cup all-purpose flour",
            "10 tablespoons unsalted butter, softened, divided",
            "\u00bd cup chopped pecans",
            "\u2153 cup packed brown sugar",
            "2 large eggs, beaten",
            "\u00bd cup evaporated milk",
            "1 teaspoon vanilla extract",
            "1 teaspoon cinnamon",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground cloves",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sweet potatoes and oil in a bowl and toss to coat. Lay on a shallow baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast until slightly charred, about 45 minutes, turning regularly to prevent burning on the bottom.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While potatoes are roasting, combine 1/3 cup brown sugar and flour in a medium-sized bowl. Add 3 tablespoons butter and cut into the dry ingredients. Stir in pecans until evenly mixed. Set crumble mixture aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/3 cup brown sugar, remaining 7 tablespoons butter, eggs, evaporated milk, vanilla extract, cinnamon, salt, cloves, and nutmeg in a large mixing bowl. Set aside until potatoes are done roasting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove potatoes from oven and reduce heat to 325 degrees F (165 degrees C). Transfer sweet potatoes to the brown sugar mixture in the large mixing bowl. Toss until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon sweet potato mixture into a 1 1/2-quart casserole dish. Top with crumble mixture, distributing evenly to form a crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the hot oven until lightly browned on top, 30 to 40 minutes. Let rest for 10 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
