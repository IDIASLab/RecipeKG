    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12081/real-pecan-pie/",
        "name": "Real Pecan Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1071425.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1998-03-07T17:38:35.000Z",
        "description": "This pie is filled with pecans.  It's guaranteed \"stick to your teeth\" goodness, so don't serve it to the faint of heart, i.e.,  those finicky about manners! Dark or light corn syrup may be used.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 9 inch pie",
        "recipeIngredient": [
            "2 eggs",
            "\u00be cup corn syrup   ",
            "\u00be cup white sugar",
            "1\u2009\u00bd cups pecan halves",
            "1 teaspoon vanilla extract",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, slightly beat the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in corn syrup, sugar, and vanilla. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly stir in pecans, till well-coated with egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pie shell.  Bake at 350 degrees F (175 degrees C) for 20 minutes.  Turn the oven down to 300 degrees F (150 degrees C), and bake for another 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool before serving.   Store leftovers covered in refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pecan Pie Recipes"
        ],
