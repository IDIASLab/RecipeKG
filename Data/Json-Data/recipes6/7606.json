    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257525/allergy-friendly-green-banana-muffins/",
        "name": "Allergy-Friendly Green Banana Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Allergy-Friendly Green Banana Muffins"
        },
        "datePublished": "2020-04-11T03:50:13.000Z",
        "description": "These muffins go so fast in my house! The spinach is indiscernible and the sweetness is just right. We have these for breakfast, snack, or dessert. Store in an airtight container in the refrigerator for 3 to 4 days.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1 cup rice flour",
            "\u00bd cup coconut flour",
            "\u00bd cup cornmeal",
            "1 teaspoon xanthan gum",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon fine sea salt",
            "2 tablespoons warm water",
            "1\u2009\u00bd teaspoons dry egg replacer (such as Ener-G\u00ae)",
            "3 tablespoons applesauce, or as needed",
            "\u00be cup oat milk",
            "\u00be cup maple syrup",
            "1 (5 ounce) package baby spinach",
            "\u00bc cup canola oil",
            "2 teaspoons pure vanilla extract",
            "2 medium ripe bananas, mashed",
            "\u00bd cup vegan chocolate chips (such as Enjoy Life\u00ae)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a muffin tin with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk rice flour, coconut flour, cornmeal, xanthan gum, baking powder, baking soda, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk water and egg replacer together in a glass measuring cup. Add enough applesauce to make 1/4 cup. Transfer mixture to a blender; add oat milk, maple syrup, baby spinach, oil, and vanilla extract. Blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour spinach mixture over the flour mixture; mix until just combined. Fold mashed bananas and chocolate chips into the batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop batter into the prepared tin, filling each cup 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 25 minutes. Transfer to a wire rack to cool completely.\n"
            }
        ],
