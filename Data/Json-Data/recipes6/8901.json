        "mainEntityOfPage": "https://www.allrecipes.com/recipe/275679/air-fryer-mushrooms/",
        "name": "Air Fryer Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8056635.jpg",
            "width": 0,
            "height": 0
