        "name": "Tea Party Sandwiches (Puerto Rican Version)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4473752.jpg",
            "width": 3648,
            "height": 2736
