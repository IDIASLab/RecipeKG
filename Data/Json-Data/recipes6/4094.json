    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/277243/vegan-bbq-beans/",
        "name": "Vegan BBQ Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9150223.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-05T05:21:01.000Z",
        "description": "BBQ beans - a childhood favorite that's easy to make vegan and sneaks in dill pickles as the secret ingredient. Serve with bread.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon vegan margarine (such as Earth Balance\u00ae)",
            "2 cups canned pinto beans, rinsed and drained",
            "1\u2009\u00bd tablespoons chopped onion",
            "\u00bc cup sweet pickle relish",
            "2 tablespoons dill pickle juice",
            "2 tablespoons ketchup",
            "1 tablespoon molasses",
            "1 tablespoon tomato paste",
            "1 tablespoon brown sugar",
            "\u00bc teaspoon hot sauce (such as Tabasco\u00ae)",
            "1 dash liquid smoke flavoring",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon salt-free seasoning blend",
            "\u00bc teaspoon dry mustard"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt vegan margarine in a skillet over medium heat. Add onion. Cook and stir until translucent, about 5 minutes. Mix together with beans in a casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir relish, pickle juice, ketchup, molasses, tomato paste, brown sugar, hot sauce, liquid smoke, garlic powder, salt seasoning, and mustard together in a bowl. Add to the bean mixture and mix to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until sauce is thick and casserole is heated through, about 1 hour.\n"
            }
        ],
