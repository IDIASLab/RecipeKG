        "mainEntityOfPage": "https://www.allrecipes.com/recipe/32232/coffee-flavored-liqueur-ii/",
        "name": "Coffee Flavored Liqueur II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060886.jpg",
