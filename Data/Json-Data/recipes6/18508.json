    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/39988/peach-salsa-ii/",
        "name": "Peach Salsa II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4029488.jpg",
            "width": 1359,
            "height": 1359
        },
        "datePublished": "2020-06-19T03:08:00.000Z",
        "description": "Make the most of peach season with a few jars of this sweet peach salsa.",
        "prepTime": "P0DT1H0M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "7 cups",
        "recipeIngredient": [
            "4 cups fresh peaches - peeled, pitted and chopped",
            "\u00bd cup chopped onion",
            "\u00bd cup chopped red bell pepper",
            "4 jalapeno peppers, minced",
            "\u00bc cup chopped fresh cilantro",
            "3 cloves garlic, minced",
            "1\u2009\u00bd teaspoons ground cumin",
            "\u00bc cup distilled white vinegar",
            "1 teaspoon grated lime zest",
            "\u00bc cup white sugar",
            "1 (49 gram) package light fruit pectin crystals",
            "3\u2009\u00bd cups white sugar",
            "canning jars"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sterilize jars and lids.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, combine peaches, onion, pepper, cilantro, garlic, cumin, vinegar, and lime zest. Stir together pectin and 1/4 cup sugar. Bring to a boil, and stir in remaining 3 1/2 cups sugar. Boil for 1 minute, stirring constantly. Remove from heat, and stir for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peach salsa into prepared jars, leaving 1/4 inch air space. Seal, and process in a boiling water bath for 15 minutes.\n"
            }
        ],
