    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282600/strawberry-white-chocolate-chip-cookies/",
        "name": "Strawberry-White Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8174951.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-05-27T09:20:40.000Z",
        "description": "Chewy chocolate chip cookies with the amazing addition of crushed, freeze-dried strawberries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "32 cookies",
        "recipeIngredient": [
            "2 ounces freeze-dried strawberries",
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "\u00bd teaspoon baking powder",
            "1 cup unsalted butter, softened",
            "1 cup brown sugar",
            "\u00bd cup white sugar",
            "2 large eggs",
            "2 teaspoons vanilla extract",
            "\u00be teaspoon strawberry extract",
            "2 cups white chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a rack in the center of the oven and preheat to 350 degrees F (175 degrees C). Line two baking sheets with parchment paper. Crush freeze-dried strawberries to a fine consistency.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, salt, and baking powder together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, brown sugar, and white sugar together in a larger mixing bowl until fluffy. Add eggs, crushed strawberries, vanilla extract, and strawberry extract. Mix in dry ingredients in three additions, adding white chocolate chips with the last addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop cookies with a 1 1/2 tablespoon-sized scoop and place onto the prepared baking sheets, 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until lightly golden, about 14 minutes. Remove from the oven and let cool on the cookie sheets for 3 to 5 minutes before removing to wire racks to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Chip Cookie Recipes",
            "White Chocolate"
        ],
