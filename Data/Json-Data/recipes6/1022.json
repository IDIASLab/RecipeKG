    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233305/ice-cream-pie/",
        "name": "Ice Cream Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Ice Cream Pie"
        },
        "datePublished": "2020-03-05T05:10:45.000Z",
        "description": "My great-grandma's ice cream pie includes a Rice Krispies&reg; crust and peanut buttery chocolate topping. This recipe can be changed to accommodate different tastes by changing the flavor of ice cream. My cousin has tried several different ones and loves them all.",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H55M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "3 tablespoons butter",
            "3 tablespoons corn syrup",
            "2 tablespoons brown sugar",
            "2\u2009\u00bd cups crispy rice cereal (such as Rice Krispies\u00ae)",
            "\u00bc cup peanut butter, slightly melted",
            "1 (12 ounce) jar chocolate fudge sauce, divided",
            "1 teaspoon corn syrup, or to taste",
            "2 quarts vanilla ice cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix butter, 3 tablespoons corn syrup, and brown sugar together in a saucepan; bring to a boil. Remove from heat and stir in rice cereal. Transfer and press mixture into a 9-inch pie pan. Freeze until firm, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir peanut butter, 1/4 cup fudge sauce, and 1 teaspoon corn syrup together in a bowl. Spread half the peanut butter mixture onto the crust. Return to freezer until set, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the remaining half of peanut butter mixture into the fudge sauce jar; stir well. Refrigerate peanut butter fudge sauce until firm, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon and pack ice cream into crust; freeze until firm, 10 to 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peanut butter chocolate sauce into a microwave-safe bowl; heat in microwave until warm, 30 seconds to 1 minute. Serve alongside pie.\n"
            }
        ],
