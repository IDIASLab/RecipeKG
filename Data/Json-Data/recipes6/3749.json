    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274242/no-churn-vegan-pistachio-almond-ice-cream/",
        "name": "No-Churn Vegan Pistachio-Almond Ice Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "No-Churn Vegan Pistachio-Almond Ice Cream"
        },
        "datePublished": "2020-03-05T05:19:57.000Z",
        "description": "This no-churn vegan pistachio-almond ice cream is luscious, creamy, and delicious, and is made in an ice cream maker.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT5H35M",
        "recipeYield": "6 cups",
        "recipeIngredient": [
            "2 cups almond milk",
            "1 (14 ounce) can coconut milk",
            "1 cup brown sugar",
            "1 cup light corn syrup",
            "1 tablespoon coconut oil",
            "1 cup almonds",
            "5 tablespoons pistachio paste",
            "1 tablespoon vanilla extract",
            "1 teaspoon vanilla extract",
            "1 tablespoon almond extract",
            "2 teaspoons almond extract",
            "\u00bd teaspoon salt",
            "1 drop green food coloring "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine almond milk, coconut milk, sugar, corn syrup, and coconut oil in a medium-sized saucepan. Bring to a simmer over medium heat, stirring continuously with a whisk, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add almonds to a high-speed blender. Pour in milk mixture and blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain mixture through a fine-mesh sieve into a bowl to remove remaining chunks of nuts. Add pistachio paste, 1 tablespoon plus 1 teaspoon vanilla extract, 1 tablespoon plus 2 teaspoons extract, salt, and food coloring; mix until thoroughly combined. Pour 1/2 of the mixture into an airtight container and store in the refrigerator. Pour remaining mixture into ice cube trays, cover with plastic wrap, and freeze until solid, about 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove ice cube trays from freezer and transfer frozen ice cream to the blender. Add refrigerated mixture and blend until smooth. Pour ice cream into an airtight container and freeze until solid, about 1 hour, or eat immediately as soft-serve ice cream.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pistachio Dessert Recipes"
