    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255375/dragon-eye-muffins/",
        "name": "Dragon Eye Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Dragon Eye Muffins"
        },
        "datePublished": "2020-03-05T05:15:38.000Z",
        "description": "Dragon eyes, also called longans, are small, round tropical fruits a little larger than grapes. You could try substituting lychees if you can't get dragon eyes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "18 muffins",
        "recipeIngredient": [
            "1\u2009\u00bc cups brown sugar",
            "\u00be cup butter, softened",
            "2 eggs",
            "2\u2009\u00bc cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00be cup milk",
            "\u2153 cup peeled, seeded, and minced dragon eyes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease 2 muffin tins or line with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar and butter together in a bowl until creamy. Add eggs, one at a time, beating well after each one.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, and salt in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour mixture, milk, and dragon eyes into the sugar mixture. Spoon batter into the prepared muffin tins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until a toothpick inserted into the center comes out clean, 15 to 20 minutes.\n"
            }
        ],
