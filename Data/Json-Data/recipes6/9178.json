    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280043/lazy-breakfast-casserole/",
        "name": "Lazy Breakfast Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lazy Breakfast Casserole"
        },
        "datePublished": "2020-05-13T01:57:51.000Z",
        "description": "This breakfast casserole is prepared ahead and allows you to enjoy family and friends while it bakes. Its wide appeal allows for use during holidays, showers, or just lazy weekends.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT9H5M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "nonstick cooking spray",
            "8 slices Texas toast, crusts removed, cubed",
            "2 pounds bulk pork sausage",
            "6 large eggs, beaten",
            "2 cups milk",
            "1 teaspoon dry mustard",
            "1 teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "2 cups shredded mild Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray a 9x13-inch pan with cooking spray. Place cubed bread into the bottom of the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a large skillet over medium-high heat. Cook and stir sausage in the hot skillet until browned and crumbly, 5 to 7 minutes. Place on top of bread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix eggs, milk, dry mustard, salt, and pepper together in a bowl and pour over sausage. Top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for 8 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until set, about 45 minutes. Place foil over top the last 15 minutes to prevent the cheese from hardening.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs"
        ],
