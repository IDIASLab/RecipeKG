    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280131/orange-strawberry-sorbet/",
        "name": "Orange-Strawberry Sorbet",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Orange-Strawberry Sorbet"
        },
        "datePublished": "2020-06-13T04:26:56.000Z",
        "description": "This orange-strawberry sorbet is very easy to make and very refreshing during hot summers. The kids love it, and so do the moms as it is a lot better than an ice cream! Garnish with fresh strawberries and mint leaves before serving.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT10H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bd cup water",
            "\u00bd cup white sugar",
            "\u00bd cup diced fresh strawberries",
            "2 cups orange juice with pulp",
            "\u00bd teaspoon lemon extract",
            "1 pinch salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour water and sugar into a medium saucepan over medium heat to make simple syrup. Stir occasionally until sugar dissolves. Bring to a boil for 2 to 4 minutes. Remove from heat and let cool, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add strawberries and sugar syrup to a blender and blend until smooth. Leave a few chunks of strawberries if you like. Add orange juice, lemon extract, and salt and give it a swirl. Refrigerate for 1 to 2 hours; this helps it freeze in the ice cream maker quickly as it has a lot of water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into an ice cream maker and freeze according to manufacturer's instructions, about 20 minutes. Transfer to an airtight container and freeze for 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scoop sorbet and serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Strawberry Dessert Recipes"
        ],
