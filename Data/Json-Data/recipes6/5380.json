    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274780/minestrone-with-pesto/",
        "name": "Minestrone with Pesto",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Minestrone with Pesto"
        },
        "datePublished": "2020-03-10T02:53:56.000Z",
        "description": "Minestrone is a thick, mixed vegetable soup using almost any combination of seasonal vegetables. Short cuts of pasta or rice may also be added. This version includes pesto sauce.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 tablespoons olive oil",
            "1 leek, sliced",
            "1 large onion, finely chopped",
            "2 carrots, finely chopped",
            "1 celery rib, finely chopped",
            "2 cloves garlic, finely chopped",
            "2 potatoes, peeled and diced",
            "6\u2009\u00bc cups hot vegetable broth",
            "1 bay leaf",
            "1 sprig fresh thyme",
            "salt and freshly ground black pepper to taste",
            "2 zucchini, finely chopped, or more to taste",
            "\u00be cup fresh peas",
            "3 medium tomatoes, peeled and finely chopped",
            "2 cups cannellini beans, drained and rinsed",
            "3 tablespoons pesto sauce",
            "1 tablespoon freshly grated Parmesan cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil over medium heat in a stockpot. Stir in leek and onion and cook for 5 to 6 minutes. Add carrots, celery, and garlic; cook for 5 minutes. Add potatoes and cook for 2 to 3 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour hot vegetable broth into the pot and stir well. Add bay leaf and thyme; season with salt and pepper. Bring to a boil; reduce heat and let simmer for 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in zucchini and peas. Let simmer for 5 minutes. Add tomatoes, cover, and let simmer for 5 to 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Uncover the pot and stir in beans. Let simmer for 10 minutes; stir in pesto sauce and let simmer for 5 minutes. Remove from heat and let stand for 3 to 4 minutes. Serve topped with Parmesan cheese.\n"
            }
        ],
