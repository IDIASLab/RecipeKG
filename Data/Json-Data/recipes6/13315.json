        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12223/pecan-pumpkin-pie-i/",
        "name": "Pecan Pumpkin Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1864853.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1998-06-26T03:49:27.000Z",
        "description": "A combination of the best pies:  Pecan and Pumpkin!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "2 eggs",
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "1 tablespoon all-purpose flour",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon ground ginger",
            "\u00bd teaspoon salt",
            "2 cups solid pack pumpkin puree",
            "1 (14 ounce) can sweetened condensed milk",
            "2 tablespoons butter",
            "1 tablespoon orange zest",
            "\u00bc cup packed brown sugar",
            "\u00be cup pecan halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine eggs, sugars, flour, spices and salt.  Blend in pumpkin.  Gradually add milk; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into 9-inch pie shell.  Bake in 450 degree F (230 degrees C) oven 10 minutes. Reduce heat to 350 degrees F (175 degrees C) and bake 40 to 50 minutes longer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle pecan mixture over pie the last 10 minutes before removing from the oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Topping:  Combine 2 tablespoons butter, 1 tablespoon grated orange rind,   1/4 cup brown sugar and  3/4 cup whole pecans.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pumpkin Pie Recipes"
        ],
