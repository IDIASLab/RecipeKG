    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/255089/low-fat-whole-wheat-cranberry-pecan-scones/",
        "name": "Low-Fat Whole Wheat Cranberry Pecan Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5072050.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T05:15:33.000Z",
        "description": "These scones are egg-free and come out crispy-crunchy with just the right amount of sweetness.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H42M",
        "recipeYield": "8 scones",
        "recipeIngredient": [
            "2 cups whole wheat pastry flour",
            "\u00bd cup brown sugar",
            "1 tablespoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 tablespoons butter",
            "\u00bd cup dried cranberries",
            "\u00bd cup chopped pecans",
            "\u00bd cup skim milk, or more as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C). Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk whole wheat flour, brown sugar, baking powder, baking soda, and salt together in a large bowl. Cut in butter with a pastry blender until coarse crumbs form. Stir in cranberries and pecans. Mix in skim milk until dough comes together and is the consistency of cookie dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out onto a lightly floured surface. Pat into a 1-inch thick round. Cut into 8 wedges with a greased knife. Arrange 2 inches apart on the baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 12 to 15 minutes. Transfer to a wire rack to cool, about 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Scone Recipes"
        ],
