        "recipeIngredient": [
            "1 pint strawberries, quartered",
            "1 pint blueberries, rinsed and drained",
            "1 pint fresh blackberries, rinsed and drained",
            "1 pint fresh raspberries",
            "1 pint red currants",
            "1 tablespoon white sugar",
            "3 tablespoons chopped fennel greens"
