        "name": "Rosemary Smoked Coffee Lemonade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4528651.jpg",
            "width": 3456,
            "height": 5184
