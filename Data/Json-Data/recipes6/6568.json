        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279327/smoked-salmon-tartlets/",
        "name": "Smoked Salmon Tartlets",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F566095.jpg",
            "width": 0,
            "height": 0
