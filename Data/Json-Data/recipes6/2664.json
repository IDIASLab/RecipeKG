    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256113/pecan-pie-cupcakes/",
        "name": "Pecan Pie Cupcakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4163952.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "2020-03-05T05:15:52.000Z",
        "description": "If you like pecan pie, you'll love these cupcakes!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "9 cupcakes",
        "recipeIngredient": [
            "cooking spray",
            "1 cup brown sugar",
            "1 cup chopped pecans",
            "\u00bd cup all-purpose flour",
            "2 eggs",
            "\u00bd cup butter, melted",
            "1 teaspoon vanilla extract, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a muffin tin with paper liners and coat liners with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, pecans, and flour in a large bowl; make a well in the center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in a large bowl with an electric mixer until foamy, 3 to 5 minutes. Fold in butter and vanilla. Pour into the center of the brown sugar mixture. Stir until batter is well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon batter into the muffin tin, filling each cup 2/3 of the way.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 20 to 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
