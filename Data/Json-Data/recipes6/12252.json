        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10282/butterscotch-oat-bars/",
        "name": "Butterscotch Oat Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5998029.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "1997-09-01T02:42:56.000Z",
        "description": "These bar cookies are a great replacement to the classic brownie! Try using grated orange zest in  the place of the vanilla for a citrusy twist!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "1 cup butter, softened",
            "\u00be cup white sugar",
            "\u00be cup packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "3 cups quick cooking oats",
            "2 cups butterscotch chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking soda, and cinnamon in small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter or margarine, sugar, brown sugar, eggs and vanilla in large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually beat in flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in oats and butterscotch chips.   Spread dough into greased 15 x 10 inch pan.  Bake for 18-20 minutes or until very lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
