    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10370/cherry-oatmeal-cookies/",
        "name": "Cherry Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1003163.jpg",
            "width": 791,
            "height": 790
        },
        "datePublished": "1997-12-26T07:45:45.000Z",
        "description": "Chewy oatmeal cookies with added cherries!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup shortening",
            "\u00bd cup butter",
            "1\u2009\u00bd cups white sugar",
            "2 eggs",
            "1 tablespoon maraschino cherry juice",
            "1 teaspoon vanilla extract",
            "2 cups rolled oats",
            "\u00bd cup maraschino cherries, drained and chopped",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together flour, baking soda and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, cream together shortening, butter or margarine, sugar, eggs, cherry juice and vanilla.  Stir in flour mixture and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend in oatmeal, chopped cherries and chopped nuts.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll into balls the size of walnuts and slightly mash down.  Bake to a golden brown, 8 - 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cherry Dessert Recipes"
        ],
