    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10994/stefs-whoopie-pies-with-peanut-butter-frosting/",
        "name": "Stef's Whoopie Pies with Peanut Butter Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F666275.jpg",
            "width": 1409,
            "height": 1299
        },
        "datePublished": "1998-07-11T15:35:25.000Z",
        "description": "Not like other whoopie pies, these cake cookies are even more chocolaty and cakey!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd cup unsweetened cocoa powder",
            "\u00bd cup hot water",
            "1 teaspoon vanilla extract",
            "1 teaspoon baking soda",
            "1 cup white sugar",
            "1 cup butter",
            "1 egg",
            "1 tablespoon butter",
            "1\u2009\u00be cups confectioners' sugar",
            "\u00bc cup peanut butter",
            "\u00bd cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together 1 cup of the butter or margarine and the egg.  Add 1 cup white sugar, vanilla and hot water.  Stir in the flour, cocoa, and baking soda and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop from a teaspoon onto cookie sheets and bake at 375 degrees F (190 degrees C) for 10 to 12 minutes.  Sandwich cookies together with Peanut Butter Frosting.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Peanut Butter Frosting:  Blend 1 tablespoon butter and peanut butter.  Stir in confectioners' sugar and add enough milk to make frosting soft enough to spread.  Spread between the flat sides of two cookies and sandwich together.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Whoopie Pie Recipes"
        ],
