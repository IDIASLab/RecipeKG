        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14950/caramel-snack-mix/",
        "name": "Caramel Snack Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7823046.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-12-16T21:47:23.000Z",
        "description": "This is the most requested item I bring to the office. It has become the Christmas gift of choice from my kitchen.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "15 to 20 servings",
        "recipeIngredient": [
            "\u00bd cup butter",
            "\u00be cup white corn syrup",
            "1 cup packed brown sugar",
            "1 cup chopped pecans",
            "1 cup almonds",
            "1 (12 ounce) package crispy corn and rice cereal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 275 degrees F (135 degrees C). Spray a large roasting pan with non-stick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium-size microwave safe bowl, mix butter, white corn syrup and brown sugar. Place the mixture in the microwave and cook 2 minutes, or until butter melts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the cereal, pecans and almonds into the prepared roasting pan. Pour the melted butter mixture over the cereal and nuts and mix gently until the cereal and nuts are coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 hour, stirring every 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "As the snack mix is cooling, be sure to continue to stir so that the mix will not harden in one big  lump.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Party Mix Recipes"
        ],
