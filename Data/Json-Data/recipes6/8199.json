    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/264425/4-ingredient-no-bake-chocolate-peanut-butter-keto-cookies/",
        "name": "4-Ingredient, No-Bake Chocolate Peanut Butter Keto Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "4-Ingredient, No-Bake Chocolate Peanut Butter Keto Cookies"
        },
        "datePublished": "2020-04-22T03:22:56.000Z",
        "description": "Craving something delicious but low carb? Try these easy vegan no-bake peanut butter chocolate cookies, made with only 4 ingredients and totally keto-friendly.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H2M",
        "totalTime": "P0DT1H47M",
        "recipeYield": "20 cookies",
        "recipeIngredient": [
            "2 cups peanut butter",
            "\u00bd cup yacon syrup",
            "\u00be cup coconut flour",
            "2 cups vegan chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place peanut butter and yacon syrup in a microwave-safe bowl. Microwave at 60% power until melted, about 1 minute. Add coconut flour; mix well until a thick dough is formed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into small balls and place on the prepared baking sheet; press into flat cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Freeze until cookies are hard, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in a microwave-safe glass or ceramic bowl in 15-second intervals, stirring after each interval, 1 to 3 minutes. Dip the cookies in the chocolate until fully coated. Chill in the fridge until set, about 30 minutes.\n"
            }
        ],
