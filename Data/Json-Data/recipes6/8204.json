        "recipeIngredient": [
            "2 bananas",
            "\u00be cup freshly ground peanut butter",
            "2 teaspoons honey",
            "\u00be cup oat flour",
            "\u00bd cup coconut flour",
            "\u00bc cup pea protein powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Puree bananas, peanut butter, and honey in a food processor until smooth. Place mixture in a bowl and add oat flour, coconut flour, and pea powder. Stir until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press into a 10-inch square pan and refrigerate until hardened, about 30 minutes.\n"
            }
