    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/257419/banana-split-pancakes/",
        "name": "Banana Split Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4487177.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "2020-03-05T05:16:12.000Z",
        "description": "A yummy treat that I like to make when my nieces and nephews spend the night. Great with a glass of chocolate milk.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 tablespoon brown sugar",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt",
            "2 ripe bananas",
            "1 cup milk",
            "1 egg",
            "2 tablespoons canola oil",
            "1 teaspoon vanilla extract",
            "4 ounces semisweet chocolate chips",
            "2 cups sliced fresh strawberries",
            "1 (7 ounce) can whipped cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix flour, brown sugar, baking powder, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash bananas in a large bowl using a fork. Add milk, egg, oil, and vanilla extract; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir banana mixture and chocolate chips into the flour mixture until batter is blended but still slightly lumpy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a lightly oiled griddle over medium heat. Drop 1/4 cup batter onto the griddle and cook until bubbles form and the edges are dry, 3 to 4 minutes. Flip and cook until browned on the other side, 2 to 3 minutes. Repeat with remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top pancakes with strawberries and whipped cream.\n"
            }
        ],
