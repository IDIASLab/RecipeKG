    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18521/rich-white-bread/",
        "name": "Rich White Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7978899.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-05-20T20:38:52.000Z",
        "description": "A great white bread that uses the food processor. It's a good bread to make if you're in a hurry.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 - 8x4 inch loaf",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "2 tablespoons white sugar",
            "1 (.25 ounce) package rapid rise yeast",
            "\u00bc teaspoon salt",
            "3 tablespoons butter",
            "1 egg",
            "\u00be cup warm water (110 degrees F/45 degrees C)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Insert metal blade in food processor bowl. Add flour, sugar, yeast, and salt; process 10 seconds to combine. Place butter and egg in food processor. Begin processing, then slowly pour warm water through feed tube just until dough forms a ball, about 10 to 15 seconds. Continue processing for 60 seconds to knead dough. Carefully remove dough from processor bowl  and place on a lightly floured surface. Cover and let rest for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into a loaf and place in a lightly greased 8x4 inch loaf pan. Cover and let rise until doubled, about 45 minutes. Meanwhile, preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for about 30 minutes or until the top is golden brown and the bottom of the loaf sounds hollow when tapped.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes",
            "White Bread Recipes"
        ],
