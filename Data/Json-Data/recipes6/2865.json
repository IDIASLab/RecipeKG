    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/258937/simple-refrigerator-cookies/",
        "name": "Simple Refrigerator Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Simple Refrigerator Cookies"
        },
        "datePublished": "2020-03-05T05:16:32.000Z",
        "description": "These refrigerator cookies are super easy and can be kept in the freezer for whenever a craving arises. The date-nut variation in the notes is a perfect holiday cookie and a tradition in my husband's family.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H38M",
        "recipeYield": "36 cookies",
        "recipeIngredient": [
            "1 cup brown sugar",
            "\u00bd cup butter-flavored shortening",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "1\u2009\u00be cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar, shortening, egg, and vanilla extract together in a bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour, baking soda, and salt together in a separate bowl; mix into sugar-shortening mixture until a dough forms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into a roll 2 inches in diameter; wrap in waxed paper and twist the ends to seal. Chill dough until firm, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unwrap dough; use a thin, sharp knife to make 1/8-inch slices. Place cookies on an ungreased baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 8 to 10 minutes.\n"
            }
        ],
