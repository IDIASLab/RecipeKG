        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10056/spritz-cookies-ii/",
        "name": "Spritz Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2793282.jpg",
