    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10229/oats-and-pumpkin-pinwheels/",
        "name": "Oats and Pumpkin Pinwheels",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2550212.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1997-12-14T03:10:01.000Z",
        "description": "Pinwheel cookies with a pumpkin filling and sesame seed coating.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 cup rolled oats",
            "\u00bc teaspoon baking soda",
            "1\u2009\u00bd cups white sugar",
            "\u00bd cup butter, softened",
            "2 egg whites",
            "1 cup canned pumpkin",
            "\u00bd teaspoon pumpkin pie spice",
            "\u2153 cup sesame seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In small bowl, combine flour, oats and baking soda; set aside. In large mixing bowl, beat 1 cup sugar and butter or margarine until fluffy; mix in egg whites. Stir in dry ingredients. On waxed paper, press dough into 16 x 12 inch rectangle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small bowl, combine pumpkin, remaining 1/2 cup sugar and pumpkin pie spice; mix well. Spread mixture over dough to 1/2 inch of edge. Roll dough, beginning at the narrow end. Sprinkle sesame seeds over roll, pressing gently into dough. Wrap in waxed paper; freeze until firm or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray cookie sheet with non-stick cooking spray. Cut frozen dough into 1/4 inch slices; place on cookie sheet. Bake 9 - 11 minutes or until golden brown. Remove to wire rack; cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
