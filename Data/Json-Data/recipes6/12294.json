    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10351/chocolate-swirls/",
        "name": "Chocolate Swirls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F727014.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-12-13T20:23:15.000Z",
        "description": "If you love chocolate and cream cheese, you'll love these cookies. At little goes a long way.  Make them small, they do not spread.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "\u00bd cup packed brown sugar",
            "\u00bd cup white sugar",
            "1 (3 ounce) package cream cheese",
            "1 egg",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 cup semisweet chocolate chips, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large bowl, cream together butter, brown sugar, and white sugar until smooth. Beat in cream cheese, egg, and vanilla. Combine flour, baking powder, and salt; mix into creamed mixture to form a soft dough. Fold in the melted chocolate until the dough is just marbled. Do not over mix. Drop dough by heaping teaspoons onto ungreased cookie sheets. Cookies should be about 2 inches apart.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 12 minutes in the preheated oven, or until edges are light golden brown. Cool completely before storing.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
