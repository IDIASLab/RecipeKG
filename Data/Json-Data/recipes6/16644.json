        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24378/green-grape-salad/",
        "name": "Green Grape Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1117029.jpg",
            "width": 6016,
