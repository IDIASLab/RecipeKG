    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274119/mighty-mini-loaves/",
        "name": "Mighty Mini Loaves",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mighty Mini Loaves"
        },
        "datePublished": "2020-03-05T05:19:55.000Z",
        "description": "Single-serve mini meatloaves.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "6 loaves",
        "recipeIngredient": [
            "1 (12 ounce) package bacon",
            "1 cup dry bread stuffing mix",
            "\u2154 cup grated Parmesan cheese",
            "1\u2009\u00bd tablespoons Worcestershire sauce",
            "1\u2009\u00bd tablespoons soy sauce",
            "1 egg",
            "1 (1 ounce) package dry onion soup mix",
            "salt and ground black pepper to taste",
            "1 cup shredded Swiss cheese",
            "\u2154 cup ketchup",
            "\u2154 cup prepared yellow mustard",
            "\u00bc cup brown sugar",
            "1 medium onion, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and let cool; crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon, stuffing, Parmesan cheese, Worcestershire, soy sauce, egg, onion soup mix, salt, and pepper in a large bowl and mix well. Divide meat mixture into 6 individual portions. Mold each portion into oval-like shapes. Press top of each oval in and add Swiss cheese. Mold meat back up over the tops, enclosing cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix ketchup, mustard, and brown sugar together in a separate bowl. Cover each loaf with mixture. Place loaves in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the centers comes out clean, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While loaves bake, saute onion over medium-high heat in a frying pan, 5 to 7 minutes. Top baked loaves with onions and let stand for 5 minutes to cool before serving.\n"
            }
        ],
