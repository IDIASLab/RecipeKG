        "recipeIngredient": [
            "3 tablespoons brown sugar",
            "3 tablespoons dried parsley",
            "2 tablespoons garlic salt",
            "2 tablespoons chicken bouillon granules",
            "1 packet instant tomato soup mix",
            "1 tablespoon cayenne pepper",
            "1 tablespoon dried rosemary",
            "1 tablespoon ground sage",
            "1 tablespoon paprika",
            "1 tablespoon onion salt",
            "1 tablespoon ground oregano",
            "1 teaspoon ground ginger",
            "1 teaspoon dried marjoram",
            "1 teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, parsley, garlic salt, chicken bouillon, tomato soup mix, cayenne pepper, rosemary, sage, paprika, onion salt, oregano, ginger, marjoram, and black pepper in a blender; pulse until ingredients are pulverized, 3 to 4 minutes.\n"
            }
        ],
