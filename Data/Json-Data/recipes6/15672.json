    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20550/extra-special-banana-pudding/",
        "name": "Extra Special Banana Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F723579.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-08-21T21:29:10.000Z",
        "description": "This has been a favorite of our family for years.  The coconut adds a special crunchy touch.  You can use vanilla pudding, if you prefer.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9x13 inch dish",
        "recipeIngredient": [
            "2 cups flaked coconut",
            "2 cups all-purpose flour",
            "1 cup butter, softened",
            "\u00bd cup white sugar",
            "2 (5 ounce) packages instant banana pudding mix",
            "6 cups cold milk",
            "3 bananas, sliced",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, use fingers to mix together coconut, flour and sugar with butter until a crumbly mixture is formed.  Spread on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until light brown, stirring occasionally, 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare pudding mix with cold milk according to package directions.  In a 9x13 inch dish, sprinkle two-thirds of the coconut mixture.  Spread the prepared pudding over the crust.  Top with sliced bananas and whipped topping.  Sprinkle remaining crust on top and refrigerate until serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
        ],
