    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279751/southern-macaroni-salad/",
        "name": "Southern Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9354243.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-20T19:13:39.000Z",
        "description": "A creamy blend of mayo and veggies sure to please any with a heart for a good ole' Southern tradition. My grandma use to make this macaroni salad all the time at every holiday.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (16 ounce) package elbow macaroni",
            "1 (8 ounce) jar mayonnaise (such as Hellman's\u00ae)",
            "6 large eggs, at room temperature",
            "3 medium tomatoes, quartered",
            "3 stalks celery, chopped",
            "6 ounces radishes, diced",
            "1 white onion, diced",
            "1 teaspoon salt, or to taste",
            "1 teaspoon ground black pepper, or to taste",
            "1 pinch celery seed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place eggs in another pot and cover with water. Bring to a boil; turn off heat and let sit for 10 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain cooked macaroni and place back into the pot; add mayonnaise and stir until creamy. Dice boiled eggs and mix into pasta. Fold in tomatoes, celery, radishes, and onion. Transfer to a big bowl and season with salt, pepper, and celery seed.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Macaroni Salad Recipes"
        ],
