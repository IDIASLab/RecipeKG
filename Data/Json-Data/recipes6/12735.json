    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11000/double-chocolate-walnut-brownies/",
        "name": "Double Chocolate Walnut Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4467134.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1998-07-20T15:25:40.000Z",
        "description": "If you are a chocolate lover you will like this brownie recipe. It has  alot of chocolate in it. But make sure you have a glass of milk handy because you will need it.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "4 (1 ounce) squares unsweetened chocolate",
            "2 cups white sugar",
            "3 eggs",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1\u2009\u00bd cups chopped walnuts",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter and 4 squares unsweetened chocolate in a medium size saucepan over moderate heat. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in sugar gradually with a wooden spoon until thoroughly combined.  Add eggs, one at a time, beating well after each addition; stir in vanilla. Stir in flour until thoroughly combined. Stir 1 cup of the  walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread into greased 13 x 9 x 2 inch pan. Combine remaining 1/2 cup walnuts with chocolate chips; sprinkle over top of brownie mixture, pressing down lightly. Bake in a preheated oven for  35 minutes or until top springs back when lightly pressed with fingertip. Cool completely in pan on wire rack. Cut into bars or squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Walnut Dessert Recipes"
        ],
