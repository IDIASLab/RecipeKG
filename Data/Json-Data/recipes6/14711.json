    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17250/apple-cheese-quick-bread/",
        "name": "Apple Cheese Quick Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F286301.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-03-29T16:41:06.000Z",
        "description": "Apples, cheese and walnuts make this bread a moist and yummy treat. It tastes great with cream cheese on it!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 9x5 inch loaf",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "\u2153 cup white sugar",
            "\u2153 cup honey",
            "2 eggs",
            "1 cup whole wheat flour",
            "1 cup all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "2 Granny Smith apples - peeled, cored and shredded",
            "4 ounces Swiss cheese, shredded",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the butter and sugar until light and fluffy. Beat in the honey and eggs. In a separate bowl, sift together the whole wheat flour, all-purpose flour, baking powder, soda and salt. Add the flour mixture to the butter and egg mixture; mix until combined. Fold in the apple, cheese and walnuts. Spoon batter into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 50 to 60 minutes, or until a toothpick inserted into the center of the loaf comes out clean. Turn the loaf out onto a wire rack and let cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Fruit Bread Recipes"
        ],
