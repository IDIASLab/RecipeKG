    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11201/banana-chocolate-chip-cookies/",
        "name": "Banana Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1896401.jpg",
            "width": 1936,
            "height": 2592
        },
        "datePublished": "1998-12-29T13:38:00.000Z",
        "description": "This recipe uses very ripe bananas, the ones which you would not want to eat.  The riper the bananas are, the more flavor they have.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon baking soda",
            "1 cup white sugar",
            "\u2154 cup butter, softened",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 cup mashed bananas",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease cookie sheets. Sift the flour, baking powder, salt, and baking soda together, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter with the sugar until light and fluffy. Beat in the eggs and vanilla. Mix in the mashed bananas. Add the flour mixture, and stir until just combined. Stir in the chocolate chips. Drop by spoonfuls onto prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
        ],
