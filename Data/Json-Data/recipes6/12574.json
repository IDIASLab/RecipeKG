        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10766/sunflower-drop-cookies/",
        "name": "Sunflower Drop Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8058624.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-03-26T18:43:16.000Z",
        "description": "Good, slightly different because of the sunflower seeds",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1\u2009\u215b cups all-purpose flour",
            "1\u2009\u215b cups whole wheat flour",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "1 cup butter",
            "5 tablespoons brown sugar",
            "5 tablespoons white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 cup sunflower seeds",
            "2 cups flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, cream together sugar and butter or margarine.  Beat in eggs and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour, salt and baking soda.  Add coconut and sunflower seeds.  Mix together well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by spoonfuls onto greased cookie sheet.  Bake at 350 degrees F (175 degrees C) for 8 to 10 minutes.\n"
            }
        ],
