    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/238105/glazed-ham-with-mango-sauce/",
        "name": "Glazed Ham with Mango Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Glazed Ham with Mango Sauce"
        },
        "datePublished": "2020-03-05T05:12:05.000Z",
        "description": "A twist on the traditional ham glazes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H45M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "1 ham",
        "recipeIngredient": [
            "1 (10 pound) fully-cooked, bone-in ham",
            "2 cups mango nectar, divided, or more as needed",
            "\u00bd cup packed brown sugar",
            "2 tablespoons Dijon mustard",
            "2 teaspoons grated fresh ginger root",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground cloves",
            "1 cup mango preserves",
            "\u00bd cup white wine"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Score the top of the ham in a diamond pattern with serrated knife. Place ham on a roasting rack in a roasting pan. Pour 1 1/2 cups mango nectar over ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the internal temperature of the ham is 140 degrees F (60 degrees C), 1 1/2 to 2 hours. Increase oven temperature to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir brown sugar, Dijon mustard, ginger root, cinnamon, and cloves together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine mango preserves, white wine, and remaining 1/2 cup mango nectar together in a saucepan; bring to a boil, stir in brown sugar mixture, reduce heat to low, and simmer until glaze thickens, 2 to 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Brush ham all over with glaze. Continue baking until glaze is bubbling, about 10 minutes more. Remove ham from oven and let rest for 15 to 20 minutes before carving.\n"
            }
        ],
