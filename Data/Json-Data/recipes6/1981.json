        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/241087/chai-butter-tarts/",
        "name": "Chai Butter Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2015636.jpg",
            "width": 2048,
            "height": 2048
        },
        "datePublished": "2020-03-05T05:12:57.000Z",
        "description": "I took my favorite butter tart recipe and spiced it up a bit!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "20 3-inch tarts",
        "recipeIngredient": [
            "20 (3 inch) unbaked tart shells",
            "\u00bd cup packed brown sugar",
            "\u00bc cup butter, softened",
            "2 eggs",
            "1 cup golden corn syrup",
            "1 teaspoon vanilla extract",
            "\u00bd teaspoon lemon juice",
            "\u00bc teaspoon ground cardamom",
            "\u215b teaspoon ground cinnamon",
            "\u215b teaspoon ground nutmeg",
            "\u215b teaspoon ground ginger",
            "1 cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Place tart shells on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat brown sugar and butter together with an electric mixer until creamy; beat in eggs, corn syrup, vanilla extract, lemon juice, cardamom, cinnamon, nutmeg, and ginger.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide raisins evenly into the bottom of tart shells; pour about 2 tablespoons syrup mixture into each tart shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pastry is golden and filling is set, about 20 minutes; remove to cool on a wire rack.\n"
            }
        ],
