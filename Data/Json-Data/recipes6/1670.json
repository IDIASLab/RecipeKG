    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/239033/brown-sugar-shortbread/",
        "name": "Brown Sugar Shortbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1715472.jpg",
            "width": 1200,
            "height": 1200
        },
        "datePublished": "2020-03-05T05:12:16.000Z",
        "description": "These shortbread rounds with a sugared edges and a hint of sea salt are perfect for tea time, coffee breaks, or after-school snacks.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "8 ounces unsalted butter at room temperature",
            "\u00bd teaspoon Diamond Crystal\u00ae Kosher Salt",
            "3\u2009\u00bd ounces dark brown sugar",
            "8 ounces all-purpose flour",
            "Demerara, turbinado, or other coarse sugar, as needed",
            "Diamond Crystal\u00ae Coarse Sea Salt, as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter, Diamond Crystal&reg; Kosher Salt and dark brown sugar in a bowl for 5 minutes with an electric mixer on high speed. Add flour and mix on low speed until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Scrape dough onto a sheet of parchment paper; shape into a log about 2 inches in diameter. Wrap with the parchment paper. Refrigerate until chilled completely, about an hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly oil cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove dough from parchment and roll in demerara sugar to coat. Slice into rounds 1/4 inch thick and place on cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle each cookie with additional demerara sugar and a pinch of Diamond Crystal&reg; Coarse Sea Salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until very lightly browned and firm to the touch, about 8 to 10 minutes.\n"
            }
        ],
