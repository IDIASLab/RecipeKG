    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273424/carrot-cumin-parsley-soup/",
        "name": "Carrot Cumin Parsley Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6595374.jpg",
            "width": 2160,
            "height": 2160
        },
        "datePublished": "2020-04-15T20:41:21.000Z",
        "description": "A delicious twist on the basic carrot soup. With a few changes, I've made this into a relatively low-carb, delicious soup.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 pound carrots, peeled and sliced",
            "1 small onion, roughly chopped",
            "3 cloves garlic, chopped",
            "1 teaspoon ground coriander",
            "1 teaspoon ground cumin",
            "4 cups chicken bone broth",
            "\u00bd cup chopped fresh parsley",
            "\u00bd cup plain Greek yogurt",
            "salt and ground black pepper to taste",
            "\u00bc cup chopped fresh cilantro, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large soup pot over medium-high heat. Add carrots and onion; cook until slightly softened, about 5 minutes. Add garlic, coriander, and cumin; cook until fragrant, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add bone broth and bring to a boil. Lower heat and simmer until the carrots are tender, 5 to 7 minutes. Add parsley; cook for 1 minute more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Puree with an immersion blender or in a blender until smooth. Add yogurt; blend until thoroughly mixed. Season with salt and pepper. Ladle into bowls and garnish with cilantro.\n"
            }
        ],
