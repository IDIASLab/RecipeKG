    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17954/rhubarb-bread-ii/",
        "name": "Rhubarb Bread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2303936.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2000-05-07T22:47:40.000Z",
        "description": "This yummy quick bread is a snap to make. There's no better way to celebrate spring than by making a delicious rhubarb dessert.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 9x5 inch loaf",
        "recipeIngredient": [
            "1\u2009\u00bd cups brown sugar",
            "\u00bd cup vegetable oil",
            "1 egg",
            "1 cup buttermilk",
            "1 teaspoon salt",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups rhubarb, cut into 1/2 inch pieces",
            "\u00bd cup chopped walnuts",
            "\u00bc cup white sugar",
            "1 teaspoon butter, melted",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Lightly grease a 9x5 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, stir together brown sugar, oil, egg, buttermilk, salt and vanilla. Gradually stir in flour just until moistened. Fold in rhubarb and walnuts. Pour batter into prepared pan. Combine 1/4 cup sugar, melted butter and cinnamon; sprinkle on top of batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 60 to 75 minutes, or until a toothpick inserted into center of loaf comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Fruit Bread Recipes"
        ],
