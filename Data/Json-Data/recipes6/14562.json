        "recipeIngredient": [
            "\u00bc cup vegetable oil",
            "2 tablespoons red wine vinegar",
            "1\u2009\u00bd teaspoons white sugar",
            "\u00bd teaspoon dried dill weed",
            "\u215b teaspoon salt",
            "\u215b teaspoon onion powder",
            "\u215b teaspoon garlic powder",
            "\u215b teaspoon dry mustard",
            "\u215b teaspoon ground black pepper"
