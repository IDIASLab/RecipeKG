    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12692/mocha-cheese-pie/",
        "name": "Mocha Cheese Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mocha Cheese Pie"
        },
        "datePublished": "1999-09-26T03:45:56.000Z",
        "description": "This is a very easy recipe to make and is very full of flavor!! I do believe all will love it! This pie is at its best when garnished with whipped topping and chocolate curls.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "8 ounces semisweet chocolate chips",
            "1 tablespoon instant coffee powder ",
            "1 tablespoon boiling water",
            "2 (8 ounce) packages cream cheese, softened",
            "\u2153 cup white sugar",
            "3 eggs",
            "\u00bc cup frozen whipped topping, thawed",
            "1 (9 inch) pie shell"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a double boiler, over hot (not boiling) water, melt chocolate morsels; stir until smooth, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a cup, dissolve coffee in 1 tablespoon boiling water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat cream cheese and sugar together until smooth.  Add eggs one at a time, beating well after each addition. Add melted chocolate and coffee; mix well, then blend in frozen whipped topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into pie shell and bake in preheated oven for 35 to 40 minutes. Turn oven off and allow pie to stand in oven with door ajar for 15 minutes. Remove from oven and let cool. Refrigerate 20 to 30 minutes before serving. Garnish with whipped topping and chocolate curls just prior to serving, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies"
        ],
