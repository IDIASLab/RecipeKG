        "recipeIngredient": [
            "2 pounds ground beef",
            "6 tablespoons Worcestershire sauce",
            "2 tablespoons dried parsley",
            "2 tablespoons dried basil",
            "1 tablespoon brown sugar",
            "1 tablespoon ground black pepper",
            "garlic salt, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix ground beef, Worcestershire sauce, parsley, basil, brown sugar, black pepper, and garlic salt together in a bowl; form into 10 patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook patties in a skillet, working in batches, until browned and desired doneness is reached, 4 to 5 minutes per side.\n"
            }
