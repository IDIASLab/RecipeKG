    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/267846/easy-pecan-tassies/",
        "name": "Easy Pecan Tassies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F625957.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T05:18:27.000Z",
        "description": "This recipe for pecan tassies was handed down to me by a great friend of mine.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "\u00bd cup butter, softened",
            "3 ounces cream cheese, softened",
            "\u00be cup brown sugar",
            "\u00bd cup chopped pecans",
            "1 egg",
            "1 tablespoon butter, softened",
            "1 teaspoon vanilla extract",
            "1 dash salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, butter, and cream cheese in a bowl. Cover and chill in the refrigerator for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll chilled dough into 24 balls and press into the cups of a mini muffin tin to make pastry shells.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, pecans, egg, butter, vanilla extract, and salt together in a bowl. Spoon 1/2 to 3/4 teaspoon filling into each pastry shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 25 minutes.\n"
            }
        ],
