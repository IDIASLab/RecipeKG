    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9649/apricot-raisin-cookies/",
        "name": "Apricot Raisin Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F231977.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These cookies are fruity and spicy...very delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup dried apricots",
            "1 teaspoon baking powder",
            "1 teaspoon ground allspice",
            "2 cups all-purpose flour",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00bd cup shortening",
            "1 cup white sugar",
            "1 teaspoon baking soda",
            "1 tablespoon warm water",
            "1 egg",
            "1 cup raisins",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grind apricots in food processor or blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small  bowl, combine flour, baking powder, spices and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream the shortening and sugar.  Dissolve baking soda in warm water and add to creamed mixture, beating until smooth.  Beat in the egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly blend in the dry ingredients, mixing until well blended.  Fold in the apricots, raisins and pecans.  Cover and chill for one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F.  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by teaspoonfuls two inches apart.  Bake for 18 to 20 minutes.\n"
            }
        ],
