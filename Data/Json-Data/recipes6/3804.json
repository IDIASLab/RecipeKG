    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274540/whole-wheat-apple-snack-bars/",
        "name": "Whole Wheat Apple Snack Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Whole Wheat Apple Snack Bars"
        },
        "datePublished": "2020-03-05T05:20:09.000Z",
        "description": "Quick and easy, wholesome snack bars are easy to take to work or to pack in the kids' lunches.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "12 bars",
        "recipeIngredient": [
            "\u00be cup packed brown sugar",
            "\u00bd cup unsweetened applesauce",
            "\u00bc cup vegetable oil",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2 cups whole wheat flour",
            "1 tablespoon ground cinnamon",
            "1 teaspoon baking powder",
            "1 cup apples - peeled, cored, and sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a 7-inch square baking tin, or line with paper liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk brown sugar, applesauce, oil, eggs, and vanilla extract together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, cinnamon, and baking powder in a separate bowl. Stir into egg mixture until moistened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon 1/2 the batter into the prepared baking tin and spread evenly. Place apple slices on top and cover with remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the top springs back lightly when pressed, about 30 minutes. Remove from the oven and let cool completely before removing from the tin and cutting into bars, about 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Granola Bar Recipes"
