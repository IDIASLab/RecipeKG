    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/57829/venison-jerky/",
        "name": "Venison Jerky",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4877587.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:09:46.000Z",
        "description": "This is the best jerky recipe I have found!  You can use either ground venison or beef!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT5H0M",
        "totalTime": "P0DT5H25M",
        "recipeYield": "1 pound",
        "recipeIngredient": [
            "2 pounds lean ground venison or beef",
            "2\u2009\u00bd teaspoons salt",
            "2 teaspoons monosodium glutamate (MSG)",
            "1 teaspoon hot pepper sauce",
            "2 teaspoons sugar-based curing mixture (such as Morton\u00ae Tender Quick\u00ae)",
            "1\u2009\u00bd teaspoons barbeque seasoning",
            "2 teaspoons water",
            "2 teaspoons hickory-flavored liquid smoke",
            "2 teaspoons garlic powder",
            "\u00bd teaspoon pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine ground venison, salt, MSG, hot sauce, curing mixture, barbeque seasoning, water, liquid smoke, garlic powder, and pepper; mix thoroughly.  Cover, and refrigerate for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line food dehydrator trays with plastic wrap.  Pat meat mixture into a very thin layer on the trays.  Place in dehydrator set according to manufacturers directions.  Once the meat has firmed up, after about the first 2 hours, remove it from the plastic, cut into strips, and replace onto the racks.  Continue until the meat is done drying, about 5 to 10 hours total depending on machine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When finished, blot off any grease with a paper towel.  Store in the refrigerator in a jar with a tight fitting lid.\n"
            }
        ],
