    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/272333/vegan-roasted-red-pepper-and-carrot-soup/",
        "name": "Vegan Roasted Red Pepper and Carrot Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Roasted Red Pepper and Carrot Soup"
        },
        "datePublished": "2020-03-10T02:53:49.000Z",
        "description": "Try this easy and delicious vegan carrot soup recipe! Great for a cold winter day!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "5 carrots, peeled and cut into 1/2 inch pieces",
            "2 red bell peppers, quartered",
            "1 large onion, cut into large pieces",
            "3 cloves garlic, peeled",
            "salt and ground black pepper to taste",
            "2 teaspoons ground paprika",
            "1 teaspoon ground cumin",
            "1 tablespoon olive oil",
            "1 (32 fluid ounce) container vegetable broth",
            "1 bay leaf",
            "\u00bd teaspoon dried oregano",
            "1 pinch red pepper flakes, or to taste",
            "\u00bd teaspoon apple cider vinegar, or more to taste",
            "1 dash hot sauce,  or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix carrots, red bell peppers, onion, garlic, paprika, cumin, salt, and pepper together in a bowl. Transfer to a rimmed baking sheet. Drizzle olive oil on top and toss.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in the preheated oven until tender, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring vegetable broth, bay leaf, and oregano to a boil in a soup pot. Add roasted vegetables to the pot. Remove bay leaf and add red pepper. Simmer for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend soup with an immersion blender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add apple cider vinegar to pureed soup. Serve with hot sauce.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Carrot Soup Recipes"
        ],
