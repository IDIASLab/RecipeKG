        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284875/blackened-flounder-with-mango-slaw/",
        "name": "Blackened Flounder with Mango Slaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8160944.jpg",
