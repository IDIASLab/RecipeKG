    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8298/pecan-pie-cake-i/",
        "name": "Pecan Pie Cake I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2075981.jpg",
            "width": 2287,
            "height": 1763
        },
        "datePublished": "2008-06-26T23:24:29.000Z",
        "description": "A very rich and moist cake. Can be made with any flavored cake mix, but we like the butter pecan.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 10x15 inch cake",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "1 cup packed brown sugar",
            "1 (18.25 ounce) package butter pecan cake mix",
            "2 tablespoons water",
            "2 cups chopped pecans",
            "2 eggs",
            "\u00bd cup butter, softened",
            "2 eggs",
            "1 cup milk",
            "1 cup white sugar",
            "1 cup light corn syrup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reserve 3/4 cup cake mix for second layer. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the first layer: In a large bowl, combine 1/2 cup margarine or butter, brown sugar, cake mix (minus 3/4 cup), water, pecans and 2 eggs. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into pan. Bake at 325 degrees F (165 degrees C) for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the second layer: In a large bowl, combine 1/2 cup butter or margarine, 2 eggs, milk, sugar, corn syrup and reserved 3/4 cup cake mix. Mix well and pour over first layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C), for 25 to 30 minutes.\n"
            }
        ],
