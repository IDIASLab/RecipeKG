    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280050/chicken-breasts-with-portobello-mushrooms/",
        "name": "Chicken Breasts with Portobello Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chicken Breasts with Portobello Mushrooms"
        },
        "datePublished": "2020-05-07T07:34:29.000Z",
        "description": "This chicken is coated in bread crumbs and Parmesan cheese, and then covered with portobello mushrooms. It's good and easy!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "nonstick cooking spray",
            "\u00bd cup grated Parmesan cheese",
            "\u00bc cup seasoned bread crumbs",
            "2 skinless, boneless chicken breasts, halved",
            "1 tablespoon olive oil",
            "1 small shallot, chopped",
            "4 portobello mushrooms, sliced",
            "1 tablespoon chopped fresh parsley",
            "\u00bc teaspoon ground black pepper",
            "4 thin slices  mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Spray an oven-proof casserole dish with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix Parmesan cheese and bread crumbs together in a shallow dish or on waxed paper. Press mixture into both sides of chicken breasts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a small frying pan over medium-high heat. Add shallot and saute until soft, about 5 minutes. Add mushrooms, parsley, and pepper. Cook, turning once, for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange chicken flat into the prepared casserole dish. Top with the mushroom mixture, divided evenly over each piece. Arrange slices of mozzarella cheese on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a fork can be inserted into the chicken breasts with ease and they are no longer pink in the centers, 20 to 25 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
