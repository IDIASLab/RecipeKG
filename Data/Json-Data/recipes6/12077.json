    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9989/simple-sugar-cookies/",
        "name": "Simple Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27490.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1997-08-18T21:00:18.000Z",
        "description": "One of the simplest sugar cookies ever!  When baked, they are delicious and it's hard to keep them in the cookie jar for long...because everyone eats them too quickly.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u2153 cup white sugar",
            "\u2153 cup shortening",
            "1 egg",
            "\u2154 cup honey",
            "1 teaspoon lemon extract",
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).  Lightly grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sugar, shortening, egg, honey, and lemon extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in remaining ingredients and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough 1/4 inch thick.  Use cookie cutters to cut into desired shapes.  Place 1 inch apart on lightly greased cookie sheet.  Bake 7 to 8 minutes or until no indentation remains when touched.  Cool and decorate.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
