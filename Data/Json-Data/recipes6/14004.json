        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15062/pudding-from-scratch/",
        "name": "Pudding from Scratch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8272296.jpg",
            "width": 0,
            "height": 0
