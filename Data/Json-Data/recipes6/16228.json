        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23532/tea-punch/",
        "name": "Tea Punch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F696565.jpg",
            "width": 3000,
            "height": 4000
        },
        "datePublished": "2000-09-13T20:11:57.000Z",
        "description": "A different twist to a standard punch. Brewed tea is mixed with orange juice, pineapple juice, lemonade and ginger ale.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT4H30M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup strong brewed black tea",
            "4 cups orange juice",
            "4 cups pineapple juice",
            "4 cups prepared lemonade",
            "1 (2 liter) bottle ginger ale, chilled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a pitcher, combine sugar and tea. Stir until sugar is dissolved. Stir in orange juice, pineapple juice and lemonade. Chill in refrigerator for 4 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Just before serving, pour chilled juice mixture into a punch bowl and stir in ginger ale.\n"
            }
        ],
