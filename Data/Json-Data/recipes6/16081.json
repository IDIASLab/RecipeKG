        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22879/cherry-blossom-fudge/",
        "name": "Cherry Blossom Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F835635.jpg",
            "width": 250,
