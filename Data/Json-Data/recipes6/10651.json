        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7182/old-fashioned-sally-lunn-muffins/",
        "name": "Old-Fashioned Sally Lunn Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3599.jpg",
            "width": 800,
            "height": 600
        },
        "datePublished": "1999-06-02T11:09:30.000Z",
        "description": "This is an old Southern recipe for a quick and easy solution to a need for homemade muffins.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "3 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bc cup shortening",
            "\u00bc cup white sugar",
            "1 egg, beaten",
            "1 cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl mix together the flour, baking powder, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl cream together the shortening and sugar. Stir in beaten egg, and milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add flour mixture alternately with liquid mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill greased muffin tins two-thirds of the way full. Bake in a preheated 375 degree F(190 degrees C) oven for 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Muffin Recipes"
