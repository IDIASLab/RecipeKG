    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279950/savory-roasted-pumpkin-seeds/",
        "name": "Savory Roasted Pumpkin Seeds",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Savory Roasted Pumpkin Seeds"
        },
        "datePublished": "2020-05-05T00:44:41.000Z",
        "description": "Addictive, tasty, crunchy pumpkin seeds. My family and friends beg me for these every year. This recipe is for 1 cup of pumpkin seeds but you can double or triple as needed.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT3H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup raw pumpkin seeds",
            "1 tablespoon salted butter, melted",
            "\u00bd tablespoon Worcestershire sauce",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon onion powder",
            "\u00bc teaspoon salt, or more to taste "
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Clean all pulp and guts from pumpkin seeds. Soak in cold water for a minimum of 2 hours, up to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C). Line a baking sheet with parchment paper to prevent sticking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain pumpkin seeds and mix with butter, Worcestershire, garlic powder, onion powder, and 1/4 teaspoon salt in a bowl. Spread seeds in a single layer on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until crisp, stirring and flipping halfway through, 1 to 1 1/2 hours. Sprinkle with additional salt to taste as soon as you remove the sheet from the oven.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Nuts and Seeds",
            "Pumpkin Seed Recipes"
        ],
