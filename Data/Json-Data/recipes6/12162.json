        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "6 eggs",
            "1\u2009\u00bd cups white sugar",
            "1 cup vegetable oil",
            "1 teaspoon anise extract",
            "2 cups all-purpose flour",
            "2\u2009\u00bd teaspoons baking powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat six eggs.  Add sugar, oil, and anise flavoring and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in the flour and baking powder.  Beat well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Smooth batter into  a 10 x 15 inch  greased and floured pan.  Bake for 25 - 30 minutes.  Cool then cut in strips.\n"
            }
        ],
