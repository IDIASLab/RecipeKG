        "description": "Flourless cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "3 egg whites",
            "1\u2009\u00bc cups white sugar",
            "\u00bc teaspoon salt",
            "\u00bd teaspoon vanilla extract",
            "2\u2009\u00bd cups flaked coconut",
            "1\u2009\u00bd cups cornflakes cereal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (170 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until stiff (the egg whites not your arm).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in sugar, salt and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend coconut and corn flakes and add to egg white mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop on ungreased parchment paper.  Bake for 15-18 minutes.\n"
            }
        ],
