        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11029/no-bake-fudge-cookies/",
        "name": "No Bake Fudge Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4514919.jpg",
