        "mainEntityOfPage": "https://www.allrecipes.com/recipe/254490/brown-sugar-chicken-brine/",
        "name": "Brown Sugar Chicken Brine",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5887114.jpg",
            "width": 3024,
            "height": 3024
