        "mainEntityOfPage": "https://www.allrecipes.com/recipe/62424/purple-apple-slaw/",
        "name": "Purple Apple Slaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1995294.jpg",
