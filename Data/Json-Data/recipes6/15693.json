    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20658/apple-pie-bread/",
        "name": "Apple Pie Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1329539.jpg",
            "width": 2272,
            "height": 1704
        },
        "datePublished": "2000-08-14T18:13:30.000Z",
        "description": "A delicious bread that always gets happy faces!  Leave the peels on the apples for added taste and color.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 - 9x5 inch loaf",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 cup white sugar",
            "4 teaspoons baking powder",
            "1 teaspoon salt",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground cloves",
            "1\u2009\u00bd cups milk",
            "1 egg",
            "2 tablespoons vegetable oil",
            "1\u2009\u00bd cups diced apples"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C) and lightly grease a 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine flour, sugar, baking powder, salt, cinnamon and cloves.  In a separate bowl, mix together milk, egg and oil until well blended.  Stir milk mixture into flour mixture.  Fold in apple.  Pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 hour 20 minutes, until loaf springs back when touched lightly in center.  Cool in pan 10 minutes before removing to wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
