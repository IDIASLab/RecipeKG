    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279447/grilled-chipotle-tenderloin/",
        "name": "Grilled Chipotle Tenderloin",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grilled Chipotle Tenderloin"
        },
        "datePublished": "2020-04-02T20:19:27.000Z",
        "description": "Spicy Mexican-inspired marinade for an excellent grilled pork tenderloin. Serve as a main dish or slice as appetizers. This even tastes great cold.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT6H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "\u00bc cup chopped fresh cilantro",
            "3 canned chipotle chiles in adobo sauce, finely chopped",
            "2 large cloves garlic, minced",
            "3 tablespoons honey",
            "2 tablespoons lime juice",
            "1 tablespoon soy sauce",
            "2 teaspoons ground cumin",
            "2 (10 ounce) pork tenderloins, trimmed of fat"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk cilantro, chiles, garlic, honey, lime juice, soy sauce, and cumin together in a bowl and pour into a resealable plastic bag. Add pork tenderloins, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator for 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove tenderloins from the marinade and shake off excess. Discard the remaining marinade and bring pork to room temperature, 20 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill tenderloins on the preheated grill until slightly pink in the center, about 10 minutes per side. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C).\n"
            }
        ],
