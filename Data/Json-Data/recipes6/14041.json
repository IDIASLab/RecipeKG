    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15153/snickerdoodle-supreme/",
        "name": "Snickerdoodle Supreme",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F577930.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-27T23:54:04.000Z",
        "description": "My recipe uses nutmeg as part of the ingredients to roll the cookies in before baking.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1\u2009\u00bc cups white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "2 teaspoons cream of tartar",
            "2 tablespoons white sugar",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream shortening and sugar. Stir in the eggs and vanilla. Sift together the flour, baking soda, and cream of tartar, stir into the creamed mixture. Cover dough and chill for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). In a small saucer, stir together the remaining sugar, with the cinnamon and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1 inch balls and roll the balls in the sugar mixture. Place 2 inches apart on an unprepared cookie sheet. Bake for 8 to 10 minutes in the preheated oven, edges will be golden brown. Remove from cookie sheets to cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Snickerdoodle Recipes"
        ],
