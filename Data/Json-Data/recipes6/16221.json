    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23452/marinara-sauce-ii/",
        "name": "Marinara Sauce II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F875919.jpg",
            "width": 640,
            "height": 480
        },
        "datePublished": "2000-09-28T03:19:22.000Z",
        "description": "Marinara sauce that is great on any type of pasta.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "4 tablespoons olive oil",
            "4 cloves garlic, minced",
            "1 small onion, chopped",
            "1 (28 ounce) can whole peeled tomatoes",
            "1 (28 ounce) can crushed tomatoes",
            "3 teaspoons dried basil leaves",
            "1 teaspoon white sugar",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a skillet over medium heat, saute garlic and onion in the olive oil; about 10 minutes.  Break apart the whole tomatoes with your hands and add to the pan along with the crushed tomatoes, basil, sugar, salt and pepper.  Cover and simmer 20 minutes, stirring occasionally.\n"
            }
        ],
