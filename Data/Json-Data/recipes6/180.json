    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220962/easter-ham/",
        "name": "Easter Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4503775.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2020-03-05T05:08:44.000Z",
        "description": "This is a family favorite that we make year-round.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "1 (7 pound) boneless ham",
            "2 cups brown sugar",
            "3 tablespoons prepared yellow mustard, or as needed",
            "1 (8 ounce) can pineapple rings in juice, drained and juice reserved",
            "1 cup honey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Score ham by making 1/2-inch deep diagonal cuts, creating a diamond pattern; transfer ham to a roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar with enough yellow mustard to make a smooth, pourable sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour reserved pineapple juice over ham, followed by the honey.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour brown sugar mixture over honey and arrange pineapple rings decoratively atop ham. Secure pineapple rings with toothpicks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until diamond pattern on ham splits open, 1 to 1 1/2 hours. Baste ham every 15 minutes with pan drippings.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Ham",
            "Whole"
        ],
