    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10335/peanut-butter-choco-chip-cookies/",
        "name": "Peanut Butter Choco Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8571306.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-01-14T09:37:27.000Z",
        "description": "I hope this will be of help to all those cookie lovers out there who can't  consume flour.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups peanut butter",
            "2 cups white sugar",
            "4 eggs",
            "2 cups semisweet chocolate chips",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine peanut butter, sugar and eggs. Mix well with wooden spoon till smooth and well-blended. Stir in chocolate chips and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded tablespoonfuls on ungreased cookie sheets two inches apart. Bake 10 to 12 minutes or till lightly browned. Cool on cookie sheets for a few minutes than transfer to wire rack. Cool completely. Store in airtight containers up to a week\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
