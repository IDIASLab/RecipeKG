        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9856/peanut-butter-crispies-i/",
        "name": "Peanut Butter Crispies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1077121.jpg",
