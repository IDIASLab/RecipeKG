    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274939/cranberry-white-chocolate-oatmeal-bars/",
        "name": "Cranberry-White Chocolate Oatmeal Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cranberry-White Chocolate Oatmeal Bars"
        },
        "datePublished": "2020-03-05T05:20:18.000Z",
        "description": "Delicious bars with oatmeal, dried cranberries, walnuts, and white chocolate.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1 cup unsifted all-purpose flour",
            "1 cup quick-cooking oats",
            "\u00be cup brown sugar",
            "\u00bd cup butter, softened",
            "1 (14 ounce) can sweetened condensed milk",
            "1 cup sweetened dried cranberries (such as Craisins\u00ae)",
            "1 cup chopped walnuts",
            "1 cup white chocolate chips",
            "1 tablespoon grated orange zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, oats, brown sugar, and butter in a large bowl. Set 1/2 cup of the mixture aside. Press remaining mixture over the bottom of a 9x13-inch glass baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until firm, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour sweetened condensed milk over the crust. Sprinkle cranberries, walnuts, chocolate chips, and orange zest over the milk. Top with the reserved oat mixture; press down lightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue baking until lightly browned, about 25 minutes. Cool completely before cutting into 12 bars.\n"
            }
        ],
