        "name": "Microwave Garlic-and-Herb Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7866064.jpg",
            "width": 0,
