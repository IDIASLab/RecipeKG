    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11145/sesame-thumbprint-cookies/",
        "name": "Sesame Thumbprint Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3455355.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1998-11-20T04:15:34.000Z",
        "description": "Easy to make sesame seed coated shortbread-type of cookie with jam or preserves in the middle. You choose your favorite fruit. Very tasty.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "\u00bc cup white sugar",
            "1 teaspoon almond extract",
            "\u00bd teaspoon salt",
            "2 cups all-purpose flour",
            "2 tablespoons sesame seeds",
            "\u00be cup any flavor fruit jam"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter and sugar together. Add the almond extract and the salt. Stir in the flour and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape tablespoonfuls of the dough into walnut-sized balls then roll the balls in sesame seeds to cover completely. Place balls on ungreased cookie sheets and flatten slightly. With your thumb, indent the center of each cookie. Fill indentations with jam or preserves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) for 10 to 12 minutes. Remove cookies from baking sheets and let cool on a rack. Store cooled cookies in an air-tight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Thumbprint Cookie Recipes"
        ],
