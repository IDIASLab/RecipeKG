    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8234/one-bowl-cheesecake/",
        "name": "One-Bowl Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F197898.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-05-16T12:08:25.000Z",
        "description": "A simple no crust cheesecake with sour cream topping.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 deep dish pie plate",
        "recipeIngredient": [
            "3 (8 ounce) packages cream cheese",
            "1 cup white sugar",
            "\u00bc teaspoon almond extract",
            "\u00bc teaspoon salt",
            "5 eggs",
            "1 cup sour cream",
            "2 tablespoons white sugar",
            "\u00bc teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).  In a large bowl, combine cream cheese, 1 cup sugar, almond extract, eggs and salt. Beat until smooth. Pour into a greased deep dish pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F  (165 degrees C) for 45 to 50 minutes. Remove from oven and let cool for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Topping: combine sour cream, 2 tablespoons sugar and 1/4 teaspoon vanilla extract and stir until smooth. Spread evenly on top of the baked cheesecake, then bake 10 minutes at 325 degrees F (165 degrees C) to set the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cool, refrigerate several hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cheesecake Recipes"
