    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10679/neapolitan-cookies-ii/",
        "name": "Neapolitan Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1844913.jpg",
            "width": 2272,
            "height": 2272
        },
        "datePublished": "1998-03-14T19:00:36.000Z",
        "description": "Refrigerator roll and slice cookie, a variation of the Spumoni Cookie (without pistachio and green food color). Almonds can be used in place of the walnuts.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 1/2 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "1 egg",
            "2\u2009\u00bc cups all-purpose flour",
            "\u00bd teaspoon salt",
            "\u00bc cup chopped walnuts, toasted",
            "1 (1 ounce) square unsweetened chocolate, melted",
            "2 tablespoons maraschino cherries, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together butter or margarine, sugar, vanilla and egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour and salt. Divide dough into 3 equal parts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix nuts into one part. Mix melted and cooled chocolate into the second part. Mix cherries into remaining part.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line bottom and ends of loaf pan (9 x 5 x 3 inches) with aluminum foil; allow ends of foil to extend 5 inches over end of pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press nut dough evenly in bottom of pan. Press chocolate dough on top of nut dough. Press cherry dough on top of chocolate dough. Cover dough with the extended ends of foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate at least 2 hours but no longer than 6 weeks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove foil and dough from pan and remove foil from dough. Cut dough crosswise into 1/4 inch slices and cut each slice across the dough layers into half so that each half contains three layers. Place 1 inch apart on ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until set (about 10 minutes).   Immediately remove from cookie sheet.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
