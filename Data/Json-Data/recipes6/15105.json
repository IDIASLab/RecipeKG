    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18250/barbaras-hot-sauce/",
        "name": "Barbara's Hot Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3502938.jpg",
            "width": 5472,
            "height": 3648
        },
        "datePublished": "2000-04-27T13:48:52.000Z",
        "description": "Texans love Mexican food, especially the hot sauce. We judge a Mexican restaurant by the hot sauce served before the meal. This is my version. Spices can be adjusted to taste.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT3H0M",
        "totalTime": "P0DT3H20M",
        "recipeYield": "30 or more servings",
        "recipeIngredient": [
            "64 ounces canned crushed tomatoes",
            "2 cups minced onion",
            "6 jalapeno peppers, chopped",
            "2 teaspoons ground cumin",
            "2 tablespoons salt",
            "2 tablespoons white sugar",
            "\u00bd cup white vinegar",
            "1 teaspoon minced garlic",
            "1 (15 ounce) can tomato sauce",
            "\u00bd (6 ounce) can tomato paste",
            "\u00bd cup ketchup",
            "1 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large soup or stock pot, combine tomatoes, onion, jalapenos, cumin, salt, sugar, vinegar, garlic, tomato sauce, tomato paste, ketchup, and water (add enough water to bring the mixture to your desired consistency). Bring mixture to a slow boil, reduce heat and let simmer for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let the mixture cool completely, then pour into individual jars. Store in refrigerator. Will keep for several weeks.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
