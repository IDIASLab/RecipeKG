        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24315/creamed-cooked-cabbage/",
        "name": "Creamed Cooked Cabbage",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F818133.jpg",
            "width": 2048,
            "height": 1536
