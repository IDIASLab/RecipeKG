    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280352/apple-stuffed-french-toast/",
        "name": "Apple-Stuffed French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple-Stuffed French Toast"
        },
        "datePublished": "2020-05-28T05:27:24.000Z",
        "description": "Full of sugary, spiced flavor that's sure to make your mornings brighter, this baked apple-stuffed French toast is easy to prepare and enjoy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 9x13-inch pan",
        "recipeIngredient": [
            "6 medium Granny Smith apples - peeled, cored, and thinly sliced",
            "\u00bd cup white sugar",
            "2 tablespoons ground cinnamon",
            "1 cup unsalted butter",
            "2 cups packed brown sugar",
            "1 tablespoon water",
            "1 cup chopped walnuts",
            "12 slices day-old French bread",
            "6 large eggs",
            "1\u2009\u00bd cups milk",
            "2 tablespoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Set a rack in the lower third of the oven and preheat to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place apples in a saucepan; cover with water. Bring to a boil; allow to cook for 2 to 3 minutes. Drain apples and toss with white sugar and cinnamon. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter with brown sugar and 1 tablespoon of water in a saucepan over medium-low heat. Add walnuts and simmer for 10 minutes. Pour into the prepared baking pan and let cool, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover butter mixture with a layer of bread. Spread apple mixture evenly on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs, milk, and vanilla extract together in a large mixing bowl. Dip remaining bread slices in egg mixture, then place in a layer over the apple mixture. Pour remaining egg mixture over bread, filling the pan nearly to the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and puffed and a knife inserted into the center comes out clean, 40 to 50 minutes. Remove from the oven and let cool for 10 minutes, then turn out onto a cookie sheet. Slice and serve.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "French Toast Recipes"
        ],
