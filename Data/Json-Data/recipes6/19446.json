    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/58934/hobo-bread/",
        "name": "Hobo Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6542693.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-06-19T03:09:51.000Z",
        "description": "Two kinds of raisins beautify this humble quick bread. This is an unusual bread baked in 3 coffee cans. It must be started the night before.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT9H15M",
        "recipeYield": "3 (1 pound) coffee cans",
        "recipeIngredient": [
            "1 cup raisins",
            "1 cup golden raisins",
            "4 teaspoons baking soda",
            "2 cups boiling water",
            "4 cups all-purpose flour",
            "2 cups sugar",
            "\u00bd teaspoon salt",
            "\u00bc cup vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the dark and golden raisins in a medium bowl. Dissolve baking soda in boiling water, and pour over raisins. Let stand overnight without stirring.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease three 1 pound sized coffee cans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, stir together the flour, sugar and salt. Stir in the raisins with their water, and the oil until well blended. Batter will be thick. Divide the batter evenly between the 3 cans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 hour in the preheated oven, or until a toothpick inserted into the crown comes out clean. Lay cans on their side to loosen the bread as it cools.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
