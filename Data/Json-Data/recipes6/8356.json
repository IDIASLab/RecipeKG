    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279854/cheesy-chicken-casserole/",
        "name": "Cheesy Chicken Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8125365.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-24T18:43:47.000Z",
        "description": "Quick, easy, cheesy, and crunchy chicken casserole! A great comfort food.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 casserole",
        "recipeIngredient": [
            "1 cup water",
            "1 cup chicken broth",
            "1 cup uncooked white rice",
            "1\u2009\u00bd cooked boneless chicken breasts, shredded, or more to taste",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (8 ounce) can water chestnuts, drained and chopped",
            "2 cups shredded Cheddar cheese",
            "\u00be cup chopped fresh broccoli",
            "\u00be cup chopped mushrooms",
            "1 pinch cayenne pepper, or to taste",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring water, chicken broth, and rice to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until rice is tender and water has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While rice is cooking, preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place shredded chicken in a large casserole dish. Add soup, water chestnuts, Cheddar cheese, broccoli, mushrooms, cayenne, salt, and pepper. Stir well, then spread the top so it's even.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until nicely browned and bubbly, 30 to 45 minutes.\n"
            }
        ],
