        "mainEntityOfPage": "https://www.allrecipes.com/recipe/270577/beer-floats/",
        "name": "Beer Floats",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6348105.jpg",
            "width": 5184,
            "height": 3456
