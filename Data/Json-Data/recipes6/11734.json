    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9438/eggnog-cookies-i/",
        "name": "Eggnog Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7497544.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "What a delectable treat for Eggnog lovers everywhere! These cookies have a wonderful spicy aroma and are great tasting. The fragrance of nutmeg and cinnamon will infuse your home with holiday spirit as you bake these cookies. They make delicious gifts and are a welcome addition to cookie exchanges.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups white sugar",
            "\u00be cup butter, softened",
            "\u00bd cup eggnog",
            "1 teaspoon vanilla extract",
            "2 egg yolks",
            "1 teaspoon ground nutmeg",
            "2\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, cinnamon and nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar and butter until light. Add eggnog, vanilla, and egg yolks; beat at medium speed with mixer until smooth.  Add flour mixture and beat at low speed until just combined. Do not overmix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto ungreased cookie sheet 1 inch apart. Sprinkle lightly with nutmeg.  Bake 20 to 23 minutes until bottoms turn light brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Spice Cookie Recipes"
        ],
