    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9637/applesauce-walnut-bars/",
        "name": "Applesauce Walnut Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1915464.jpg",
            "width": 2560,
            "height": 1418
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Very moist and chewy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "48 bars",
        "recipeIngredient": [
            "2 (1 ounce) squares unsweetened chocolate, chopped",
            "1 cup all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd cup shortening",
            "1\u2009\u00bc cups white sugar",
            "2 eggs",
            "\u00bd cup applesauce",
            "1 teaspoon vanilla extract",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 13x9 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate in the microwave or in a bowl over a pan of simmering water. Stir frequently until smooth. Remove from heat, and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the shortening and sugar until smooth.  Beat in the eggs one at a time, stirring constantly until well blended. In a separate bowl, mix together  applesauce, melted chocolate and vanilla.  Add to the egg mixture, stirring until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the flour, baking powder, cinnamon and salt; gradually stir into the batter. Fold in chopped walnuts. Spread evenly into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 to 25 minutes in the preheated oven, or until a toothpick inserted in the middle of cake comes out clean.  Let cool in pan before cutting.\n"
            }
        ],
