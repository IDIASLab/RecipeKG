    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9593/old-fashioned-oatmeal-cookies/",
        "name": "Old Fashioned Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F128651.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These are the best oatmeal cookies I have ever eaten.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup raisins",
            "1 cup water",
            "\u00be cup shortening",
            "1\u2009\u00bd cups white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon ground cloves",
            "2 cups rolled oats",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Simmer raisins and water over medium heat until raisins are plump, about 15 minutes.  Drain raisins, reserving the liquid.  Add enough water to reserved liquid to measure 1/2 cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the shortening, sugar, eggs and vanilla thoroughly.  Stir in reserved liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend in the remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded teaspoons about 2 inches apart onto ungreased baking sheets.  Bake 8 to 10 minutes or until light brown.\n"
            }
        ],
