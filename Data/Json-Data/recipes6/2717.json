    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/256606/whole-wheat-chocolate-waffles/",
        "name": "Whole Wheat Chocolate Waffles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Whole Wheat Chocolate Waffles"
        },
        "datePublished": "2020-03-05T05:16:01.000Z",
        "description": "Bored of peanut butter and Nutella&reg; sandwiches? Say hello to chocolate-filled waffles for your wholesome and delicious breakfast or brunch!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H18M",
        "recipeYield": "4 waffles",
        "recipeIngredient": [
            "\u00bd cup all-purpose flour",
            "\u00bd cup whole wheat flour",
            "1 tablespoon flax seeds",
            "1 tablespoon hemp seeds",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00bd cup low-fat milk, warmed",
            "2 eggs",
            "1 tablespoon vegetable oil",
            "1 teaspoon vanilla extract",
            "\u00bc cup low-fat milk",
            "3 tablespoons dark brown sugar",
            "3 tablespoons unsweetened cocoa powder",
            "1 tablespoon unsalted butter",
            "cooking spray"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk all-purpose flour, whole wheat flour, flax seeds, hemp seeds, baking powder, cinnamon, and salt together in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk 1/2 cup warm milk, eggs, vegetable oil, and vanilla extract together in a separate bowl. Pour over flour mixture; mix until batter is just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/4 cup milk, brown sugar, cocoa powder, and butter in a small microwave-safe bowl. Heat in the microwave until butter is melted, 20 to 30 seconds. Stir well until sauce is smooth. Pour half of the sauce into another bowl; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat a waffle iron according to manufacturer's instructions. Grease with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread a thin layer of batter in the iron using a spatula. Pour some of the chocolate sauce on top. Cover with another layer of batter. Close and cook until golden brown, 3 to 5 minutes. Repeat with remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drizzle reserved chocolate sauce over waffles before serving.\n"
            }
        ],
