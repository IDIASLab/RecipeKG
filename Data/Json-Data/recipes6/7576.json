        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix yellow cake mix, chocolate chips, oil, eggs, and brown sugar together in a bowl. Pour into an ungreased 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 20 to 25 minutes.\n"
            }
