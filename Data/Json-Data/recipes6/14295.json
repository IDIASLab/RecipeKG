        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15786/pumpkin-butter-ii/",
        "name": "Pumpkin Butter II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F565046.jpg",
            "width": 1434,
            "height": 1585
        },
        "datePublished": "1999-10-23T18:23:17.000Z",
        "description": "This lovely spread will be gobbled up by children and grown-ups alike. Put it on the breakfast table during the holidays for a seasonal treat.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "3\u2009\u00bd cups pumpkin puree",
            "4\u2009\u00bd cups white sugar",
            "1 tablespoon pumpkin pie spice",
            "1 (.25 ounce) envelope unflavored gelatin"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a 2 quart saucepan, combine the pumpkin, sugar, pumpkin pie spice and gelatin; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place over medium high heat and bring to a boil. Reduce heat and simmer, stirring constantly for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer to sterile containers, and chill in the refrigerator until serving.\n"
            }
        ],
