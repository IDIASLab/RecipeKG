    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18168/boston-brown-bread-ii/",
        "name": "Boston Brown Bread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F620687.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-02-22T16:52:02.000Z",
        "description": "This bread is so tasty because the raisins soak overnight and then release their moisture into the bread as it bakes. It's a great bread for raisin lovers.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT14H30M",
        "recipeYield": "1 - 9x5 inch loaf",
        "recipeIngredient": [
            "1 cup white sugar",
            "1\u2009\u00bd cups raisins",
            "2 teaspoons baking soda",
            "1 teaspoon salt",
            "1 tablespoon margarine",
            "1\u2009\u00bd cups boiling water",
            "1 egg",
            "2 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together sugar, raisins, baking soda, salt, margarine and boiling water. Cover and let stand overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "The following day, preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg and flour into raisin mixture. Pour batter into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 hour, or until a toothpick inserted into center of the loaf comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
        "recipeCuisine": [],
        "author": [
