    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15118/cheese-soup-with-broccoli/",
        "name": "Cheese Soup with Broccoli",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8143010.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-01-16T15:55:20.000Z",
        "description": "A great Cheese Soup recipe. It can be used as a light meal with salad, but we like it best with ham.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 onion, chopped",
            "6 tablespoons margarine",
            "\u2153 cup all-purpose flour",
            "salt and pepper to taste",
            "4 cups milk",
            "3 cups chicken broth",
            "1 carrot, shredded",
            "1 cup broccoli florets",
            "\u00bd cup chopped celery",
            "1 pound processed cheese food (eg. Velveeta), cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan on medium high heat, saute the onions in the butter or margarine until tender. Add the flour and salt and pepper to taste. Mix until smooth. Add the milk slowly, until mixture is thick and bubbling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a smaller saucepan, bring the chicken broth to a boil. Add the carrot, celery, and broccoli. Cook about 5 minutes, or until tender. Combine the broth mixture with the milk mixture and stir until fully blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the cheese. Allow soup to heat over medium heat until cheese is melted. Important: DO NOT let the soup boil because the cheese will separate and curdle the soup. Serve hot and enjoy!\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Broccoli Soup Recipes"
        ],
