    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233611/teriyaki-beef-jerky/",
        "name": "Teriyaki Beef Jerky",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5739843.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2020-03-05T05:10:50.000Z",
        "description": "Strips of beef are marinated in teriyaki sauce, soy sauce, and a blend of pineapple and garlic in this easy smoked jerky recipe.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT6H0M",
        "totalTime": "P0DT12H15M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 cups teriyaki sauce",
            "1 cup soy sauce",
            "1 cup brown sugar",
            "1 dash Worcestershire sauce",
            "\u00bc pound fresh pineapple, peeled",
            "2 cloves garlic",
            "2 pounds beef round, cut into 1/2-inch thick strips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix teriyaki sauce, soy sauce, brown sugar, and Worcestershire sauce in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pineapple and garlic in a food processor or blender; process until smooth. Pour pineapple mixture and teriyaki mixture into a resealable plastic bag. Add beef, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator for 6 to 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat smoker to 225 to 250 degrees F (110 to 120 degrees C). Drain beef and discard marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Smoke beef in the preheated smoker until jerky is dry and chewy but not crispy, 6 to 8 hours. Store jerky in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Jerky Recipes"
        ],
