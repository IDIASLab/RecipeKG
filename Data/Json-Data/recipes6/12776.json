    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11055/old-time-soft-sugar-cookies/",
        "name": "Old Time Soft Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F583400.jpg",
            "width": 1725,
            "height": 1731
        },
        "datePublished": "1998-10-01T04:26:27.000Z",
        "description": "Delicious and easy drop sugar cookie!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "1 cup white sugar",
            "1 egg",
            "\u00be cup buttermilk",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together the shortening and the sugar. Stir in the egg, buttermilk and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the flour, baking soda and salt. Add the flour mixture to the creamed mixture and stir to combine. Chill dough in the refrigerator for 1 hour. Drop mixture by rounded teaspoons 2 inches apart on a lightly greased baking sheet. If desired sprinkle tops of cookies with white sugar before  baking.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) for 7 to 9 minutes or until set.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
