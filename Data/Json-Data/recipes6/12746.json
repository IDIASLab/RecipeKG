        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11015/devil-dogs/",
        "name": "Devil Dogs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7015651.jpg",
