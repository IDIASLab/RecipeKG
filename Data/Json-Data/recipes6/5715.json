    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229432/smoked-turkey-rice-soup/",
        "name": "Smoked Turkey Rice Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6253446.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-03-10T06:52:00.000Z",
        "description": "A great way to use the carcass of a holiday smoked turkey.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 smoked turkey carcass",
            "1 (32 fluid ounce) container vegetable broth",
            "2 cups water",
            "2 cups chopped fresh green beans",
            "1\u2009\u00bd cups frozen corn",
            "1 cup shredded carrot",
            "1 cup white rice",
            "\u00bd cup chopped celery, or to taste",
            "\u00bd cup chopped onion",
            "\u00bd cup chopped green bell pepper",
            "\u00bd cup chopped red bell pepper",
            "1 (4 ounce) package cream cheese, or more to taste",
            "1 cup shredded Cheddar cheese, or to taste",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place turkey carcass in a large soup pot and pour vegetable broth and water over the turkey frame. Bring to a boil, reduce heat to low, and simmer until meat falls from the bones, 1 to 2 hours. Remove carcass, pick the meat from the bones, and discard bones. Chop turkey meat and return to broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir green beans, corn, carrot, rice, celery, onion, green bell pepper, and red bell pepper into soup; bring to a simmer and cook until celery is tender and rice is cooked, about 1 more hour. Stir cream cheese and Cheddar cheese into soup until melted and smooth. Season with black pepper.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Turkey Soup Recipes"
        ],
