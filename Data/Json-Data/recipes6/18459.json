    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/38647/orange-buttermilk-salad/",
        "name": "Orange Buttermilk Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1013952.jpg",
            "width": 3387,
            "height": 3387
        },
        "datePublished": "2020-06-19T03:07:50.000Z",
        "description": "A refreshing chilled salad with orange gelatin, pineapple, buttermilk, and whipped topping.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (20 ounce) can crushed pineapple, undrained",
            "3 tablespoons white sugar",
            "1 (6 ounce) package orange flavored Jell-O\u00ae",
            "2 cups buttermilk",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix pineapple and sugar together in a medium saucepan, and bring to a low boil, stirring occasionally. Stir in gelatin until completely dissolved, remove from heat, and cool to room temperature. Add buttermilk, and refrigerate 1 hour, or until partially set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently fold in whipped topping. Pour mixture into a lightly oiled gelatin mold. Refrigerate 8 hours, or until completely set.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Dessert Salad Recipes",
            "Jell-O&reg; Salad"
        ],
