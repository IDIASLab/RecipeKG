        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10181/ranger-cookies-i/",
        "name": "Ranger Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4583921.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1997-11-30T07:26:38.000Z",
        "description": "This is an old recipe from the 20's and 30's.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 cup flaked coconut",
            "1 teaspoon baking soda",
            "1 cup shortening",
            "\u00bd teaspoon salt",
            "1 cup white sugar",
            "\u00bd teaspoon baking powder",
            "1 cup packed brown sugar",
            "2 cups rolled oats",
            "2 eggs",
            "2 cups crisp rice cereal",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream shortening, add sugar gradually.  Add eggs and vanilla; beat until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, salt, and baking powder together and mix with the first mixture.  Add oatmeal, rice cereal, and coconut.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by tablespoons on slightly greased cookie sheet and bake for 12 minutes.\n"
            }
        ],
