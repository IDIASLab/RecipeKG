    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19846/daiquiri-chiffon-cheesecake-with-pretzel-crust/",
        "name": "Daiquiri Chiffon Cheesecake with Pretzel Crust",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F35549.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2000-07-13T05:09:18.000Z",
        "description": "Try this delicious pretzel crust cheesecake!  You'll be glad you did!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT8H30M",
        "recipeYield": "1 - 9 inch cheesecake",
        "recipeIngredient": [
            "1\u2009\u00bc cups crushed pretzels",
            "\u00bd cup white sugar",
            "\u215c cup butter, melted",
            "1 (8 ounce) package cream cheese",
            "1\u2009\u00bd cups milk",
            "1 (3.5 ounce) package instant vanilla pudding mix",
            "\u00bd cup lime juice",
            "1 teaspoon rum flavored extract",
            "2 teaspoons grated lime zest",
            "1 teaspoon grated lemon zest",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "lime zest, garnish",
            "2 limes, thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Crust: In a medium bowl, combine pretzel crumbs, sugar and butter. Press mixture into the bottom and 1 3/4 inches up the side of a 9 inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for about 10 minutes, or until firm and lightly browned. Remove from oven and let cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Filling: Allow cream cheese to reach room temperature. In a large bowl, beat cream cheese until smooth. In a separate bowl, combine pudding mix and milk, then mix in with cream cheese. Beat in lime juice, rum extract and lime and lemon zest.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold 1/2 of the whipped topping into the mixture, then pour into cooled crust. Cover and refrigerate for at least 8 hours. Garnish with remaining whipped topping, lime zest and slices, if desired.\n"
            }
        ],
