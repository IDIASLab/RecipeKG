    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16213/sherry-chiffon-pie/",
        "name": "Sherry Chiffon Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sherry Chiffon Pie"
        },
        "datePublished": "1999-11-15T00:57:08.000Z",
        "description": "An oldie from my mother's recipe file. A heavenly cream pie accented with almonds and vanilla, surrounded by a chocolate cookie crust. This recipe contains raw eggs. We recommend that pregnant women, young children, the elderly and the infirm do not consume raw eggs.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) prepared chocolate cookie crumb crust",
            "1 (.25 ounce) package unflavored gelatin",
            "\u00bc cup cold water",
            "2 cups scalded milk",
            "\u215b teaspoon salt",
            "\u00bc cup white sugar",
            "2 eggs, separated",
            "\u00bd teaspoon almond extract",
            "2 tablespoons sherry",
            "\u00bc cup chopped almonds",
            "4 teaspoons white sugar",
            "\u00bd cup chopped almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soften gelatin in cold water for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a double boiler pan combine milk, salt, and 1/4 cup sugar. Stir until dissolved. Whisk in egg yolks and cook in double boiler until thickened, stirring constantly. Stir gelatin into custard mixture until thoroughly dissolved. Cool mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir almond extract, sherry, and 1/4 cup almonds into custard.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium mixing bowl whip egg whites until frothy. Add 4 teaspoons of sugar, one at a time, whipping constantly until whites form soft peaks. Fold egg whites into custard.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour custard mixture into pie shell. Sprinkle with chopped almonds. Serve chilled.\n"
            }
        ],
