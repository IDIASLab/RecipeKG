    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25596/lemon-pudding-cake-ii/",
        "name": "Lemon Pudding Cake II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1132293.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2020-06-19T03:06:19.000Z",
        "description": "A light cake that bakes on top of lemon pudding.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 8 inch square pan",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "1 pinch salt",
            "3 tablespoons butter, melted",
            "\u00bc cup sifted all-purpose flour",
            "1 teaspoon grated lemon zest",
            "\u00bc cup lemon juice",
            "1\u2009\u00bd cups milk",
            "3 egg yolks",
            "3 egg whites"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour an 8 inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine sugar, salt and melted butter. Beat in the flour. Stir in the lemon zest and lemon juice. Combine milk and egg yolks, and add to lemon mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal mixing bowl, beat egg whites until stiff peaks form. Fold 1/3 of the whites into the batter, then quickly fold in remaining whites until no streaks remain. Pour batter into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set cake pan in a water bath. Bake in the preheated oven for 40 minutes, or until top is lightly browned. Serve warm or cold.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Lemon Dessert Recipes"
        ],
