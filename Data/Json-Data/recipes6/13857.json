        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14184/corn-and-black-bean-salad/",
        "name": "Corn and Black Bean Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F245445.jpg",
