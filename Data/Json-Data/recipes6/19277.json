    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/55859/date-and-orange-slice-bars/",
        "name": "Date and Orange Slice Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Date and Orange Slice Bars"
        },
        "datePublished": "2020-06-19T03:09:36.000Z",
        "description": "These are awesome cookies - My mother used to make these every Christmas!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "24 bar cookies",
        "recipeIngredient": [
            "8 ounces chopped pitted dates",
            "\u00bd cup white sugar",
            "2 tablespoons all-purpose flour",
            "1 cup water",
            "\u00be cup shortening",
            "1 cup packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 teaspoon baking soda",
            "2 tablespoons hot water",
            "1\u2009\u00be cups all-purpose flour",
            "\u00bc teaspoon salt",
            "2 (10.5 ounce) packages orange slice candies, cut in half lengthwise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan combine the dates, 1/2 cup of sugar, 2 tablespoons of flour, and water. Bring to a boil, and cook until thickened, about 10 minutes. Remove from heat, and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the shortening and brown sugar until smooth. Beat in the eggs one at a time, then stir in the vanilla. Dissolve the baking soda in hot water, then stir into the creamed mixture. Combine 1 3/4 cups of flour and salt; stir into the batter just until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread half of the batter in the bottom of the prepared pan. Cover with a layer of  orange slice candy, then spread the date mixture over the orange candy. Cover with the remaining batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 minutes in the preheated oven, or until golden brown. Cool, and cut into bars.\n"
            }
        ],
