    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10167/smore-bars-i/",
        "name": "S'More Bars I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7480071.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-11-26T04:44:09.000Z",
        "description": "These taste amazingly like the ones we use to make sitting by the fire.  You could use chocolate chips (semi-sweet or milk chocolate) instead of the chocolate bar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "\u00bd cup butter, softened",
            "1 egg",
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1\u2009\u00bd cups milk chocolate candy bar, chopped",
            "3 cups miniature marshmallows",
            "\u00bd cup chopped peanuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).  Lightly grease 13 x 9 x 2 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixer bowl combine sugar, butter and egg until creamy (2 or 3 minutes).   Reduce mixer speed and add flour, baking powder and salt until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into baking pan.   Bake for 10 to 12 minutes, until set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle chopped chocolate bar  over batter and top with marshmallows and peanuts.  Continue baking 10 to 12 minutes until marshmallows are lightly browned.  Cool and cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
