    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9701/better-than-grandmas-brownies/",
        "name": "Better than Grandma's Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F19336.jpg",
            "width": 500,
            "height": 375
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "These are the best, but don't tell grandma!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "6 (1 ounce) squares unsweetened chocolate",
            "\u00be cup butter",
            "4 eggs",
            "2 tablespoons vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "2\u2009\u00be cups white sugar",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F.  Grease and flour a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In double boiler over low heat, melt the chocolate and butter together, stirring frequently until smooth.  Remove from heat and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, mix together eggs, vanilla, salt and sugar. Beat with an electric mixer for 8 to 10 minutes. Slowly pour in chocolate mixture and flour. Stir in the walnuts.  Spread batter evenly in the prepared pan\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 17 to 20 minutes in the preheated oven, until the top is crackled. Let cool for 2 hours before cutting into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Nut Brownie Recipes"
        ],
