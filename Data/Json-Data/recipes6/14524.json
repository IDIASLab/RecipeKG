    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16422/asian-chicken-noodle-salad/",
        "name": "Asian Chicken Noodle Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F906130.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-03-11T00:04:24.000Z",
        "description": "This easy to prepare noodle salad has plenty of seeds, nuts, greens, chicken, and vegetables.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (3 ounce) package ramen noodle pasta, crushed",
            "2 tablespoons butter, melted",
            "\u00bd cup sunflower seeds   ",
            "\u00bd cup pine nuts",
            "3 cups shredded bok choy",
            "5 green onions, thinly sliced",
            "1 cup diced, cooked chicken breast meat",
            "1 (5 ounce) can water chestnuts, drained",
            "12 pods snow peas",
            "\u00bd cup vegetable oil",
            "\u00bc cup rice wine vinegar",
            "1 tablespoon soy sauce",
            "\u00bc cup white sugar",
            "1 tablespoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). In a large bowl, mix the noodles, sunflower seeds, and pine nuts with melted butter until evenly coated. Spread the mixture in a thin layer on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 7 to 10 minutes in the preheated oven, stirring occasionally, until evenly toasted. Remove from heat, and cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl toss together the noodle mixture, bok choy, green onions, chicken, water chestnuts, and snow peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare the dressing by blending the oil, vinegar, soy sauce, sugar, and lemon juice. Pour over salad, and toss to evenly coat. Serve immediately, or refrigerate until chilled.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Green Salad Recipes"
        ],
