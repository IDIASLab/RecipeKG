    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10349/cut-out-cookies/",
        "name": "Cut-Out Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1058830.jpg",
            "width": 2340,
            "height": 2340
        },
        "datePublished": "1997-12-19T02:11:56.000Z",
        "description": "Given to me by an aunt that made the most delicious Christmas cookies I have ever tasted!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "3\u2009\u00bd cups all-purpose flour",
            "1 cup shortening",
            "1 cup white sugar",
            "2 teaspoons baking powder",
            "2 eggs",
            "2 teaspoons vanilla extract",
            "1 teaspoon salt",
            "1 cup confectioners' sugar",
            "2 tablespoons water",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together flour, shortening, white sugar, baking powder and eggs.  Stir in 2 teaspoons vanilla and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate dough for easier roll-out. Bake at 375 degrees F (190 degrees C) for 8-10 minutes (watch carefully).  Add glaze to cooled cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Glaze:  Mix 1 cup confectioners' sugar, water (to desired consistency), 1/2 tsp. vanilla and preferred food color for cookie icing. Decorate with favorite sprinkles.\n"
            }
        ],
