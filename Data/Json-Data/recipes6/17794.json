    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/26226/sarahs-raisin-cookies/",
        "name": "Sarah's Raisin Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1060448.jpg",
            "width": 1300,
            "height": 960
        },
        "datePublished": "2020-06-19T03:06:54.000Z",
        "description": "A hint of almond flavor makes these special.  They don't last long in our cookie jar!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "3 eggs, beaten",
            "2 teaspoons vanilla extract",
            "\u00bd teaspoon almond extract",
            "1\u2009\u00bc cups raisins",
            "1 cup butter, softened",
            "1 cup light brown sugar",
            "1 cup white sugar",
            "2\u2009\u00bd cups all-purpose flour",
            "2 teaspoons baking soda",
            "\u00bd teaspoon salt",
            "1 teaspoon ground cinnamon",
            "2 cups quick-cooking oats"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine the eggs, vanilla extract, almond extract and raisins. Cover and refrigerate for at least one hour. Preheat oven to 350 degrees F (175 degrees C). Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the butter, brown sugar and white sugar until smooth. Combine the flour, baking soda, salt and cinnamon; stir into the sugar mixture. Mix in the raisin mixture until well blended. Stir in oats. Roll into 1 inch balls and place them 2 inches apart onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10 minutes in the preheated oven. Allow cookies to cool on baking sheet for 5 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Spice Cookie Recipes"
        ],
