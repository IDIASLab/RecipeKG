    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13551/joeys-bread-pudding/",
        "name": "Joey's Bread Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F882147.jpg",
            "width": 2188,
            "height": 1741
        },
        "datePublished": "1998-11-09T21:47:23.000Z",
        "description": "This is a very custardy bread pudding. Very ONO-licious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 tablespoons butter",
            "1 (1 pound) loaf sliced white bread, toasted",
            "12 eggs",
            "3 tablespoons vanilla extract",
            "5 cups half-and-half cream",
            "3 cups white sugar",
            "1 pinch ground cinnamon",
            "1 pinch ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Butter each slice of bread on both sides.  Tear buttered bread into little pieces, and put into a very large mixing bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs with vanilla, and then pour over bread.  Stir in half-and-half and sugar.  Pour into a  9x13 inch baking dish.  Sprinkle the cinnamon and nutmeg over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered in the preheated oven for 55 to 60 minutes. Remove from oven, and cool.  Serve hot or cold.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Bread Pudding Recipes"
        ],
