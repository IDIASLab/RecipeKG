    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/25769/chocolate-blossoms/",
        "name": "Chocolate Blossoms",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Blossoms"
        },
        "datePublished": "2020-06-19T03:06:27.000Z",
        "description": "These are great little cookies that look very pretty when baked, they are kind of like Chocolate Crinkles.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bc cup butter, melted",
            "\u2153 cup unsweetened cocoa powder",
            "1 cup white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bc cup confectioners' sugar for rolling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the butter and cocoa until smooth. Beat in the sugar, eggs and vanilla until well blended. Combine the flour, baking soda and salt; gradually blend into the chocolate mixture to form a dough. Cover dough and chill for at least 2 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease cookie sheets. Roll dough into 1 inch balls and roll the balls in the confectioners' sugar. Place cookies 3 inches apart onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 12 minutes in the preheated oven, until the tops of the cookies have cracked. remove from baking sheets to cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
