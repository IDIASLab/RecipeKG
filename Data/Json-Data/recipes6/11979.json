        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9851/pfeffernuesse-cookie-mix/",
        "name": "Pfeffernuesse Cookie Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F942166.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-08-04T20:14:16.000Z",
        "description": "Spicy German cookie",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "4 cups all-purpose flour",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd cup white sugar",
            "\u00be cup light molasses",
            "1\u2009\u00bc teaspoons baking soda",
            "\u00bd cup butter",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "2 eggs",
            "\u00bd teaspoon ground cloves",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together flour, sugar, baking soda, spices, and dash black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large saucepan, combine molasses and butter; heat and stir until butter melts. Cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in eggs. Add dry ingredients to  molasses mixture; mix well. Cover. Chill several hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into 1\" balls. Place on greased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (180 degrees C) for  12 to 14 minutes or until cookies are done. Remove. Cool. Roll in powdered sugar.\n"
            }
        ],
