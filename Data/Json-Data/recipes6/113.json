        "recipeIngredient": [
            "1\u2009\u00bd cups ketchup",
            "\u00be cup brown sugar",
            "\u00bc cup bourbon whiskey",
            "\u00bc cup water",
            "1 (14 ounce) package beef smoky cocktail sausages"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, whisk the ketchup, brown sugar, bourbon whiskey, and water together until the sugar has dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the cocktail sausages, and place over medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring the sausages and sauce to a simmer; serve.\n"
            }
        ],
