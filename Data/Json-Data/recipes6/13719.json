    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13636/corn-pudding-iv/",
        "name": "Corn Pudding IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1014715.jpg",
            "width": 2299,
            "height": 2304
        },
        "datePublished": "1998-11-25T04:31:58.000Z",
        "description": "Fabulous Thanksgiving favorite. Rich, but worth every calorie! Give yourself some extra baking time just in case your oven varies. Can be halved (use 3 eggs).",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 (15 ounce) cans creamed corn",
            "2 (15.25 ounce) cans whole kernel corn",
            "5 eggs, beaten",
            "1 pint half-and-half cream",
            "1 cup heavy whipping cream",
            "2 tablespoons all-purpose flour",
            "2 tablespoons cornmeal",
            "2 tablespoons white sugar",
            "\u00bc cup butter, melted",
            "salt to taste",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cream-style corn, regular corn, eggs, half-and-half, cream, flour, cornmeal, sugar, melted butter, salt, and pepper in a 2-1/2 to 3-quart casserole.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 to 350 degrees F (165 to 175 degrees C) for 1 hour or until set.\n"
            }
        ],
