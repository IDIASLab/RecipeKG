    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279832/smoked-salmon-and-lentil-salad/",
        "name": "Smoked Salmon and Lentil Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F395255.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-04-28T01:16:26.000Z",
        "description": "A luxurious twist on a wholesome and tasty lentil salad. Serve with fresh tomatoes, mesclun, and fresh bread.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "3 cups water",
            "1\u2009\u00bd cups dry lentils",
            "1 bouquet garni",
            "10 ounces smoked salmon, or more to taste",
            "\u00bd large red onion, minced",
            "1 tablespoon snipped fresh chives",
            "\u00bc cup olive oil",
            "1 tablespoon red wine vinegar",
            "1 teaspoon Dijon mustard",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine water, lentils, and bouquet garni in a large saucepan; bring to a boil. Cover, reduce heat to low, and simmer until lentils are tender but still firm and there is only a little excess cooking liquid in the pan, about 20 minutes. Remove from the heat and let lentils cool in the pan. Discard bouquet garni.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut salmon into small strips, no longer than 1/2 inch long. Place in a large bowl with onion and chives. Add cooled lentils.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, vinegar, mustard, salt, and pepper together in a small bowl. Add to the lentil mixture and stir until well combined. Serve immediately or refrigerate until flavors have blended, 2 to 3 hours.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Seafood Salad Recipes",
            "Salmon Salad Recipes"
        ],
