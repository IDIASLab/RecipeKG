    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/222568/4-bean-baked-beans/",
        "name": "4-Bean Baked Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970144.jpg",
            "width": 2479,
            "height": 2479
        },
        "datePublished": "2020-03-05T05:09:00.000Z",
        "description": "Four different kinds of beans baked with molasses. I have substituted different kinds of beans and enjoyed all of the results. Use whatever kind of beans you like! I have also used Canadian bacon instead of regular bacon.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H45M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 (15 ounce) can kidney beans, drained",
            "1 (15 ounce) can butter beans, drained",
            "1 (15 ounce) can lima beans, drained",
            "1 (16 ounce) can pork and beans, drained",
            "\u00bd pound bacon, cut into small pieces",
            "1 large onion, chopped",
            "2 cloves garlic, chopped",
            "\u00be cup brown sugar",
            "\u00bd cup ketchup",
            "\u00bd cup vinegar",
            "\u00bc cup molasses",
            "1 teaspoon dry mustard"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place kidney beans, butter beans, lima beans, and pork and beans in a 2-quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a saucepan over medium heat; cook and stir bacon, onion, and garlic until bacon is browned, about 10 minutes. Pour off excess fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk brown sugar, ketchup, vinegar, molasses, and mustard into bacon mixture; simmer until cooked through, about 20 minutes. Pour sauce over beans in the casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bubbling, 1 hour 15 minutes.\n"
            }
        ],
