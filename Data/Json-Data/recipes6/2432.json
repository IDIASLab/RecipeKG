    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246648/korean-kalbi-jjim-braised-beef-short-ribs/",
        "name": "Korean Kalbi Jjim (Braised Beef Short Ribs)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4558016.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2020-03-05T05:13:59.000Z",
        "description": "This is a traditional meat dish eaten on special occasions like holidays and large gatherings. Enjoy with rice and side dishes (banchan).",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H25M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 pounds beef short ribs, or more to taste",
            "1 onion, quartered",
            "1 (1 inch) piece fresh ginger, sliced ",
            "2 cloves garlic",
            "5 tablespoons soy sauce",
            "\u00bc cup brown sugar",
            "2 tablespoons Korean red pepper flakes (gochugaru)",
            "4 cloves garlic, minced",
            "1 tablespoon rice vinegar",
            "1 tablespoon sesame oil",
            "2 potatoes, peeled and cut into 2-inch pieces",
            "2 carrots, peeled and cut into 2-inch pieces",
            "\u00bd cup Japanese beech mushrooms",
            "7 chestnuts (bam), or more to taste",
            "7 dried Korean dates (daechu)",
            "1 tablespoon corn syrup (mulyeot)",
            "2 green onions, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak beef short ribs in a large bowl of cold water to remove residual blood, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place short ribs, onion, ginger, and 2 cloves garlic in a large pot and cover with water. Bring to a boil, skimming any foam that floats to the top, 20 to 30 minutes. Measure out 2 cups of broth and reserve. Drain short ribs and rinse under cold water; discard onion, ginger, and garlic. Cool until easily handled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make slits in each short rib to remove or cut away excess fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return short ribs to the pot. Add reserved broth, soy sauce, brown sugar, red pepper flakes, 4 minced garlic cloves, rice vinegar, and sesame oil. Simmer over low heat until flavors combine, about 45 minutes. Add potatoes, carrots, mushrooms, chestnuts, and dates. Continue simmering until potatoes are tender and sauce thickens, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir corn syrup into the sauce. Simmer until sauce has a slightly syrupy consistency, about 5 minutes more. Garnish with green onion slices and serve.\n"
            }
        ],
