    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279570/pineapple-tea-cookies/",
        "name": "Pineapple Tea Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4067698.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2020-04-13T21:03:24.000Z",
        "description": "This is similar to another recipe on here (Spring Lime Tea Cookies from Christina Pierson) only I've switched things up a bit swapping lime out for canned pineapple.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "18 cookies",
        "recipeIngredient": [
            "\u2153 cup milk",
            "2 teaspoons pineapple juice",
            "1 cup white sugar, divided",
            "\u00bd cup unsalted butter, softened",
            "1 large egg",
            "1\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "2 teaspoons crushed pineapple",
            "2 tablespoons pineapple juice",
            "1 tablespoon crushed pineapple"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk and 2 teaspoons pineapple juice in a bowl and let stand.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix 3/4 cup sugar and butter together in a large bowl. Mix in egg and stir in pineapple juice-milk mixture. Add flour, baking powder, and baking soda. Stir in 2 teaspoons crushed pineapple. Drop batter by rounded spoonfuls onto ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are light brown, 10 to 12 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cookies are baking, make glaze by mixing remaining 1/4 cup sugar, 2 tablespoons pineapple juice, and remaining 1 tablespoon crushed pineapple together in a bowl. Brush or spoon onto cooled cookies.\n"
            }
        ],
