    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13566/cranberry-nut-bread-i/",
        "name": "Cranberry Nut Bread I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7191224.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-11-01T03:35:32.000Z",
        "description": "A moist quick bread with a faint hint of orange flavor, and the tang of bits of cranberries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 9x5-inch loaf",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00be cup white sugar",
            "\u00be teaspoon salt",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "1 cup chopped cranberries",
            "\u00bd cup chopped walnuts",
            "1 egg",
            "2 tablespoons vegetable oil",
            "\u00be cup orange juice",
            "1 tablespoon orange zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the flour, sugar, salt, baking powder, and baking soda. Add the cranberries and walnuts, and stir to coat with flour. Mix together the egg, oil, orange juice, and orange zest. Pour the egg mixture into the flour mixture, and stir until just blended. Spoon the batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 50 minutes in the preheated oven, or until a toothpick inserted near the center comes out clean. Cool in pan for 10 minutes, then remove to a wire rack, and cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Fruit Bread Recipes",
            "Cranberry Bread Recipes"
        ],
