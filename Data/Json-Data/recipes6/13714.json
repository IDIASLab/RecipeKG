        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13623/cranberry-conserve/",
        "name": "Cranberry Conserve",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2885883.jpg",
