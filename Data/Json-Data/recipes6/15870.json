    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21594/berry-cobbler/",
        "name": "Berry Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9303043.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-09-19T01:55:24.000Z",
        "description": "Great dessert, and you can substitute blueberries, blackberries or cranberries.  I use  egg substitute.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 - 9x9 inch dish",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup white sugar",
            "2\u2009\u00bd teaspoons baking powder",
            "\u00bd teaspoon salt",
            "3 tablespoons butter, melted",
            ".66 cup milk",
            "1 teaspoon vanilla extract",
            "1 egg, beaten",
            "2 cups raspberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease a 9x9 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine flour, sugar, baking powder and salt.  Stir in butter, milk, vanilla and egg until smooth.  Fold in raspberries.  Pour into prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven 35 to 40 minutes, until top is firm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Raspberry Dessert Recipes"
        ],
