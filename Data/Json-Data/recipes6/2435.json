    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/246703/almond-brown-betty/",
        "name": "Almond Brown Betty",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Almond Brown Betty"
        },
        "datePublished": "2020-03-05T05:14:00.000Z",
        "description": "This is a new twist on an old dish. Very easy to prepare and sure to be hit with layers of apples, bread, brown sugar, and almonds. Top with whipped cream or vanilla ice cream.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H8M",
        "totalTime": "P0DT1H33M",
        "recipeYield": "1 9-inch baking dish",
        "recipeIngredient": [
            "5 slices bread",
            "\u00be cup brown sugar",
            "\u00bd cup crushed graham crackers",
            "\u00bd cup sliced almonds",
            "1 tablespoon ground cinnamon",
            "2 teaspoons ground nutmeg",
            "1 teaspoon salt",
            "\u00bd cup butter",
            "\u00bd teaspoon almond extract",
            "5 apples, peeled and cubed, or more to taste",
            "1 pear, peeled and cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toast slices of bread and cut into cubes. Place in a large bowl. Add brown sugar, graham crackers, almonds, cinnamon, nutmeg, and salt; mix to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a small saucepan over low heat until melted, about 3 minutes. Stir in almond extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread half the apples and pear in the bottom of a 9-inch baking dish. Top with 1/2 of the bread mixture. Drizzle 1/2 of the butter mixture on top. Repeat layers once more with remaining ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until apples are soft and bread topping is crunchy, about 1 hour.\n"
            }
        ],
