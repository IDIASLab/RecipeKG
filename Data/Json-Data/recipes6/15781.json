    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21131/berry-blue-corn-muffins/",
        "name": "Berry-Blue Corn Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F875616.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-07-13T22:55:12.000Z",
        "description": "Moist and tender corn muffins sweet with blueberries. My daughter Melissa's favorite! We serve them warm with butter.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "1 dozen muffins",
        "recipeIngredient": [
            "1\u2009\u00bc cups all-purpose flour",
            "1 cup yellow cornmeal",
            "\u00bd cup white sugar",
            "\u00bd teaspoon salt",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "\u00bc cup vegetable oil",
            "2\u2009\u00bd teaspoons vanilla extract",
            "1 egg",
            "1 cup buttermilk",
            "1\u2009\u00bd cups fresh blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Grease muffin cups or line with paper muffin liners.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine flour, cornmeal, sugar, salt, baking powder and baking soda. In a separate bowl, beat together oil, vanilla and egg. Stir egg mixture into dry ingredients alternating with the buttermilk just until moistened. Gently fold in the blueberries. Spoon batter into prepared muffin tins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 20 to 25 minutes, until golden. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Muffin Recipes",
            "Corn Muffin Recipes"
        ],
