    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9942/frying-pan-cookies/",
        "name": "Frying Pan Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7734904.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-09-03T02:20:47.000Z",
        "description": "Peculiar looking flourless cookies. For rum-packed flavor try using 2 teaspoons rum extract in place of the vanilla.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups dates, pitted and chopped",
            "\u00bc cup butter",
            "2 eggs",
            "1 cup white sugar",
            "\u00bc teaspoon salt",
            "1 teaspoon vanilla extract",
            "\u00bd cup chopped walnuts",
            "3 cups crisp rice cereal",
            "\u00bd cup flaked coconut",
            "\u2153 cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter or margarine in heavy skillet. Add slightly beaten eggs. Stir in dates, sugar, salt and vanilla. Cook over low heat stirring constantly until thick and well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Have ready crisp rice cereal and chopped nuts in large bowl. Stir cooked mixture into them until coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill. Put by teaspoonful on cookie sheet. Chill again. When cold roll in crisp rice crumb and coconut mixture or powdered sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "No-Bake Cookie Recipes"
        ],
