    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9723/chocolate-oatmeal-bars/",
        "name": "Chocolate Oatmeal Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F958564.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Rich bars that are easy to make and a hit with everyone.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups butter",
            "1 cup packed brown sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "2 cups quick cooking oats",
            "2 tablespoons butter",
            "1 cup semisweet chocolate chips",
            "\u2154 cup evaporated milk",
            "\u00bc cup white sugar",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Grease a 9 inch square baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To prepare Filling:   In a heavy pot, melt 2 tablespoons of the butter and chocolate chips.  Add the evaporated milk and 1/4 cup of the white sugar.  Bring to a rolling boil, stirring constantly.  Remove from heat.  Stir in nuts and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To prepare cookie base:  Cream 1 1/2 cups of the butter and the brown sugar together.  Add the vanilla and the egg, and beat until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the flour, baking soda, and 1 3/4 cups of the oats and blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press 2/3 of this mixture into the bottom of the greased pan.   Spread the cooled filling on top of the cookie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the remaining 1/4 cup of oats with the remainder of the cookie base, and crumble over the filling.  Bake for 25-30 minutes.\n"
            }
        ],
