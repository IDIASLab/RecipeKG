    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16617/no-bake-cookies-v/",
        "name": "No Bake Cookies V",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F28216.jpg",
            "width": 1725,
            "height": 1177
        },
        "datePublished": "2000-02-14T00:04:53.000Z",
        "description": "An easy cookie for kids to make. Cocoa, peanut butter, and oatmeal make a chewy delicious cookie with some nutritional value.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H1M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups white sugar",
            "\u00bc cup unsweetened cocoa powder",
            "\u00bd cup milk",
            "\u00bd cup margarine",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "\u00bd cup chunky peanut butter",
            "3 cups quick cooking oats"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat, combine the sugar, cocoa, milk and margarine. Bring to a boil, stirring occasionally. Boil for 1 minute, then remove from heat and stir in the vanilla, salt, peanut butter and oats.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded spoonfuls onto waxed paper. Allow cookies to cool for at least 1 hour. Store in an airtight container.\n"
            }
        ],
