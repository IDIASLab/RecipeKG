    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10770/jan-hagel-cookies/",
        "name": "Jan Hagel Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F36528.jpg",
            "width": 2608,
            "height": 1952
        },
        "datePublished": "1997-12-21T20:07:52.000Z",
        "description": "A Dutch cookie... a favorite at Christmas",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "2 cups all-purpose flour",
            "\u00bd teaspoon ground cinnamon",
            "1 egg yolk",
            "1 egg white",
            "\u00bd cup finely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream the butter and sugar.  Add the egg yolk and beat thoroughly.  (This should be very creamy.)  Add the flour and cinnamon and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread thinly on 1 1/2 cookie sheets.  Press down mixture with fingers, then score with a fork.  Brush on a film of egg white mixed with a little water.  Sprinkle the nuts over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 1/2 hour at 300 degrees F (150 degrees C).  Remove from oven and cut into squares or oblongs.  When cold, remove from cookie sheet.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Dutch"
        ],
        "recipeCuisine": [
            "European",
            "Dutch"
        ],
