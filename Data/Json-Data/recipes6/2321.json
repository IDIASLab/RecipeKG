    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/245533/chocolate-bark/",
        "name": "Chocolate Bark",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2975270.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "2020-03-05T05:13:45.000Z",
        "description": "This is the easiest recipe I think I own. My kids are always asking me to make it because it is so good!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H11M",
        "totalTime": "P0DT1H26M",
        "recipeYield": "1 11x17-inch baking sheet",
        "recipeIngredient": [
            "saltine crackers, or as needed",
            "1 cup butter",
            "1 cup brown sugar",
            "1 (12 ounce) bag milk chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line an 11x17-inch rimmed baking sheet with parchment paper. Spread saltine crackers on baking sheet in a single layer, salted sides up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine butter and brown sugar in a saucepan; melt together over medium heat, stirring well until mixture turns a caramel color, about 5 minutes. Pour caramel mixture over crackers and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until just bubbly, about 5 minutes. Remove from oven and let cool briefly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt chocolate chips in a microwave-safe bowl in the microwave in 15-second intervals, stirring after each melting, 1 to 3 minutes. Pour chocolate over crackers and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer baking sheet to the refrigerator and chill until bark hardens, about 1 hour. Break bark up into pieces.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "Milk Chocolate"
        ],
