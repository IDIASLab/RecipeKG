        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23140/yummy-candied-yams/",
        "name": "Yummy Candied Yams",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1110431.jpg",
