    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10207/peanut-butter-cup-cookies-i/",
        "name": "Peanut Butter Cup Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1016458.jpg",
            "width": 3456,
            "height": 3456
        },
        "datePublished": "1997-12-19T18:18:27.000Z",
        "description": "Peanut butter cookies topped with miniature chocolate peanut butter cup candy.  You may need a few more or less peanut butter cups depending on how many cookies you make out of a batch of dough.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00be cup peanut butter",
            "\u00bd cup shortening",
            "\u00bd cup white sugar",
            "\u2153 cup packed brown sugar",
            "1 egg",
            "2 tablespoons milk",
            "1 teaspoon vanilla extract",
            "1\u2009\u2153 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u2153 cup granulated sugar for decoration",
            "30 miniature chocolate covered peanut butter cups, unwrapped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixer bowl cream peanut butter, shortening, 1/3 cup sugar and the brown sugar.  Add egg, milk and vanilla; beat well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour and baking soda; gradually add to creamed mixture, blending thoroughly.  Shape dough into 1 1/2 inch balls; roll the balls in sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on ungreased cookie sheet; bake 10-12 minutes.  Remove from oven and IMMEDIATELY place unwrapped peanut butter cup on top of each cookie, pressing down so that cookie cracks around edges.  Cool slightly; remove from cookie sheet.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
