    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11104/fudge-quickies/",
        "name": "Fudge Quickies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F655368.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-10-07T21:08:52.000Z",
        "description": "This is a choc-nutty no-bake cookie.  They are also good with raisins.  Good luck!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "2 cups white sugar",
            "2 cups packed brown sugar",
            "1 cup butter",
            "1 cup milk",
            "2 cups semisweet chocolate chips",
            "4 cups quick cooking oats",
            "1 cup chopped walnuts",
            "1 cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place oats, nuts, coconut, and chocolate chips in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place sugars, butter or margarine, and milk in a saucepan.  Bring to a boil, and continue to boil for 2 minutes.  Pour hot mixture over oat mixture.  Mix well.  Drop from a teaspoon onto wax paper.  Allow to set up.\n"
            }
        ],
