        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11464/apple-cinnamon-oatmeal-cookie/",
        "name": "Apple Cinnamon Oatmeal Cookie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F854407.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-06-25T17:55:37.000Z",
        "description": "Granola gives this cookie a special taste. It's delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup apple cinnamon granola",
            "\u00be cup all-purpose flour",
            "\u00bd cup packed brown sugar",
            "\u00bd cup shortening",
            "\u00bc cup white sugar",
            "1 egg",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon vanilla extract",
            "\u00bd teaspoon baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together granola, flour, brown sugar, shortening, white sugar, egg, salt, vanilla extract and baking soda together in a medium size mixing bowl.  Mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls 1 1/2 inches apart on baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 12 to 15 minutes, or until golden brown.  Cool on wire racks.\n"
            }
        ],
