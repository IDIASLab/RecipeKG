        "mainEntityOfPage": "https://www.allrecipes.com/recipe/72970/kristis-corn-salad/",
        "name": "Kristi's Corn Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4556950.jpg",
            "width": 1280,
