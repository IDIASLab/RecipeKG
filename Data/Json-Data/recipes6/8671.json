    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279964/ham-and-pineapple-pasta-salad/",
        "name": "Ham and Pineapple Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Ham and Pineapple Pasta Salad"
        },
        "datePublished": "2020-04-30T20:18:29.000Z",
        "description": "Shell pasta with ham, cheese, and pineapple--very good for large group get-togethers. Pasta lasts up to one week in a refrigerated sealed container. This salad is easily doubled and it will all be gone!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (12 ounce) package medium shell pasta",
            "3 tablespoons salted butter",
            "1 cup cubed cooked ham",
            "\u00bc cup packed brown sugar",
            "\u00bd cup mayonnaise",
            "\u00bd cup sour cream",
            "1 (8 ounce) can pineapple chunks, drained",
            "\u00bd cup red grapes",
            "\u00bd cup cubed Cheddar cheese",
            "2 tablespoons pineapple juice, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to a boil. Add shells and cook, stirring occasionally, until tender yet firm to the bite, about 9 minutes. Drain and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a medium skillet over medium heat and add ham and brown sugar. Cook for 2 to 3 minutes; do not overcook. Set aside and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mayonnaise and sour cream together in a small bowl. Combine pineapple, grapes, Cheddar cheese, and pineapple juice in a large bowl; mix in mayonnaise mixture, ham mixture, and pasta. Mix well before every serving.\n"
            }
        ],
