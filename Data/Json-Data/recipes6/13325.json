    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12237/rhubarb-cobbler/",
        "name": "Rhubarb Cobbler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6735888.jpg",
            "width": 3201,
            "height": 2498
        },
        "datePublished": "1998-06-29T23:48:00.000Z",
        "description": "My family really loves this recipe. Hope you have good luck with it!",
        "prepTime": null,
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "2 tablespoons cornstarch",
            "4 cups chopped rhubarb",
            "1 tablespoon water",
            "1 tablespoon butter, diced",
            "1 teaspoon ground cinnamon",
            "1 cup all-purpose flour",
            "1 tablespoon white sugar",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00bc cup butter",
            "\u00bc cup milk",
            "1 egg, beaten",
            "2 tablespoons white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Lightly grease a 9 inch square baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan, mix sugar and cornstarch for filling. Stir in the rhubarb and water. Bring to a boil. Cook and stir for 1 minute. Transfer to the prepared baking dish. Dot with butter, and sprinkle with cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, sift together flour, sugar, baking powder, and salt for topping. Cut in the butter until the mixture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix together milk and egg. Add all at once to dry ingredients, stirring just to moisten. Drop by teaspoonfuls on top of the rhubarb mixture. Sprinkle with sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 minutes in the preheated oven, until crisp and lightly browned.\n"
            }
        ],
