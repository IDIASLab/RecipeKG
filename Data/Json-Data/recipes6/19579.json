    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/62883/tomato-pork-loin-chops/",
        "name": "Tomato Pork Loin Chops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9355214.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-06-19T03:10:03.000Z",
        "description": "Rich, tasty, and easy to make with delicious, on hand ingredients! Serve with egg noodles, if you want.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil, divided",
            "4 boneless pork loin chops, pounded thin",
            "1 large onion, sliced",
            "\u00bd teaspoon sugar",
            "2 teaspoons chili powder",
            "\u00bd teaspoon fennel seeds, crushed",
            "\u00bd teaspoon red pepper flakes",
            "1 teaspoon dried oregano",
            "1 (8 ounce) can tomato sauce",
            "2 fluid ounces water",
            "1 teaspoon Worcestershire sauce",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a skillet over high heat, and quickly brown the pork chops on both sides. Transfer chops to platter, and keep warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce skillet heat to medium, and cook the onion until tender. Mix in the sugar, chili powder, fennel, red pepper flakes, and oregano. Stir in the tomato sauce, water, and Worcestershire sauce, and bring to a boil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the pork chops to the skillet. Cover, reduce heat to low, and cook 10 minutes. Remove cover, and continue cooking 15 minutes, or to desired doneness.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
