    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279712/slow-cooker-potato-soup-with-heavy-cream/",
        "name": "Slow Cooker Potato Soup with Heavy Cream",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8174864.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2014-03-27T22:04:36.000Z",
        "description": "I love this simple, basic recipe for slow cooker potato soup. Sometimes I add bacon or Cheddar cheese for a little variation. You can do a lot with it.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT7H0M",
        "totalTime": "P0DT7H20M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "5 cups water",
            "5 cups cubed potatoes",
            "2 cups chopped onion",
            "\u00bd cup chopped celery",
            "\u00bc cup salted butter",
            "4 cubes  chicken bouillon",
            "2 teaspoons salt",
            "1 teaspoon ground black pepper",
            "1 pinch garlic powder",
            "1 cup cold water",
            "1\u2009\u00bd tablespoons cornstarch",
            "1 pint heavy whipping cream",
            "1 (12 ounce) can evaporated milk",
            "1 tablespoon chopped fresh chives, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 5 cups water, potatoes, onion, celery, butter, bouillon cubes, salt, pepper, and garlic powder in a slow cooker. Cook until vegetables are tender, on Low for 8 to 9 hours or on High, about 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cold water and cornstarch together in a small bowl. Stir 1/2 of the cornstarch mixture, heavy cream, and evaporated milk into the soup and cook for 1 more hour until thickened, adding more of the cornstarch mixture if desired for consistency. Top with chives before serving.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Potato Soup Recipes"
        ],
