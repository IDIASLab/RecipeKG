    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234287/hatzic-bars/",
        "name": "Hatzic Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hatzic Bars"
        },
        "datePublished": "2020-03-05T05:11:01.000Z",
        "description": "This is a cookie my mom used to make every year at Christmas. The base is like a Nanaimo bar but the coconut filling makes it not as sweet.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "18 bars",
        "recipeIngredient": [
            "1 cup brown sugar",
            "\u00bd cup butter",
            "2 (1 ounce) squares semisweet chocolate, chopped",
            "1 egg",
            "2 cups graham cracker crumbs",
            "1 cup chopped walnuts",
            "1 (8 ounce) package unsweetened shredded coconut",
            "\u00bd (14 ounce) can sweetened condensed milk",
            "1 (16 ounce) package prepared chocolate frosting, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 9x9-inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt brown sugar, butter, chocolate, and egg in the top of a double boiler over simmering water, stirring frequently and scraping down the sides with a rubber spatula to avoid scorching.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir graham cracker crumbs and walnuts into chocolate mixture; press evenly into the bottom of the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together shredded coconut and condensed milk in a bowl; spread evenly over chocolate layer. Spread frosting over coconut mixture. Refrigerate until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "International Cookie Recipes",
            "Canadian Cookie Recipes"
        ],
