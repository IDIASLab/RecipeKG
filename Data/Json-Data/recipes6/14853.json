        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17587/blender-chocolate-mousse-ii/",
        "name": "Blender Chocolate Mousse II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1476672.jpg",
