    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/274254/carrot-soup-with-garlic-scapes/",
        "name": "Carrot Soup with Garlic Scapes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Carrot Soup with Garlic Scapes"
        },
        "datePublished": "2020-03-10T02:53:54.000Z",
        "description": "A delicious carrot soup with garlic scapes is perfect for cold autumn days.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons salted butter",
            "2 garlic scapes, diced",
            "1 shallot, diced",
            "1 teaspoon minced fresh ginger root",
            "2 cups diced carrots",
            "\u2153 teaspoon ground cumin",
            "1 pinch ground cinnamon",
            "salt and ground black pepper to taste",
            "3 cups vegetable broth"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter over medium heat in a medium saucepan. Add garlic scapes, shallot, and ginger. Cook and stir until fragrant, 3 to 5 minutes. Add carrots and stir to coat; cook for 3 to 5 minutes. Sprinkle cumin, cinnamon, and pepper over carrots and mix well. Cook until carrots have softened, 3 to 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour vegetable broth into the saucepan and bring to a boil. Reduce heat and let simmer, 15 to 20 minutes. Remove from heat and let cool for 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend soup using an immersion blender or an electric blender until smooth. Season with salt to taste.\n"
            }
        ],
