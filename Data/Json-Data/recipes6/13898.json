        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14342/grandmas-salad-dressing/",
        "name": "Grandma's Salad Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4543948.jpg",
