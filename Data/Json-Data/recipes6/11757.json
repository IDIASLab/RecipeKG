    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9495/chestnut-souffle/",
        "name": "Chestnut Souffle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3168689.jpg",
            "width": 2322,
            "height": 4128
        },
        "datePublished": "1999-12-03T22:44:13.000Z",
        "description": "Fluffy goodness! This easy recipe calls for chestnut puree which can be found in most specialty food stores.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 egg yolks",
            "1 cup chestnut puree",
            "\u2153 cup white sugar",
            "3 tablespoons milk",
            "1 tablespoon brandy",
            "4 egg whites",
            "1 cup whipped cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, beat the egg yolks until thick and lemon colored.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, beat together the chestnut puree, sugar, milk, and brandy. Add egg yolks and mix until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat egg whites until stiff peaks form.  Fold the beaten whites into the chestnut mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into an ungreased 1 1/2 quart souffle dish. Bake for 35 to 40 minutes. Top  with whipped cream and serve.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "French"
        ],
