    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279239/deep-fried-tilapia/",
        "name": "Deep-Fried Tilapia",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7868995.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-20T00:02:52.000Z",
        "description": "In Wisconsin, the \"Friday Fish Fry\" is a time-honored tradition. This deep-fried tilapia is my family's way of bringing it home.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "10 fillets",
        "recipeIngredient": [
            "10 (4 ounce) fillets tilapia",
            "salt and ground black pepper to taste",
            "\u2153 cup all-purpose flour",
            "1 cup buttermilk",
            "1 egg",
            "\u00bd cup all-purpose flour",
            "\u00bd cup yellow cornmeal",
            "2 tablespoons seafood seasoning (such as <a href=\"https://www.allrecipes.com/article/what-is-old-bay-seasoning/\" aria-label=\"Old Bay\">Old Bay</a>\u00ae)",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season fillets with salt and pepper. Dust with 1/3 cup of flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat buttermilk and egg in a bowl. Combine remaining flour, cornmeal, seafood seasoning, baking powder, and baking soda in another bowl. Dip all flour-dusted fillets into egg mixture, then into seasoned flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil over medium-high heat in a large frying pan or deep fryer. Fry fillets, working in batches if needed, until golden, about 2 minutes on each side. Season again with salt as soon as your take them out of the pan or fryer.\n"
            }
        ],
