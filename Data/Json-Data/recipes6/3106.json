    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/262037/pineapple-glazed-baked-ham/",
        "name": "Pineapple-Glazed Baked Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pineapple-Glazed Baked Ham"
        },
        "datePublished": "2020-03-05T05:17:19.000Z",
        "description": "This pineapple baked ham recipe is one that I have adapted to my liking through trial and error. It makes a sweet ham with a nice brown sugar crust. I serve it with asparagus and potato salad or mashed potatoes.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H45M",
        "totalTime": "P0DT3H5M",
        "recipeYield": "1 7-pound ham",
        "recipeIngredient": [
            "1 (7 pound) bone-in ham",
            "1 cup pineapple juice, divided",
            "1 cup brown sugar, divided",
            "\u00bd cup butter, melted",
            "1 cup water",
            "1 pinch ground ginger",
            "1 pinch ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ham in a large baking pan and score the surface with a knife. Mix 1/4 cup pineapple juice, 1/4 cup brown sugar, and butter together in a bowl. Inject the mixture into the ham; pour any remaining liquid over the ham. Pour an additional 1/2 cup pineapple juice and water into the pan. Rub 1/2 cup brown sugar onto the ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix remaining pineapple juice, remaining brown sugar, ginger, and cloves in a small pan over medium heat. Cook until the mixture comes to a boil; simmer until glaze is reduced and dark brown, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake ham in the preheated oven until an instant-read thermometer inserted into the thickest part of the ham and not touching the bone reads at least 160 degrees F (70 degrees C), about 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Continue baking, brushing glaze onto ham every 10 minutes, about 30 minutes more. Remove from the oven and allow to sit 10 minutes before slicing.\n"
            }
        ],
