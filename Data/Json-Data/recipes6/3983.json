    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/276179/pecan-snowball-cookie-bites/",
        "name": "Pecan Snowball Cookie Bites",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F310050.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2020-03-05T05:20:41.000Z",
        "description": "My 3-year-old loves these cookie bites, and so do most people who try them.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup unsalted butter",
            "\u00bd cup packed light brown sugar",
            "2 teaspoons vanilla extract",
            "2 cups sifted all-purpose flour",
            "2 cups finely chopped pecans",
            "1 tablespoon sifted confectioners' sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease 4 baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and brown sugar together in a bowl using an electric mixer. Stir in vanilla extract. Stir in flour gradually; mixture will be crumbly. Add pecans and work into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pinch off dough in teaspoons; roll in hands to form balls that are about 1/2-inch in diameter. Place onto the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until firm and browned on bottoms, 15 to 18 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and let cool slightly, about 5 minutes. Roll warm cookies in confectioners' sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
