        "mainEntityOfPage": "https://www.allrecipes.com/recipe/244989/mini-pecan-pie-muffins/",
        "name": "Mini Pecan Pie Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3551679.jpg",
            "width": 3324,
            "height": 3346
