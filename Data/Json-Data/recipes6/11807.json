    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9591/everything-cookies-i/",
        "name": "Everything Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F973569.jpg",
            "width": 2152,
            "height": 2152
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "As the name implies, these cookies have everything.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "1 egg, beaten",
            "1 teaspoon cream of tartar",
            "1 cup quick cooking oats",
            "1 cup vegetable oil",
            "2 cups semisweet chocolate chips",
            "1 cup crisp rice cereal",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "3 cups all-purpose flour",
            "2 tablespoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix all ingredients.  Drop by teaspoonfuls onto ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until lightly brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
