    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228992/hawaiian-pork-tenderloin/",
        "name": "Hawaiian Pork Tenderloin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F946840.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2020-03-05T05:09:28.000Z",
        "description": "Very simple, yet elegant for guests. Kids love how sweet it is. Perfect complements are rice and baked yams.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds boneless pork tenderloin",
            "\u00bd cup brown sugar",
            "1 tablespoon chili powder",
            "1 tablespoon garlic powder",
            "1 teaspoon ground black pepper",
            "\u00bc cup pineapple preserves",
            "1 onion, sliced",
            "1 orange bell pepper, cut into long strips",
            "1 red bell pepper, cut into long strips",
            "\u00bd (15 ounce) can mandarin oranges, drained and juice reserved",
            "1 (8 ounce) can crushed pineapple, drained and juice reserved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pork tenderloin in a shallow 9x13-inch; sprinkle with brown sugar, chili powder, garlic powder, and black pepper. Spread pineapple preserves over tenderloin. Arrange onion, orange bell pepper, and red bell pepper on and around tenderloin. Mix reserved juice from mandarin oranges and crushed pineapple in a small bowl; pour in at least 1/2 cup reserved juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tenderloin is no longer pink in the center, 45 to 60 minutes. Spoon mandarin oranges and pineapple over tenderloin in the last 20 minutes of cooking. An instant-read thermometer inserted into the thickest part of pork should read at least 145 degrees F (63 degrees C).\n"
            }
        ],
