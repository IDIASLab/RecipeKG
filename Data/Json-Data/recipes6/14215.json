        "totalTime": null,
        "recipeYield": "10 to 12 servings",
        "recipeIngredient": [
            "2 eggs",
            "\u00bd cup all-purpose flour",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "\u00bd cup butter, melted",
            "1 cup semisweet chocolate chips",
            "1 cup chopped nuts",
            "1 (9 inch) pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together eggs, flour, sugar, vanilla, and melted butter. Mix well. Stir in walnuts and chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pie shell. Bake for I hour.\n"
            }
        ],
