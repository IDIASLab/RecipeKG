    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15943/cherry-berry-pie/",
        "name": "Cherry-Berry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F580049.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-11-17T05:16:28.000Z",
        "description": "Be sure to use red tart pie cherries, not cherry pie filling.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "2 (9 inch)  pie shells",
            "\u00be cup white sugar",
            "2 tablespoons quick-cooking tapioca",
            "2 tablespoons cornstarch",
            "\u00bc teaspoon salt",
            "1 (16 ounce) can pitted sour cherries, drained with liquid reserved",
            "1 (10 ounce) package frozen strawberries in syrup, thawed, drained with liquid reserved",
            "1 tablespoon lemon juice",
            "2 teaspoons white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan combine 3/4 cup sugar, tapioca, cornstarch, and salt. Mix well, then stir in reserved cherry and strawberry liquids. Stirring constantly, cook mixture over medium heat 5 to 10 minutes or until it begins to thicken. Remove from heat; stir in cherries, strawberries, and lemon juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon fruit filling into pastry-lined pan. Top with second pastry. Seal and flute edges, then make steam vents in top crust. Sprinkle with 2 teaspoons sugar. Cover edges of pastry with aluminum foil to prevent excessive browning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 to 35 minutes or until golden brown. Remove foil during last 15 minutes of baking to allow pastry edges to brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Fruit Pies"
        ],
