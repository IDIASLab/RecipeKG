        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15224/tropical-chicken-i/",
        "name": "Tropical Chicken I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F86433.jpg",
            "width": 250,
            "height": 250
