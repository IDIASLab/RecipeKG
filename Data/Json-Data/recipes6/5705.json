        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228233/spicy-white-chili-with-chicken/",
        "name": "Spicy White Chili with Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5855819.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "2020-03-10T06:51:58.000Z",
        "description": "Delicious white chicken chili! Make on stove top or let it simmer all day in slow cooker.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon olive oil",
            "1 onion, chopped",
            "1 jalapeno pepper, seeded and chopped",
            "3 cloves garlic, minced",
            "4 cups chicken broth",
            "2 (15.5 ounce) cans Great Northern beans, drained and rinsed",
            "1 (15 ounce) can garbanzo beans, drained",
            "1 cup corn kernels",
            "1 (7 ounce) can diced green chiles",
            "2 tablespoons minced fresh cilantro",
            "1 tablespoon lime juice",
            "2 teaspoons ground cumin",
            "1 teaspoon ground black pepper",
            "4 cooked chicken breast halves, cubed",
            "2 tablespoons cornstarch",
            "\u00bc cup cold water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a large saucepan over medium heat; cook and stir onion, jalapeno, and garlic until onion is tender, about 5 minutes. Add chicken broth, Great Northern beans, garbanzo beans, corn, green chiles, cilantro, lime juice, cumin, and black pepper; bring to a boil. Reduce heat to medium-low and stir chicken into broth. Cover and simmer chili for 45 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cornstarch and water together in a bowl; stir into chili and bring to a boil. Cook and stir until chili is thickened, about 5 minutes.\n"
            }
        ],
