        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8917/easy-deviled-chicken/",
        "name": "Easy Deviled Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F116389.jpg",
