    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7294/chocolate-cherry-upside-down-cake/",
        "name": "Chocolate Cherry Upside Down Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F183888.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-03-18T19:06:49.000Z",
        "description": "This is an old favorite from my recipe box that I have had for 25 years that 's  non-dairy and egg free!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 (21 ounce) can cherry pie filling",
            "2\u2009\u00bc cups all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "\u00be cup unsweetened cocoa powder",
            "1\u2009\u00bd teaspoons baking soda",
            "\u00be teaspoon salt",
            "1\u2009\u00bd cups water",
            "\u00bd cup vegetable oil",
            "\u00bc cup distilled white vinegar",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spread pie filling evenly in the bottom of a greased 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl stir together flour, sugar, cocoa, baking soda, and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In another bowl combine water, oil, vinegar, and vanilla. Add these liquid ingredients to the flour mixture all at once.  Stir until just moistened.  Pour the batter evenly over the cherry pie filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degrees F ( 175 degrees C) oven for 30 to 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let cake cool for 10 minutes in pan then invert cake onto a serving dish and continue to cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Upside-Down Cake Recipes"
        ],
