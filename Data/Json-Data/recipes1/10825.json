        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17605/party-kielbasa/",
        "name": "Party Kielbasa",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F772443.jpg",
            "width": 4288,
            "height": 2848
