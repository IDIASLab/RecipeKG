    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11623/grandmas-chew-bread/",
        "name": "Grandma's Chew Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2508978.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1999-10-27T04:03:49.000Z",
        "description": "A southern style bar cookie loaded with brown sugar and pecans. Relax in the afternoon and chew on some of these.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 pound brown sugar",
            "4 eggs",
            "2 cups all-purpose flour",
            "\u00bd teaspoon salt",
            "1 teaspoon vanilla extract",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven  to 400 degrees F ( 200 degrees C ). Grease a 15x10x1 inch jellyroll pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a heavy saucepan combine the sugar and eggs. Cook over medium heat, stirring constantly until sugar is dissolved. Remove from heat, stir in the flour, salt, vanilla, and pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon mixture into the prepared baking pan and bake for 15 minutes in the preheated oven. Cool and cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
