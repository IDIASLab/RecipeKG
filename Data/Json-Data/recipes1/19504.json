    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281085/lemon-zest-pie-crust/",
        "name": "Lemon Zest Pie Crust",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1024026.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "2013-06-25T19:50:58.000Z",
        "description": "A refreshing twist on an ordinary pie crust. This lemon pie crust works nicely for tarts or berry-filled pies.",
        "prepTime": "P0DT0H30M",
        "cookTime": null,
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 9-inch pie crusts",
        "recipeIngredient": [
            "2\u2009\u00bc cups all-purpose flour",
            "\u00bd cup white sugar",
            "7 tablespoons cold salted butter, cubed",
            "3 tablespoons ice water, or more as needed",
            "1 tablespoon lemon zest",
            "1 small lemon, juiced",
            "\u00bd tablespoon pure vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour and sugar into a large bowl. Cut in butter until it is in pea-sized pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make a \"well\" in the flour mixture. Pour in water and lemon zest. Carefully fold into the flour mixture until lightly combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make another \"well\" and pour in lemon juice; lightly fold into the batter. Repeat with vanilla extract. Add more water if needed to bring the dough together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into 2 separate, equal-sized discs and cover in plastic wrap. Let rest in the refrigerator for at least 30 minutes before using.\n"
            }
        ],
