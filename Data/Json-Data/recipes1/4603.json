    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11849/greek-spaghetti-i/",
        "name": "Greek Spaghetti I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1571868.jpg",
            "width": 980,
            "height": 980
        },
        "datePublished": "1999-04-11T23:47:11.000Z",
        "description": "Cheesy spaghetti! Serve with garlic bread, a nice salad, and maybe a bottle of wine.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 pound spaghetti",
            "\u00bc pound Romano cheese, grated",
            "\u00bc pound shredded mozzarella cheese",
            "\u00bc pound shredded provolone cheese",
            "\u00bd cup margarine, melted",
            "1 pinch garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot of lightly salted boiling water, cook the spaghetti 8 to 10 minutes, until al dente, and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a medium casserole dish. In a bowl, mix the Romano cheese, mozzarella cheese, and provolone cheese. In a separate bowl, blend the melted margarine and garlic powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the bottom of the prepared dish with 1/3 of the cooked spaghetti, top with 1/3 of the cheese mixture, and drizzle with 1/3 margarine mixture. Repeat layering. Be sure to end with cheese on top. (If you really like garlic you can sprinkle between layers as desired.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 30 minutes in the preheated oven, until cheese is bubbly and lightly brown.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegetarian",
            "Side Dishes"
        ],
