    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16699/firecracker-grilled-alaska-salmon/",
        "name": "Firecracker Grilled Alaska Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F997347.jpg",
            "width": 2736,
            "height": 2736
        },
        "datePublished": "2000-01-26T20:47:44.000Z",
        "description": "These are salmon fillets in a tasty, tangy sauce with a little heat! Serve with rice and a simple stir-fry of baby corn, shiitake mushrooms and snow peas.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT6H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "8 (4 ounce) fillets salmon",
            "\u00bd cup peanut oil",
            "4 tablespoons soy sauce",
            "4 tablespoons balsamic vinegar",
            "4 tablespoons green onions, chopped",
            "3 teaspoons brown sugar",
            "2 cloves garlic, minced",
            "1\u2009\u00bd teaspoons ground ginger",
            "2 teaspoons crushed red pepper flakes",
            "1 teaspoon sesame oil",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place salmon filets in a medium, nonporous glass dish. In a separate medium bowl, combine the peanut oil, soy sauce, vinegar, green onions, brown sugar, garlic, ginger, red pepper flakes, sesame oil and salt. Whisk together well, and pour over the fish. Cover and marinate the fish in the refrigerator for 4 to 6 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare an outdoor grill with coals about 5 inches from the grate, and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill the fillets 5 inches from coals for 10 minutes per inch of thickness, measured at the thickest part, or until fish just flakes with a fork. Turn over halfway through cooking.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Salmon"
        ],
