    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13778/spinach-and-artichoke-au-gratin/",
        "name": "Spinach and Artichoke Au Gratin",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3929361.jpg",
            "width": 2227,
            "height": 1590
        },
        "datePublished": "1999-10-10T22:06:38.000Z",
        "description": "This baked spinach and artichoke casserole is a great side dish for any dinner occasion.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "2 (6.5 ounce) jars marinated artichoke hearts, drained",
            "3 (3 ounce) packages cream cheese",
            "4 tablespoons butter, softened",
            "6 tablespoons milk",
            "3 (10 ounce) packages frozen chopped spinach - thawed, drained and squeezed dry",
            "\u2153 cup grated Parmesan cheese",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place artichokes in the bottom of a 1 1/2 quart baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, beat together the cream cheese and butter until smooth. Blend in milk and stir in spinach. Mix together until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into baking dish and sprinkle top with cheese, salt and seasoned pepper; refrigerate for 24 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degree C), bake for 30 minutes or until heated through. Place under broiler to brown the cheese topping.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Spinach Casserole"
        ],
