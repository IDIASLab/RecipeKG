        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20589/ts-easy-chicken/",
        "name": "T's Easy Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7662.jpg",
