        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19381/chinese-pasta-salad/",
        "name": "Chinese Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F862393.jpg",
