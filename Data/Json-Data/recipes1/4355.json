    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11320/christmas-orange-rind-cut-out-cookies/",
        "name": "Christmas Orange Rind Cut-Out Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Christmas Orange Rind Cut-Out Cookies"
        },
        "datePublished": "1999-03-19T22:25:22.000Z",
        "description": "Fun to have the kids help cut out these cookies!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1\u2009\u00bc cups white sugar",
            "2 eggs",
            "3\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 cup sour cream",
            "2 teaspoons orange zest",
            "\u2153 cup white sugar",
            "\u2153 cup finely chopped almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream butter and 1 1/4 cups of the sugar. Add eggs, one at a time, beating well after each.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour and baking powder together. Add flour mix to the butter mixture alternately with the sour cream. Blend very well and add the orange zest. Wrap tightly and chill overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn chilled dough out on to a floured surface and roll out to 1/4 inch thick. Cut into desired shapes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the 1/3 cup white sugar and the finely chopped almonds and sprinkle over the tops of the cookies. Place cookies on the prepared baking sheet and bake at 375 degrees F (190 degrees C) for 10 to 12 minutes. Remove from oven and let cookies cool on rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cut-Out Cookie Recipes"
        ],
