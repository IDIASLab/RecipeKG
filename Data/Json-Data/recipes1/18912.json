        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215354/melon-lime-cooler/",
        "name": "Melon Lime Cooler",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F844979.jpg",
