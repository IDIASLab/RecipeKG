    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19751/cajun-oyster-pie/",
        "name": "Cajun Oyster Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4487310.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2000-04-14T13:25:52.000Z",
        "description": "A Louisiana favorite! You may add cooked vegetables to the filling if you wish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "3 thick slices bacon",
            "1 tablespoon vegetable oil",
            "2 tablespoons all-purpose flour",
            "\u00be cup milk",
            "18 shucked oysters, drained with liquid reserved",
            "1 teaspoon Worcestershire sauce",
            "\u00bd teaspoon Cajun seasoning",
            "2 (9 inch)  unbaked 9 inch pie crusts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain bacon, reserving 1 tablespoon bacon grease. Crumble bacon and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat bacon grease and vegetable oil over medium heat. Stir in flour and cook until flour is light brown. Slowly whisk in milk and 1 cup reserved oyster liquid. Stir until a thick gravy has formed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in Worcestershire sauce, Cajun seasoning and oysters. Pour mixture into a 9 inch pie shell and cover with top crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 minutes, until crust is golden.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Savory Pie Recipes",
            "Pork Pie Recipes"
        ],
