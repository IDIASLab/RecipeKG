        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10362/rolled-white-cookies/",
        "name": "Rolled White Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7735750.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-12-12T22:35:26.000Z",
        "description": "These are great for any time of the year.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "2 eggs",
            "4 tablespoons milk",
            "1 teaspoon lemon extract",
            "3 cups all-purpose flour",
            "2 teaspoons cream of tartar",
            "1 teaspoon baking soda",
            "1 tablespoon white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together 1 cup white sugar and butter.  Beat in the eggs, milk and flavoring.  Sift flour, cream of tartar and baking soda.   Stir into butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill for a short time, until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out on lightly floured board and cut with your favorite cookie cutters.  Sprinkle with 1 tablespoon white sugar.  Bake at 350 degrees F (175 degrees C) for 8 - 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cut-Out Cookie Recipes"
        ],
