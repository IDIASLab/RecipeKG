    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7643/german-christmas-gingerbread/",
        "name": "German Christmas Gingerbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F162695.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-06-05T16:59:48.000Z",
        "description": "This is a good recipe for Christmas and very different to our North American one.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 tube cake",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 cups packed brown sugar",
            "3 eggs",
            "\u2154 cup honey",
            "\u00bc cup orange liqueur ",
            "1 cup sour cream",
            "\u00bd cup orange juice",
            "1\u2009\u2154 cups all-purpose flour",
            "1 cup whole wheat flour",
            "4 teaspoons baking powder",
            "2 teaspoons ground ginger",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u00bc teaspoon ground cloves",
            "1 cup raisins",
            "1 cup blanched slivered almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the flours, baking powder, and spices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream the butter or margarine with the brown sugar.  Beat in the eggs, then the honey, orange liqueur, sour cream, and orange juice.  Beat the flour mixture into the creamed mixture, and then stir in the raisins and almonds.  Turn batter into a greased and floured tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cake at 350 degrees F (175 degrees C) for 80 minutes, or until it tests done with toothpick.  Transfer to a rack to cool.\n"
            }
        ],
