        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "36 servings",
        "recipeIngredient": [
            "1 pound maple roll sausage",
            "2 cups baking mix",
            "\u00bd pound shredded sharp Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (225 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small mixing bowl, combine maple sausage, baking mix, and cheese. Form mixture into 1 inch balls. Arrange balls in a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 minutes.\n"
            }
        ],
