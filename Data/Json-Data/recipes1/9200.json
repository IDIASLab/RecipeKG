    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19657/hot-crabmeat-dip/",
        "name": "Hot Crabmeat Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1117967.jpg",
            "width": 1936,
            "height": 1732
        },
        "datePublished": "2000-01-09T21:10:41.000Z",
        "description": "This is a wonderfully creamy and tasty dip, made rich with crabmeat and topped with toasted almonds. Serve it with slices of your favorite bread.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "1 tablespoon milk",
            "2 tablespoons minced onion",
            "1 teaspoon prepared horseradish",
            "\u00bc teaspoon salt",
            "1 pinch ground black pepper",
            "1 (6 ounce) can crabmeat, drained and flaked",
            "\u2153 cup sliced almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix the cream cheese, milk, onion, horseradish, salt, pepper and crabmeat. Spread the mixture into a pie pan or shallow baking dish. Sprinkle almonds over the crabmeat mixture\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 45 minutes, or until bubbly and lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Crab Dip Recipes"
        ],
