    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14770/pork-chop-pileups/",
        "name": "Pork Chop Pileups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2244235.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-10-11T20:22:22.000Z",
        "description": "A fast and delicious way to serve pork chops with the vegetables. Add some crusty rolls and a salad.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 pork chops",
            "2 potatoes, thinly sliced ",
            "1 (15 ounce) can green beans, drained",
            "1 onion, thinly sliced",
            "2 (10.75 ounce) cans  condensed cream of mushroom soup",
            "\u00bd cup milk",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange pork chops in a 9x13 inch baking dish. Layer potatoes, beans and onion on top of chops. Season with salt and pepper to taste. In a medium bowl combine soup and milk and mix together; pour mixture over chops and veggies. Cover dish with aluminum foil and bake in the preheated oven for 1 hour or until internal temperature of pork has reached 145 degrees F (63 degrees C).\n"
            }
        ],
