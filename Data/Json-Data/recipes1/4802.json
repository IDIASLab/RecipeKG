    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8533/quick-chicken-divan/",
        "name": "Quick Chicken Divan",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F17472.jpg",
            "width": 2288,
            "height": 1712
        },
        "datePublished": "1999-04-24T02:38:44.000Z",
        "description": "My family loves this recipe. My kids love it because the broccoli is 'hidden' under all the chicken and cheese! This is an excellent way to use up leftovers.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (10 ounce) packages frozen chopped broccoli",
            "2 cooked boneless chicken breast halves, chopped",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "\u00bd cup mayonnaise",
            "1 teaspoon lemon juice",
            "1\u2009\u00bd cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place broccoli in the bottom of a 9x13 inch baking dish. Top with the chicken.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, blend the cream of chicken soup, cream of mushroom soup, mayonnaise, and lemon juice. Pour the mixture over the chicken. Top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 35 to 40 minutes in the preheated oven, until bubbly and lightly browned.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast",
            "Creamy"
        ],
