    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23063/moose-marinade/",
        "name": "Moose Marinade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F48253.jpg",
            "width": 567,
            "height": 425
        },
        "datePublished": "2000-10-03T05:46:25.000Z",
        "description": "Even people who say they don't like wild game, will like this!! Also a great marinade for venison.",
        "prepTime": "P0DT0H5M",
        "cookTime": null,
        "totalTime": "P1DT0H5M",
        "recipeYield": "6 cups",
        "recipeIngredient": [
            "1 onion, chopped",
            "1 carrot, cut into thick strips",
            "\u00bd teaspoon chopped fresh thyme",
            "2 bay leaves",
            "3 cloves garlic, crushed",
            "\u215b cup fresh celery leaves",
            "\u215b cup fresh parsley leaves",
            "salt and pepper to taste",
            "2 (750 milliliter) bottles Burgundy wine"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large nonporous glass dish or resealable plastic bag combine the onion, carrot, thyme, bay leaves, garlic, celery, parsley, salt, pepper and wine. Mix well and add roast, chops, or whichever meat being marinated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate to marinate for 24 hours, turning bag or basting occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook drained roast/meat in an oven preheated to 350 degrees F (175 degrees C) for 25 minutes per pound of meat. Baste with strained marinade while cooking.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Marinade Recipes"
