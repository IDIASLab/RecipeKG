    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20414/party-size-potato-chip-chicken-bake/",
        "name": "Party Size Potato Chip Chicken Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Party Size Potato Chip Chicken Bake"
        },
        "datePublished": "2000-04-02T04:46:49.000Z",
        "description": "Twenty pounds of chicken breast and plenty of celery, toasted almonds, lemon juice, cheddar cheese and potato chips - of course -  make this a hearty size party pleaser of a casserole!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "50 servings",
        "recipeIngredient": [
            "20 pounds cooked chicken breast - cut into bite size pieces",
            "5 pounds celery, chopped",
            "4 bunches green onion, diced",
            "3 cups slivered, toasted almonds",
            "1 gallon mayonnaise",
            "2 cups fresh lemon juice",
            "salt and pepper to taste",
            "6 cups crushed potato chip crumbs",
            "4 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C) and lightly grease two 9x13 inch baking dishes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine the chicken, celery, green onion and almonds; mix well. In a separate large bowl mix together mayonnaise and lemon juice and season with salt and pepper to taste. Combine the two mixtures and blend all together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread potato chips in the bottom of the prepared baking dishes, then spread 1/2 of chicken mixture in each dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 35 to 45 minutes, or until bubbly. Top with remaining potato chip crumbs and cheese and bake for another 5 to 10 minutes, or until cheese is melted and bubbly.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
