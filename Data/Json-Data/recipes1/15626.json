    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282592/lemon-curd-thumbprint-cookies/",
        "name": "Lemon Curd Thumbprint Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8883012.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-02-09T05:05:45.000Z",
        "description": "Shortbread cookies are good by themselves, but adding lemon curd and zest makes them absolutely fabulous.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 dozen cookies",
        "recipeIngredient": [
            "1\u2009\u00bd cups unsalted butter, softened",
            "\u00bc cup white sugar",
            "3\u2009\u00bd cups all-purpose flour",
            "1 medium lemon, zested",
            "\u00bd cup lemon curd"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar together in a bowl until thoroughly creamed. Gradually mix in flour, 1 cup at a time. Add lemon zest and mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into 1-inch balls and place on an ungreased cookie sheet. Make an indention into the center of each using your thumb. Put a small spoonful of lemon curd into the depressions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, 15 to 20 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Thumbprint Cookie Recipes"
        ],
