    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7809/birthday-party-cake/",
        "name": "Birthday Party Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Birthday Party Cake"
        },
        "datePublished": "1998-06-07T21:37:15.000Z",
        "description": "Here is another practical birthday cake from my mother's recipe file. It makes a lovely layer cake. You may flavor it with lemon juice if you desire.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 - 8 inch layers",
        "recipeIngredient": [
            "4 eggs",
            "2 cups white sugar",
            "2 cups cake flour",
            "\u00bd teaspoon salt",
            "1 teaspoon vanilla extract",
            "1 cup boiling milk",
            "2 teaspoons baking powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat eggs and sugar together for 5 minutes.  Cover and place in the refrigerator overnight.  In the morning, beat the egg and sugar mixture for 5 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease three 8 inch cake pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour, salt, and baking powder together. Add the flour mixture alternately with the boiling hot milk to the egg and sugar mixture.  Stir in vanilla extract. Pour the batter into the prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 15 to 20 minutes. Let cakes cool then fill and frost with Seven Minute Frosting.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Yellow Cake Recipes"
        ],
