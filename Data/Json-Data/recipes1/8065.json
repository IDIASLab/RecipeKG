        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14880/blue-cheese-dressing/",
        "name": "Blue Cheese Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6799174.jpg",
