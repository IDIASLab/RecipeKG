    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12003/chicken-macaroni-salad/",
        "name": "Chicken Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F44464.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1999-02-26T20:57:00.000Z",
        "description": "A macaroni salad with a twist- chicken! You may need to adjust the amount of mayonnaise once it spends some time in the refrigerator.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT4H0M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2\u2009\u00bd pounds skinless, boneless chicken breast halves",
            "2 cups macaroni",
            "1 (15 ounce) can mixed vegetables, drained",
            "2 cups shredded lettuce",
            "3 cups mayonnaise",
            "\u00bc tablespoon dried basil",
            "salt and pepper to taste",
            "1 pinch garlic powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium skillet over medium heat, cook chicken until no longer pink and juices run clear.  Let rest until cool enough to handle, then shred with two forks.  Set aside 4 cups of shredded meat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium pot with boiling salted water cook elbow macaroni until al dente. Drain and rinse under cold water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, in a large bowl mix together the cooked chicken, drained vegetables, shredded lettuce, and mayonnaise. Add basil, pepper, salt, and garlic powder to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add drained elbow macaroni to chicken mixture. Toss to coat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for several hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Macaroni Salad Recipes"
        ],
