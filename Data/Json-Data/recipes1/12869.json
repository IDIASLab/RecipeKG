        "recipeIngredient": [
            "1 egg",
            "1 cup milk",
            "1 teaspoon vanilla extract",
            "1 cup oil for frying",
            "4 slices bread"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together egg, milk and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet, heat oil over medium high heat. Dip bread into egg mixture and slide into hot oil. Cook until golden brown, drain and serve.\n"
            }
