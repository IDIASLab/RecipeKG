    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10727/cranberry-oatmeal-cookies/",
        "name": "Cranberry Oatmeal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4580686.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "1998-03-26T03:49:56.000Z",
        "description": "A crispy chewy cookie my mom used to make when I lived in Oregon.  Dried cranberries are also divine in this.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "10 tablespoons butter",
            "\u2154 cup packed dark brown sugar",
            "1 teaspoon vanilla extract",
            "2 eggs",
            "1\u2009\u00bd teaspoons baking soda",
            "1 pinch salt",
            "1\u2009\u2154 cups whole wheat flour",
            "1 cup rolled oats",
            "2 cups chopped cranberries",
            "\u00be cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend butter and sugar until fluffy.  Add eggs and vanilla and mix well. In a separate bowl sift flour, baking soda, and salt together.  Stir into butter mixture. Stir in the oats. Fold in nuts and cranberries. Drop spoonfuls of cookie dough onto ungreased cookie sheets, spaced at least 2 inches apart. These cookies spread.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 8 to 10 minutes in the preheated oven, until the edges are crisp and the centers appear dry.  Cool on wire racks and enjoy.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cranberry Dessert Recipes"
        ],
