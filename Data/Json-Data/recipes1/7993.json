    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15991/fresh-apple-cake/",
        "name": "Fresh Apple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2636485.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-11-11T04:53:38.000Z",
        "description": "This easy apple cake gets covered with a sweet cream cheese frosting and chopped nuts.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 10 inch tube or bundt pan",
        "recipeIngredient": [
            "1\u2009\u00bd cups vegetable oil",
            "2 cups white sugar",
            "3 eggs, beaten",
            "2 teaspoons vanilla extract",
            "3 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "2\u2009\u00bd cups apples - peeled, cored and diced",
            "1 cup chopped walnuts",
            "1 cup raisins",
            "1 (16 ounce) container <a href=\"https://www.allrecipes.com/recipe/8379/basic-cream-cheese-frosting/\" aria-label=\"cream cheese frosting\">cream cheese frosting</a>",
            "\u00bc cup finely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease one 10 inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat together the oil, eggs, vanilla and sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift together the flour, baking soda, salt, cinnamon and nutmeg. Add to egg mixture; stir in apples, nuts and raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into prepared pan and bake at 325 degrees F (165 degrees C) for 1 1/2 hrs or until a cake tester inserted in the center comes out clean. Cool and frost with cream cheese icing. Sprinkle top with 1/4 cup fine chopped nuts.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
