    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6732/chocolate-chip-pumpkin-bread/",
        "name": "Chocolate Chip Pumpkin Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F538863.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1998-06-25T17:09:52.000Z",
        "description": "I make this recipe during the holidays. It is moist, and freezes well! I bake them in coffee cans, and wrap them in colored cellophane to give as gifts.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "3 loaves",
        "recipeIngredient": [
            "3 cups white sugar",
            "1 (15 ounce) can pumpkin puree",
            "1 cup vegetable oil",
            "\u2154 cup water",
            "4 eggs",
            "3\u2009\u00bd cups all-purpose flour",
            "1 tablespoon ground cinnamon",
            "1 tablespoon ground nutmeg",
            "2 teaspoons baking soda",
            "1\u2009\u00bd teaspoons salt",
            "1 cup miniature semisweet chocolate chips",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour three 1 pound size coffee cans, or three 9x5 inch loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine sugar, pumpkin, oil, water, and eggs.  Beat until smooth.  Blend in flour, cinnamon, nutmeg, baking soda, and salt.  Fold in chocolate chips and nuts. Fill cans 1/2 to 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 1 hour, or until an inserted knife comes out clean.  Cool on wire racks before removing from cans or pans.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
