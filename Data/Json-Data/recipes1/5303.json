        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13269/chicken-and-rice-soup-ii/",
        "name": "Chicken and Rice Soup II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F775488.jpg",
