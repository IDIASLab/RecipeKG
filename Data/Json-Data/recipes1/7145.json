        "name": "Vanilla Ice Cream VI",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F31101.jpg",
            "width": 353,
            "height": 338
