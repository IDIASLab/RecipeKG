        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 medium potatoes, peeled and cubed",
            "6 medium carrots, chopped",
            "\u00bd cup olive oil",
            "5 cloves garlic, minced",
            "1 tablespoon chopped fresh rosemary, or more to taste",
            "1 tablespoon chopped fresh oregano, or more to taste",
            "1 tablespoon kosher salt",
            "\u00bc teaspoon ground black pepper",
            "\u00bc teaspoon chili powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put potatoes and carrots into a baking pan. Add olive oil, garlic, rosemary, oregano, salt, pepper, and chili powder; mix until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender when poked with a fork, about 35 minutes. Remove from the oven and let cool before serving.\n"
            }
        ],
