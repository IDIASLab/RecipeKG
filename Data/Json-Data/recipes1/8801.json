    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16315/poertzelki/",
        "name": "Poertzelki",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1106923.jpg",
            "width": 1664,
            "height": 2496
        },
        "datePublished": "1999-12-23T18:31:35.000Z",
        "description": "This not-too-sweet treat is traditionally served only on New Year's Day.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H50M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups warm water (110 degrees F/45 degrees C)",
            "1 tablespoon active dry yeast",
            "2 tablespoons white sugar",
            "4 cups all-purpose flour, divided",
            "4 eggs, lightly beaten",
            "\u00bc cup butter, room temperature",
            "2\u2009\u00bd teaspoons salt",
            "\u00bd cup milk",
            "3 cups raisins",
            "vegetable oil for frying"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve yeast in warm water; add sugar and 2 cups flour. Mix to combine, cover bowl with plastic, and let rise about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in remaining flour, eggs, butter, salt, milk, and raisins until fully combined. Cover bowl and let rise again for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by tablespoonfuls in hot fat and cook until brown on both sides. Cookie usually flips over when done on the first side. Be sure fat isn't too hot or your cookie may be raw inside when dark on the outside. Drain on paper towels. Sprinkle with granulated or powdered sugar, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
