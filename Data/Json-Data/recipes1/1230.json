        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6663/crispy-cheese-twists/",
        "name": "Crispy Cheese Twists",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1838958.jpg",
