    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12495/black-bottom-pie-i/",
        "name": "Black Bottom Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Black Bottom Pie I"
        },
        "datePublished": "1998-12-24T16:53:08.000Z",
        "description": "A 'more rum than most' Black Bottom Pie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie crust, baked",
            "1 (.25 ounce) package unflavored gelatin",
            "1 tablespoon cornstarch",
            "1\u2009\u00bc cups white sugar",
            "1\u2009\u00be cups milk",
            "1 teaspoon vanilla extract",
            "4 egg yolks",
            "1\u2009\u2153 cups semisweet chocolate chips",
            "\u00bd cup rum",
            "4 egg whites",
            "\u00bc cup cold water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve gelatin in cold water, and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, mix cornstarch, 3/4 cup sugar, milk, and egg yolks.  Cook, stirring, until bubbly and thick.  Remove from heat, and add vanilla.  Divide mixture in half.  Add chocolate chips to one half, and stir until melted and smooth.  Pour into pastry shell.  Chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir gelatin mixture into the other half of the hot egg yolk mixture.  Stir in rum.  Chill until slightly thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until soft peaks form.  Gradually add remaining 1/2 cup sugar, and beat to stiff peaks.  Fold into partially set gelatin/rum mixture. Chill until mix will mound, then spoon into pie shell on top of chocolate layer. Chill overnight.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes",
            "Rum"
        ],
