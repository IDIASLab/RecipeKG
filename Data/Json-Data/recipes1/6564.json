        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12060/garlic-butter-sauce-ii/",
        "name": "Garlic Butter Sauce II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1228969.jpg",
