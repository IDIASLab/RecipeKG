        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup corn syrup   ",
            "\u00bd cup peanut butter",
            "\u00bd cup packed brown sugar",
            "2 cups crispy rice cereal",
            "1 cup confectioners' sugar",
            "2 tablespoons butter",
            "2 tablespoons milk",
            "1 pinch salt",
            "2 teaspoons unsweetened cocoa powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In saucepan, over medium heat, melt corn syrup, peanut butter and brown sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and add cereal.  Mix well and press into buttered 9  x  9 inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make icing:  Combine the rest of the ingredients.  Add more milk, if necessary, until smooth and spreadable.\n"
            }
        ],
