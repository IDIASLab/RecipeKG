        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8005/two-step-creamy-cheesecake/",
        "name": "Two Step Creamy Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7646429.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "1998-06-29T18:27:08.000Z",
        "description": "My Mom use to make this cheesecake for us kids when we were coming home from school. Very easy to do, and Yummy.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch cheesecake",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u2153 cup white sugar",
            "4 cups frozen whipped topping, thawed",
            "1 (9 inch) prepared graham cracker crust",
            "\u00bd pint sliced fresh strawberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and sugar until smooth.  Fold  in whipped topping.  Spoon mixture into graham crumb crust.  Refrigerate for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Once the cheesecake has set, garnish with sliced strawberries.  Serve.\n"
            }
        ],
