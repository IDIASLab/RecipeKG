    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11137/mosaic-cookies/",
        "name": "Mosaic Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mosaic Cookies"
        },
        "datePublished": "1998-12-01T00:16:46.000Z",
        "description": "These are great for gifts, wrap rolls in cellophane and tie with ribbon!  Also a hit at cookie exchanges!  They are very colorful and festive-looking!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "4 (1 ounce) squares semi-sweet chocolate",
            "3 tablespoons margarine",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "1 egg white",
            "1 cup confectioners' sugar",
            "\u00bd pound rainbow colored miniature marshmallows",
            "1 cup chopped walnuts",
            "\u00be cup graham cracker crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt the chocolate squares and margarine together. Stir in the vanilla and the pinch salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg white and slowly add the confectioners' sugar. Blend well. Stir in the chocolate mixture, the marshmallows and the chopped nuts. Blend well. Shape mixture into a log shape and roll firmly in the graham cracker crumbs. Refrigerate. Slice log when ready to serve.\n"
            }
        ],
