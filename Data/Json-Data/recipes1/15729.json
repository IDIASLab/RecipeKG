        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray the inside of a 9x13 casserole dish with olive oil cooking spray. Place asparagus in the dish and lightly spray spears with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle asparagus with Parmesan cheese, sea salt, and garlic powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast in preheated oven until fork easily punctures thickest part of stem, about 12 minutes.\n"
            }
        ],
