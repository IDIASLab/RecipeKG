        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220398/tangy-sweet-potato-fries/",
        "name": "Tangy Sweet Potato Fries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F803647.jpg",
            "width": 3968,
            "height": 2976
        },
        "datePublished": "2012-02-28T22:59:53.000Z",
        "description": "These turned my non-sweet potato-loving husband into a 'that's my favorite dish' kind of man!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "cooking spray",
            "3 large sweet potatoes, cut into wedges",
            "\u00bd cup zesty Italian salad dressing",
            "\u00bc cup grated Parmesan cheese",
            "1 tablespoon garlic powder",
            "cracked black pepper, or to taste",
            "1 tablespoon grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spray a baking sheet with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sweet potato wedges, Italian dressing, 1/4 cup Parmesan cheese, and garlic powder together in a bowl until sweet potatoes are evenly coated. Season with black pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange potato wedges on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 to 20 minutes, then flip potatoes over; continue baking until easily pierced with a fork, 15 to 20 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle sweet potato fries with the remaining 1 tablespoon Parmesan cheese to serve.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Sweet Potatoes"
        ],
