    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13222/jambalaya-i/",
        "name": "Jambalaya I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2839416.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-05-22T01:57:56.000Z",
        "description": "This is just a touch of New Orleans cuisine, zesty and flavorful! Plus it has the ease of being made in a slow cooker.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2 cups diced ham",
            "2 onion, chopped",
            "2 stalks celery, diced",
            "1 green bell pepper, chopped",
            "2 (14.5 ounce) cans stewed tomatoes",
            "\u00bc cup tomato paste",
            "3 cloves garlic, minced",
            "1 tablespoon minced fresh parsley",
            "\u00bd teaspoon dried thyme",
            "2 whole cloves",
            "2 tablespoons vegetable oil",
            "1 cup converted long-grain white rice",
            "1 pound medium shrimp - peeled and deveined"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a slow cooker combine and thoroughly mix the ham, onions, celery, bell pepper, tomatoes, tomato paste, garlic, parsley, thyme leaves, cloves, salad oil and rice. Cover and cook on low for 8 to 10 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "One hour before serving, turn slow cooker to high. Stir in the uncooked shrimp. Cover and cook until the shrimp are pink and tender.\n"
            }
        ],
