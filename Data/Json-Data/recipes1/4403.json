    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13144/spicy-and-sour-soup/",
        "name": "Spicy and Sour Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6698599.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-03-23T21:23:30.000Z",
        "description": "This spicy and sour soup could be a light meal in itself, full of peppers, snow peas, water chestnuts, green onions, and tofu.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "3 tablespoons vegetable oil",
            "2 red bell peppers, julienned",
            "1 bunch green onions, sliced diagonally into 1/2 inch pieces",
            "2 cups chicken broth",
            "2 cups vegetable stock",
            "2 tablespoons soy sauce",
            "2 teaspoons red wine vinegar",
            "\u00bd teaspoon crushed red pepper flakes",
            "\u00bc teaspoon salt",
            "\u215b teaspoon ground black pepper",
            "2 tablespoons cornstarch",
            "3 tablespoons water",
            "1 teaspoon sesame oil",
            "\u00bd pound fresh snow peas",
            "1 pound firm tofu, cubed",
            "1 (8 ounce) can sliced water chestnuts, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large saucepan over medium-high heat. Add peppers and onion; stir-fry about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chicken broth, vegetable stock, and soy sauce. Bring to a boil. Lower heat and let simmer for about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together vinegar, red pepper flakes, salt, pepper, cornstarch, water, and sesame oil in a small bowl until smooth. Add to soup with fresh snow peas; cook for about 5 minutes or until thickened and bubbly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add tofu and water chestnuts. Gently heat through, about 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes"
        ],
