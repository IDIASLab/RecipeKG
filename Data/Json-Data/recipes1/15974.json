    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285207/blueberry-lemon-cereal-cookies/",
        "name": "Blueberry-Lemon Cereal Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F809274.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-01-11T00:10:32.000Z",
        "description": "You would never guess that these tender little morsels are whole grain. Not a soft, cake-like cookie, but crisp.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1 cup whole wheat flour",
            "1 cup cornflakes, lightly crushed",
            "1 cup crispy rice cereal",
            "\u00bd cup rolled oats",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u00bd cup canola oil",
            "\u00bd cup packed brown sugar",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract",
            "1 large egg",
            "1 medium lemon, zested",
            "\u00bd cup dried blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk flour, cornflakes, rice cereal, oats, baking soda, and salt together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat canola oil, sugars, vanilla, egg, and lemon zest in a separate bowl until well blended. Stir in dried blueberries. Add to the dry ingredients and stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough 2 inches apart onto insulated cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until just starting to brown, 10 to 12 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely.\n"
            }
        ],
