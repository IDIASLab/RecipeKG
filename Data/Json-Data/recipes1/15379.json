        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10123/make-ahead-toffee-bars/",
        "name": "Make Ahead Toffee Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F971109.jpg",
            "width": 1200,
            "height": 1200
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This recipe uses a make ahead mix that can be stored in the refrigerator for up to 4 weeks.  Add the ingredients listed below to the make ahead mix for delicious Toffee Bars.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups Make Ahead Cookie Mix",
            "1 cup packed brown sugar",
            "5 tablespoons butter",
            "1 teaspoon vanilla extract",
            "1 egg",
            "1 cup semisweet chocolate chips",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).  Grease a  9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Make Ahead Cookie Mix with brown sugar, butter, vanilla and egg.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread dough in pan.  Bake 17 to 24 minutes until top springs back when touched.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Immediately sprinkle with chocolate chips.  Let stand 2-3 minutes to melt then spread chocolate over bars and sprinkle with nuts.  Cut when cooled.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Bar Cookie Recipes"
        ],
