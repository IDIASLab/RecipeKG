        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11230/puppy-chow-ii/",
        "name": "Puppy Chow II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1826346.jpg",
            "width": 1704,
            "height": 1705
