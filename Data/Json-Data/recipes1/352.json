    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10306/half-cup-cookies/",
        "name": "Half-Cup Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Half-Cup Cookies"
        },
        "datePublished": "1997-11-13T13:49:28.000Z",
        "description": "These crunchy drop cookies are nutritious enough for an on-the-go breakfast, when coupled with a glass of milk.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2 cups whole wheat flour",
            "1 teaspoon baking powder",
            "\u00be teaspoon salt",
            "1 teaspoon ground cinnamon",
            "2 eggs",
            "\u00bc cup milk",
            "\u00bd teaspoon vanilla extract",
            "\u00bd cup peanut butter",
            "\u00bd cup honey",
            "\u00bd cup granola",
            "\u00bd cup packed brown sugar",
            "\u00bd cup butter, softened",
            "\u00bd cup semisweet chocolate chips",
            "\u00bd cup chopped walnuts",
            "\u00bd cup flaked coconut",
            "\u00bd cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, salt, and cinnamon; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large bowl, beat eggs, milk, and vanilla.  Beat in peanut butter, honey, brown sugar, and butter until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour mixture until well blended.  Stir in chocolate chips, nuts, coconut, raisins, and granola until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop batter by heaping spoonful 1 inch apart on lightly greased baking sheets.  Bake at 375 degrees F (190 degrees C) for 10 minutes until golden on bottom.  Cool on rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
