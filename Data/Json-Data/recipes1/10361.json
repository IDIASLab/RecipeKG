    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18406/baked-potato-salad-ii/",
        "name": "Baked Potato Salad II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4471323.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "2000-03-22T03:44:43.000Z",
        "description": "A cheesy potato salad perked up by the addition of crunchy bacon and savory green olives. This one will require that you have a copy of the recipe with you, because everyone will want it!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "8 potatoes",
            "\u00bd pound bacon",
            "\u00bc cup chopped onion",
            "1 cup mayonnaise",
            "1 pound processed American cheese, cubed",
            "salt and pepper to taste",
            "\u00bd cup pitted and sliced green olives"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add whole potatoes to large pot of boiling, salted water.  Boil for 20 minutes, or until tender. Remove potatoes from the water and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain, crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel and cube cooled potatoes, and transfer to a large bowl.  Add onion, mayonnaise, cheese, salt and pepper, and mix together with the potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place mixture in a lightly oiled 9x11 inch baking dish.  Top with the crumbled bacon and the olives.  Bake uncovered for 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Potato Salad Recipes",
            "Creamy Potato Salad Recipes"
        ],
