    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17725/chocolate-chip-muffins-ii/",
        "name": "Chocolate Chip Muffins II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F969421.jpg",
            "width": 1536,
            "height": 1536
        },
        "datePublished": "2000-01-11T06:03:17.000Z",
        "description": "Very tasty muffins with yogurt for moisture and plenty of chocolate chips.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "\u00bd cup butter, softened",
            "\u00bd cup white sugar",
            "2 eggs",
            "1 cup milk",
            "\u00bd cup plain yogurt",
            "1 teaspoon vanilla extract",
            "3 cups self-rising flour",
            "\u00bd cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease 12 muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together butter and sugar. Beat in eggs one at a time then stir in the milk, yogurt and vanilla. Blend in the flour, then fold in the chocolate chips. Scoop batter into the prepared muffin cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in prepared oven for 15 to 20 minutes. Move to a wire rack to cool.\n"
            }
        ],
