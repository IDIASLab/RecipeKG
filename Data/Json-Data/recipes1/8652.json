    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11663/kourambiedes-iii/",
        "name": "Kourambiedes III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F29952.jpg",
            "width": 604,
            "height": 453
        },
        "datePublished": "1999-12-16T03:25:10.000Z",
        "description": "I got this butter ball recipe from my mother-in-law  who is Greek. I make a double batch every Christmas and my hubby eats most of them. Enjoy!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup unsalted butter",
            "4 tablespoons confectioners' sugar",
            "1 teaspoon vanilla extract",
            "2 cups sifted all-purpose flour",
            "1 cup chopped pecans",
            "36 whole cloves",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream the butter and confectioners' sugar together. Stir in the vanilla, then the flour, then the pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into walnut sized balls and insert a clove into each one. Place on an unprepared cookie sheet and bake for 15 to 18 minutes in the preheated oven. Roll the cookies in powdered sugar while they are still hot. Remove cloves before serving or warn guests to remove them.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Greek"
        ],
