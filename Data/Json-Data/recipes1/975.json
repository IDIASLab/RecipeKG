    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10703/thumbprint-cookies-ii/",
        "name": "Thumbprint Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1538678.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1998-03-22T01:12:29.000Z",
        "description": "Small, delicious cookies with a small indentation filled with jam or preserves. Use your favorite nuts to coat the cookies.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H13M",
        "totalTime": "P0DT0H43M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup butter, room temperature",
            "\u00bc cup packed brown sugar",
            "1 egg",
            "\u00bd teaspoon vanilla extract",
            "1 cup all-purpose flour",
            "\u00bc teaspoon salt",
            "\u00be cup finely chopped pecans",
            "\u2153 cup apricot preserves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Grease 2 baking sheets or line them with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate the egg, reserving the egg white. Mix the butter and brown sugar; beat in the egg yolk. Stir in the vanilla. Mix in the flour and salt, stirring until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly beat the egg white.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape the dough into balls. Roll in beaten egg white, then nuts. Place the cookies on the baking sheets about 2 inches apart. Bake in the preheated oven for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove cookies from oven. With thumb,  dent each cookie. Put jelly or preserves in each thumbprint. Bake for another 8 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Thumbprint Cookie Recipes"
        ],
