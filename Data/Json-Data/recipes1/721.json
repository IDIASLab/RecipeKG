        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10519/bachelor-buttons-i/",
        "name": "Bachelor Buttons I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2197817.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1998-02-14T18:52:31.000Z",
        "description": "An old favorite of my mother's.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 3 1/2 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "\u00bd teaspoon salt",
            "4 teaspoons baking powder",
            "4 tablespoons white sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 cup butter",
            "\u00bd cup milk",
            "1 cup raspberry preserves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease muffing tins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients together, cut in butter with fork or pastry cutter. Beat egg, add milk and vanilla then stir quickly into dry mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out on well floured surface to about 1/4 inch thickness and cut with round cookie cutter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place in muffin tins and drop a teaspoon of raspberry jam in center.  Fold sides in slightly and bake for 20-25 minutes or until lightly golden.\n"
            }
        ],
