        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9363/leg-of-lamb/",
        "name": "Leg of Lamb",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1121971.jpg",
