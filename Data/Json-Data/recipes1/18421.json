    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229448/chef-johns-granola/",
        "name": "Chef John's Granola",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3956850.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2012-10-04T18:11:22.000Z",
        "description": "There's no better way to enjoy sweet, juicy, late-summer fruit than a parfait with yogurt and crunchy, homemade granola.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "\u00bc cup packed brown sugar",
            "3 tablespoons maple syrup",
            "2 tablespoons vegetable oil",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups rolled oats",
            "\u00bd cup chopped almonds",
            "\u00bd cup raw pumpkin seeds",
            "\u2153 cup shredded coconut",
            "\u00bd cup dried currants"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C). Line a baking sheet with a silicone liner.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, maple syrup, oil, and salt in a bowl. Add oats, almonds, pumpkin seeds, and coconut. Mix well, making sure all ingredients are coated evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour onto prepared baking sheet and distribute in an even layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 1 hour, stirring every 10 to 15 minutes lightly with the tines of a fork so mixture toasts evenly and granola is golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a bowl and, while mixture is still hot, mix in currants. Cool. Store in an airtight container until ready to use.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Cereals",
            "Granola Recipes"
        ],
