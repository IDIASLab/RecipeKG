    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19115/amazin-raisin-cake/",
        "name": "Amazin' Raisin Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1126186.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2000-05-24T01:04:02.000Z",
        "description": "This is a copy of the recipe from my Grandmothers' recipe book. It's an excellent apple spice raisin cake that has the amazing ingredient of mayonnaise!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 - 2-layer 9 inch cake",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "2 cups white sugar",
            "1 cup mayonnaise",
            "\u2153 cup milk",
            "2 eggs",
            "2 teaspoons baking soda",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground cloves",
            "3 cups apple - peeled, cored, and chopped",
            "1 cup raisins",
            "1 cup coarsely chopped walnuts",
            "2 cups heavy cream",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour two 9 inch pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large bowl with mixer at low speed, beat together flour, sugar, mayonnaise, milk, eggs, baking soda, cinnamon, nutmeg, salt, and cloves, scraping bowl frequently, for 2 minutes. Batter will be thick. Sir in the chopped apples, raisins and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread batter into prepared pan. Bake in the preheated oven for 45 minutes, or until a toothpick inserted into the center of the cake comes out clean. Cool in pans on wire racks for 10 minutes. Remove and cool on wire racks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, whip the cream and confectioners' sugar until stiff peaks form. Fill and frost the cake.\n"
            }
        ],
