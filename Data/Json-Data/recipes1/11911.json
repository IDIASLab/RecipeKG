    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/24088/buffalo-chicken-wings-ii/",
        "name": "Buffalo Chicken Wings II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F228851.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-06-18T06:53:37.000Z",
        "description": "Wings dredged in flour and baked with the flavors of blue cheese salad dressing, pepper sauce and vinegar.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "24 pieces",
        "recipeIngredient": [
            "12 wing, bone removeds chicken wings, split and tips discarded",
            "3 tablespoons butter, divided",
            "\u00bc cup all-purpose flour",
            "1 tablespoon distilled white vinegar",
            "3 teaspoons hot pepper sauce",
            "\u00bc teaspoon salt",
            "\u00bd cup blue cheese salad dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 2 tablespoons butter in a 9x13 inch baking dish in preheated oven. Coat chicken with flour, shake off excess and place in dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake uncovered in preheated oven for 20 minutes. Turn pieces and bake uncovered for another 20 to 25 minutes, or until chicken is cooked through and no longer pink inside when thickest pieces are cut in the center. Drain on paper toweling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl combine the remaining 1 tablespoon butter, vinegar, pepper sauce and salt and mix until well blended. Add chicken and toss until evenly coated with mixture. Serve with bleu cheese salad dressing.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "Sparkle"
        ],
