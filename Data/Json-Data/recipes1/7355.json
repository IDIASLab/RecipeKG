    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16782/no-peek-irish-beef-stew/",
        "name": "No Peek Irish Beef Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F606322.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "1999-10-11T20:07:55.000Z",
        "description": "Wonderful thick, spicy stew that is so easy to make that you can do it on the busiest of days! Great for a crowd!! No peeking until it's done!!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT5H0M",
        "totalTime": "P0DT5H10M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 pounds cubed stew meat",
            "6 carrots, chopped",
            "2 onions, chopped",
            "2 cups cubed potatoes",
            "1 tablespoon brown sugar",
            "3 tablespoons tapioca flour",
            "1 cup tomato-vegetable juice cocktail"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a roasting pan, combine the meat, carrots, onions and potatoes. In a separate bowl, combine the sugar, tapioca flour and juice cocktail and mix well. Pour this mixture over the meat and vegetables and cover tightly with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 5 hours.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Irish Stew Recipes"
        ],
