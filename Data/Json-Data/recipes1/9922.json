    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19695/herbed-cheese-puffs/",
        "name": "Herbed Cheese Puffs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7261297.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-02-26T21:21:53.000Z",
        "description": "Filled with a savory cheese and herb mixture, these puffy biscuits are simple, delicious and make a great party appetizer. I have made these for years and they are always a hit!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H12M",
        "totalTime": "P0DT0H22M",
        "recipeYield": "30 puffs",
        "recipeIngredient": [
            "1 (4 ounce) package grated Parmesan cheese",
            "4 ounces Romano cheese, grated",
            "1 cup mayonnaise",
            "2 teaspoons minced fresh basil",
            "2 teaspoons minced fresh oregano",
            "1 bunch green onions, chopped",
            "1 clove garlic, minced",
            "1 (12 ounce) can refrigerated biscuit dough"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C). Lightly grease a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together Parmesan cheese, Romano cheese, mayonnaise, basil, oregano, green onions and garlic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate each unbaked biscuit vertically into 3 pieces. Spread 1 tablespoon cheese mixture on each piece.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange pieces in a single layer on baking sheet. Bake in the preheated oven 10 to 12 minutes, or until puffed and golden brown. Serve warm!\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Cheese"
        ],
