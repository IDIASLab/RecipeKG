    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18060/simple-cauliflower-soup/",
        "name": "Simple Cauliflower Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F154109.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-04-16T02:31:41.000Z",
        "description": "A nice and simple healthy soup. First the cauliflower is sauteed in butter with potatoes and onions, and then simmered in vegetable stock.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons unsalted butter",
            "1 onion, chopped",
            "1 head cauliflower, broken into small florets",
            "1 potato, peeled and diced",
            "2 cups vegetable stock",
            "2 cups milk",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt the butter in a large pot over medium heat. Saute the onion in the butter for 5 minutes. Stir in the cauliflower and potato and saute for 5 more minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in the stock, bring to a boil, reduce heat to low and simmer for 20 minutes, or until all vegetables are tender. Add the milk, stirring well to blend. Season with salt and pepper to taste.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Cauliflower"
        ],
