    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11394/elenas-cookies/",
        "name": "Elena's Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1059198.jpg",
            "width": 1952,
            "height": 3264
        },
        "datePublished": "1999-05-05T20:35:44.000Z",
        "description": "This is a recipe I created with my 4 year old daughter with things we had on hand.  The cookies are delicious and soft.  She and my 17 month old son both loved them!  You can use rice, oatmeal, or mixed infant cereal.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 1/2 dozen",
        "recipeIngredient": [
            "\u2154 cup butter, softened",
            "\u00bd cup applesauce",
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 cup dry infant cereal",
            "1 cup all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd cup wheat germ"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease or line baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium sized bowl mix the shortening, applesauce, white sugar, brown sugar, egg and vanilla. Beat until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In another bowl combine the infant cereal, flour, baking soda, salt and wheat germ. Add the dry ingredients to the wet ingredients and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon tablespoonfuls of dough onto the prepared baking sheets. Bake at 375 degrees F (190 degrees C) for 8 to 10 minutes or until just set. Let cool on wire racks before storing.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
