        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Trim the ends off of the radishes and peel a band of radish-skin from around the middle of the radish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Steam the radishes in a covered microwave safe container for 8 minutes, or until fork tender. Drain and toss with butter, serve immediately.\n"
            }
