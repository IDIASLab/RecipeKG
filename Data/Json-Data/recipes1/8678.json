    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14948/tomato-bacon-squares/",
        "name": "Tomato Bacon Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F878537.jpg",
            "width": 2119,
            "height": 1715
        },
        "datePublished": "1999-12-17T14:23:33.000Z",
        "description": "Swiss cheese perfectly compliments the bacon, tomatoes & bell peppers.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 appetizers",
        "recipeIngredient": [
            "6 slices bacon",
            "\u2153 cup chopped green bell pepper",
            "\u2153 cup chopped onion",
            "4 roma (plum) tomatoes, seeded and chopped",
            "1 teaspoon dried basil",
            "2 tablespoons mayonnaise",
            "1 clove crushed garlic",
            "1 refrigerated pizza crust dough",
            "\u00be cup shredded Swiss cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet over medium heat. Fry bacon until crisp. Drain on paper towels.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Crumble bacon into a medium-size mixing bowl. Mix in bell pepper, tomatoes, and basil. In a separate small bowl, combine mayonnaise and garlic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll pizza crust into a 12x15 inch rectangular baking sheet. Spread the mayonnaise mixture evenly over the crust. Sprinkle the bacon mixture over the mayonnaise, and top the entire pizza with cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 18 to 20 minutes or until the top is bubbly and the crust is golden brown. Cool and cut the pizza into 24 squares.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Bacon",
            "Appetizers"
        ],
