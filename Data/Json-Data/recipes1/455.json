        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10363/miami-chocolate-chip-cookies/",
        "name": "Miami Chocolate Chip Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F25014.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "1997-12-16T13:18:14.000Z",
        "description": "These cookies are moist and very tasty. My kids' favorites!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00be cup butter",
            "\u00be cup white sugar",
            "\u00be cup packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd cup sour cream",
            "2 cups semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix butter, sugars, eggs and vanilla until smooth. In a small bowl, mix flour and baking powder. Add the flour mixture to the butter mixture and slowly add the sour cream. Mix well and add the chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place by teaspoonful on an ungreased cookie sheet. Bake for 9 to 10 minutes or until slightly golden. Remove from cookie sheet and cool.  Enjoy!\n"
            }
        ],
