    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14001/pineapple-fried-rice/",
        "name": "Pineapple Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2621.jpg",
            "width": 3008,
            "height": 2000
        },
        "datePublished": "1999-07-27T21:38:31.000Z",
        "description": "A version of fried rice using tofu and pineapple. Light, tasty, and easy. Serve with hot sauce or extra soy sauce.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (8 ounce) can crushed pineapple with juice",
            "4 cups water",
            "2 cups white rice",
            "1 tablespoon peanut or walnut oil",
            "2 eggs, beaten",
            "\u00bd teaspoon sesame oil",
            "1 (12 ounce) package tofu, diced",
            "\u00be cup chopped mushrooms",
            "3 tablespoons soy sauce",
            "3 green onions, thinly sliced",
            "1 cup diced carrots"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Open can of crushed pineapple and drain juice into a cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, combine the liquid from the can of crushed pineapple with 3 cups water, bring to a boil. Add rice. Bring mixture to boil. Cover and reduce heat to simmer. Cook 25 to 30 minutes or until rice is tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a non-stick wok heat the 1 tablespoon walnut or peanut oil.  Add the eggs and cook without stirring, until set.  Slide eggs out of the wok to a plate - cut into short, narrow strips.  In the same wok, heat the sesame oil and stir fry the tofu with the mushrooms, soy sauce, green onions, and carrots for about 4 minutes.  Stir in cooked rice, pineapple, and egg strips. Heat until everything is heated through.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Rice",
            "Fried Rice Recipes"
        ],
