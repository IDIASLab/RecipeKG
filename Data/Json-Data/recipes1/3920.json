        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Saute bacon and onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lentils, bacon, onion, celery, carrots, water or stock, tomatoes, and bay leaf together in a large pot and simmer until tender, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sliced wieners and salt and pepper to taste, and continue cooking for about 10 minutes.\n"
            }
