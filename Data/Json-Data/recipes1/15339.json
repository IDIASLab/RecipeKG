    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9670/granola-bars-i/",
        "name": "Granola Bars I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F335642.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "My son makes these bars for camping trips and soccer tournaments.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00be cup all-purpose flour",
            "\u00be cup whole wheat flour",
            "\u00bd cup ground walnuts",
            "\u00be teaspoon salt",
            "2 teaspoons baking powder",
            "\u00bd cup shortening",
            "1\u2009\u00be cups packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1\u2009\u2153 cups granola",
            "\u00bd cup dried cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Lightly grease a 9 inch square pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, cream the shortening, brown sugar, eggs, and vanilla extract.  Beat well after the addition of each ingredient.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In small saucepan, cover the cranberries in water and boil for 10 minutes.  Drain and chop into small pieces.  Add to egg mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly add in white and wheat flours, walnuts, salt and baking powder.  Mix until well blended. Fold in granola and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread dough into baking pan and bake 20 to 25 minutes.  Let cool before cutting into bars.  Store in airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Granola Bar Recipes"
