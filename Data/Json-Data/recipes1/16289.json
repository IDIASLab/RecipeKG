    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282904/chicken-carbonara/",
        "name": "Chicken Carbonara",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F461249.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-09-01T21:10:18.000Z",
        "description": "This chicken carbonara is simply delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package spaghetti",
            "2 teaspoons olive oil",
            "1 (4 ounce) package pancetta bacon, diced",
            "2 cloves garlic, minced",
            "2\u2009\u00bd cups whipping cream",
            "1 cup freshly grated Parmesan cheese",
            "8 large egg yolks",
            "\u00bc cup chopped fresh basil",
            "\u00bc cup chopped fresh Italian parsley",
            "4 cups shredded cooked chicken",
            "salt and freshly ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally, until tender yet firm to the bite, about 12 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat olive oil in a large, heavy frying pan over medium heat. Add pancetta and garlic; saute until brown and crisp, about 8 minutes. Remove from the heat and cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk cream, Parmesan, egg yolks, basil, and parsley together in a large bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir chicken into the pancetta mixture until combined. Add cooked spaghetti and cream mixture. Toss over medium-low heat until chicken is heated through and sauce forms a thick coating over the spaghetti, about 4 minutes. Do not boil or the eggs will scramble. Season with salt and pepper and transfer to a large, wide serving bowl.\n"
            }
        ],
