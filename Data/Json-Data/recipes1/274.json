    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10041/drop-butter-cookies/",
        "name": "Drop Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F41773.jpg",
            "width": 1664,
            "height": 1248
        },
        "datePublished": "1997-10-24T02:43:37.000Z",
        "description": "I found this recipe in my Grandmother 's file box. They just melt in your mouth. It made a big hit with my family.  You can also use chocolate chips or nuts instead of raisins.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "\u2153 cup white sugar",
            "1 egg",
            "\u00be cup all-purpose flour",
            "\u00bd teaspoon vanilla extract",
            "2 tablespoons raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter, and sugar continuing to beat. Beat in egg, then flour, and then vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop 2 inches apart on a greased cookie sheet, and top with a raisin, chocolate chip, or nut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
