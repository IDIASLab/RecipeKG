    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11367/nutmeg-logs/",
        "name": "Nutmeg Logs",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Nutmeg Logs"
        },
        "datePublished": "1999-04-19T02:14:10.000Z",
        "description": "This recipe has won best of show, a blue ribbon and three white ribbons. They are nicely spiced and are lovely to present.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 teaspoon ground nutmeg",
            "1 cup butter",
            "\u00be cup white sugar",
            "1 egg",
            "1 tablespoon vanilla extract",
            "2 teaspoons rum flavored extract",
            "3 tablespoons butter",
            "1 teaspoon vanilla extract",
            "1 teaspoon rum flavored extract",
            "2\u2009\u00bd cups confectioners' sugar",
            "2\u2009\u00be tablespoons evaporated milk",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream 1 cup of the butter with the 1 teaspoon nutmeg and the white sugar until light and fluffy. Beat in the egg, 1 tablespoon vanilla and 2 teaspoon rum flavoring. Gradually mix in the flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a lightly floured surface, shape pieces of the dough into long rolls 1/2 inch in diameter. Cut the rolls in 3 inch lengths (to look like small logs). Place cookies on ungreased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 12 to 15 minutes or until light golden brown. Let cookies cool then spread with frosting on tops and sides. Mark frosting with the tines of a fork to resemble bark on a log. Sprinkle lightly with nutmeg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting: Cream 3 tablespoons of the butter with 1 teaspoon of the vanilla and 1 teaspoon of the rum flavoring. Blend in 1/2 cup of the confectioners' sugar and beat well. Add the remainder of the confectioners' sugar alternately with the evaporated milk. Beat well after each addition and continue to beat until of spreading consistency.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
