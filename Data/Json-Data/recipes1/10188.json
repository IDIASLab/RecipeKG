        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16733/bobs-blue-cheese-dressing/",
        "name": "Bob's Blue Cheese Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1024247.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2000-03-13T17:39:47.000Z",
        "description": "Prepare a flavorful summer salad by serving this dressing over mixed mesclun greens, hearts of romaine, sliced cucumber, tomato, and red onion.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "1 cup crumbled blue cheese",
            "1 cup mayonnaise",
            "1 cup sour cream",
            "2 teaspoons lemon juice",
            "1 teaspoon hot pepper sauce",
            "2 teaspoons Worcestershire sauce",
            "2 tablespoons chopped fresh parsley",
            "1 tablespoon honey",
            "1 teaspoon ground black pepper",
            "1 teaspoon sea salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, whisk together the blue cheese, mayonnaise, sour cream, lemon juice, hot pepper sauce, Worcestershire sauce, parsley, honey, pepper, and salt.  Chill until serving.\n"
            }
        ],
