    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14125/quick-vegan-gravy/",
        "name": "Quick Vegan Gravy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F723028.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-10-22T18:19:12.000Z",
        "description": "Delicious vegan gravy! This is great  to make biscuits and gravy or for Thanksgiving over mashed potatoes. Pareve margarine is Kosher margarine, it can be bought at most grocery stores.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 1/2 cups",
        "recipeIngredient": [
            "1 tablespoon pareve margarine",
            "\u00bc cup all-purpose flour",
            "3 cups vegetable broth",
            "2 tablespoons miso paste",
            "2 tablespoons warm water",
            "2 teaspoons soy sauce",
            "\u00bc teaspoon onion powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl combine miso and warm water; stir until miso has dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a 2 quart saucepan, melt margarine over medium heat. Stir in flour until dissolved. Add vegetable broth, miso mixture, soy sauce and onion powder. Cook, stirring, over medium heat, until thickened.\n"
            }
        ],
