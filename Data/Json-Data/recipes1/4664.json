        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13204/avocado-frappe/",
        "name": "Avocado Frappe",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F41961.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-19T06:32:58.000Z",
        "description": "Very simple and tasty. A refreshing, chilled frappe for springtime!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "2 avocados",
            "1 tablespoon orange zest",
            "\u215b teaspoon ground ginger",
            "1 teaspoon salt",
            "1 pinch ground black pepper",
            "3 cups milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel avocados and remove the pits. Cut flesh into chunks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put into blender with orange peel, ginger salt and pepper. Blend, adding milk a little at a time, until mixture is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into dish, cover and refrigerate. Serve chilled. Garnish with avocado slices if desired.\n"
            }
        ],
