    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285505/maple-pecan-french-toast-bake/",
        "name": "Maple-Pecan French Toast Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Maple-Pecan French Toast Bake"
        },
        "datePublished": "2008-09-10T17:46:11.000Z",
        "description": "This French toast bake is a savory breakfast/brunch dish that's always a hit!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT9H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups milk",
            "1\u2009\u00bd cups half-and-half",
            "6 large eggs",
            "1 tablespoon maple syrup",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "1 (1 pound) loaf French bread, cut into 1-inch slices",
            "1 cup light brown sugar",
            "1 cup coarsely chopped pecans",
            "\u00bd cup unsalted butter, softened",
            "2 tablespoons maple syrup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix milk, half-and-half, eggs, maple syrup, vanilla, cinnamon, and nutmeg together in a large bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer bread slices in a greased 9x13-inch baking dish. Pour egg mixture over the bread slices. Cover and refrigerate, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Remove French toast from the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, pecans, butter, and maple syrup together; spread topping evenly over the French toast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven until puffed and golden brown, about 40 minutes.\n"
            }
        ],
