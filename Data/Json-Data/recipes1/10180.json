        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16902/the-supreme-shepherds-pie/",
        "name": "The Supreme Shepherd's Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2946551.jpg",
            "width": 2988,
