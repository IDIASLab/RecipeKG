    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21740/onion-spaghetti/",
        "name": "Onion Spaghetti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2004810.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2000-05-01T09:26:54.000Z",
        "description": "Onion Spaghetti is a great filling vegetarian dish that comes with a BANG! You would never guess that it's vegetarian!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "4 large onions, sliced",
            "2 cloves garlic, crushed",
            "1 tablespoon tomato paste",
            "1 (14.5 ounce) can whole peeled tomatoes",
            "1 teaspoon salt",
            "\u00bd teaspoon freshly ground black pepper",
            "1 tablespoon chopped fresh basil",
            "\u00bc cup water",
            "1 pound spaghetti"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat.  Cook onions and garlic in oil until soft.  Stir in tomato paste, tomatoes, salt, pepper, basil and water.  Cover, reduce heat to low, and simmer 20 minutes, until thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While sauce is simmering, bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss sauce with hot pasta.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pasta",
            "Spaghetti"
        ],
