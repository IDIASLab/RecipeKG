    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10981/easy-chocolaty-brownies/",
        "name": "Easy Chocolaty Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27004.jpg",
            "width": 2189,
            "height": 1642
        },
        "datePublished": "1998-06-29T11:53:10.000Z",
        "description": "This is a really easy recipe that uses no oil or eggs.  It whips up in a flash and is especially good for impromptu gatherings of kids.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 1/2 dozen",
        "recipeIngredient": [
            "1 (3.9 ounce) package instant chocolate pudding mix",
            "2 cups milk",
            "1 (18.25 ounce) package chocolate cake mix",
            "1 cup semisweet chocolate chips",
            "1 cup candy-coated milk chocolate pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).   Spray 15 x 10 inch jelly roll pan with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare pudding as directed in large mixing bowl.  Whisk in the cake mix.  Stir in chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared pan.  Sprinkle with mini M and M's, if desired.  Bake for 25 to 30 minutes, or until middle of brownies spring back up when touched.  Cool completely and cut into 30 squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cake Mix Cookie Recipes"
        ],
