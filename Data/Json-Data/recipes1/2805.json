    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13609/squash-casserole-i/",
        "name": "Squash Casserole I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F141677.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-11-08T14:58:18.000Z",
        "description": "This casserole makes a rich, creamy vegetable side dish. Stuffing mix may be used in place of the bread crumbs if you prefer. It tastes even better the next day for leftovers.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds crookneck yellow squash, sliced",
            "1 small onion, chopped",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (8 ounce) container sour cream",
            "\u00bc cup butter, melted",
            "1\u2009\u00bd cups bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the squash and onion in a large saucepan with just enough water to cover. Bring to a boil over medium heat, and cook until tender, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the squash, onion, and remaining liquid from the saucepan to a 2 1/2 quart casserole dish. Stir in chicken soup, sour cream, and melted butter. The liquid from the cooked squash should be enough so that mixture is slightly soupy. Sprinkle the bread crumbs over the top. The bread will absorb some of the liquid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, for 30 to 45 minutes in the preheated oven, until browned and bubbly.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Squash",
            "Summer Squash"
        ],
