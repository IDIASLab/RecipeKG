    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10369/great-grams-sugar-cookies/",
        "name": "Great Gram's Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F963542.jpg",
            "width": 1598,
            "height": 1600
        },
        "datePublished": "1997-12-21T20:47:40.000Z",
        "description": "This recipe has been handed down for generations in my family.  Hope you like them as much as we do!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen (approx.)",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup shortening",
            "2 eggs",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup buttermilk",
            "\u00bd teaspoon ground nutmeg",
            "2 teaspoons vanilla extract",
            "6 cups all-purpose flour",
            "1 egg white",
            "2 tablespoons white sugar",
            "2 tablespoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix all ingredients, except egg white, ground cinnamon and 2 T white sugar,  in a large bowl until soft dough is formed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out on floured surface to 1/4 inch thickness and cut with cookie cutters.  Paint cookies with egg white and sprinkle with cinnamon sugar, or colored sugars.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 8-10 minutes on greased cookie sheets.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
