    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14923/cocktail-meatballs-ii/",
        "name": "Cocktail Meatballs II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1082723.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "1999-11-20T15:41:06.000Z",
        "description": "An extremely easy appetizer that keeps them coming back for seconds.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "32 servings",
        "recipeIngredient": [
            "2 pounds lean ground beef",
            "1\u2009\u00bc cups grape jelly",
            "1 cup chili sauce",
            "2 tablespoons fresh lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Line a large roasting pan with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape ground beef into golf sized balls. Arrange them on the large roasting pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the meatballs for 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the meatballs are baking, combine the jelly, chili sauce and lemon juice in a Dutch oven. When the meatballs finish cooking add them to the Dutch oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the oven and simmer on low for 30 minutes. Serve the meatballs hot.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
            "Meatball Appetizer Recipes"
        ],
