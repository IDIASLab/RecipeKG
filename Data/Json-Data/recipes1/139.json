    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9929/fresh-fig-cookies/",
        "name": "Fresh Fig Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4557965.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1997-08-26T01:49:13.000Z",
        "description": "A good moist cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup shortening",
            "1 egg",
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cloves",
            "1 cup chopped fresh figs",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar and shortening and add beaten egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients and blend with creamed mixture.  Fold in figs and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by spoonfuls on greased sheet.  Bake for 15 to 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Fig Dessert Recipes"
        ],
