        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7739/coconut-chocolate-cake-i/",
        "name": "Coconut Chocolate Cake I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F201613.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-06-06T06:03:49.000Z",
        "description": "A very rich cake that tastes almost like a candy bar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 8 or 9 inch layer cake",
        "recipeIngredient": [
            "1 (18.25 ounce) package dark chocolate cake mix",
            "1 cup white sugar",
            "2 tablespoons butter",
            "\u00be cup evaporated milk",
            "20 large marshmallows",
            "14 ounces flaked coconut",
            "3 tablespoons unsweetened cocoa powder",
            "4 cups confectioners' sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix and bake cake mix as directed for two 8 or 9 inch layers. Let layers cool then split each one in half horizontally to make a total of 4 layers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat mix together the white sugar, butter or margarine, and 1/2 cup of the evaporated milk. Cook for 10 minutes. Stir in the marshmallows and mix until smooth. Stir in the coconut. Spread the filling between the cake layers.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together the cocoa, remaining 1/4 cup evaporated milk, the confectioner's sugar, and the vanilla. Beat until smooth and spread over top and sides of cake.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "Dark Chocolate"
        ],
