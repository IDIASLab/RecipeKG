        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21467/tri-color-pasta-salad/",
        "name": "Tri-Color Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F231661.jpg",
