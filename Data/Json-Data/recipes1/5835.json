        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12802/diannes-crab/",
        "name": "Dianne's Crab",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7765341.jpg",
