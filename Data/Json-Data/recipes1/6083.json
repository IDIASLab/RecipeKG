    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14178/red-broccoli-salad/",
        "name": "Red Broccoli Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3830667.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1999-07-29T02:04:39.000Z",
        "description": "This a hearty meat and vegetable salad: two pounds of maple flavored bacon make it very filling.  It's a meal in itself!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "10 to 12 servings",
        "recipeIngredient": [
            "2 pounds maple-flavored bacon",
            "1 large head fresh broccoli, chopped",
            "\u00be cup chopped celery",
            "\u00bc cup minced green onions",
            "\u00bc cup diced red onion",
            "1\u2009\u00bd cups seedless grapes, halved",
            "\u00be cup blanched slivered almonds",
            "\u00bc cup white sugar",
            "2 tablespoons distilled white vinegar",
            "1 cup mayonnaise"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet. Cook, turning frequently, over medium high heat until evenly browned. Cool, and then crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Spread slivered almonds on a cookie sheet.  Bake for approximately 12 to 14 minutes or until lightly browned, turning once during toasting. Cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix together mayonnaise, sugar, and vinegar. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine broccoli, crumbled bacon, celery, green onions, red onions, grapes, and toasted almonds. Toss with mayonnaise dressing.  Chill for several hours in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Broccoli Salad Recipes"
        ],
