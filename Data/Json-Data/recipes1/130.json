    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9923/forgotten-kisses/",
        "name": "Forgotten Kisses",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1014946.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "1997-08-23T18:10:45.000Z",
        "description": "This meringue cookie sets up overnight. I bake lots of cookies and then end my day by mixing up a batch of these kisses and turning out the light!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "7 dozen",
        "recipeIngredient": [
            "3 egg whites",
            "\u00be teaspoon cream of tartar",
            "\u00bc teaspoon salt",
            "1\u2009\u00bd cups white sugar",
            "\u00be teaspoon vanilla extract",
            "2 cups semisweet chocolate chips",
            "3 drops red food coloring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).  Spray cookie sheets with non-stick spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until frothy. Add cream of tartar and salt. Beat until very stiff.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sugar slowly by teaspoonful. Beat until glossy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in vanilla, food coloring, and chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by spoonfuls onto greased sheet. Place in oven. turn OFF the oven. Leave til oven is cold. DO NOT OPEN THE DOOR TO PEEK ... THE COOKIES WILL TURN GUMMY!!!\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
