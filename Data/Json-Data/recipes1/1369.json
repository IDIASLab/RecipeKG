    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6690/mothers-banana-bread/",
        "name": "Mother's Banana Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F202383.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-05-12T17:13:00.000Z",
        "description": "This is an old recipe that belonged to my mother.  I've been making this bread since I was a teenager.  At a candy striper bake sale a doctor bought this bread and said that this was the best banana bread that he had tasted.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 - 8x4 inch loaf",
        "recipeIngredient": [
            "1\u2009\u00be cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bc teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u2154 cup white sugar",
            "\u2153 cup butter, softened",
            "2 eggs",
            "1 cup mashed banana",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease an 8x4 inch loaf pan.  Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, sift together flour, baking powder, baking soda, salt, and sugar.  Add butter, eggs, nuts, and mashed banana. Beat until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared pan and bake about 1 hour, or until a toothpick inserted in the center of the loaf comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
        "recipeCuisine": [],
        "author": [
