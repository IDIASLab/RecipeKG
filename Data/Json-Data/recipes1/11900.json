    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18815/easy-easy-casserole/",
        "name": "Easy Easy Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F14313.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2000-06-18T02:36:21.000Z",
        "description": "Ground turkey, potatoes and cream-style corn make this a delicious and hearty casserole.  It's very, very good and my family just chows it down. No leftovers here!!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ground turkey",
            "4 potatoes, peeled and sliced",
            "2 tablespoons butter",
            "salt and pepper to taste",
            "1 (15 ounce) can cream-style corn",
            "1 (10.75 ounce) can  condensed tomato soup"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium-high heat, place the turkey and saute for 5 to 10 minutes, or until browned.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the sliced potatoes in the bottom of a lightly greased 2-quart casserole dish, cover with butter and season with salt and pepper to taste. Then layer the cream-style corn over the potatoes, top with the browned turkey meat, and then top with the tomato soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and bake at 350 degrees F (175 degrees C) for 60 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
