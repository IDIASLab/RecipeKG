        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12079/pumpkin-orange-crunch-pie/",
        "name": "Pumpkin Orange Crunch Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4375434.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1998-05-20T20:37:17.000Z",
        "description": "A family favorite for the Thanksgiving dinner.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 cup packed brown sugar",
            "1 tablespoon cornstarch",
            "1\u2009\u00bd teaspoons pumpkin pie spice",
            "\u00bc teaspoon salt",
            "2 cups solid pack pumpkin puree",
            "1\u2009\u2154 cups evaporated milk",
            "2 eggs",
            "1 tablespoon brown sugar",
            "1 tablespoon butter",
            "1 tablespoon all-purpose flour",
            "\u00bd cup chopped walnuts",
            "2 teaspoons orange zest",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup brown sugar, cornstarch, pumpkin pie spice, salt, and pumpkin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in milk and eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pastry shell.  Filling is generous--crimp edges high.  Bake at 400 degrees F (205 degrees C) for 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine remaining ingredients - 1 Tablespoon brown sugar, butter or margarine, flour, walnuts, and orange peel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pie from oven, and spoon this nut mixture over pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return pie to oven, and bake 5 - 10 minutes more.  Remove from oven and let cool.\n"
            }
        ],
