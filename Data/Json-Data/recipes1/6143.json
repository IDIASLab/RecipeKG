        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12038/bohemian-macaroni-and-cheese/",
        "name": "Bohemian Macaroni and Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F455322.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "1999-08-02T15:38:52.000Z",
        "description": "This is a great way to spruce up macaroni and cheese into a complete meal! Try using imitation bacon bits, if the real stuff is not readily available in your refrigerator.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 (7.25 ounce) packages dry macaroni and cheese",
            "1 green bell pepper, chopped",
            "1 onion, chopped",
            "1 large tomato, chopped",
            "1 pound bacon - cooked and crumbled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Prepare both boxes of macaroni and cheese according to package directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, in a large bowl mix together the chopped bell green pepper, onion, tomato, and crumbled bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in prepared macaroni and cheese to large bowl with vegetables and bacon. Stir to incorporate ingredients evenly. Pour into a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degree F (175 degree C) oven for 45 to 60 minutes, or until crispy on top. Serve warm.\n"
            }
        ],
