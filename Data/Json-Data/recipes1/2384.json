        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7964/fluffy-buttercream-frosting/",
        "name": "Fluffy Buttercream Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F663172.jpg",
