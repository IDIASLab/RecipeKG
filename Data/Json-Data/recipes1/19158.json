    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/279000/nonas-orange-glazed-easter-ham/",
        "name": "Nona's Orange-Glazed Easter Ham",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F04%2F02%2F279000_NonasOrangeGlazedEasterHam_32047_MFS.jpg",
            "width": 895,
            "height": 895
        },
        "datePublished": "2013-04-03T17:18:50.000Z",
        "description": "This orange-glazed ham is the way Nona does it. Delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT10H40M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (8 pound) bone-in smoked ham",
            "2 fruit, (2-5/8\" dia, sphere)s oranges, juiced",
            "3 tablespoons brown sugar",
            "1 tablespoon dry mustard",
            "1 tablespoon whole cloves, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place ham in a large pot and cover with water. Soak 8 hours to overnight to remove salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Line a roasting pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix orange juice, brown sugar, and dry mustard together in a small bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain ham and pat dry. Place in the prepared roasting pan. Score the outside of the ham in a diagonal pattern and coat with orange juice mixture. Insert cloves at cross marks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through and glaze is well browned, about 2 hours. Let sit for 20 to 30 minutes before carving.\n"
            }
        ],
