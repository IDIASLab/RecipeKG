        "mainEntityOfPage": "https://www.allrecipes.com/recipe/77744/vegetable-dill-dip/",
        "name": "Vegetable Dill Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1009078.jpg",
            "width": 960,
