        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9108/fluffy-cranberry-salad/",
        "name": "Fluffy Cranberry Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F982.jpg",
