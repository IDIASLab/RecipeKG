        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22789/quick-shrimp-scampi-pasta/",
        "name": "Quick Shrimp Scampi Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F430299.jpg",
