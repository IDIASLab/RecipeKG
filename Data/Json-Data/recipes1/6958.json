    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15023/mondis-super-simple-chicken/",
        "name": "Mondi's Super Simple Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F117495.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-09-23T15:06:57.000Z",
        "description": "This recipe is a family favorite, particularly with my husband. It is easy to assemble and creates a fabulous aroma as it bakes. Serve with wild rice if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 bone-in chicken breast halves, with skin",
            "4 tablespoons butter",
            "1 tablespoon dried basil",
            "1 teaspoon garlic salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts in a lightly greased 9x13 inch baking dish or roasting pan. Dot each breast with 1 tablespoon of butter or margarine, then season each breast with dried basil and garlic salt to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for approximately 1 to 1 1/4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
