    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10102/vanilla-wafers-ii/",
        "name": "Vanilla Wafers II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F836384.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "1997-09-21T04:28:04.000Z",
        "description": "I got this recipe years ago.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "3 cups sifted all-purpose flour",
            "3 teaspoons baking powder",
            "1 tablespoon milk",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream the butter and sugar together until light and fluffy. Blend in eggs and beat well. Sift together flour and baking powder; add to creamed mixture.  Add milk and vanilla; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and chill for 1 to 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll very thin.  Cut with cookie cutter dipped in flour. Place on greased cookie sheet. Bake at 400 degrees F (200 degrees C) for 6 to 8 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cut-Out Cookie Recipes"
        ],
        "recipeCuisine": [],
        "author": [
