    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284149/indian-chicken-65/",
        "name": "Indian Chicken 65",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9239567.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2007-11-15T03:11:09.000Z",
        "description": "This is a spicy, dry chicken recipe from India that is great for serving as an appetizer at parties.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT2H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds skinless, boneless chicken breasts, cut into bite-sized pieces",
            "\u00bc cup fresh curry leaves, chopped, or more to taste",
            "2 tablespoons plain yogurt",
            "1 tablespoon garlic paste",
            "2 teaspoons chili powder",
            "2 teaspoons ground coriander",
            "2 teaspoons garam masala",
            "1 teaspoon ground ginger",
            "1 drop red food coloring, or as desired",
            "1 cup vegetable oil for frying, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix chicken, curry leaves, yogurt, garlic paste, chili powder, coriander, garam masala, and ginger together. Cover and let marinate in a refrigerator for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add a few drops of red food coloring and mix so that chicken becomes tinted with red.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 inches oil in a deep saucepan over medium heat. Lower chicken pieces carefully into the hot oil, working in batches, and fry, stirring frequently, until browned and no longer pink in the centers, about 5 minutes. Transfer to a plate lined with paper towels to drain. Repeat with remaining chicken pieces.\n"
            }
        ],
