        "recipeIngredient": [
            "2 cups vegetable oil",
            "\u00be cup distilled white vinegar",
            "\u00bc cup molasses",
            "2 tablespoons soy sauce",
            "2 tablespoons lemon-pepper seasoning",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon onion powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the oil, vinegar, molasses, soy sauce, lemon-pepper seasoning, garlic powder, and onion powder in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To use, marinate chicken for at least 8 hours before cooking as desired.\n"
            }
