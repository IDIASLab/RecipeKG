    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16157/company-chicken-pasta-salad-with-grapes/",
        "name": "Company Chicken Pasta Salad with Grapes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F906682.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-09-26T12:40:03.000Z",
        "description": "This is a special treat to fix for your guests. Enjoy it with some hot garlic bread, if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2 cups small seashell pasta",
            "3\u2009\u00bd cups chopped poached chicken breast meat",
            "\u00bd cup diced celery",
            "1 cup seedless green grapes, halved",
            "salt to taste",
            "\u00bc teaspoon ground white pepper",
            "\u2153 cup mayonnaise",
            "\u00bd teaspoon curry powder",
            "\u00bc teaspoon ground cumin",
            "2 tablespoons rice vinegar",
            "1 tablespoon white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta, and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine chicken, celery, grapes, cooked pasta, salt, and white pepper; mix well and reserve.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine mayonnaise, curry powder, cumin, rice vinegar, and sugar; whisk ingredients together and toss with chicken mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover salad with plastic wrap and refrigerate for at least 2 to 4 hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Chicken Pasta Salad Recipes"
        ],
