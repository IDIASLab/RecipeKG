        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13840/creamed-peas-and-onions/",
        "name": "Creamed Peas and Onions",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4377270.jpg",
            "width": 2448,
