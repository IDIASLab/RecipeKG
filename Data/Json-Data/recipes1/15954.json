    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285278/budget-cinnamon-rolls/",
        "name": "Budget Cinnamon Rolls",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Budget Cinnamon Rolls"
        },
        "datePublished": "2009-12-24T10:12:00.000Z",
        "description": "Warm, tasty, and best of all, cheap. A great treat if you have a sweet tooth but are on a budget.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 rolls",
        "recipeIngredient": [
            "1 (8 ounce) package refrigerated crescent rolls",
            "\u00bd cup brown sugar",
            "1 tablespoon unsalted butter, melted",
            "1 tablespoon ground cinnamon",
            "1 teaspoon unsweetened cocoa powder",
            "\u00bc teaspoon ground black pepper",
            "\u00bd teaspoon unsalted butter",
            "1 tablespoon milk",
            "\u00bd cup powdered sugar",
            "1 pinch kosher salt",
            "1 tablespoon powdered sugar",
            "3 drops orange extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a cookie sheet with parchment paper, a silicone mat, or aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unroll crescent roll dough and pull apart at the seams to separate into 8 triangles.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix brown sugar, melted butter, cinnamon, cocoa powder, and pepper for filling together in a bowl until moist but crumbly. Add a thin layer of filling on each triangle, from the base to about 1-inch from the top. Roll up triangles from the base to the top; transfer to the prepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, 10 to 13 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the rolls are baking, combine milk and butter for glaze in a microwave-safe bowl. Microwave until butter is almost melted, about 20 seconds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 1/4 cup powdered sugar and salt; whisk until smooth. Add remaining 1/4 cup plus 1 tablespoon powdered sugar; whisk until smooth. It should be about the consistency of pancake batter. Stir in orange extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove rolls from the oven and transfer to a plate. Pour glaze over hot rolls. Let cool for 5 minutes before serving.\n"
            }
        ],
