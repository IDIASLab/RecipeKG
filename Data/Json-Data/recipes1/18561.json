    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230157/brussels-sprouts-with-bacon-dressing/",
        "name": "Brussels Sprouts with Bacon Dressing",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F939678.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2012-11-16T17:22:58.000Z",
        "description": "This creative uncooked Brussels sprouts recipe has a great contrast between the raw, crunchy vegetable and the rich, smoky bacon dressing.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H9M",
        "totalTime": "P0DT0H24M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bc pounds Brussels sprouts",
            "1 tablespoon vegetable oil",
            "4 ounces bacon, sliced",
            "\u2153 cup cider vinegar",
            "1 lemon, juiced",
            "2 tablespoons brown sugar",
            "salt and ground black pepper to taste",
            "1 pinch cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice each Brussels sprout in half, cut each half into into strips, and discard the cores. Transfer to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a skillet over medium heat. Cook and stir bacon until browned and crisp, 8 to 10 minutes. Stir in cider vinegar, lemon juice, and brown sugar. Season with salt, black pepper, and cayenne pepper to taste. Increase heat to high and boil for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour hot bacon mixture over the bowl of Brussels sprouts and toss to coat completely. Season with salt and black pepper to taste.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Brussels Sprouts"
        ],
