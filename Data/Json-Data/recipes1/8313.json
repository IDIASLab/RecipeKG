        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16199/fabulous-spinach-salad/",
        "name": "Fabulous Spinach Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F570038.jpg",
            "width": 3008,
            "height": 2000
        },
        "datePublished": "1999-11-24T21:08:43.000Z",
        "description": "This salad is hearty enough to be used as a main entree, or it makes a great side dish.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 bunch fresh spinach - torn, washed and dried",
            "10 fresh mushrooms, sliced",
            "1 onion, thinly sliced",
            "4 eggs",
            "4 tomatoes, chopped",
            "sea salt to taste",
            "\u2153 cup olive oil",
            "\u215b cup rice wine vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a saucepan and cover with cold water. Bring water to a boil. Cover, remove from heat, and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool, peel and chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the spinach, mushrooms, onion, eggs and tomatoes; toss together and sprinkle with salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the oil and vinegar. Pour over salad and toss to coat.\n"
            }
        ],
