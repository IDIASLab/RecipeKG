    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15935/delicious-apple-crisp/",
        "name": "Delicious Apple Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1053497.jpg",
            "width": 2884,
            "height": 2036
        },
        "datePublished": "1999-01-04T19:41:39.000Z",
        "description": "This takes a little longer but is truly worth the extra effort. You can use the apple filling as topping for ice cream, for apple crisp, apple pie, or anything else you like to top with apples.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 cups water",
            "2 tablespoons ground cinnamon",
            "1 cup white sugar",
            "2 teaspoons cornstarch",
            "9 Granny Smith apples - peeled, cored and finely diced",
            "1 cup quick cooking oats",
            "3 tablespoons butter, melted",
            "\u00bc cup packed brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Apple Filling: In a large saucepan combine water, cinnamon, sugar, and cornstarch. Mix well, then add apples to pan. Simmer over medium heat, stirring occasionally, until apples are tender. Pour apple filling into a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Topping: In a small bowl, combine oats, melted butter or margarine, and brown sugar. Mix well and sprinkle over apple filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 20 minutes or until topping is golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
