        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8213/italian-cheesecake/",
        "name": "Italian Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2059811.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-04-19T06:00:18.000Z",
        "description": "A slightly lighter version of cheesecake without the crust!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 -9 inch cake",
        "recipeIngredient": [
            "1\u2009\u00bd pounds ricotta cheese",
            "2 cups confectioners' sugar",
            "3 eggs",
            "1\u2009\u00bd teaspoons vanilla extract",
            "\u00bd teaspoon almond extract",
            "\u00bd teaspoon rum flavored extract",
            "1 tablespoon grated lemon zest",
            "\u00bc cup fresh lemon juice",
            "1 tablespoon grated orange zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C). Grease and flour one 9 inch round springform or regular pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the ricotta, confectioners' sugar and eggs. Blend well. Stir in vanilla, almond extract, rum extract, lemon juice, lemon zest and orange zest. Beat by hand until smooth and creamy. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) for 40 minutes, until golden. Place on a rack and cool.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Italian"
