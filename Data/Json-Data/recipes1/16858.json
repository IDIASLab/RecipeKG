    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285064/rich-apple-scones/",
        "name": "Rich Apple Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Rich Apple Scones"
        },
        "datePublished": "2011-09-10T08:36:58.000Z",
        "description": "A rich apple scone. Serve with butter.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 scones",
        "recipeIngredient": [
            "2 cups self-rising flour",
            "1 pinch salt",
            "1 tablespoon superfine white sugar",
            "\u2153 cup unsalted butter, cut into small pieces",
            "1 medium apple - peeled, cored, and chopped",
            "5 tablespoons milk",
            "2 tablespoons honey",
            "1 large egg, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Lightly grease a cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour and salt into a mixing bowl. Mix in sugar. Rub in butter with your fingers until the mixture resembles bread crumbs. Stir in apples until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix milk, honey, and egg together in a separate bowl. Add to the dry ingredients and mix to form a dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out the dough on a lightly floured surface to a thickness of 3/4 inch. Cut scones out with a cookie cutter of your choice. Place on the prepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden, 8 to 10 minutes. Let cool before serving.\n"
            }
        ],
