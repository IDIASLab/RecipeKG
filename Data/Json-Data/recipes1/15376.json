    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9788/chocolate-almond-macaroons/",
        "name": "Chocolate-Almond Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6340746.jpg",
            "width": 3264,
            "height": 3264
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Ground almonds and chocolate put a new spin on an old favorite.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups almonds",
            "1 cup white sugar",
            "\u00bc teaspoon ground cinnamon",
            "\u215b teaspoon salt",
            "2 eggs",
            "\u00bd teaspoon almond extract",
            "\u00be cup chopped semisweet chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Line cookie sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Finely grind the almonds, sugar, cinnamon and salt in a food processor.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate one of the eggs and add the white, the other egg, and almond extract and process until the mixture  holds together.  Transfer to a bowl and stir in the chocolate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Moisten hands and roll mixture into 1-inch balls. Place on prepared  cookie sheet.  Flatten to 1/3-inch thick rounds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until tops puff  and centers are still soft, about 12 minutes.    Remove to a rack and cool completely.  Replace parchment paper for next  round of cookies.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Almond Dessert Recipes"
        ],
