    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283451/fresh-fig-and-walnut-cookies/",
        "name": "Fresh Fig and Walnut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fresh Fig and Walnut Cookies"
        },
        "datePublished": "2007-08-01T22:04:13.000Z",
        "description": "A yummy, moist cookie made with fresh figs. So good you can't stop at one. These also freeze well.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "42 cookies",
        "recipeIngredient": [
            "2 cups white sugar",
            "1 cup butter-flavored shortening (such as Crisco\u00ae)",
            "2 teaspoons vanilla extract",
            "4 large eggs",
            "4 cups all-purpose flour",
            "2 teaspoons baking soda",
            "2 teaspoons baking powder",
            "1\u2009\u00bc teaspoons ground cloves",
            "1 teaspoon salt",
            "2\u2009\u00bd cups chopped fresh figs",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar, shortening, and vanilla together in a mixing bowl. Add eggs, one at a time, mixing well after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking soda, baking powder, cloves, and salt together in a separate bowl. Add to the wet ingredients and mix until blended; mixture will be a little dry. Stir in figs and walnuts; mixture will get added moisture from the figs. Use a medium cookie scoop to drop spoonfuls of batter onto the prepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges start to brown, 12 to 13 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely. Repeat to scoop and bake remaining batches.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Nut Cookie Recipes",
            "Walnut"
        ],
