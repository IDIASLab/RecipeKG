        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7440/chocolate-torte-frosting/",
        "name": "Chocolate Torte Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F470534.jpg",
