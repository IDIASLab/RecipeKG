        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15972/vinegar-pie-vii/",
        "name": "Vinegar Pie VII",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F569330.jpg",
