    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20736/egg-and-olive-crostini/",
        "name": "Egg and Olive Crostini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1122561.jpg",
            "width": 3306,
            "height": 3311
        },
        "datePublished": "2000-04-06T22:47:41.000Z",
        "description": "Take those leftover hard boiled eggs and turn them into this tasty appetizer. The kids will really like these thick pieces of bread covered with a zesty egg and olive mixture.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "6 eggs",
            "1 (3 ounce) package cream cheese, softened",
            "1 tablespoon lemon juice",
            "1 teaspoon prepared Dijon-style mustard",
            "\u00bc teaspoon ground black pepper",
            "2 tablespoons sliced pimento-stuffed green olives",
            "1 tablespoon chopped fresh chives",
            "1 tablespoon olive oil",
            "paprika"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place eggs in a medium saucepan and cover with cold water. Bring water to a boil and immediately remove from heat. Cover and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool, peel and chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C). Lightly grease a large baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, thoroughly blend cream cheese, lemon juice, prepared Dijon-style mustard and ground black pepper. Stir in chopped eggs, olives and chives.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each bread slice into 4 triangles. Lay triangles flat and create a shallow hollow on the face of each. Brush  with olive oil and fill with mixture from the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange slices on baking sheet and bake in the preheated oven 10 minutes, or until bread is lightly toasted. Garnish with paprika before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Vegetable",
            "Olives"
        ],
