    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11022/sues-oatmeal-macadamia-nut-cookies/",
        "name": "Sue's Oatmeal Macadamia Nut Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F21300.jpg",
            "width": 286,
            "height": 225
        },
        "datePublished": "1998-08-02T03:37:21.000Z",
        "description": "This is an old family recipe that I 'tweaked' by adding macadamia nuts.  I bake about 1,000 of these every year for our St Joseph Garden Tour and I don't even have any crumbs left.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "3 cups rolled oats",
            "\u00bd cup chopped macadamia nuts",
            "1 cup shortening",
            "1 cup white sugar",
            "1 cup packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour, resift with salt and baking soda.  Stir in oats and nuts and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together sugar and shortening.  Add eggs and vanilla.  Add dry ingredients and beat til thoroughly mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough in waxed paper like logs, shaping as you roll. Refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Unroll cookie dough and slice 1/4 inch thick and place on an ungreased cookie sheet.   Bake at 350 degrees F (175 degrees C) for 10 to 12 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
