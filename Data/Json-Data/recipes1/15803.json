    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282481/pineapple-chicken-stuffed-peppers/",
        "name": "Pineapple Chicken-Stuffed Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pineapple Chicken-Stuffed Peppers"
        },
        "datePublished": "2009-08-15T13:46:28.000Z",
        "description": "A variation on traditional stuffed bell peppers using chicken, pineapple, and bacon to make a tasty meal.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "6 stuffed peppers",
        "recipeIngredient": [
            "4 (5 ounce) skinless, boneless chicken breast halves",
            "2 cups uncooked instant rice",
            "2 cups water",
            "6 medium green bell peppers - tops, seeds, and membranes removed",
            "1 tablespoon minced garlic",
            "1 teaspoon poultry seasoning",
            "1 teaspoon ground black pepper",
            "nonstick cooking spray",
            "8 slices bacon",
            "3 slices fresh pineapple",
            "1 cup tomato sauce",
            "\u00bd cup maple syrup",
            "1 tablespoon soy sauce",
            "\u00bc cup shredded Monterey Jack cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken in a saucepan and add water to cover. Bring to a boil over medium heat. Reduce heat to low, cover, and simmer until no longer pink in the center and the juices run clear, about 10 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Remove chicken and set aside to cool slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "At the same time, stir water and rice together in a pot; bring to a boil. Reduce heat to medium, cover, and simmer until water is absorbed, about 10 minutes. Fluff with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut cooled chicken into 1-inch cubes and place in a bowl with garlic, poultry seasoning, and black pepper; mix together until chicken is coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Spray a baking dish with nonstick spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and crumble into small pieces when cool enough to handle. Reserve 1 tablespoon bacon grease.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Trim and core pineapple slices. Cut into 1/2-inch chunks and reserve any juices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine rice, chicken, crumbled bacon, reserved bacon grease, pineapple chunks and juice, tomato sauce, maple syrup, and soy sauce in a large bowl. Mix thoroughly. Spoon mixture into green peppers and sprinkle with Monterey Jack cheese. Place in the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until filling is hot and cheese is melted, about 45 minutes.\n"
            }
        ],
