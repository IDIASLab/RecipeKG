    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11107/easy-cookies/",
        "name": "Easy Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3458430.jpg",
            "width": 1920,
            "height": 1920
        },
        "datePublished": "1998-07-13T17:01:09.000Z",
        "description": "Sooo easy!  This cookie is very malleable, and can be tailored to your families tastes.  Try peanut butter, oatmeal, raisins, or chocolate chips as additions.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package vanilla flavored cake mix",
            "2 egg yolks",
            "1 cup butter",
            "1 teaspoon vanilla extract",
            "\u2153 cup granulated sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg yolks and margarine together until well blended.  Stir in vanilla.  Gradually add cake mix while beating with an electric mixer.  Mixture should be stiff.  If it is sticky to the touch, refrigerate for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form teaspoonfuls of the dough into balls, and roll in sugar. Place on ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 11 minutes for soft cookies, and for 13 minutes for crispy cookies.\n"
            }
        ],
