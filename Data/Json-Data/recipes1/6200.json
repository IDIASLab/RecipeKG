        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13996/moms-cucumbers/",
        "name": "Mom's Cucumbers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2518563.jpg",
