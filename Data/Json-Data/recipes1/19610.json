    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/240747/mommas-healthy-meatloaf/",
        "name": "Momma's Healthy Meatloaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1995630.jpg",
            "width": 2293,
            "height": 2289
        },
        "datePublished": "2013-07-24T22:05:16.000Z",
        "description": "This is a super healthy and simplified version of a family favorite.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x5-inch loaf",
        "recipeIngredient": [
            "cooking spray",
            "1 tablespoon olive oil",
            "1 green bell pepper, diced",
            "\u00bd cup diced sweet onion",
            "\u00bd teaspoon minced garlic",
            "1 pound extra-lean (95%) ground beef",
            "1 cup whole wheat bread crumbs",
            "2 large eggs",
            "\u00be cup shredded carrot",
            "\u00be cup shredded zucchini",
            "salt and ground black pepper to taste",
            "\u00bc cup ketchup, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Spray a 9x5-inch loaf pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat olive oil in a skillet over medium heat; cook and stir green bell pepper and onion in the hot oil until onion is transparent and bell pepper is softened, 5 to 10 minutes. Add garlic and cook until fragrant, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ground beef, bread crumbs, eggs, carrot, zucchini, salt, pepper, and bell pepper mixture in a large bowl; mix well using your hands. Press meat mixture into the prepared loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until no longer pink in the center, 35 to 40 minutes. An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C). Spread ketchup on the top of the meatloaf and continue baking until bubbling, about 5 minutes more.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Meatloaf Recipes",
            "Beef Meatloaf Recipes"
        ],
