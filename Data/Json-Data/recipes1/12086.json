    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19288/brown-sugar-drops/",
        "name": "Brown Sugar Drops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4553440.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "2000-06-27T22:39:05.000Z",
        "description": "Tasty little drop cookies made with buttermilk and brown sugar. A rewarding treat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "2 cups packed brown sugar",
            "2 eggs",
            "\u00bd cup buttermilk",
            "3\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the shortening and brown sugar until smooth. Beat in the eggs one at a time then stir in the buttermilk. Combine the flour, baking soda and salt; stir into the buttermilk mixture until well blended. Cover dough and chill for at least one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease cookie sheets. Drop dough by rounded teaspoonfuls 2 inches apart onto the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10 minutes in the preheated oven, until almost no imprint remains when touched with your finger. Remove from cookie sheets immediately to cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
