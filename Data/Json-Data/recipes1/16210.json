    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283485/mamies-wild-plum-tart/",
        "name": "Mamie's Wild Plum Tart",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F436477.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-07-20T19:36:54.000Z",
        "description": "This is the plum tart my French \"mamie\" would make for a weekday dessert.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 9-inch tart",
        "recipeIngredient": [
            "1 (9 inch) refrigerated pie crust dough",
            "\u00bd teaspoon unsalted butter, or as needed",
            "10 medium plums, or more as needed, halved and pitted",
            "1 tablespoon ground cinnamon",
            "\u00bc cup white sugar, or more to taste",
            "1 tablespoon powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C). Grease a 9-inch tart pan with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out the pie dough and press crust into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange plums snugly, cut-sides down, on the crust. Sprinkle with cinnamon, then sprinkle with 1/4 cup sugar, or up to 1/2 cup depending on the sweetness of the plums.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the tops of the plums are slightly browned and the juices are bubbling, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven and let rest until the juices have set, 10 to 15 minutes. Sprinkle with a bit of powdered sugar, cut, and serve.\n"
            }
        ],
