        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10600/mock-macaroons/",
        "name": "Mock Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970129.jpg",
            "width": 1940,
            "height": 1936
        },
        "datePublished": "1998-02-27T04:23:30.000Z",
        "description": "Oatmeal \"marinated\" in brown sugar and vegetable oil makes for crispy edges and a chewy middle.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2 cups rolled oats",
            "1 cup packed brown sugar",
            "\u00bd cup vegetable oil",
            "1 egg",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon almond extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine oatmeal, brown sugar and oil in a bowl.  Stir to mix well. Cover and let stand at least 5 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pre-heat oven to 350 degrees F (175 degrees C).  Grease cookie sheets - use non-stick if possible.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the egg, salt and almond extract to oatmeal and stir well until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by rounded teaspoonfuls about 1-1/2 inches apart on greased cookie sheets. Bake 7 or 8 minutes or until edges are browned and center is golden. Remove from cookie sheet and cool on racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Macaroon Recipes"
