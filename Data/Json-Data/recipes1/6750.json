    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14432/asparagus-salad/",
        "name": "Asparagus Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Asparagus Salad"
        },
        "datePublished": "1999-09-05T23:16:29.000Z",
        "description": "My Grandmother has made this quite a few times and always gets raves. It is so yummy, especially on a hot summer day.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "\u00bd cup white wine vinegar",
            "1 cup water",
            "\u00bd teaspoon salt",
            "2 (.25 ounce) envelopes unflavored gelatin",
            "1 tablespoon lemon juice",
            "2 teaspoons grated onion",
            "1 cup chopped celery",
            "\u00bd cup chopped pecans",
            "\u00bd (4 ounce) jar pimentos",
            "1 (10 ounce) can canned asparagus, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine the sugar, white vinegar and 1 cup water. Bring to boil and stir until dissolved.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, whisk together the salt, gelatin and 1/2 cup water. Add to saucepan, stir and cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Once mixture is cool, add lemon juice, onion, celery, pecans, pimentos and asparagus. Mix well and place into favorite mold. Refrigerate until ready to serve.\n"
            }
        ],
