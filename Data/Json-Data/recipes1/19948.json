    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285120/rice-paper-rolls-with-shrimp-and-fresh-herbs/",
        "name": "Rice Paper Rolls with Shrimp and Fresh Herbs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1052424.jpg",
            "width": 3264,
            "height": 1836
        },
        "datePublished": "2013-10-07T17:57:25.000Z",
        "description": "A quick and simple way to make Thai-style rice paper rolls at home.",
        "prepTime": "P0DT0H45M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "24 rolls",
        "recipeIngredient": [
            "\u00bc (8 ounce) package dried rice vermicelli noodles",
            "40 medium (blank)s cooked and peeled shrimp",
            "24 medium (blank)s rice paper sheets",
            "1 medium red bell pepper, julienned",
            "1 medium mango, sliced",
            "1 (8.5 ounce) package Asian slaw mix",
            "1 bunch cilantro, finely chopped",
            "1 bunch mint, finely chopped",
            "1 bunch Thai basil, chopped",
            "3 tablespoons mirin",
            "1 tablespoon tamari",
            "1 tablespoon rice vinegar",
            "\u00bc teaspoon ground ginger",
            "\u00bc teaspoon sesame oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a pot of water to a boil. Remove from the heat and add vermicelli noodles. Let noodles soak until they're tender, about 5 minutes. Drain and let cool, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice shrimp in half lengthwise.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a rice paper sheet in hot water for about 4 seconds. Remove, shake off excess water, and transfer to a clean, flat surface. Starting with the shrimp, layer the other roll ingredients carefully at the top of the wrapper (leaving about 2/3 of the wrapper below the contents). Fold the left and right edges in and over the filling. Starting from the top, fold the wrap tightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Repeat the wetting, filling, and wrapping process to make remaining rolls until out of ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mirin, tamari, rice vinegar, ginger, and sesame oil for sauce together in a small, resealable container. Seal and shake well. Pour into a dipping dish and serve with rolls.\n"
            }
        ],
