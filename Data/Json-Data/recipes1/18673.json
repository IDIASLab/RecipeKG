        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218055/cheesy-peas/",
        "name": "Cheesy Peas",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F810087.jpg",
