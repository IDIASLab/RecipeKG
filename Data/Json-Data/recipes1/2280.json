    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7936/colby-cheesecake/",
        "name": "Colby Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Colby Cheesecake"
        },
        "datePublished": "1998-08-10T18:36:04.000Z",
        "description": "Delightful with fruit.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P1DT1H40M",
        "recipeYield": "1 9-inch cheesecake",
        "recipeIngredient": [
            "1\u2009\u00bd cups crushed zwieback toast",
            "\u2153 cup white sugar",
            "2 teaspoons ground nutmeg",
            "6 tablespoons butter, melted",
            "2 (8 ounce) packages cream cheese, softened",
            "2 cups shredded Colby cheese",
            "\u00be cup white sugar",
            "3 tablespoons all-purpose flour",
            "3 tablespoons milk",
            "1 teaspoon vanilla extract",
            "\u215b teaspoon salt",
            "3 eggs",
            "1 (16 ounce) container vanilla yogurt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "For the crust, combine the zwieback crumbs, 1/3 cup sugar, nutmeg, and melted butter. Press onto bottom and part way up sides of a 9-inch springform pan.  Chill crust while preparing filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the filling, beat cream cheese in large mixer bowl until fluffy. Stir in Colby cheese, 3/4 cup sugar, flour, milk, vanilla, and salt. Beat on medium speed of mixer until well-blended, about 3 minutes. Beat in eggs, one at a time, beating well after each addition. Pour into chilled crust. Bake for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 250 degrees F (120 degrees C).  Bake until the center of the cheesecake is set, about 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and let stand 10 minutes. Spread yogurt over top. Return to oven and bake 10 minutes. Cool completely on wire rack. Chill several hours or overnight.  To serve, place the cake on a serving plate and carefully remove the sides of the pan.\n"
            }
        ],
