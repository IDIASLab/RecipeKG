    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18410/fresh-corn-pudding/",
        "name": "Fresh Corn Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F427410.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-04-27T21:14:39.000Z",
        "description": "This is yummy comfort food made with fresh corn!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd cup butter",
            "2 eggs, beaten",
            "2 teaspoons salt",
            "2 tablespoons white sugar",
            "ground black pepper to taste",
            "2 cups milk",
            "\u00be cup all-purpose flour",
            "2 cups fresh corn"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Place butter in a 9x13 inch baking pan and set in oven to melt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, whisk together eggs, salt, sugar, pepper, milk and flour. When mixture is smooth, stir in corn. Remove pan from oven when butter is melted. Pour butter into corn mixture and stir well. Pour corn mixture into baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 hour or until set in center and golden brown on top.\n"
            }
        ],
