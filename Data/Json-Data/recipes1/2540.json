    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8018/sauerkraut-apple-cake/",
        "name": "Sauerkraut Apple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Sauerkraut Apple Cake"
        },
        "datePublished": "1998-09-20T19:26:55.000Z",
        "description": "A simple Amish recipe with a very moist texture everyone will love.  Serve with cream cheese frosting.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9 x 13 inch cake",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "2 teaspoons ground cinnamon",
            "\u00bd teaspoon ground nutmeg",
            "1\u2009\u00bd cups white sugar",
            "2 tablespoons molasses",
            "4 eggs",
            "1 cup vegetable oil",
            "16 ounces sauerkraut, drained and rinsed",
            "1 apple - peeled, cored and finely chopped",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together flour, baking powder, baking soda, salt, and spices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine oil, molasses, and sugar with an electric mixer.  Mix in eggs.  Blend in dry ingredients.  Stir in sauerkraut, apples, and nuts by hand.  Pour batter into a greased and floured 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 35 minutes.  Remove from oven, and cool on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
