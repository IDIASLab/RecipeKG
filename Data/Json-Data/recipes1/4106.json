    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13090/mushroom-spinach-soup/",
        "name": "Mushroom Spinach Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F639447.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-02-28T05:22:07.000Z",
        "description": "This is an elegant soup and a great first course before a game main course. Serve with grated Parmesan cheese.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 tablespoons butter",
            "3 leeks, chopped",
            "2 onions, chopped",
            "2 cloves garlic, minced",
            "2 pounds chopped mushrooms",
            "2 teaspoons dried savory",
            "\u00bc teaspoon dried oregano",
            "\u2153 cup sherry",
            "9 cups chicken stock",
            "2 tablespoons tomato paste",
            "1 bay leaf",
            "salt to taste",
            "ground black pepper to taste",
            "10 leaves fresh spinach"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Saute butter, leeks, onion, and garlic until clear. Add mushrooms (except the 8 reserved chopped mushrooms), savory, oregano, sherry, stock, tomato paste and bay leaf. Simmer for 30 minutes or so.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain out the vegetables.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the reserved mushrooms and spinach leaves into the broth, and cook until spinach is wilted. Add salt and pepper to taste. Serve garnished with Parmesan cheese if desired.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Spinach Soup Recipes"
        ],
