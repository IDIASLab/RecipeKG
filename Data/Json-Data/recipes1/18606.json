    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284928/fajita-chicken-stuffed-pepper-appetizer/",
        "name": "Fajita Chicken-Stuffed Pepper Appetizer",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fajita Chicken-Stuffed Pepper Appetizer"
        },
        "datePublished": "2012-12-01T23:48:59.000Z",
        "description": "Fajita chicken-stuffed peppers. I am personally not a fan of beef-stuffed peppers so after some experimentation I came up with this! Serve with your favorite crispy/soft tortillas, salsa, sour cream, and all the fixings!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "\u00bd cup water",
            "1 large egg",
            "1 (1 ounce) package fajita seasoning mix",
            "2 (16 ounce) packages mini sweet bell peppers",
            "1 pound skinless, boneless chicken breasts, diced",
            "1 cup uncooked white rice",
            "1 small red onion, diced",
            "2 cups shredded Mexican cheese blend",
            "4 stalks green onions, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix water, egg, and fajita seasoning in a bowl until well blended; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut tops off of peppers and remove the seeds. Set each pepper in the cup of a nonstick muffin tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix chicken, rice, and red onion into the water mixture. Stuff peppers evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 20 minutes. Top with Mexican cheese and green onions and continue to bake until rice is tender and cheese has melted, about 20 minutes more.\n"
            }
        ],
