        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20105/karens-salmon-salad/",
        "name": "Karen's Salmon Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1966662.jpg",
            "width": 2049,
            "height": 2049
        },
        "datePublished": "2000-05-03T16:08:34.000Z",
        "description": "This dish makes a great summer meal. Cool and refreshing, just add bread and your favorite sorbet for dessert. Sit back, relax and enjoy!!!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT1H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (16 ounce) package elbow macaroni",
            "2 ripe tomatoes, diced",
            "4 green onions, chopped",
            "2 dill pickles, diced",
            "1 (14.75 ounce) can salmon, drained",
            "\u00bd cup mayonnaise",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When pasta has come to room temperature, combine it with tomatoes, green onions, pickles, salmon, mayonnaise, salt and pepper in a large bowl and mix well. Chill before serving.\n"
            }
        ],
