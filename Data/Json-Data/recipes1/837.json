        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10587/chocolate-wafer-cookies/",
        "name": "Chocolate Wafer Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F947180.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-03-01T20:37:47.000Z",
        "description": "Chocolate and mint filled cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup butter",
            "1 cup white sugar",
            "\u00bd cup packed brown sugar",
            "2 eggs",
            "2 tablespoons water",
            "1 teaspoon vanilla extract",
            "45 chocolate covered thin mints"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift together flour, baking soda and salt.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter or margarine with white and brown sugars. Blend and beat well.  Add eggs and water.  Stir in vanilla.  Add dry ingredients and mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and refrigerate for 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Enclose 1 chocolate mint wafer in 1 tablespoon of chilled dough.  Bake on greased cookie sheet for 10-12 minutes.\n"
            }
        ],
