    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273080/blueberry-french-toast-casserole/",
        "name": "Blueberry French Toast Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Blueberry French Toast Casserole"
        },
        "datePublished": "2008-11-10T16:05:31.000Z",
        "description": "Makes an excellent breakfast or dessert. Also, impressive and easy to bring to a potluck or morning meeting. Delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT9H40M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "12 slices bread, cut into 1-inch cubes",
            "2 (8 ounce) packages cream cheese, cubed",
            "1 cup fresh blueberries",
            "12 large eggs",
            "2 cups milk",
            "\u2153 cup maple syrup",
            "1 cup water",
            "1 cup white sugar",
            "2 tablespoons cornstarch",
            "1 cup fresh blueberries",
            "1 tablespoon unsalted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Grease a 9x13-inch pan and spread half of the bread cubes over the bottom. Top with cream cheese and 1 cup blueberries. Layer remaining bread cubes on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix together eggs, milk, and syrup in a bowl. Pour over the bread cubes in the pan. Cover and refrigerate for 8 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the refrigerator and let stand for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven, covered, for 30 minutes. Uncover and continue to bake until a knife inserted into the center comes out clean, about 25 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While casserole bakes, combine water, sugar, and cornstarch for sauce. Bring to a boil until thickened. Reduce to a simmer and add blueberries and butter until cooked down. Serve on top of casserole or on the side.\n"
            }
        ],
