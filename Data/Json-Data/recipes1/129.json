    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9922/springerle-i/",
        "name": "Springerle I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F309491.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-08-23T17:52:55.000Z",
        "description": "This is the old German recipe ... belonged to my mom-in-law at the time. I've been baking these for the past 30+ years! It remains a family favorite!! Look for a springerle rolling pin in antique stores.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 dozen",
        "recipeIngredient": [
            "4 eggs",
            "2 tablespoons butter",
            "2 teaspoons baking powder",
            "\u00bc teaspoon salt",
            "2 cups white sugar",
            "4 cups all-purpose flour",
            "\u00bc cup anise seed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Beat eggs in large mixing bowl until very light.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add sugar and butter. Cream together until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, baking powder, and salt. Add dry ingredients and combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough until smooth ... add more flour to get a smooth dough if necessary.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover dough and allow to chill in refrigerator for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll onto slightly floured  board to 1/2 inch thickness. Then roll again with springerle roller to make designs. Cut at border. Sprinkle anise seed on clean tea towel and place cookies on this. Allow to stand overnight (don't cover) to dry.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 12 to 15 minutes at 325 degrees F (170 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool completely. Store in tight tin container ... the longer they are stored, the more anise flavor they take up.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "German"
        ],
