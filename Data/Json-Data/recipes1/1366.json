        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10875/biscochitos-ii/",
        "name": "Biscochitos II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F151932.jpg",
            "width": 250,
