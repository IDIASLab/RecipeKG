        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10430/italian-wedding-cakes/",
        "name": "Italian Wedding Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Italian Wedding Cakes"
        },
        "datePublished": "1998-01-31T01:33:00.000Z",
        "description": "Frosted, cake-like cookies.  To enjoy for special holidays I sprinkle on colored jimmies before the icing dries.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "2 eggs",
            "1 cup milk",
            "4 tablespoons baking powder",
            "1 tablespoon almond extract",
            "1 pinch salt",
            "\u00bd cup melted shortening",
            "3\u2009\u00bd cups all-purpose flour",
            "2 cups confectioners' sugar",
            "1 tablespoon almond extract",
            "8 teaspoons water",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1 cup sugar, 2 eggs and 1 cup milk.  Add baking powder, 3 1/2 cups flour, and salt.  Then add warmed, melted shortening and almond flavoring.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Flour a cookie sheet (no grease) and drop cookie batter from a teaspoon onto sheet. Bake for 10-15 minutes.  Cookies will be cake-like.  Be sure to bake them on center rack.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting:  Mix together confectioners' sugar, 1 tablespoon almond flavoring, water and 2 tablespoons of melted butter.  Stir and frost cooled cookies.\n"
            }
        ],
