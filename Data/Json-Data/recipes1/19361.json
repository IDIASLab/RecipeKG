        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229612/worlds-best-bloody-mary-mix/",
        "name": "World's Best Bloody Mary Mix",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1047002.jpg",
            "width": 1900,
            "height": 1900
