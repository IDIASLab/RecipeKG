        "recipeYield": "5 servings",
        "recipeIngredient": [
            "1 pound ground beef",
            "1 onion, chopped",
            "4 cups water",
            "1\u2009\u00bd teaspoons ground black pepper",
            "4 cups cubed potatoes",
            "3 (8 ounce) cans tomato sauce",
            "2 teaspoons salt",
            "\u00bd teaspoon hot pepper sauce"
