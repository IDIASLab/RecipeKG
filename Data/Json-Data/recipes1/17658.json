    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/215589/no-bake-sugar-free-strawberry-cheesecake/",
        "name": "No Bake Sugar Free Strawberry Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F824314.jpg",
            "width": 1771,
            "height": 1450
        },
        "datePublished": "2012-02-16T19:29:38.000Z",
        "description": "I have a grandmother who is diabetic, and I wanted to create a cheesecake with no added sugar. My whole family loved it, it's now the only one I make",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 8-inch pie",
        "recipeIngredient": [
            "\u00be cup graham cracker crumbs",
            "3 tablespoons butter, melted",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "1 (8 ounce) package cream cheese, softened",
            "1\u2009\u00bd cups milk",
            "1 (1 ounce) package cheesecake flavor sugar-free instant pudding mix",
            "2 pints fresh strawberries, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together graham cracker crumbs, melted butter, cinnamon, and nutmeg in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the mixture into an 8-inch pie dish. Refrigerate while making filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese in a mixing bowl with an electric mixer on medium speed until softened. reduce the speed to low, and gradually beat in milk, a little at a time (mixture will be watery). Use a rubber spatula to scrape cream cheese from the sides of the bowl, if necessary.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in pudding mix until the filling is thick and smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon half of the cream cheese filling into the bottom of the graham cracker crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread half the strawberries over the filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Repeat cheesecake layer and strawberry layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill pie in refrigerator until set and cold, at least 1 hour.\n"
            }
        ],
