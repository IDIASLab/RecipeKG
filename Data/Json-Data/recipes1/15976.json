        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213849/caprese-on-a-stick/",
        "name": "Caprese on a Stick",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1387537.jpg",
            "width": 1478,
            "height": 1478
