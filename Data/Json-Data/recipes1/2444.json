        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6890/portugal-cakes/",
        "name": "Portugal Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Portugal Cakes"
        },
        "datePublished": "1998-09-01T16:36:35.000Z",
        "description": "A different muffin for Sunday brunch--or afternoon tea.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 medium sized muffins",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup butter, room temperature",
            "4 eggs",
            "\u00bc cup dry sherry",
            "1 cup all-purpose flour",
            "1 cup dried currants",
            "1 pinch ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees C (190 degrees F). Grease or spray with non-stick cooking spray 12 muffin tins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Separate the eggs. Lightly beat the egg yolks. Beat the egg whites until stiff peaks form.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the sugar with the butter until light and fluffy. Add the beaten egg yolks and sherry. Gently fold in the beaten egg whites.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour with the nutmeg and gently stir into the egg mixture until smooth. Stir in the currants. Pour the batter into the prepared tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until the tops are golden brown and a tester inserted into the center of a muffin comes out clean, about 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Muffin Recipes"
