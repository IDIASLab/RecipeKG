    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16088/hush-puppies-i/",
        "name": "Hush Puppies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4119823.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-12-23T01:43:22.000Z",
        "description": "A delicious treat from the American South. These tender morsels smell great and taste even better!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 quarts vegetable oil for frying",
            "3 cups cornmeal",
            "2 teaspoons baking powder",
            "1\u2009\u00bd teaspoons salt",
            "1\u2009\u00bd cups milk",
            "\u00bd cup water",
            "1 egg, beaten",
            "1 small onion, minced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil to 365 degrees F (185 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, combine corn meal, baking powder, salt, milk, and water. Mix in egg and chopped onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape the batter into small balls, approximately 1 tablespoon each.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a long handled spoon to gently slide the hush puppies into the hot oil. Cook 8 or 10 at a time, until golden brown. Remove from the oil with a slotted spoon, and roll on paper towels to briefly drain. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Hushpuppie Recipes"
        ],
