        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 (12 ounce) package vanilla wafers",
            "4 cups sifted confectioners' sugar",
            "\u00bd cup butter, softened",
            "1 (6 ounce) can frozen orange juice concentrate, thawed",
            "1 cup chopped pecans",
            "1 cup flaked coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Crush vanilla wafers to make fine crumbs.  Add sugar, butter or margarine, and orange juice concentrate.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 1 inch balls.  Roll balls in pecans or coconut.  Chill until firm.\n"
            }
        ],
