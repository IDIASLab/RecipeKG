        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9504/cream-cheese-and-chopped-dried-beef-ball/",
        "name": "Cream Cheese and Chopped Dried Beef Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F490581.jpg",
            "width": 250,
