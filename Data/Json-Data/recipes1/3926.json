    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11258/poppy-seed-cookies-ii/",
        "name": "Poppy Seed Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8158721.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-02-16T00:39:03.000Z",
        "description": "I always make these with orange peel, but lemon peel works just as well for all you lemon lovers out there.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup unsalted butter, cubed",
            "1 cup white sugar",
            "2 egg yolks",
            "2 tablespoons grated orange zest",
            "\u00bd teaspoon ground nutmeg",
            "\u00bc teaspoon salt",
            "2\u2009\u00bc cups cake flour",
            "2 tablespoons poppy seeds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine the butter, sugar, egg yolks, lemon or orange zest, nutmeg and salt. Beat until light and fluffy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour, then add it and the poppy seeds to the butter mixture. Mix until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into 4 equal parts. Form each piece into a 2 inch diameter cylinder and wrap in plastic and refrigerate for 2 hours or until firm. At this point you can freeze the dough for later use.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut dough cylinders into 1/4 inch thick slices and place on ungreased baking sheets. Bake at 350 degrees F (175 degrees C) for 9 minutes. Let cookies cool on sheets for 2 minutes, then transfer to wire racks to cool. Once cool, store in an airtight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
