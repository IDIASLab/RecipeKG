    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11854/spaghetti-salad-i/",
        "name": "Spaghetti Salad I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F216593.jpg",
            "width": 3056,
            "height": 2292
        },
        "datePublished": "1999-04-10T00:33:34.000Z",
        "description": "Fresh veggies and a rich variety of seasonings make this a great springtime dish for a party or a family picnic! Garnish with tomatoes and parsley and serve with a spaghetti salad smile!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 pound spaghetti, broken into pieces",
            "1 (16 ounce) bottle Italian-style salad dressing",
            "1 tablespoon grated Parmesan cheese",
            "1 tablespoon sesame seeds",
            "1 tablespoon poppy seeds",
            "2 teaspoons seasoning salt",
            "1 teaspoon paprika",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon cayenne pepper",
            "1 cucumber, chopped",
            "1 red onion, diced",
            "2 tomatoes, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook spaghetti according to package directions. Drain, rinse with cold water and drain well. Transfer to large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In medium bowl, whisk together salad dressing, cheese, seeds, salt, paprika, garlic powder and peppers until well blended. Stir in cucumber and onion. Pour mixture over spaghetti and toss lightly to coat evenly. Cover and refrigerate for at least 2 hours or up to 24 hours. Top with tomatoes to serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Vegetarian Pasta Salad Recipes"
        ],
