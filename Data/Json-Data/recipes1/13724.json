    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22707/scalloped-potatoes-iii/",
        "name": "Scalloped Potatoes III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2735218.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2000-10-15T12:39:36.000Z",
        "description": "This is a great recipe that makes a very large amount!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "2 (10.75 ounce) cans  condensed cream of mushroom soup",
            "1 (10.75 ounce) can  condensed cream of celery soup",
            "1 (1 ounce) package dry onion soup mix",
            "1 teaspoon garlic powder",
            "2\u2009\u00bd cups low-fat milk",
            "10 pounds potatoes, peeled and sliced",
            "2 teaspoons salt",
            "1 teaspoon ground black pepper",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl whisk together mushroom soup, celery soup, onion soup mix, garlic powder and milk.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large roasting pan layer potatoes and soup mix, ending with soup mix.  Season with salt and pepper; sprinkle cheese over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 60 to 80 minutes, or until potatoes are cooked.  Let stand 5 to 10 minutes before serving.\n"
            }
        ],
