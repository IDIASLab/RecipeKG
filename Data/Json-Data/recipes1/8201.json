    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14955/savory-twice-baked-sweet-potatoes/",
        "name": "Savory Twice Baked Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F466522.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-11-21T20:10:55.000Z",
        "description": "These twice baked tubers are both sweet and spicy.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "6 sweet potatoes",
            "\u00bc cup butter",
            "1 small onion, minced",
            "1 tablespoon packed brown sugar",
            "\u00be tablespoon orange zest",
            "\u00be teaspoon salt",
            "\u00bc teaspoon ground cumin",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground black pepper",
            "\u215b teaspoon ground allspice",
            "\u00bc cup sour cream",
            "2 tablespoons chopped fresh cilantro",
            "2 egg whites"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place potatoes on baking sheet and bake until just softened, about 1 hour. Allow to cool for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice off 1/2 inch lengthwise from tops of potatoes. Scoop out pulp, leaving 1/4 inch thick shells. Reserve 4 shells; discard remaining 2.  Mash pulp.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small skillet, melt butter over medium heat.  Add onion; cook until softened, about 8 minutes.  Add brown sugar, zest, salt, cumin, cinnamon, pepper and allspice. Cook until sugar melts, about 2-3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine onion mixture with mashed sweet potatoes.  Stir in sour cream and cilantro.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In bowl with mixer on high speed, beat egg whites until soft peaks form.  Gently fold whites into sweet potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon or pipe mixture back into shells.  Place on baking sheets; bake until filling is just set and tops begin to brown, about 15 to 20 minutes.\n"
            }
        ],
