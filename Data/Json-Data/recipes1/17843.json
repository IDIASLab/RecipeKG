    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/221275/banana-berry-pancakes/",
        "name": "Banana Berry Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F982363.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2012-04-18T18:05:22.000Z",
        "description": "Banana-nut pancakes are topped with yogurt and sliced berries then folded in half for a special breakfast or brunch treat.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "8 pancakes",
        "recipeIngredient": [
            "Crisco\u00ae Original No-Stick Cooking Spray",
            "1 large egg, slightly beaten",
            "\u2154 cup milk",
            "2 tablespoons Crisco\u00ae Pure Canola Oil ",
            "1 (7 ounce) package Martha White\u00ae Banana Nut Flavored Muffin Mix, or Whole Grain Banana Nut Muffin Mix",
            "1 cup lowfat vanilla yogurt",
            "2 cups berries, sliced strawberries, blueberries and/or raspberries",
            "1 teaspoon Powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat griddle or large skillet (if electric, heat to 350 degrees F). Spray lightly with no-stick cooking spray. Combine egg, milk, oil and muffin mix in medium bowl; stir until large lumps disappear (batter will be slightly lumpy).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour about 1/4 cup batter for each pancake onto hot griddle. Cook 1 to 2 minutes or until bubbles begin to break on surface. Turn, cook 1 to 2 minutes or until golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon yogurt and berries on half of each pancake. Fold over. Sprinkle with powdered sugar, if desired.\n"
            }
        ],
        "recipeCategory": [
            "Trusted Brands: Recipes and Tips",
            "MarthaWhite&reg;"
        ],
