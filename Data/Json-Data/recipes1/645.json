        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10425/almond-chocolate-coconut-cookies-i/",
        "name": "Almond Chocolate Coconut Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6642460.jpg",
            "width": 2448,
