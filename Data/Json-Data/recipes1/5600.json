        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8267/amish-cake/",
        "name": "Amish Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F847803.jpg",
            "width": 4608,
            "height": 3456
        },
        "datePublished": "1999-06-29T02:16:32.000Z",
        "description": "Quick, Easy, Yummy! Pineapple makes it moist and walnuts give it some crunch.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 9x13-inch cake",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1\u2009\u00bd cups white sugar",
            "2 teaspoons baking powder",
            "2 eggs, beaten",
            "1 teaspoon vanilla extract",
            "1 (20 ounce) can crushed pineapple with juice",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix flour, sugar and baking powder. Add eggs, vanilla and pineapple with juice. Blend well. Add walnuts and mix in.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into an ungreased 9x13 inch pan. Bake at 350 degrees F (175 degrees C ) for 40 minutes.\n"
            }
        ],
