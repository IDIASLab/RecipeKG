        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fcf-images.us-east-1.prod.boltdns.net%2Fv1%2Fstatic%2F1033249144001%2F7323e2f9-629c-4006-9afd-df09398c26a8%2F3ab481dc-804e-47a3-8c55-a6e032bffb31%2F1280x720%2Fmatch%2Fimage.jpg",
