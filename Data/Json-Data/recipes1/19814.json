    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283094/hearty-buckwheat-pancakes/",
        "name": "Hearty Buckwheat Pancakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hearty Buckwheat Pancakes"
        },
        "datePublished": "2013-09-17T02:47:08.000Z",
        "description": "Watch these scrumptious buckwheat pancakes puff up as they cook. Although made with whole grain, the whipped egg whites make them light and fluffy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "12 pancakes",
        "recipeIngredient": [
            "1 cup buckwheat flour",
            "1 cup whole wheat pastry flour",
            "2 tablespoons white sugar",
            "2 teaspoons baking powder",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "2 cups buttermilk",
            "\u00bc cup butter, melted",
            "2 large eggs, separated, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine buckwheat flour, pastry flour, sugar, baking powder, baking soda, and salt in a bowl; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat buttermilk, melted butter, and egg yolks together in a separate bowl. Stir in dry ingredients until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a glass, metal, or ceramic bowl until stiff peaks form. Gently fold egg whites into the batter, until just blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a lightly greased griddle or frying pan over medium heat. Working in batches, pour or ladle 1/4 cup batter onto the heated surface for each pancake. Cook until golden brown, 3 to 5 minutes per side.\n"
            }
        ],
