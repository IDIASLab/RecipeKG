        "recipeIngredient": [
            "\u00bd cup plain yogurt",
            "\u00bc cup sour cream",
            "2 teaspoons honey",
            "2 cups shredded red cabbage",
            "1 Granny Smith apple - peeled, cored and chopped",
            "3 tablespoons minced onion",
            "\u00bc cup chopped parsley",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a salad bowl, combine and whisk together the yogurt, sour cream and honey. Refrigerate until chilled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the cabbage, apple, onion, parsley, and salt and pepper to taste. Mix well and serve chilled.\n"
            }
