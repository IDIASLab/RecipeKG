    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15098/date-filled-cookie-bars/",
        "name": "Date Filled Cookie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F941944.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-03-17T20:57:58.000Z",
        "description": "The name says it all. A delicious bar cookie with rich date filling.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9x13 inch pan",
        "recipeIngredient": [
            "1 pound dates, pitted and chopped",
            "1 cup white sugar",
            "\u00bd cup water",
            "1 teaspoon lemon zest",
            "2 teaspoons orange zest",
            "1\u2009\u00bd cups rolled oats",
            "1\u2009\u00be cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "1 cup packed brown sugar",
            "1 teaspoon ground cinnamon",
            "1 cup chopped walnuts",
            "1 cup butter, melted",
            "\u2153 cup confectioners' sugar for dusting"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan bring the dates, sugar, water, lemon zest and orange zest to a boil. Boil for 3 minutes, stirring constantly, then remove from the heat and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the rolled oats, flour, baking soda, brown sugar and cinnamon.  Stir in the walnuts and melted butter. Mixture will be somewhat crumbly. Press half of the mixture into the bottom of the prepared pan.  Spread the date filling evenly over the crust. Crumble the rest of the crust mixture over the filling, and pat down slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 to 30 minutes in the preheated oven. Cut into bars while warm. Dust with confectioners' sugar when cooled.\n"
            }
        ],
