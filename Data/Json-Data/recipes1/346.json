    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10160/almond-crescents-ii/",
        "name": "Almond Crescents II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6202873.jpg",
            "width": 2165,
            "height": 2165
        },
        "datePublished": "1997-11-12T22:17:26.000Z",
        "description": "Crisp, delicate almond cookies.  A Christmas Favorite!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "\u00bd cup butter",
            "\u2154 cup ground blanched almonds",
            "1\u2009\u2154 cups all-purpose flour",
            "\u00bc teaspoon salt",
            "\u00bd teaspoon vanilla extract",
            "1 cup confectioners' sugar",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix together shortening, butter and almonds.  Sift in flour and salt.  Add vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill dough until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (170 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough with hands into 3 inch lengths, pencil thick.  Form into crescents on ungreased baking sheet.  Bake until just slightly brown -- about 16 minutes. Cool on pan\n"
            },
            {
                "@type": "HowToStep",
                "text": "While still slightly warm, carefully dip in a mixture of confectioner's sugar and  cinnamon.  These are extremely fragile cookies -- handle with care!\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Almond Dessert Recipes"
        ],
