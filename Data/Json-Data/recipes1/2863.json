        "name": "Easy Corn Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fcf-images.us-east-1.prod.boltdns.net%2Fv1%2Fstatic%2F1033249144001%2F63cac44b-5550-4c78-b6e3-bd75fc0e40e3%2F697ad60c-6884-4e01-8e8e-7c96313cf084%2F1280x720%2Fmatch%2Fimage.jpg",
