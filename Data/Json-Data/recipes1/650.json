    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10423/hearts-delight/",
        "name": "Heart's Delight",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F247398.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-01-31T22:37:10.000Z",
        "description": "Another recipe from our annual Heart Party on Valentine's Day.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "\u00bd cup white sugar",
            "2 teaspoons almond extract",
            "5 drops red food coloring",
            "2\u2009\u00bd cups sifted all-purpose flour",
            "\u2153 cup granulated sugar for decoration",
            "5 tablespoons all-purpose flour",
            "1 cup milk",
            "1 cup white sugar",
            "1 cup butter",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large bowl, with electric mixer at medium speed, beat 1 cup butter or margarine with 1/2 cup sugar, almond extract, and food coloring until light and fluffy.  With wooden spoon, stir in 2 1/2 cups flour until smooth and well combined.  Divide dough into 2 parts.  Refrigerate, covered, 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "On lightly sugared surface, roll out dough, one part at a time, about 1/4- inch thick.  Using heart shaped cookie cutter, cut out cookies.  Place, one inch apart, on ungreased cookie sheets.  Bake 20 minutes. Cool completely on wire rack.  Frost  when cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Frosting:  Cook 5 Tablespoons flour and milk until thick, stirring constantly.  Let cool.  Cream together 1 cup sugar, 1 cup butter and 1 teaspoon vanilla.  Add flour/milk mixture.  Beat until consistency to spread.  When finished it looks like whipped cream.  Spread on cookies.\n"
            }
        ],
