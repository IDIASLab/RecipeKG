        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14658/sweet-and-sour-meatballs-i/",
        "name": "Sweet and Sour Meatballs I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3073006.jpg",
