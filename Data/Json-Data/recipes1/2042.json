    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10995/chocolate-chocolate-chip-cookies-iii/",
        "name": "Chocolate Chocolate Chip Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F118168.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-07-11T03:50:09.000Z",
        "description": "A double dose of chocolate!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "2 cups semisweet chocolate chips",
            "6 tablespoons butter, softened",
            "1 cup white sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "2 eggs",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder and salt. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 1 cup of chocolate chips over low heat.  Cream butter and sugar.  Add melted chocolate chips and vanilla; beat in eggs; add flour mixture and remaining chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap in plastic and freeze until firm (about 20 minutes).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make small balls (1 inch); roll in confectioners' sugar.   Place on ungreased cookie sheet and bake for 10-12 minutes at 350 degrees F (175 degrees C). Cool on wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Chip Cookie Recipes",
            "Double Chocolate"
        ],
