        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10551/festive-cherrettes/",
        "name": "Festive Cherrettes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3490049.jpg",
            "width": 3117,
            "height": 3131
        },
        "datePublished": "1998-02-18T03:31:01.000Z",
        "description": "My dad's favorite cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00be cup shortening",
            "\u00bc cup butter",
            "1 teaspoon salt",
            "\u00bd cup confectioners' sugar",
            "2 tablespoons vanilla extract",
            "2 cups all-purpose flour",
            "1 cup chopped pecans",
            "20 candied cherries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend together shortening, butter, salt, sugar, and vanilla.  Add flour and pecans and mix into soft dough.  Measure out level tablespoonfuls of dough and roll in palms of hands to form round balls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place on greased baking sheets.  Press a little indentation in center of each ball with a thimble and put a half of candied cherry on top of each.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a 325 degree F (165 degrees C) oven for 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
