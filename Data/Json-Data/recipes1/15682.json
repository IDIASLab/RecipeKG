    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282913/pina-colada-banana-pudding/",
        "name": "Pina Colada Banana Pudding",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pina Colada Banana Pudding"
        },
        "datePublished": "2009-03-26T19:33:21.000Z",
        "description": "A tropical variation on an old Southern favorite.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT1H25M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "3 cups cold milk",
            "1 (4.6 ounce) package instant coconut cream pudding mix",
            "1 (8 ounce) container sour cream",
            "1 (16 ounce) container non-dairy whipped topping, divided",
            "1 cup crushed pineapple with juice",
            "\u00bd cup shredded coconut",
            "1 teaspoon rum flavoring",
            "1 teaspoon coconut flavoring",
            "1 (11 ounce) box vanilla wafer cookies",
            "2 medium bananas, sliced, or more to taste",
            "\u00bc cup toasted shredded coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk cold milk and pudding mix together in a large mixing bowl for 2 minutes. Allow to sit for 5 minutes until soft-set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix sour cream and 1/2 of the whipped topping into the pudding; mix well. Stir in pineapple with juice, 1/2 cup shredded coconut, rum flavoring, and coconut flavoring.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a layer of wafer cookies in the bottom of a trifle dish. Add a layer of sliced bananas and a layer of the pudding mixture. Repeat layers until all ingredients have been used, reserving a few cookies for garnish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with remaining whipped topping and garnish with remaining cookies and toasted coconut. Chill for 1 hour before serving.\n"
            }
        ],
