    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10153/chocolate-teddy-bear-cookies/",
        "name": "Chocolate Teddy Bear Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F284433.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1997-11-23T16:31:34.000Z",
        "description": "Cookies shaped like teddy bears.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "14 cookies",
        "recipeIngredient": [
            "\u2154 cup butter",
            "1 cup white sugar",
            "2 teaspoons vanilla extract",
            "2 eggs",
            "2\u2009\u00bd cups all-purpose flour",
            "\u00bd cup unsweetened cocoa powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream together the butter or margarine, sugar and vanilla.  Add the eggs.  In a separate bowl, mix dry ingredients.   Gradually add to the creamed mixture and blend.  Chill until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "For each cookie:  Roll:  1 ball for the body (1 inch),  1 ball for the head (3/4 inch),  4 small balls for arms/legs (1/2inch),  2 smaller balls for ears, and 1 tiny ball for the nose.  Arrange on ungreased cookie sheet.  With tooth pick, draw eyes and mouth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 6 minutes or until set.  Let cool 1 minute, then remove from cookie sheet.  Optional:  Make a ribbon around their necks using licorice whips (after baking).\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "International Cookie Recipes",
            "American Cookie Recipes"
        ],
