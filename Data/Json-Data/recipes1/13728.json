    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22681/garlic-top-sirloin-pot-roast/",
        "name": "Garlic Top Sirloin Pot Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F41919.jpg",
            "width": 2592,
            "height": 1944
        },
        "datePublished": "2000-10-15T16:57:02.000Z",
        "description": "Top sirloin roast with garlic, potatoes, onions, carrots, and green peppers cooked slow cooked to perfection.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT6H0M",
        "totalTime": "P0DT6H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 teaspoon salt",
            "1 teaspoon freshly ground black pepper",
            "1 teaspoon paprika",
            "1 (3 pound) top sirloin roast",
            "6 cloves garlic, slivered",
            "6 Yukon Gold potatoes, peeled and quartered",
            "4 carrots, cut into 2 inch pieces",
            "2 large sweet onions, peeled and chopped",
            "\u00bd cup water",
            "\u00bd cup beef broth",
            "3 cubes  beef bouillon",
            "1 bay leaf",
            "2 large green bell peppers, cut into 2 inch pieces"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rub salt, pepper and paprika into the meat. With a small knife, make slits in the roast. Press the garlic slivers into the roast.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the potatoes, carrots, and onions in a large slow cooker. Place meat on top of the vegetables. Pour in water and beef broth, and add bouillon cubes and bay leaf.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lid on slow cooker, and cook for 6 hours on High, or 8 hours on Low. During the last half hour of cooking, add the green peppers.\n"
            }
        ],
        "recipeCategory": [
