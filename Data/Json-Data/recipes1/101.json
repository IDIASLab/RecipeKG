    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9906/date-drop-cookies-i/",
        "name": "Date Drop Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F222021.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-08-16T19:56:30.000Z",
        "description": "Delicious soft & chewy date drop cookies with walnuts. Easy to make and quick to eat!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 teaspoon salt",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bd cups white sugar",
            "2 eggs",
            "3\u2009\u00bd cups sifted all-purpose flour",
            "\u00be teaspoon baking soda",
            "5 tablespoons milk",
            "2 cups dates, pitted and chopped",
            "\u00be cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine margarine or butter, salt, and vanilla; blend well. Add sugar and cream well. Beat eggs and blend into mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour with soda and add to the creamed mixture, alternately with the milk. Mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in dates and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop from teaspoon and flatten with glass covered with damp cloth. Sprinkle with sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees for 12 to 15 minutes. Cool and enjoy.\n"
            }
        ],
