    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10195/christmas-lizzies/",
        "name": "Christmas Lizzies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F759327.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1997-12-18T01:55:35.000Z",
        "description": "Fruit cake-like cookies.  Store in an  airtight container.  The can also be frozen. Cookies are better after a few days, they soften.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 dozen",
        "recipeIngredient": [
            "\u00bc cup butter",
            "\u00bd cup packed brown sugar",
            "2 eggs",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking soda",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "1\u2009\u00bd teaspoons ground nutmeg",
            "\u00bd teaspoon ground cloves",
            "3 cups raisins",
            "\u00bd cup bourbon",
            "4 cups pecan halves",
            "3 cups candied cherries",
            "\u2153 pound diced candied lemon peel"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak fruits in bourbon at least 1 hour to plump.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (170 degrees C).  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream margarine or butter, gradually adding sugar and eggs.  Add dry ingredients, then fruit and nuts.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop from teaspoon onto greased cookie sheets.  Bake for 15 minutes.   Store in an airtight container.\n"
            }
        ],
