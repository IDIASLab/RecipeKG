    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22626/moms-best-fruitcake/",
        "name": "Mom's Best Fruitcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4709806.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2000-10-10T03:31:27.000Z",
        "description": "These are best made several weeks ahead of time.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H45M",
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "1 pound Brazil nuts",
            "1 pound pitted dates",
            "\u00be cup candied cherries",
            "1 pound candied pineapple",
            "\u00be cup sifted all-purpose flour",
            "\u00be cup white sugar",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon baking powder",
            "3 eggs",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).  Line 2 small loaf pans with parchment or wax paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine Brazil nuts, dates, cherries and  pineapple.  Over the fruit and nuts sift the flour, sugar, salt and baking powder.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl combine eggs and vanilla; beat until foamy.  Pour over fruit mixture and mix well.  Spoon batter into prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 1 1/2 hours, or until a toothpick inserted into the cake comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Holiday Cake Recipes"
        ],
