    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9348/bills-divinity/",
        "name": "Bill's Divinity",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7471992.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "1998-12-11T02:15:45.000Z",
        "description": "My brother used to make this every year.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups white sugar",
            "\u00bd cup light corn syrup",
            "\u00bc teaspoon salt",
            "\u00bd cup water",
            "2 egg whites",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a 2-quart saucepan, combine sugar, corn syrup, salt, and water. Cook to hard-ball stage (260 degrees F, or 125 degrees C), stirring only until sugar dissolves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, beat egg whites to stiff peaks. Gradually pour syrup over egg whites, beating at high speed on electric mixer. Add vanilla and beat until candy holds its shape, about 4-5 minutes. Quickly drop from a teaspoon onto wax paper; cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Divinity Recipes"
        ],
