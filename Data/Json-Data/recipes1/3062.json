    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12222/frosty-strawberry-pie/",
        "name": "Frosty Strawberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F94272.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-12-05T21:18:18.000Z",
        "description": "A very yummy summer treat that my mom makes and you can substitute sugar-free gelatin. Be sure to keep it refrigerated! Garnish with whipped cream and walnut halves.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 8 or 9 inch pie",
        "recipeIngredient": [
            "1 (3 ounce) package strawberry flavored Jell-O\u00ae",
            "2 cups sliced fresh strawberries",
            "2 cups vanilla ice cream",
            "1\u2009\u00bc cups boiling water",
            "1 (9 inch) prepared graham cracker crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dissolve gelatin in boiling water and gradually add ice cream, stirring until melted.  Note: if pie is to be chilled 3-4 hours before serving, increase to 1 1/2 cups water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill til thick but NOT set (15-25 minutes) and then fold in strawberries and pour into pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill until firm; garnish with whipped cream and walnut halves.\n"
            }
        ],
        "recipeCategory": [
