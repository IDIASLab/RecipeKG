    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281610/marinated-turkey-breast-roast/",
        "name": "Marinated Turkey Breast Roast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8783150.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-07-17T20:22:17.000Z",
        "description": "This is a delicious marinade I use for a boneless turkey breast roast.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT9H35M",
        "recipeYield": "9 servings",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "\u00bc cup soy sauce",
            "3 cloves garlic, minced, or more to taste",
            "2 tablespoons brown sugar",
            "2 teaspoons dried basil",
            "1 (3 pound) boneless turkey breast",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, soy sauce, garlic, brown sugar, and basil together in a bowl and pour into a resealable plastic bag. Add turkey breast, coat with the marinade, squeeze out excess air, and seal the bag. Marinate in the refrigerator, 8 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a roasting pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove turkey breast from the marinade and shake off excess. Place turkey in the prepared roasting pan and season with salt and pepper. Set remaining marinade aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook turkey in the preheated oven for 1 hour. Baste with the reserved marinade and cook until no longer pink in the center and the juices run clear, about 20 minutes more. An instant-read thermometer inserted into the center should read at least 165 degrees F (77 degrees C).\n"
            }
        ],
