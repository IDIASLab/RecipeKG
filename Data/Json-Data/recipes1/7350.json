        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14762/one-skillet-pork-supper/",
        "name": "One Skillet Pork Supper",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5695726.jpg",
            "width": 3024,
