    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231966/broccoli-cornbread-with-cheese/",
        "name": "Broccoli Cornbread with Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F999768.jpg",
            "width": 1197,
            "height": 1200
        },
        "datePublished": "2012-06-02T11:37:04.000Z",
        "description": "This moist bread with a sweet corn flavor can be served as a side dish with any meaty entree. Adding Cheddar cheese helps seal the deal to convert the picky broccoli hater.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch corn bread",
        "recipeIngredient": [
            "1 cup cottage cheese",
            "\u00be cup butter, melted",
            "4 eggs",
            "2 (8.5 ounce) packages corn bread/muffin mix",
            "1 (10 ounce) package frozen chopped broccoli, thawed",
            "1 large onion, chopped",
            "1\u2009\u00bd cups shredded Cheddar cheese, divided"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cottage cheese, butter, and eggs in a large bowl. Stir corn bread mixes into butter mixture until just moistened. Fold in broccoli, onion, and 1 cup Cheddar cheese. Pour batter into prepared baking dish and sprinkle with remaining 1/2 cup Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 35 to 40 minutes. Cool in the baking dish for 10 minutes before slicing and serving warm.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Cornbread Recipes"
        ],
