    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281021/hawaiian-pizza-with-chicken/",
        "name": "Hawaiian Pizza with Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Hawaiian Pizza with Chicken"
        },
        "datePublished": "2007-02-11T16:42:49.000Z",
        "description": "This a tasty pizza with pineapple that is fun to do with a significant other. The flavors blend well together. Tastes great with a glass of white wine.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 pizza",
        "recipeIngredient": [
            "\u00bd pound skinless, boneless chicken breast halves",
            "1 cup teriyaki marinade",
            "1 cooking spray",
            "1 (8 ounce) can pineapple tidbits, drained",
            "\u00be cup pizza sauce",
            "1 (14 ounce) package refrigerated pizza crust",
            "10 slices Canadian bacon, quartered",
            "1 (8 ounce) package shredded mozzarella cheese",
            "1 pinch dried oregano, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Marinate chicken breasts in teriyaki marinade in the refrigerator for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Lightly spray a 9x13-inch baking pan with cooking spray. Place marinated chicken on the prepared pan; discard excess marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chicken is no longer pink in the center and the juices run clear, about 20 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Remove from the oven and let cool slightly, about 10 minutes. Keep oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lay drained pineapple tidbits between paper towels to pick up excess juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pizza crust on a baking sheet and cover with sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dice cooled chicken into small pieces. Cover pizza sauce with Canadian bacon, then chicken pieces, then with pineapple. Sprinkle with mozzarella cheese and top with oregano.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until cheese is a light golden brown, 15 to 17 minutes. Remove from the oven and cut into triangles.\n"
            }
        ],
