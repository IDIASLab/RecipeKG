    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22875/apple-crunch-pie-ii/",
        "name": "Apple Crunch Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F102346.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-16T00:15:27.000Z",
        "description": "A unique apple pie recipe with a crunchy, sweet topping and old fashion filling.  You may vary the amounts of oatmeal and pecans in the topping to your liking.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "8 Macintosh apples - peeled, cored and chopped",
            "\u00bc cup white sugar",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground allspice",
            "\u00be cup rolled oats",
            "\u00bd cup brown sugar",
            "\u00bc teaspoon ground nutmeg",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd cup butter",
            "\u00be cup chopped pecans",
            "1 (9 inch) unbaked pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine apples, white sugar, 1/2 teaspoon cinnamon and allspice. Pour filling into pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine oatmeal, brown sugar, nutmeg and 1/2 teaspoon cinnamon. Cut in the butter and mix with fingers until crumbly.  Spoon over the top of the apples. Sprinkle top with pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 45 minutes, or until golden brown.\n"
            }
        ],
