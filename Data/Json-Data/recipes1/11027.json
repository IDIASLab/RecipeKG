        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle pecans over bottom of graham cracker crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare pudding according to directions on package; pour into pie crust. Cover and refrigerate for at least 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with whipped cream and chopped pecans when ready to serve.\n"
            }
