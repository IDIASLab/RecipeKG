    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11634/spice-bars/",
        "name": "Spice Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4097859.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-11-02T21:10:00.000Z",
        "description": "These are Sister Rachel's Spice Bars.  If you like yummy, spicy, chewy cookie bars, you'll love these!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "30 bars",
        "recipeIngredient": [
            "\u00be cup shortening",
            "\u00be cup white sugar",
            "\u00bc cup honey",
            "\u00bc cup molasses",
            "2 teaspoons baking soda",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground ginger",
            "\u00bd teaspoon ground cloves",
            "2\u2009\u00bd cups all-purpose flour",
            "\u00be cup raisins",
            "1 cup confectioners' sugar",
            "3 tablespoons milk",
            "\u00bd teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x 13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream the white sugar and shortening together. Stir in the molasses and honey. Sift together the flour, baking soda, cinnamon, ginger, and cloves; add to the creamed mixture and mix until well blended. Finally, stir in the raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the batter evenly into the prepared baking pan. Bake for 20 to 30 minutes in the preheated oven, bars should turn golden brown. Top will appear smooth and dry to the touch.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile ... make the icing. In a smaller bowl, stir the confectioners' sugar together with the milk and vanilla. Drizzle over the spice bars while they are still warm. Allow cookies to cool before cutting.\n"
            }
        ],
