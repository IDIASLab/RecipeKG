        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15835/rhubarb-custard-pie-v/",
        "name": "Rhubarb Custard Pie V",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F640190.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-29T12:22:51.000Z",
        "description": "This recipe has been in our family for years! Love it!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell",
            "2\u2009\u00bd cups fresh rhubarb, cut into 1 inch pieces",
            "1 cup white sugar",
            "\u2153 cup all-purpose flour",
            "1 pinch ground cinnamon",
            "2 eggs, beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread rhubarb pieces evenly into pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine sugar, flour, and cinnamon. Mix well, then stir in eggs. Pour mixture over rhubarb layer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 to 45 minutes until filling is set.\n"
            }
        ],
