        "name": "Sheet Pan Sausage and Seasonal Vegetables",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8981995.jpg",
            "width": 0,
            "height": 0
