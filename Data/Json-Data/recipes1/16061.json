    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284726/white-chocolate-cranberry-bars/",
        "name": "White Chocolate Cranberry Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "White Chocolate Cranberry Bars"
        },
        "datePublished": "2010-03-19T04:39:23.000Z",
        "description": "This is pretty close to the Cranberry Bliss Bars they sell at Starbucks&reg;, but I think these are way better!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT4H15M",
        "recipeYield": "24 bars",
        "recipeIngredient": [
            "1\u2009\u00bc cups light brown sugar",
            "1 cup unsalted butter, softened",
            "3 large eggs",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1 teaspoon ground ginger",
            "\u00bc teaspoon salt",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00be cup dried cranberries",
            "\u00bd (12 ounce) package white chocolate chips",
            "Frosting:",
            "\u00bd (8 ounce) package cream cheese, softened",
            "3 cups powdered sugar",
            "4 teaspoons freshly squeezed lemon juice",
            "\u00bd teaspoon vanilla extract",
            "\u00bc cup dried cranberries, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Generously grease a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare cake: Beat brown sugar and butter with an electric mixer until smooth. Add eggs, vanilla, ginger, and salt; beat until well combined. Gradually beat in flour until smooth. Stir in cranberries and white chocolate chips with a spoon. Pour batter into the prepared baking pan and spread evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until light brown, 30 to 35 minutes. Remove from the oven and let cool, about 30 to 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare frosting: Beat cream cheese, powdered sugar, lemon juice, and vanilla with an electric mixer until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread frosting over cooled cake, then sprinkle chopped cranberries over top. Let sit for 3 hours. Cut in thirds down the length of the pan, then in quarters across the width. Then cut each square diagonally to create 24 triangular pieces.\n"
            }
        ],
