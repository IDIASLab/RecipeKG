    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23033/tomato-sauce-with-sausage/",
        "name": "Tomato Sauce with Sausage",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F87917.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2000-08-19T01:20:14.000Z",
        "description": "A meaty sauce with plenty of fresh basil and garlic.  A very good sauce that also freezes well.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "8 cups",
        "recipeIngredient": [
            "\u00bc cup olive oil",
            "2 pounds Italian sausage, casings removed",
            "2 large onions, chopped",
            "\u00bc cup chopped garlic",
            "1 (6 ounce) can tomato paste",
            "2 (28 ounce) cans whole peeled tomatoes",
            "1 cup water",
            "1 cup chopped fresh basil",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan heat oil and saute sausage until brown, about 6 minutes. Add onion and garlic to pot and saute  about 8 minutes. Mix in tomato paste, tomatoes, water and basil. Bring to a boil. Break up tomatoes. Reduce heat to medium and simmer until thickened, about 45 minutes. Season with salt and pepper.\n"
            }
        ],
