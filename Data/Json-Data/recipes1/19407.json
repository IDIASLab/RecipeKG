    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283130/red-curry-noodles/",
        "name": "Red Curry Noodles",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Red Curry Noodles"
        },
        "datePublished": "2013-06-04T19:52:49.000Z",
        "description": "Thai-inspired red curry broth over rice noodles. A quick and savory main dish.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (8 ounce) package dried rice noodles",
            "4 tablespoons red curry paste",
            "1 tablespoon olive oil, or to taste",
            "1 cup water",
            "1 cup vegetable broth",
            "1 (12 ounce) package extra-firm tofu, cut into 1/2-inch cubes",
            "1 cup baby carrots, cut into thin slices",
            "\u00bd large onion, cut into thin strips",
            "1 (13.5 ounce) can light coconut milk",
            "1 medium red bell pepper, cut into thin strips",
            "\u00bc cup soy sauce",
            "2 teaspoons Thai sweet chili sauce, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place rice noodles in a bowl of cold water to soak.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine curry paste and olive oil in a skillet over medium heat. Mix until well combined and cook for 1 to 2 minutes. Stir in water and vegetable broth. Add tofu, carrots, and onion; bring to a boil. Cover and cook until vegetables are tender, 3 to 5 minutes. Add coconut milk and bell pepper. Stir in soy sauce and chili sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain rice noodles and submerge in a bowl of boiling water for 2 minutes. Drain again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide rice noodles into 4 bowls. Ladle sauce and veggies right over top.\n"
            }
        ],
