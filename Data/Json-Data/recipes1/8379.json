        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9462/peanut-butter-marshmallow-squares/",
        "name": "Peanut Butter Marshmallow Squares",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F306989.jpg",
            "width": 250,
