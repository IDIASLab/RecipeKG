    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281634/pan-fried-pork-chops-with-apple-and-pepper-gravy/",
        "name": "Pan-Fried Pork Chops with Apple and Pepper Gravy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9411080.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-12-14T20:12:20.000Z",
        "description": "Pork chops are marinated and pan-fried with apples, assorted peppers, and onions in a blend of autumn flavors. A perfect celebration of harvest flavors to warm and comfort on a chilly night.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups apple cider",
            "3 tablespoons olive oil",
            "2 tablespoons Dijon mustard",
            "1 tablespoon white wine vinegar",
            "1 tablespoon chopped fresh rosemary",
            "1 tablespoon ground paprika",
            "salt and ground black pepper to taste",
            "4 (1 1/2 inches thick) pork chops",
            "1 large apple - peeled, cored, and cubed",
            "1 medium green bell pepper, sliced",
            "1 medium red bell pepper, sliced",
            "1 medium yellow bell pepper, sliced",
            "1 medium onion, sliced",
            "\u00bc cup all-purpose flour",
            "\u00bc cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix apple cider, oil, Dijon mustard, vinegar, rosemary, paprika, salt, and pepper together in a bowl. Trim pork chops of fat and place into mixture. Refrigerate for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove pork chops from marinade; reserve marinade and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a nonstick pan over medium-high heat. Cook pork chops in the hot pan until browned, about 5 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the pork chops are cooking, marinate apple, bell peppers, and onion in the reserved marinade for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add marinated apple to the pork chops; cook until brown, 3 to 5 minutes. Pour the rest of the marinade, peppers, and onion into the pan and let simmer, uncovered, for 20 minutes. Remove the pork chops, apples, and peppers from the pan and place in a serving dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour and milk with the remaining liquid in the pan; simmer until thickened, 2 to 3 minutes. Pour the gravy on top of the pork chops.\n"
            }
        ],
