    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/216451/mary-schmidts-cranberry-sauce/",
        "name": "Mary Schmidt's Cranberry Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F548487.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2013-01-19T07:01:03.000Z",
        "description": "This homemade sauce is super easy to make and you will steal bites of it for days out of the fridge; it's really delicious!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT8H50M",
        "recipeYield": "4 cups",
        "recipeIngredient": [
            "1 cup cranberry-raspberry juice",
            "1 (12 ounce) package fresh cranberries, rinsed",
            "1 cup pure maple syrup",
            "1 orange, zested",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour the cranberry-raspberry juice into a saucepan. Add the cranberries, maple syrup, and orange zest. Bring to a boil over medium-high heat, then reduce heat to low, cover, and cook until the cranberries pop, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the heat, and stir in the chopped walnuts. Let cool for 30 minutes, then scrape into a serving bowl. Cover, and refrigerate overnight before serving.\n"
            }
        ],
