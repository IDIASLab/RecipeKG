        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17578/depression-cake-ii/",
        "name": "Depression Cake II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F62515.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-01-25T11:11:29.000Z",
        "description": "A surprisingly good cake that has no eggs, milk, or butter in it. My grandmother was quite proud of it.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "2 cups packed brown sugar",
            "2 cups hot water",
            "2 tablespoons bacon grease",
            "2 cups raisins",
            "3 cups all-purpose flour",
            "1 teaspoon salt",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "1 teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan combine the brown sugar, hot water, bacon grease, and raisins over medium heat. Bring to a boil for 5 minutes, then set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease and flour two 8x4-inch loaf pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, stir together the flour, salt, baking soda, cinnamon, and cloves. Add the ingredients from the saucepan and mix until well blended. Divide evenly between the 2 prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 to 50 minutes in the preheated oven. Cool in pans for 10 minutes before removing to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Spice Cake Recipes"
        ],
