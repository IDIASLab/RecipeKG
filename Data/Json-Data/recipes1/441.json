        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup butter, melted",
            "2 eggs",
            "2 cups all-purpose flour",
            "\u00bd teaspoon baking soda",
            "1\u2009\u00bd teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix sugar, butter or margarine and eggs.  Add dry ingredients and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let dough stand for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop 1/2 teaspoon-full  2 inches apart on greased, floured cookie sheet.  Bake 10 minutes or less, until edges tan.  Sprinkle with cinnamon or sugar.\n"
            }
        ],
