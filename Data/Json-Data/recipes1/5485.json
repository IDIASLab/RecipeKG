        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) pie crust, baked",
            "3 bananas",
            "2\u2009\u00bd cups cold milk",
            "2 (3.5 ounce) packages instant vanilla pudding mix",
            "2 cups frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice 2 of the bananas and arrange in bottom of baked and cooled pastry shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cold milk into a medium bowl. Add pudding mix and whisk until thoroughly combined. Fold in 1/2 cup of the whipped topping. Pour into pastry shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill at least 3 hours before serving. Garnish with remaining whipped topping and slices of the remaining banana.\n"
            }
        ],
