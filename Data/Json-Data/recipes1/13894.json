    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23013/instant-yeast-rolls/",
        "name": "Instant Yeast Rolls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F570182.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-22T18:40:46.000Z",
        "description": "When you're looking for quick and easy dinner rolls that will go with any dish, this recipe is for you! The melted butter adds a special touch.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1 (.25 ounce) package active dry yeast",
            "\u00bc cup warm water (110 degrees F/45 degrees C)",
            "\u00bc cup buttermilk",
            "\u00bc cup white sugar",
            "\u00bc cup vegetable oil",
            "2 cups self-rising flour",
            "\u00bc cup butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, dissolve yeast in warm water. Let stand until creamy, about 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the buttermilk, sugar, vegetable oil and flour to the yeast mixture; mix well. Roll dough out 1/2 inch thick and cut into circles with a biscuit cutter. Place a tablespoon of butter on top of each biscuit and fold biscuits in half. Place rolls on lightly greased baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 10 to 15 minutes, until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes",
            "Rolls and Buns"
        ],
