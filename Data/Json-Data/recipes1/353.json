    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10170/chocolate-nut-biscotti/",
        "name": "Chocolate Nut Biscotti",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2930190.jpg",
            "width": 2448,
            "height": 2448
        },
        "datePublished": "1997-11-13T20:53:34.000Z",
        "description": "Delicious, not too crispy biscotti, which can be dipped in or drizzled with melted chocolate.  (You can use slivered almonds instead of the hazelnuts, if you wish.)",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bc teaspoon salt",
            "\u00bd cup butter, softened",
            "\u00be cup white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 (4 ounce) package bittersweet chocolate bar with almond nougat, chopped",
            "1 cup chopped hazelnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C). Grease and lightly flour cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix flour, baking powder and salt.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter and sugar in large bowl until light and fluffy. Beat in eggs and vanilla. Gradually add flour mixture, beating well after each addition. Stir in chocolate and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into two equal parts. On floured surface, shape dough into two logs, 14 inches long, 1-1/2 inches wide and 1 inch thick. Place two inches apart on prepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 minutes or until lightly browned. Remove to a cutting board.  Let cool 5 minutes. Cut into 3/4 inch thick diagonal slices and return to cookie sheet, standing upright.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 10 minutes more or until slightly dry. Cool completely on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
