    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283826/lemony-chicken-tenders/",
        "name": "Lemony Chicken Tenders",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9150891.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-10-19T16:48:38.000Z",
        "description": "This is a quick way to add wonderful flavor to boring chicken.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "cooking spray",
            "1 (16 ounce) package boneless, skinless chicken tenders",
            "4 teaspoons unsalted butter, softened",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground paprika",
            "\u2153 cup lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Spray an 8x11-inch baking pan with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken tenders in a single layer in the prepared pan. Spread butter on the tenders, then evenly sprinkle with salt and paprika. Drizzle with lemon juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake, uncovered, in the preheated oven until chicken is no longer pink in the center, 25 to 30 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).\n"
            }
        ],
