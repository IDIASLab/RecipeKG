    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282141/pumpkin-cookies-with-cream-cheese-filling/",
        "name": "Pumpkin Cookies with Cream Cheese Filling",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pumpkin Cookies with Cream Cheese Filling"
        },
        "datePublished": "2010-11-17T19:12:34.000Z",
        "description": "Moist pumpkin cookies with homemade cream cheese filling. These are so good that I have to make them year-round.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "18 cookies",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "1 teaspoon baking powder",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1 dash ground nutmeg",
            "1 cup white sugar",
            "\u00bd cup packed light brown sugar",
            "\u00bd cup unsalted butter, softened",
            "2 large eggs",
            "1 teaspoon vanilla extract",
            "\u00be cup pumpkin puree",
            "\u00bd cup whole milk",
            "\u00bd cup cream cheese, softened",
            "6 tablespoons unsalted butter, softened",
            "1 teaspoon vanilla extract",
            "2 cups powdered sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line baking sheets with silicone mats or buttered parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, cinnamon, baking soda, salt, and nutmeg for cookies in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat white sugar, brown sugar, and butter in a large mixing bowl with an electric mixer until light and fluffy. Beat in eggs and vanilla extract until well blended. Slowly beat in pumpkin and milk. Add flour mixture, beating until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop tablespoonfuls of dough onto the prepared baking sheets with a tablespoon or cookie scoop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until bottoms are lightly browned and tops are firm, 12 to 14 minutes. Cool for about 5 minutes on the baking sheets and then loosen the bottoms with a spatula; let cool completely on wire racks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While cookies cool, combine cream cheese, butter, and vanilla extract together for filling in a bowl with an electric mixer. Beat in powdered sugar until well blended and filling is smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Frost the bottom of a cookie, then press the bottom of another cookie on the filling to make a sandwich. Repeat with the remaining cookies.\n"
            }
        ],
