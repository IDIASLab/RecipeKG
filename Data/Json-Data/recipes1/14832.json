    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283126/thai-coconut-curry/",
        "name": "Thai Coconut Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Thai Coconut Curry"
        },
        "datePublished": "2006-12-24T04:38:23.000Z",
        "description": "This coconut curry is the result of several attempts to replicate my favorite Thai diner's curry. It makes for a colorful presentation and looks like you worked a lot longer and harder than you did! Serve over sticky white rice.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 tablespoon peanut oil",
            "2 tablespoons finely chopped lemon grass",
            "2 tablespoons finely chopped fresh ginger",
            "2 cloves garlic, finely chopped",
            "1 cup light coconut milk",
            "1 pound medium shrimp, peeled and deveined, tails left on",
            "\u00bc cup diced red bell pepper",
            "\u00bc cup chopped fresh cilantro, divided",
            "2 stalks green onion, thinly sliced",
            "2 tablespoons lime juice",
            "1 teaspoon fish sauce",
            "1 teaspoon soy sauce",
            "1 teaspoon sweet red chili sauce, or to taste",
            "1 pinch white sugar",
            "2 cups warm cooked rice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large pan over medium heat. Saute lemon grass, ginger, and garlic until fragrant, 1 to 2 minutes. Add coconut milk and bring to a boil. Keep at a steady boil until reduced by half, about 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add shrimp, bell pepper, 1/2 of the cilantro, green onion, lime juice, fish sauce, soy sauce, chili sauce, and sugar. Simmer for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve curry over warm rice and garnish with remaining cilantro.\n"
            }
        ],
