    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282395/fried-buttermilk-chicken-strips/",
        "name": "Fried Buttermilk Chicken Strips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9048383.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-05-18T22:21:36.000Z",
        "description": "These buttermilk chicken strips are crunchy on the outside, moist on the inside!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds chicken tenders",
            "2 cups buttermilk",
            "1\u2009\u00bd cups all-purpose flour",
            "1 tablespoon salt",
            "1 tablespoon garlic salt",
            "2 teaspoons mustard powder",
            "1 teaspoon ground paprika",
            "1 teaspoon ground black pepper",
            "2 cups peanut oil, or as needed",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Soak chicken tenders in buttermilk for 20 to 30 minutes in the refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While chicken is soaking, combine flour, salt, garlic salt, mustard powder, paprika, and pepper in a large brown bag.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain chicken and shake lightly to remove excess buttermilk. Chicken should still be damp. Drop chicken into the bag of seasoned flour and shake until well coated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop chicken into hot oil, about 6 pieces at a time, depending on the size of your pot. Cook until golden brown, crispy, and no longer pink in the centers, 6 to 8 minutes. Remove from oil and drain on paper towels. Sprinkle with salt and pepper while still warm.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Chicken Tenders Recipes"
        ],
