    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14214/turkey-macaroni-salad/",
        "name": "Turkey Macaroni Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F965625.jpg",
            "width": 1197,
            "height": 1200
        },
        "datePublished": "1999-08-12T20:39:55.000Z",
        "description": "Although it is a great salad to have during the summer, my family enjoys it all year round.  There are never any leftovers!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "8 ounces dry fusilli/spiral pasta",
            "1 cup mayonnaise",
            "1 cup plain yogurt",
            "\u00bc cup chopped green onions",
            "1 teaspoon salt",
            "\u215b teaspoon ground white pepper",
            "3 cups cooked turkey, cubed",
            "1 cup chopped celery"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cook pasta according to package directions.  Drain, rinse with cold water, and drain again.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine mayonnaise, yogurt, onions, salt, and white pepper.  Gently stir in turkey, pasta, and celery.  Cover, and chill until serving time.\n"
            }
        ],
