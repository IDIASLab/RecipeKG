    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13785/easy-pumpkin-bread/",
        "name": "Easy Pumpkin Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F917751.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-11-01T18:14:42.000Z",
        "description": "This recipe is quite good and I hope you enjoy it just as much as I do.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup packed brown sugar",
            "3 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bd cup butter flavored shortening",
            "2 eggs",
            "1 cup milk",
            "2 cups pumpkin puree",
            "1 teaspoon vanilla extract",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour one 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, stir together the flour, brown sugar, baking powder and salt. Mix in the shortening until mixture resembles coarse crumbs. Make a well in the center, and pour in eggs, milk, pumpkin and vanilla. Mix well, then stir in nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 to 50 minutes, or until a toothpick inserted into the center comes out clean. Let cool in pan for 10 minutes, then turn out onto a wire rack and cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
