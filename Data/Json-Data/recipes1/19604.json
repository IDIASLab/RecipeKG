    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284317/easy-japanese-fried-rice/",
        "name": "Easy Japanese Fried Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1030671.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2013-07-23T02:29:05.000Z",
        "description": "I love being able to make fried rice at home, and this recipe is as easy as you could ask for!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cups jasmine rice",
            "2 cups chicken stock",
            "1\u2009\u00bd cups water",
            "4 tablespoons butter, divided, or more to taste",
            "2 large eggs, beaten",
            "2 tablespoons vegetable oil",
            "3 tablespoons sesame seeds, or to taste",
            "2 tablespoons soy sauce (such as Kikkoman\u00ae), or to taste",
            "\u215b teaspoon salt",
            "\u215b teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place rice, chicken stock, and water in a medium saucepan; bring to a boil. Reduce heat to low, cover, and simmer until tender and liquid has been absorbed, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the heat, stir in 2 tablespoons butter, and let cool to room temperature, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat vegetable oil in a skillet over medium heat. Add eggs and scramble, stirring often to chop chunks of egg into small bits with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place rice in a large mixing bowl, add eggs, and toss together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt remaining 2 tablespoons butter in a large frying pan over medium-high heat. Dump rice mixture into the hot pan. Add sesame seeds, 2 tablespoons soy sauce, salt, and pepper. Cook, stirring often, for 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Taste and add more butter or soy sauce as desired. Serve.\n"
            }
        ],
