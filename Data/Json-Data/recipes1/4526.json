        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11351/good-as-grandmas-sugar-cookies/",
        "name": "Good-As-Grandma's Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F39513.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-02T22:02:53.000Z",
        "description": "Sugar cookies that are as good as your grandma's! They're delicious!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup white sugar",
            "1 egg",
            "3 teaspoons milk",
            "1 teaspoon vanilla extract",
            "3 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C). Line baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter and the sugar together until light and fluffy. Beat in the egg, milk and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour, baking powder and salt together. Add the flour mixture the butter mixture and stir to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoonfuls of the dough on the prepared baking sheets and bake at 400 degrees F (205 degrees C) for 5 to 8 minutes or until just golden.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
