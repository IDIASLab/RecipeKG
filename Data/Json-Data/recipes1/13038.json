    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21668/corn-relish-ii/",
        "name": "Corn Relish II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F994914.jpg",
            "width": 2992,
            "height": 2992
        },
        "datePublished": "2000-09-02T01:44:18.000Z",
        "description": "This is a super easy, very tasty relish recipe that improves with refrigeration. It's a great way to use summer corn and other fresh garden vegetables, and tastes great with a variety of meals. Present one of the colorful jars to a friend!",
        "prepTime": "P0DT1H0M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "14 cups",
        "recipeIngredient": [
            "8 ears corn, husked and cleaned",
            "2 pounds tomatoes - peeled, seeded, and coarsely chopped",
            "1\u2009\u00bd cups chopped green bell peppers",
            "\u00be cup chopped red bell pepper",
            "\u00bd cucumber, chopped",
            "1 cup chopped onion",
            "1 cup white sugar",
            "2 cups apple cider vinegar",
            "1 tablespoon salt",
            "1 teaspoon celery seed",
            "1 teaspoon mustard seed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut the corn from the cobs. Scrape the cobs with a large spoon to remove remaining juices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, mix the corn kernels and juice, tomatoes, green bell peppers, red bell peppers, cucumber and onion.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix the sugar, apple cider vinegar, salt, celery seed and mustard seed. Pour into the saucepan over vegetable mixture. Bring to a boil, then simmer 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the finished relish to sterile jars. Refrigerate until serving.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Relish Recipes"
        ],
