    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284571/deep-fried-cajun-turkey/",
        "name": "Deep-Fried Cajun Turkey",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F928407.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-11-28T00:08:40.000Z",
        "description": "Free up valuable oven space and have a turkey that's ready in a fraction of the time with this Cajun-flavored, deep-fried turkey.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (12 pound) whole turkey, neck and giblets removed",
            "1 (17 ounce) bottle Creole butter injectable marinade",
            "\u00bc cup spice and herb seasoning (such as Tony Chachere's\u00ae)",
            "3 gallons peanut oil for frying, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring turkey to room temperature, 30 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set turkey in a deep fryer and fill with enough water to cover the turkey. Remove turkey and mark the water level. Empty water, pat the deep fryer completely dry, and fill with peanut oil to the mark. Heat oil to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Inject legs, thighs, wings, and breast full of Creole butter marinade until it can't take anymore. Rub the seasoning inside the turkey cavity and all over the skin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place turkey in the hot oil and cook until no longer pink at the bone and the juices run clear, about 42 minutes. Carefully remove from the deep fryer; an instant-read thermometer inserted into the thickest part of the thigh, near the bone, should read 165 degrees F (74 degrees C). Cover with a doubled sheet of aluminum foil, and allow to rest for 15 minutes before slicing.\n"
            }
        ],
