    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14410/pea-salad-ii/",
        "name": "Pea Salad II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F219683.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-27T00:08:19.000Z",
        "description": "This is a colorful and creamy pasta salad with sweet peas and pickles.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 cup macaroni",
            "3 eggs",
            "\u00bc cup Ranch-style salad dressing",
            "1 teaspoon white sugar",
            "2 teaspoons white wine vinegar",
            "salt to taste",
            "\u00bc teaspoon ground black pepper",
            "2 cups frozen green peas, thawed",
            "3 green onions, chopped",
            "2 medium sweet pickles, chopped",
            "2 stalks celery, chopped",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place eggs in a saucepan and cover completely with cold water. Bring water to a boil. Cover, remove from heat, and let eggs stand in hot water for 10 to 12 minutes. Remove from hot water, cool, peel and chop.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the salad dressing, sugar, vinegar, salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the pasta, eggs, peas, green onion, pickles, celery and cheese. Pour dressing over salad; mix well and chill before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Green Pea Salad Recipes"
        ],
