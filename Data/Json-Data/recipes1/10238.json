    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17921/chocolate-mint-cookies-ii/",
        "name": "Chocolate Mint Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chocolate Mint Cookies II"
        },
        "datePublished": "2000-03-16T22:34:20.000Z",
        "description": "Here is a cookie like the one that the girl scouts sell. Easy to make and wonderful!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H8M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package devil's food cake mix",
            "2 eggs",
            "2 tablespoons water",
            "2 tablespoons vegetable oil",
            "\u00bd cup unsweetened cocoa powder",
            "2 cups semisweet chocolate chips",
            "3 drops peppermint oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the cake mix, eggs, water, oil and cocoa; mix until smooth, about 3 minutes. Let stand for 15 to 20 minutes to set up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Grease cookie sheets. Roll the dough into very small balls, about 1/2 inch. Place onto the prepared cookie sheet, 2 inches apart and smash down flat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10 minutes in the preheated oven. Allow cookies to cool on baking sheet for 5 minutes before removing to a wire rack to cool completely. Melt chocolate chips in a microwave oven or in a bowl over a pan of simmering water. Stir in the peppermint oil. Dip the cookies into the chocolate, or spread it on. set on parchment lined sheets and chill to set.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Chocolate Cookie Recipes"
        ],
