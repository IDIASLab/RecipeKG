        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11253/mock-peanut-brittle/",
        "name": "Mock Peanut Brittle",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F373393.jpg",
            "width": 3008,
