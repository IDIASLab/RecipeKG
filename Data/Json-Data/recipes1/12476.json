        "mainEntityOfPage": "https://www.allrecipes.com/recipe/31627/skillet-fried-corn/",
        "name": "Skillet Fried Corn",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F380987.jpg",
