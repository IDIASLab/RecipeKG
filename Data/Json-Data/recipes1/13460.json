    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23060/venison-stroganoff/",
        "name": "Venison Stroganoff",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9169406.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-10-03T18:30:24.000Z",
        "description": "This is an easy and very good recipe that my family loves, cooked with venison.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 pound venison, cut into cubes",
            "salt and pepper to taste",
            "garlic powder to taste",
            "1 onion, chopped",
            "2 (10.75 ounce) cans  condensed cream of mushroom soup",
            "1 (16 ounce) package uncooked egg noodles",
            "1 (8 ounce) container sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Season venison with salt, pepper and garlic powder to taste. Saute onion in a large skillet; when soft, add venison and brown. Drain when venison is no longer pink and add soup. Reduce heat to low and simmer.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, bring a large pot of lightly salted water to a boil. Add noodles and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When noodles are almost done cooking, stir sour cream into meat mixture. Pour meat mixture over hot cooked noodles and serve.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Game Meats",
            "Venison"
        ],
