        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21088/snappy-green-beans/",
        "name": "Snappy Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F239976.jpg",
