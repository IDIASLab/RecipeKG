    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282068/light-and-flaky-biscuits/",
        "name": "Light and Flaky Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F827079.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-02-20T18:35:30.000Z",
        "description": "Flaky and light biscuits. Perfect addition to homemade soup.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "14 biscuits",
        "recipeIngredient": [
            "1 cup milk",
            "1 large egg",
            "3 cups all-purpose flour",
            "2 tablespoons white sugar",
            "4\u2009\u00bd teaspoons baking powder",
            "\u00be teaspoon cream of tartar",
            "\u00be teaspoon salt",
            "\u00be cup cold margarine"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 450 degrees F (230 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk milk and egg together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix flour, sugar, baking powder, cream of tartar, and salt together in a bowl. Add margarine and cut in with a pastry blender or 2 knives. Add milk-egg mixture and mix just enough to combine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough briefly, 5 to 7 times. Roll to a 1 1/2-inch thickness and cut out 14 biscuits.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden brown, about 12 minutes.\n"
            }
        ],
