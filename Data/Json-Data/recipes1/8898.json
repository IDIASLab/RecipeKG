    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15286/margarita-balls-ii/",
        "name": "Margarita Balls II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F796262.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-29T17:35:41.000Z",
        "description": "This is a great old Texas recipe. Everyone seems to like it, but it's obviously only for grown-ups.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 (12 ounce) package vanilla wafers",
            "2 cups small pretzel twists",
            "6 ounces cream cheese",
            "1 pound confectioners' sugar",
            "\u00be cup frozen margarita mix, thawed",
            "2 tablespoons tequila",
            "2 tablespoons brandy-based orange liqueur (such as Grand Marnier\u00ae)",
            "1 tablespoon grated lime zest",
            "\u2154 cup white sugar",
            "4 drops green food coloring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Using a food processor or blender, process all of the vanilla wafers and pretzels to fine crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine all of the crumbs with the confectioners' sugar. In another bowl, mix together the cream cheese, margarita mix, tequila and Grand Marnier, stir into the crumb mixture. Divide dough into two pieces, wrap and refrigerate for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the white sugar into two small bowls. Add the zest of 1 lime to each bowl. Stir the food coloring into one of the bowls. Unwrap dough and roll into walnut sized balls. Roll half of the balls in the green sugar, and the other half in the white sugar. Store in the refrigerator in the unlikely event that there are any left!\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Liqueur Dessert Recipes",
            "Orange Liqueur"
        ],
