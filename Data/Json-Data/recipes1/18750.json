    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284950/cranberry-pecan-microwave-acorn-squash/",
        "name": "Cranberry-Pecan Microwave Acorn Squash",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cranberry-Pecan Microwave Acorn Squash"
        },
        "datePublished": "2013-01-13T22:33:10.000Z",
        "description": "This is really good for the holidays, plus it's quick and easy. Microwaving first makes it a lot easier to slice the squash.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 squash halves",
        "recipeIngredient": [
            "2 medium acorn squash",
            "\u2154 cup graham cracker crumbs",
            "\u2153 cup chopped pecans",
            "\u2153 cup unsalted butter, softened",
            "3 tablespoons packed brown sugar",
            "\u00bc teaspoon ground nutmeg",
            "\u00bd cup whole berry cranberry sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pierce each squash in several places to allow steam to escape, then place 2 inches apart in the microwave. Microwave on high for 6 minutes. Remove to a cutting board.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each squash in half crosswise; remove and discard seeds. Arrange halves with cut-sides down on a microwave-safe plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover tightly with plastic wrap and microwave until tender, 7 to 9 minutes. Remove stems and cut a thin slice from the pointed ends to prevent tipping. Flip so cut-sides are facing up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix graham cracker crumbs, pecans, butter, brown sugar, and nutmeg in a bowl. Spoon 1/4 of the mixture into each squash half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave, uncovered, until filling is heated through, 2 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve topped with cranberry sauce.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash",
            "Winter Squash",
            "Acorn Squash"
        ],
