    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18457/lemon-cherry-pie/",
        "name": "Lemon-Cherry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lemon-Cherry Pie"
        },
        "datePublished": "2000-05-17T20:37:51.000Z",
        "description": "This is a wonderfully light recipe for summer that doesn't have a heavy 'cherry pie filling' taste.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT4H20M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (16 ounce) can cherry pie filling",
            "1 (9 inch) prepared graham cracker crust",
            "1 (4.3 ounce) package non-instant lemon pudding mix",
            "1 cup milk",
            "1 egg yolk",
            "\u00bd cup white sugar",
            "1 cup cream cheese",
            "1 (8 ounce) container frozen whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Layer 2/3 of the pie filling in the bottom of graham cracker crust; reserve remaining 1/3 for topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, combine the pudding with milk, egg yolk, and sugar. Cook over medium heat until pudding has set up. Pudding will be very thick; this is okay. Remove pudding from stove and transfer to a 1 quart bowl. Stir in the cream cheese until completely combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in 1/2 of the whipped topping into the pudding mixture. Pour mixture into pie shell and top with the remaining whipped topping. Garnish top of pie with cherries that are in reserved pie filling. Cover and refrigerate for 3 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies",
            "Lemon Pies"
        ],
