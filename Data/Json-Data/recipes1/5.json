        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9806/peanut-butter-kiss-cookies/",
        "name": "Peanut Butter Kiss Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4559046.jpg",
