    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15382/chocolate-sugar-cookies/",
        "name": "Chocolate Sugar Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F746534.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-12-03T03:48:51.000Z",
        "description": "These rich chocolate cookies are chewy and a bit spicy with the flavor of cinnamon. Take them to your next holiday cookie exchange!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00be cup shortening",
            "1 cup white sugar",
            "1 egg",
            "\u00bc cup light corn syrup",
            "2 (1 ounce) squares unsweetened chocolate, melted",
            "2 cups all-purpose flour",
            "\u00bc teaspoon salt",
            "1 teaspoon baking soda",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, cream shortening, sugar, and egg. Stir in syrup and melted chocolate. Sift flour, salt, baking soda, and ground cinnamon; add to creamed mixture. Chill 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough 1/8 inch thick on a well-floured pastry cloth. Cut into shapes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake on an ungreased cookie sheet for 10 to 12 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
