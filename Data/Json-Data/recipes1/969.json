    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10720/georges-fruit-cookies/",
        "name": "George's Fruit Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F37589.jpg",
            "width": 640,
            "height": 480
        },
        "datePublished": "1998-03-21T04:27:22.000Z",
        "description": "A man named George gave my mother this recipe when he was eating lunch one day while putting in a new furnace over 50 years ago!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 - 5 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "2 tablespoons molasses",
            "1 cup chopped walnuts",
            "1 (8 ounce) can fruit cocktail, drained",
            "1 cup raisins",
            "1 teaspoon vanilla extract",
            "1 teaspoon ground cloves",
            "1 teaspoon ground cinnamon",
            "1 teaspoon baking soda",
            "3 tablespoons hot water",
            "\u00bd teaspoon salt",
            "3\u2009\u00bd cups sifted all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). In a small bowl, dissolve baking soda in warm water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together the shortening and the sugar.  Beat the eggs and mix in.  Stir in molasses, chopped nuts, fruit cocktail, raisins and vanilla.  Mix in spices, baking soda and salt.  Sift the flour and stir in.  Mix until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoon on a cookie sheet and bake for 10 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
