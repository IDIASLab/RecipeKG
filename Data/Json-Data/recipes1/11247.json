    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20659/twice-baked-potatoes/",
        "name": "Twice Baked Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2858255.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-05-11T16:29:22.000Z",
        "description": "This is great as a side dish or as an appetizer. Melted cheese and crumbled bacon are the highlights of the flavorful, creamy filling.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 large baking potatoes",
            "\u00bd pound bacon",
            "4 tablespoons butter",
            "1 large onion, chopped",
            "\u00bd cup chopped fresh mushrooms",
            "1 teaspoon crushed red pepper",
            "1 teaspoon garlic powder",
            "1 teaspoon ground black pepper",
            "1 teaspoon chopped fresh chives",
            "1 teaspoon salt",
            "1 (8 ounce) container sour cream",
            "1 (8 ounce) package shredded Cheddar cheese",
            "1 teaspoon dry bread crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). Use a fork to pierce the potato skins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the potatoes unwrapped for about 1 hour, or until soft,  in the preheated oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain, crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Over medium-low heat melt the butter in a large saucepan. Combine onion, mushrooms, red pepper, garlic powder, pepper, chives and salt. Cook slowly, stirring occasionally until the onions are soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice open the baked potatoes and, keeping the skins intact, scoop the insides into a medium bowl. Transfer the onion mixture to the bowl. Mix in the sour cream. Pour in 1/2 of the cheese and continue mixing until all ingredients are well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a large spoon, fill the potato skins with the mixture. Top with bread crumbs, the remaining cheese and bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return the potatoes to the preheated oven and continue baking for about 15 minutes, until the cheese is melted and the filling is slightly brown.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Twice Baked Potato Recipes"
        ],
