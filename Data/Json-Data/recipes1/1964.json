        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12258/meringue-crust/",
        "name": "Meringue Crust",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5225574.jpg",
