    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285873/easy-cheesecake-cookies/",
        "name": "Easy Cheesecake Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F718997.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-10-16T02:52:18.000Z",
        "description": "Easy, bite-sized cookies with cheesecake filling. They are so good and so fast and easy to make. Fun recipe to make with your kids. My 3-year-old and I always make these together!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "20 cookies",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "1 (4.8 ounce) package graham crackers, finely crushed",
            "1\u2009\u00bd teaspoons baking powder",
            "1 stick unsalted butter, softened",
            "\u00bd cup brown sugar",
            "1 large egg, divided",
            "\u2153 cup cream cheese, softened",
            "\u00bc cup white sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, crushed graham crackers, and baking powder in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter, brown sugar, and egg white together in a separate bowl with an electric mixer until creamy. Add to the dry ingredients and mix until moist.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, sugar, and egg yolk in another bowl; beat until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a small ice cream scoop to scoop cookie batter onto the prepared baking sheet. Press a little pocket into the center of each cookie with a small spoon. Add equal amounts of cream cheese filling to each pocket.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are golden, about 12 minutes. Remove from the oven and let cool before removing from the baking sheet.\n"
            }
        ],
