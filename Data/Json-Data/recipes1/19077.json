        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/231887/cheesy-mashed-potato-stuffed-mushrooms/",
        "name": "Cheesy Mashed Potato Stuffed Mushrooms",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F996604.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2013-03-18T18:42:22.000Z",
        "description": "These Cheesy Mashed Potato Stuffed Mushrooms are so elegant your guests will think you slaved over them. Your secret is safe with us.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "24 stuffed mushrooms",
        "recipeIngredient": [
            "1 (14.1 ounce) package Idahoan\u00ae Buttery Golden Selects Mashed Potatoes",
            "1\u2009\u00bd cups shredded white Cheddar cheese",
            "\u00bd cup chives, chopped",
            "1 cup chopped, cooked bacon or prepared bacon bits",
            "6 tablespoons olive oil",
            "4 tablespoons minced garlic",
            "24 large mushrooms, or more depending on size"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare mashed potatoes according to package directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add shredded cheese (reserve enough to top mushrooms), chives and bacon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove stems from clean mushrooms and stuff caps with potato mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread olive oil and minced garlic on bottom of walled cookie sheet or baking dish. Place mushrooms on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle shredded cheddar over the top of the mushrooms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20-25 minutes.\n"
            }
        ],
