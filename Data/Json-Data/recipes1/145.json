    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9933/pinwheel-cookies-i/",
        "name": "Pinwheel Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1912067.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1997-08-27T18:40:10.000Z",
        "description": "These cookies are mixed, wrapped in wax paper, chilled, sliced and then baked.  My grandmother and my mother both made these and now I make them for my family.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups sifted all-purpose flour",
            "\u00bd teaspoon baking powder",
            "1 pinch salt",
            "\u00bd cup butter",
            "\u00bd cup white sugar",
            "1 egg yolk",
            "3 tablespoons milk",
            "1 (1 ounce) square unsweetened chocolate"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine butter and sugar and egg yolk. Add dry ingredients and milk alternately.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into 2 equal parts.  Melt chocolate over low heat and add to one half of the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a rolling pin and roll each half into a rectangle. Put the chocolate rectangle on top of the vanilla one. Roll up like a jelly roll in wax paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill several hours, best if chilled overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice them and bake at 400 degrees (200 degrees C) for about 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
