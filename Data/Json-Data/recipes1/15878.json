    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283495/japanese-cucumber-salad/",
        "name": "Japanese Cucumber Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9082736.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-10-18T17:33:47.000Z",
        "description": "This Japanese cucumber salad is quick, crisp, cool, and refreshing, with a tangy-sweet flavor. You'll love this amazingly simple veggie side dish. It is even good the next day!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT2H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 medium cucumbers",
            "6 tablespoons rice wine vinegar",
            "6 tablespoons white sugar",
            "2 tablespoons soy sauce",
            "\u00bd teaspoon sesame seeds, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel cucumbers. Halve lengthwise and remove seeds. Slice into 1/4-inch thick half moons.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix vinegar, sugar, and soy sauce together in a bowl until sugar is completely dissolved. Add cucumbers and stir until coated. Cover and refrigerate for at least 2 hours before serving. Sprinkle with sesame seeds and serve cold.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Cucumber Salad Recipes"
