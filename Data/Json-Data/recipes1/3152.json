        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15016/pecan-cups/",
        "name": "Pecan Cups",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5763307.jpg",
            "width": 1884,
            "height": 1536
        },
        "datePublished": "1998-12-12T04:20:30.000Z",
        "description": "Rich and crunchy, these little treats are perfect for parties.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "18 pecan cups",
        "recipeIngredient": [
            "3 ounces cream cheese",
            "\u00bd cup butter",
            "1 cup all-purpose flour",
            "\u00be cup packed brown sugar",
            "1 tablespoon butter",
            "1 egg, beaten",
            "1 teaspoon vanilla extract",
            "\u2154 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Crust: In a medium mixing bowl blend softened cream cheese and 1/2 cup butter or margarine until thoroughly combined. Add flour and mix well. Place in refrigerator to chill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium mixing bowl combine brown sugar and 1 tablespoon butter or margarine. Add egg and vanilla, and blend well. Stir in pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pat crust into the bottoms of paper muffin cups. Pour in filling, distributing evenly between all cups. Bake in preheated oven for 30 minutes.\n"
            }
        ],
