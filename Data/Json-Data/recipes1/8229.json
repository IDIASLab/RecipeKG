        "recipeYield": "1 pound",
        "recipeIngredient": [
            "1 pound fresh mushrooms, stems removed",
            "1 (12 ounce) package chicken-flavor stuffing mix",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "10\u2009\u00be fluid ounces milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. (175 degrees C). Grease one 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare stuffing according to package directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill mushrooms with stuffing and layer in baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dilute can of soup with one can of milk. Pour over the mushrooms, cover and bake for 25 minutes.\n"
            }
        ],
