        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14401/red-white-and-blue-slaw-salad/",
        "name": "Red, White and Blue Slaw Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F665935.jpg",
            "width": 250,
