        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229848/pickled-eggs-from-egg-farmers-of-ontario/",
        "name": "Pickled Eggs from Egg Farmers of Ontario",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2347809.jpg",
            "width": 2592,
            "height": 2592
        },
        "datePublished": "2012-10-26T01:40:22.000Z",
        "description": "These hard-boiled eggs are pickled in a spicy solution containing fresh gingerroot, mustard seeds, and bay leaf.",
        "prepTime": "P0DT0H12M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H22M",
        "recipeYield": "12 eggs",
        "recipeIngredient": [
            "2 cups white vinegar",
            "1 cup water",
            "2 tablespoons sliced fresh gingerroot",
            "1 teaspoon salt",
            "1 teaspoon mustard seed",
            "1 teaspoon black peppercorns",
            "1 bay leaf",
            "12 hard-boiled (cooked) eggs, peeled"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place vinegar, water, gingerroot, salt, mustard seed, peppercorns and bay leaf in a saucepan over medium heat. Bring to a boil. Reduce heat and simmer for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and let solution cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place eggs in a sealable jar. Pour enough vinegar solution over the eggs to cover them. Close jar with lid.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate for at least two days before serving.\n"
            }
        ],
