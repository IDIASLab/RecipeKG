        "mainEntityOfPage": "https://www.allrecipes.com/recipe/230103/buttery-garlic-green-beans/",
        "name": "Buttery Garlic Green Beans",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6890589.jpg",
            "width": 3024,
