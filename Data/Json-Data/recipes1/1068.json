    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10762/macadamia-nut-bars/",
        "name": "Macadamia Nut Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Macadamia Nut Bars"
        },
        "datePublished": "1998-04-05T07:16:11.000Z",
        "description": "Here's another macadamia nut recipe.  Depending on the size of the bar, this recipe makes from 1 to 3 dozen bars.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "2 cups packed light brown sugar",
            "1 cup shortening",
            "1 egg",
            "1 teaspoon baking soda",
            "1 cup sour cream",
            "1 cup chopped macadamia nuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pre-heat oven to 350 degrees F (175 degrees C).  Lightly grease one 9 x 13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make the crust, start by combining the flour and brown sugar in a large bowl.  Then cut in vegetable shortening until mixture looks like coarse crumbs.  Press evenly into the baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "For the filling, beat the egg until thick and light colored.  Next, beat in the baking soda.  Lastly, stir in the sour cream.  Mix until well blended\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the filling over the crust, and distribute chopped macadamia nuts evenly over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 to 50 minutes, or until firm.  Place the pan on a wire rack to cool.  When cool, cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Nut Cookie Recipes",
            "Macadamia Nut"
        ],
