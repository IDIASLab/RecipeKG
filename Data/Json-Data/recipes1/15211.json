    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284156/creamy-chicken-and-mushroom-pie/",
        "name": "Creamy Chicken and Mushroom Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Creamy Chicken and Mushroom Pie"
        },
        "datePublished": "2008-06-23T05:25:02.000Z",
        "description": "A yummy chicken and mushroom pie that everyone will love.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 tablespoon salted butter",
            "1 tablespoon olive oil",
            "2 tablespoons all-purpose flour",
            "2 (6 ounce) skinless, boneless chicken breasts, cut into 1-inch cubes",
            "\u00bd cup chopped mushrooms, or to taste",
            "3\u2009\u00bd ounces cubed fully cooked ham",
            "1 stalk green onion, chopped",
            "1 teaspoon crushed garlic",
            "\u00bc cup milk, or more as needed",
            "salt and ground black pepper to taste",
            "1 (17.5 ounce) package frozen puff pastry, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a frying pan over medium heat. Add oil. Add chicken and cook and stir until browned, 5 to 7 minutes. Add flour and stir until chicken is coated in flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in mushrooms, ham, green onion, and garlic. Slowly add 1/4 cup milk while stirring. Cook until mixture is creamy, adding more milk if needed. Season with salt and pepper. Cook and stir for 2 to 3 more minutes, then transfer to a medium casserole dish or divide into separate small ramekins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wait until the mixture has cooled slightly, 5 to 10 minutes. Put puff pastry over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pastry has browned, 10 to 15 minutes.\n"
            }
        ],
