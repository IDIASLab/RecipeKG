    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12261/pineapple-mango-pie/",
        "name": "Pineapple Mango Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F97153.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-07-04T05:26:14.000Z",
        "description": "You may substitute peach nectar for apple juice for variety.  If on hand, I sometimes place slivered almonds on top before baking.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "4\u2009\u00bd cups mangos, peeled and sliced",
            "1 (8 ounce) can crushed pineapple, drained",
            "1\u2009\u00bc cups white sugar",
            "\u00bc cup packed light brown sugar",
            "2 tablespoons apple juice",
            "5 tablespoons instant tapioca",
            "1 tablespoon butter, softened",
            "1 recipe pastry for a 9 inch double crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the mangos, drained crushed pineapple, white sugar, brown sugar, apple juice, and tapioca. Let stand in a bowl for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour filling into the unbaked pie shell, and dot with butter or margarine. Cover with top crust, and seal edges. Cut slits in top of outer crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) for 1 hour, or until bubbles burst slowly and crust is a nice golden brown color. Let pie cool, and serve at room temperature.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies",
            "Pineapple Pie"
        ],
