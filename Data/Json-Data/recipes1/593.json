    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10401/meltaway-brownies/",
        "name": "Meltaway Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F939041.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-01-18T16:11:25.000Z",
        "description": "Moist and chewy brownies ... one of the best recipes ever!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "4 (1 ounce) squares unsweetened chocolate",
            "\u00be cup packed brown sugar",
            "\u00bc cup water",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "\u2153 cup all-purpose flour",
            "\u00bc teaspoon salt",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt together butter and chocolate in a small saucepan over low heat.  Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add and stir in brown sugar; cool slightly.  Add water, egg yolk and vanilla; beat well.  Stir in flour and salt.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until stiff peaks form, using an electric mixer at high speed.  Fold egg whites into chocolate mixture, then fold in chopped nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into greased 9 inch square pan.  Bake  for 35 minutes or until set around the edges.  Cool a few minutes and remove from pan.  Cool on rack.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Chocolate Brownie Recipes"
        ],
