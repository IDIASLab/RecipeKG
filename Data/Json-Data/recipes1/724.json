        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10166/crispy-rice-cereal-balls/",
        "name": "Crispy Rice Cereal Balls",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27540.jpg",
