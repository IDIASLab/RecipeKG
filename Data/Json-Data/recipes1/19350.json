        "recipeIngredient": [
            "1 (750 milliliter) bottle dry white wine",
            "\u00be cup peach schnapps",
            "2 tablespoons frozen orange juice concentrate, thawed",
            "1 pound peaches, pitted and sliced",
            "\u00be cup frozen raspberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir white wine, peach schnapps, and orange juice concentrate together in a large pitcher; add peach slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until completely chilled, at least 2 hours; add frozen raspberries just before serving.\n"
            }
