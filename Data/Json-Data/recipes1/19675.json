    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233746/mac-and-cheese-bake/",
        "name": "Mac and Cheese Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1050825.jpg",
            "width": 1935,
            "height": 1936
        },
        "datePublished": "2013-08-08T18:40:51.000Z",
        "description": "A yummy comfort food!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "20 ounces elbow macaroni",
            "\u00bd cup butter",
            "\u215c cup all-purpose flour",
            "6 cups milk",
            "1\u2009\u00bd pounds shredded sharp Cheddar cheese",
            "\u00be cup bread crumbs",
            "2 tablespoons melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally, until cooked through but firm to the bite, 8 minutes; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 1/2 cup butter in a large pot over medium heat; stir flour into butter until smooth. Stream milk into the butter mixture while stirring; bring to a boil. Cook and stir until thickened, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce heat to medium-low. Add Cheddar cheese in small batches, stirring each into the milk mixture and melting until adding the next. Add macaroni and stir to coat. Pour macaroni into prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir bread crumbs and melted butter together in a small bowl; sprinkle over the macaroni.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven until the top is golden brown, 35 to 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
