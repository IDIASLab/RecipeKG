    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14748/midwestern-pork-chop-dinner/",
        "name": "Midwestern Pork Chop Dinner",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F965153.jpg",
            "width": 3240,
            "height": 3240
        },
        "datePublished": "1999-10-11T11:24:04.000Z",
        "description": "Pork chops baked over a creamy vegetable bed of potatoes, carrots, kohlrabi and seasonings. A tasty tummy warmer for a cold night. You may substitute onion for kohlrabi if it's not available.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H50M",
        "totalTime": "P0DT2H20M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 teaspoon vegetable oil",
            "6 (3/4 inch) thick pork chops",
            "salt and pepper to taste",
            "4 cups sliced peeled potatoes",
            "2 cups sliced carrots",
            "2 cups sliced kohlrabi",
            "1\u2009\u00bd teaspoons salt",
            "\u00bd teaspoon dried marjoram",
            "\u00bc teaspoon ground black pepper",
            "2 cups milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium high heat. Season pork chops with salt and pepper. Working in batches, sear the pork chops until they're browned on each side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine the potatoes, carrots, kohlrabi, salt, marjoram and pepper. Mix lightly and place mixture into a 9x13-inch baking dish. Pour milk over potato mixture and top with browned pork chops.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover baking dish with aluminum foil and bake in the preheated oven for 1 1/2 hours, or until pork chops and vegetables are very tender and internal temperature of pork has reached 145 degrees F (63 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes",
            "Baked"
        ],
