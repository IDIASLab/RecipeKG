        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17465/honey-syrup/",
        "name": "Honey Syrup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5499509.jpg",
            "width": 3024,
            "height": 4032
