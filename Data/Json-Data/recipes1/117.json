        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9896/gumdrop-gems/",
        "name": "Gumdrop Gems",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F757433.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "1997-08-20T15:08:02.000Z",
        "description": "These are easy and fun cookies for your children to help with.  Keep extra gumdrops on hand for snacking helpers!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "\u00be teaspoon vanilla extract",
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "2 eggs",
            "2 cups sifted all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups quick cooking oats",
            "48 small gumdrops"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream shortening with vanilla.  Add sugars gradually, creaming well.  Add eggs and beat thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, soda, and salt together; add to creamed mixture and mix well.  Stir in oats.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into balls and put on greased cookie sheets.  Make a hollow in each ball and place a gumdrop in each hollow.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 12 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
