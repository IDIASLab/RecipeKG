    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284804/easter-lamb-curry/",
        "name": "Easter Lamb Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easter Lamb Curry"
        },
        "datePublished": "2008-03-20T21:16:52.000Z",
        "description": "This lamb curry recipe is a third generation Easter tradition for my family. We serve it over rice with condiments including mango chutney, mint jelly, chopped cashews, and chopped banana.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H50M",
        "totalTime": "P0DT3H0M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 (4 pound) leg of lamb",
            "3 tablespoons butter",
            "2\u2009\u00bd cups water, divided",
            "2 teaspoons curry powder, or more to taste",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "2 tablespoons all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Debone, trim, and cube the leg of lamb.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat butter in a Dutch oven over medium-high heat. Brown lamb in the hot butter, 5 to 7 minutes. Add 2 cups water, or just enough to cover, along with curry powder, salt, and pepper. Reduce heat and simmer until tender, about 2 1/2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove meat from broth and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir flour and remaining 1/2 cup of water together and add to broth. Heat, while stirring, until thickened. Return meat to broth and serve.\n"
            }
        ],
