        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14918/easy-crab-dip/",
        "name": "Easy Crab Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4503278.jpg",
