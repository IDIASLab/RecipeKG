    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10472/gelatin-spritz-cookies/",
        "name": "Gelatin Spritz Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7468146.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-02-07T19:15:29.000Z",
        "description": "Spritz or drop cookies, colorful and flavorful - very easy.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "3\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups butter",
            "1 (3 ounce) package fruit flavored Jell-O\u00ae mix",
            "1 teaspoon baking powder",
            "1 cup white sugar",
            "1 egg",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter, sugar and gelatin. Add egg and vanilla. Beat well. Gradually add flour and baking powder. Blend until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate dough until firm.  (If using a cookie press, no need to refrigerate dough before use.)  Roll into balls and press with bottom of glass with sugar. (Use an ungreased cookie sheet). Bake at 400 degrees F (205 degrees C) for 7-10 minutes.\n"
            }
        ],
