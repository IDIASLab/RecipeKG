    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7898/applesauce-oatmeal-cake/",
        "name": "Applesauce Oatmeal Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F338338.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-06-14T16:15:19.000Z",
        "description": "This cake is nice and moist.  Great for gift giving. Walnuts and pecans are interchangeable in this recipe.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 tube cake",
        "recipeIngredient": [
            "\u00be cup butter, softened",
            "1\u2009\u00bd cups white sugar",
            "4 eggs",
            "1\u2009\u215b cups applesauce",
            "\u00be cup milk",
            "1\u2009\u00bd cups all-purpose flour",
            "1\u2009\u00bd cups rolled oats",
            "\u00bc teaspoon salt",
            "1 tablespoon baking powder",
            "1\u2009\u00bd teaspoons ground cinnamon",
            "\u00be teaspoon ground nutmeg",
            "\u00be teaspoon ground cloves",
            "1\u2009\u00bd cups raisins",
            "1\u2009\u00bd cups coarsely chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Stir together flour, oats, salt, baking powder, and spices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream the butter or margarine with the sugar.  Beat in the eggs, then the applesauce and milk.  Beat flour mixture into applesauce mixture.  Stir in the raisins and nuts.  Turn the batter into a greased and floured tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake the cake in a preheated oven at 350 degrees F (175 degrees C) for 65 minutes, or until it tests done.  Let cake cool on a wire rack.\n"
            }
        ],
