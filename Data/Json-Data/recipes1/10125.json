    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16383/basic-crepes/",
        "name": "Basic Crepes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8167618.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-03-10T21:18:00.000Z",
        "description": "Here is a simple but delicious crepe batter which can be made in minutes. It's made from ingredients that everyone has on hand.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "8 crepes",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "2 eggs",
            "\u00bd cup milk",
            "\u00bd cup water",
            "\u00bc teaspoon salt",
            "2 tablespoons butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, whisk together the flour and the eggs. Gradually add in the milk and water, stirring to combine. Add the salt and butter; beat until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat a lightly oiled griddle or frying pan over medium high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each crepe. Tilt the pan with a circular motion so that the batter coats the surface evenly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook the crepe for about 2 minutes, until the bottom is light brown. Loosen with a spatula, turn and cook the other side. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Crepes",
            "Sweet"
        ],
