    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7437/apple-dapple-cake/",
        "name": "Apple Dapple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1127416.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1998-05-07T02:04:28.000Z",
        "description": "This recipe delivers a moist and flavorful cake studded with apple chunks, raisins, coconut, and walnuts.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 10 inch tube cake",
        "recipeIngredient": [
            "1 cup vegetable oil",
            "\u00bd cup butter, softened",
            "1\u2009\u00bd cups white sugar",
            "3 eggs",
            "3 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon vanilla extract",
            "2 cups diced apple without peel",
            "1 cup flaked coconut",
            "1 cup raisins",
            "1 cup chopped walnuts",
            "1 cup brown sugar",
            "\u00bd cup butter",
            "\u00bc cup milk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).  Grease and flour one 10 inch tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine oil, butter or margarine, and sugar together.  Beat well with a mixer.  Add eggs, flour, baking soda, salt, and vanilla; mix well.  Stir in apples, coconut, raisins, and walnuts.  Pour batter in prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 90 minutes.  Remove cake from oven.  Let it cool in the pan for a few minutes, and then remove from pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "If desired, make glaze.  In a small saucepan, heat brown sugar, butter or margarine, and milk.  Bring mixture to boil, and stir for 1 minute.  Pour over warm cake.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
