    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283013/apple-danish-bars/",
        "name": "Apple Danish Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Danish Bars"
        },
        "datePublished": "2012-01-06T12:19:18.000Z",
        "description": "An addition to any meal, even served for breakfast. These apple Danish bars would be even tastier with a powdered sugar and milk glaze drizzled on top after the Danish has cooled somewhat.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "18 bars",
        "recipeIngredient": [
            "1 large egg",
            "3 cups all-purpose flour",
            "\u00bd teaspoon salt",
            "1 cup shortening",
            "\u00bd cup milk",
            "6 cups diced apples",
            "1 cup brown sugar",
            "3 tablespoons sour cream",
            "2 tablespoons all-purpose flour",
            "1 tablespoon unsalted butter, softened",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 10x15-inch baking pan. Divide egg; set aside egg white for filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour and salt in a bowl. Cut in shortening with 2 knives or a pastry blender until the mixture resembles coarse crumbs. Mix in milk and egg yolk until incorporated. Divide dough into two equal pieces. Pat one piece into the bottom of the prepared pan; set other piece aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix apples, brown sugar, sour cream, flour, butter, and cinnamon together until well combined. Pour over the dough in the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll or pat remaining dough into a rectangle and place on top of filling. Beat reserved egg white until frothy and brush over dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake until top and bottom are golden brown, 15 to 20 minutes. Remove from the oven and allow to cool completely, about 30 minutes, before cutting into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
