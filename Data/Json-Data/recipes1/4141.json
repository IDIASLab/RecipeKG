    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11975/linguini-with-vegetables/",
        "name": "Linguini with Vegetables",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4532793.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "1999-03-02T14:32:02.000Z",
        "description": "An easy, colorful, dish that takes just 30 minutes. For variety, you can add two cups of chopped, cooked chicken or shrimp.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 to 4 servings",
        "recipeIngredient": [
            "1 pound linguini pasta",
            "3 tablespoons olive oil",
            "1 small zucchini, thinly sliced",
            "1 yellow squash, thinly sliced",
            "2 carrots, sliced thin",
            "1 red bell pepper, thinly sliced",
            "\u00bd onion, sliced",
            "1 tablespoon salt-free herb and spice blend",
            "4 cloves crushed garlic",
            "\u00bc cup white wine",
            "1 tablespoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot with boiling salted water cook linguini pasta until al dente. Drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, in a large skillet heat olive oil and add thinly sliced zucchini, squash, carrots, red bell pepper, onions, salt-free spice blend, and minced garlic. Cook on medium-high for five minutes, stirring frequently. Add white wine and lemon juice and continue cooking until vegetables are crisp-tender and liquid has reduced, about 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss cooked and drained pasta with sauteed vegetables and serve.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
