        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282776/healthy-greek-chicken-panini/",
        "name": "Healthy Greek Chicken Panini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9404296.jpg",
            "width": 0,
            "height": 0
