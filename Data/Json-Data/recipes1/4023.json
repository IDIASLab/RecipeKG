    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17699/eucharistic-bread/",
        "name": "Eucharistic Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3203465.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "1999-02-23T03:03:09.000Z",
        "description": "A quick and easy bread to make for communion services. This bread is just a little sweet and can be shaped whichever way you like.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "2 1 pound loaves",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "\u00bc cup brown sugar",
            "\u00bd tablespoon salt",
            "1 teaspoon baking soda",
            "2 tablespoons honey",
            "\u00bc cup shortening",
            "1 cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease two baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, combine flour, sugar, salt and baking soda. Stir in honey, shortening and water. Mix dough until it is smooth and well developed. Turn dough out onto a lightly floured surface and knead until smooth and supple, about 8 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the dough into two equal pieces and form into loaves. Place loaves on prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 35 to 40 minutes, or until golden.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
