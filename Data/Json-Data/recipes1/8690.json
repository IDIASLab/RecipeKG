        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15368/deviled-eggs-i/",
        "name": "Deviled Eggs I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6562963.jpg",
            "width": 1707,
