        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11282/butterfinger-cookies/",
        "name": "Butterfinger Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1003313.jpg",
            "width": 1533,
            "height": 1536
        },
        "datePublished": "1999-02-23T20:43:36.000Z",
        "description": "A great cookie for the Butterfinger candy lover!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1\u2009\u2153 cups sifted all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "\u2153 cup butter",
            "1 egg",
            "\u00be cup white sugar",
            "2 (2.1 ounce) bars chocolate-covered crispy peanut butter flavored candy (e.g., Butterfinger, \u2122), crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Lightly grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter or margarine with the sugar until light and fluffy. Beat in the egg.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour, baking soda and salt together. Toss the Butterfinger pieces with the flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the flour and Butterfinger mixture into the egg mixture. Mix until combined. Drop by teaspoonfuls onto the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 375 degrees F (190 degrees C) for about 12 minutes. Remove cookies from pan immediately and let cool on racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
