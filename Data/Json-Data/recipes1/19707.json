        "name": "Beef, Tomato, and Okra Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1035658.jpg",
            "width": 1600,
            "height": 1071
