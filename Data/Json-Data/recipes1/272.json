    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10043/persimmon-cookies-i/",
        "name": "Persimmon Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F488012.jpg",
            "width": 3504,
            "height": 2336
        },
        "datePublished": "1997-10-23T21:00:18.000Z",
        "description": "These are moist and  have a wonderful flavor of spice.  You can freeze persimmon pulp to use later if you grow your own and have an excess.  These are great fall cookies!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 ripe persimmons, pureed",
            "1 teaspoon baking soda",
            "2 cups all-purpose flour",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon ground cloves",
            "\u00bd teaspoon ground nutmeg",
            "\u00bd teaspoon salt",
            "1 egg",
            "1 cup white sugar",
            "\u00bd cup butter",
            "1 cup raisins",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve baking soda in persimmon pulp and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift flour, spices and salt together, set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together butter or margarine and sugar until fluffy, beat in egg and persimmon.  Stir in dry ingredients.  Stir in nuts and raisins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto greased cookie sheet.  Bake for 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
