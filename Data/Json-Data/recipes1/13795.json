    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22999/best-pumpkin-pie/",
        "name": "Best Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1067432.jpg",
            "width": 1972,
            "height": 1972
        },
        "datePublished": "2000-10-18T05:55:02.000Z",
        "description": "Why resist? You know you can't make it through Thanksgiving without a slice of moist, delicious pumpkin pie smothered with whipped topping! Make sure the edges of your crust are high, as this recipe creates a generous amount of filling.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1\u2009\u00bd cups pumpkin",
            "\u00be cup white sugar",
            "\u00bd teaspoon salt",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground ginger",
            "\u00bc teaspoon ground cloves",
            "\u00bc teaspoon ground nutmeg",
            "3 eggs, lightly beaten",
            "1\u2009\u00bc cups milk",
            "\u2154 cup evaporated milk",
            "1 recipe unbaked 9 inch pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the pumpkin, sugar, salt, cinnamon, ginger, cloves and nutmeg. Blend in the eggs, milk and evaporated milk. Pour the mixture into the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 50 minutes, or until a knife inserted in the center comes out clean. Cool on a metal rack before serving.\n"
            }
        ],
