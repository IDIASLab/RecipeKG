        "recipeIngredient": [
            "\u2153 cup honey",
            "1 tablespoon vegetable oil",
            "2 eggs",
            "1 tablespoon orange zest",
            "\u00bd teaspoon salt",
            "1\u2009\u00bd cups rolled oats"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
