        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8737/kowloons-chinese-chicken-wings/",
        "name": "Kowloon's Chinese Chicken Wings",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F283834.jpg",
