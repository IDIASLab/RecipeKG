        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21782/spicy-chicken-soup/",
        "name": "Spicy Chicken Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F121907.jpg",
