    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19077/precious-pineapple-cake/",
        "name": "Precious Pineapple Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2232633.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2000-03-22T13:57:30.000Z",
        "description": "This wonderful cake starts with a cake mix to which you add pineapple and pecans. It is very moist, even better the second day.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "3 - 8 inch round pans",
        "recipeIngredient": [
            "\u00bd cup margarine, softened",
            "3 eggs",
            "1 (18.25 ounce) package yellow cake mix",
            "1 (20 ounce) can crushed pineapple with juice",
            "\u00bd cup chopped pecans",
            "\u00bd cup margarine, softened",
            "1 (8 ounce) package cream cheese, softened",
            "4 cups confectioners' sugar",
            "\u00bd cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour 3 - 8  inch round pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl cream 1/2 cup margarine until smooth. Blend in the eggs. Add the cake mix and crushed pineapple with juice and mix until smooth. Stir in the 1/2 cup chopped pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared pans. Bake in the preheated oven for 20 to 30 minutes, or until a toothpick inserted into the centers of the cakes comes out clean. Allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To make the frosting: In a medium bowl cream 1/2 cup softened margarine and the cream cheese until smooth. Gradually blend in the confectioners' sugar and 1/2 cup chopped pecans.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Pineapple Cake Recipes"
        ],
