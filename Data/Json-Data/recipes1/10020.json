    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17168/sonora-chicken-pasta/",
        "name": "Sonora Chicken Pasta",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4553365.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2000-03-04T13:42:22.000Z",
        "description": "Great pasta with cheese sauce topped with beans, tomatoes, and chicken",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "1 (15 ounce) can black beans, rinsed and drained",
            "1 pound processed cheese food, cubed",
            "10 ounces sour cream",
            "1 pound penne pasta",
            "1 dash hot pepper sauce",
            "1 dash Worcestershire sauce",
            "crushed red pepper flakes to taste",
            "salt and pepper to taste",
            "2 tomatoes, chopped",
            "\u00bd bunch green onions, diced",
            "2 grilled chicken breasts, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, heat drained black beans over medium heat; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, melt cheese and sour cream.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain and place in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add hot pepper sauce, Worcestershire sauce, dried red pepper flakes, and salt and pepper to taste to cheese sauce when melted; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon sauce over pasta and top with beans, tomatoes, green onions and chicken; serve.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
