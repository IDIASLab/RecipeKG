    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13433/lenas-minestrone/",
        "name": "Lena's Minestrone",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F125893.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-08T20:27:23.000Z",
        "description": "Minestrone means 'big soup' in Italian, and this soup is sure to satisfy even the biggest of appetites. Served with a crisp loaf of bread, it's a meal in itself! Garnish with grated Romano cheese.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 to 12 servings",
        "recipeIngredient": [
            "6 potatoes, peeled and cubed",
            "6 carrots, chopped",
            "4 stalks celery, chopped",
            "1 onion, chopped",
            "2 cloves garlic, minced",
            "1 (15 ounce) can kidney beans",
            "\u00be cup tomato paste",
            "2\u2009\u00bd quarts water",
            "1\u2009\u00bd cups seashell pasta",
            "1 teaspoon dried oregano",
            "1 teaspoon dried basil",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot ombine the potatoes, carrots, celery, onion, garlic, beans, tomato paste and water. Cook over medium heat until all the vegetables are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer soup to a food processor or blender and puree until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return soup to pot and stir in pasta. If desired add additional water to  thin soup. Stir in the oregano and basil. Season to taste with salt and pepper. Cook over medium heat unitl pasta is al dente. Serve hot topped with freshly grated Romano.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Minestrone"
        ],
