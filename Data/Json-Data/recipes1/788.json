        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10667/fruit-madness-cookies/",
        "name": "Fruit Madness Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Fruit Madness Cookies"
        },
        "datePublished": "1998-02-23T08:27:07.000Z",
        "description": "May be drizzled with vanilla confectioners' sugar icing when cool, if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 -3 dozen",
        "recipeIngredient": [
            "1 cup crushed cornflakes cereal",
            "2 cups shredded coconut",
            "\u00bd teaspoon salt",
            "1 cup sweetened condensed milk",
            "1 cup dates, pitted and chopped",
            "1 cup coarsely chopped prunes",
            "1 cup chopped dried figs",
            "1 cup chopped golden raisins",
            "1 cup dried currants"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Lightly grease two cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large bowl combine cereal flakes, coconut and salt. Stir in condensed milk.  Stir all fruit and blend thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls 1 inch apart on greased cookie sheets.  Bake 12-15 minutes until golden. Cool on wire racks.\n"
            }
        ],
