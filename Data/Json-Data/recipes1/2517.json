    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7984/two-bowl-cake/",
        "name": "Two Bowl Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F755244.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-09-14T02:36:58.000Z",
        "description": "I don't remember when I got this recipe, but it has been a favorite for years.  It's quick, easy and makes a moist cake that keeps well.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9x13 inch cake",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "2 cups white sugar",
            "5 tablespoons unsweetened cocoa powder",
            "2 teaspoons baking soda",
            "1 teaspoon salt",
            "2 cups cold water",
            "\u2154 cup vegetable oil",
            "2 tablespoons distilled white vinegar",
            "1 tablespoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine the flour, sugar, cocoa, soda, and salt--stir to mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine water, oil, vinegar, and vanilla. Dump the dry ingredients into the water mixture and stir with a fork until smooth. DO NOT BEAT. Pour batter into an UNGREASED 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 35 to 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Chocolate Cake Recipes"
        ],
