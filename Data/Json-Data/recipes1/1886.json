        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6733/red-pepper-biscuits/",
        "name": "Red Pepper Biscuits",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F808838.jpg",
