        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13673/creamed-onions-i/",
        "name": "Creamed Onions I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6360071.jpg",
            "width": 3456,
