        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15389/red-potato-soup/",
        "name": "Red Potato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4873420.jpg",
