    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17188/easy-mexican-chicken-bake/",
        "name": "Easy Mexican Chicken Bake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Mexican Chicken Bake"
        },
        "datePublished": "2000-01-12T15:40:04.000Z",
        "description": "I got this recipe from my mother, and it is delicious. Even my picky three year old loves it! It uses tortilla chips to make it crunchy. It's very Mexican style, and quite cheesy!",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "6 boneless, chicken breast halves - cooked, skinned ",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  condensed nacho cheese soup",
            "1 pound processed cheese, cubed",
            "\u00bd teaspoon chili powder",
            "1 (14.5 ounce) package nacho-flavor tortilla chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the chicken, mushroom soup, chicken soup, nacho cheese soup, process cheese food and chili powder to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread a layer of tortilla chips in the bottom of a 9x13 inch baking dish. Spread the mixture over the chips and top with the remaining chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 45 minutes, or until all the cheese is melted and bubbly.\n"
            }
        ],
