        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9694/candied-cherry-macaroons/",
        "name": "Candied Cherry Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Candied Cherry Macaroons"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "A delicious variation on traditional macaroons.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd pound almond paste",
            "1 cup white sugar",
            "3 egg white",
            "\u2153 cup confectioners' sugar",
            "2 tablespoons cake flour",
            "\u215b teaspoon salt",
            "2 tablespoons chopped candied cherries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cover cookie sheets with parchment paper or aluminum foil, shiny side up.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl, mix almond paste and white sugar until well blended. Mix in egg whites one at a time. Combine the flour, confectioners' sugar and salt; blend into the almond paste mixture. Stir in cherries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Force dough through a cookie press or drop by teaspoonfuls onto cookie sheets.  Cover and let stand for 30 minutes. Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 25 minutes.  Remove parchment or foil from cookie sheets and let cool.  Peel off macaroons.\n"
            }
        ],
