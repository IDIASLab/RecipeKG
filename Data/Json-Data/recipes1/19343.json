    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285302/creamy-garlic-hummus/",
        "name": "Creamy Garlic Hummus",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Creamy Garlic Hummus"
        },
        "datePublished": "2013-05-20T22:29:12.000Z",
        "description": "This version of hummus is extra creamy. Great for dipping! Optional: Drizzle a little olive oil over and sprinkle with paprika before serving.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 large head garlic",
            "5 tablespoons olive oil, divided",
            "1 (15 ounce) can chickpeas, rinsed and drained",
            "\u00bc cup warm water",
            "3 tablespoons tahini",
            "2 tablespoons lemon juice",
            "\u00bd teaspoon sea salt",
            "1 pinch cayenne pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut off the very top of the garlic clove (about 1/4 inch) so that the tops of the cloves are slightly exposed. Coat with 1 tablespoon olive oil and wrap securely in foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roast the garlic in the preheated oven for 1 hour. Remove and let sit until cool enough to handle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Squeeze the soft, roasted garlic out of each clove into the bowl of a food processor or blender. Add chickpeas, remaining oil, water, tahini, lemon juice, salt, and cayenne; process until completely smooth.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Hummus Recipes"
        ],
