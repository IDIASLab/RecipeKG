    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10693/chinese-almond-cakes/",
        "name": "Chinese Almond Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chinese Almond Cakes"
        },
        "datePublished": "1998-03-19T07:25:37.000Z",
        "description": "Butter cookies topped with an almond.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups sifted all-purpose flour",
            "\u00be cup white sugar",
            "\u00bc teaspoon salt",
            "1 teaspoon baking powder",
            "\u00be cup butter, softened",
            "1 egg",
            "3 tablespoons water",
            "1 teaspoon almond extract",
            "36 blanched almonds",
            "1 egg yolk"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift flour, sugar, salt and baking powder together in a large bowl.  Using pastry blender or knives, cut in butter until mixture resembles coarse cornmeal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat the egg together with 2 tablespoons of the water and the almond extract.  Add this to the flour mixture and mix with a fork until dough leaves the sides of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a lightly floured surface, knead the dough until it is smooth. Wrap it in foil or cling-wrap and refrigerate for one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into 1 inch balls and place them 3 inch apart on ungreased cookie sheets.  Flatten each cookie to about 1/4 inch thick and press an almond into the center of each.  Beat egg yolk with 1 tablespoon water and brush on cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 20-25 minutes or until golden brown. Remove to wire rack to cool.\n"
            }
        ],
