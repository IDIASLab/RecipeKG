        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19950/easy-grilled-chicken/",
        "name": "Easy Grilled Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4531110.jpg",
            "width": 3264,
