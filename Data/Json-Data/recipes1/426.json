        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10329/scotch-shortbread-i/",
        "name": "Scotch Shortbread I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F49359.jpg",
            "width": 2288,
            "height": 1712
        },
        "datePublished": "1997-12-10T01:10:09.000Z",
        "description": "This recipe uses rice flour.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup unsalted butter",
            "\u00bd cup confectioners' sugar",
            "\u2153 cup white rice flour",
            "2 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients over unsalted, softened butter.  Cut in butter until completely incorporated.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough on floured surface until soft, about 2 minutes.  Roll dough into 2 cylinders and cover with plastic wrap.  Chill one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Knead dough to soften a bit then roll 1/4 inch thick and cut with a cookie cutter.  Bake for 5 minutes on ungreased cookie sheet.\n"
            }
        ],
