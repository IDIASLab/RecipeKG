    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15824/garlic-cajun-ribs/",
        "name": "Garlic Cajun Ribs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F856359.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-01-16T23:52:21.000Z",
        "description": "This is a tasty, treaty baby back ribs recipe, and it's real easy to prepare. Serve the remaining glaze with the ribs.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1\u2009\u00bd gallons water",
            "2 tablespoons minced garlic",
            "4 tablespoons Cajun seasoning",
            "4 tablespoons seasoned salt",
            "4 pounds pork baby back ribs",
            "1 (18 ounce) bottle barbeque sauce",
            "2 tablespoons minced garlic",
            "2 tablespoons Cajun seasoning",
            "2 tablespoons seasoned salt",
            "6 tablespoons Worcestershire sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pot, bring the water to a boil. Season boiling water with 2 tablespoons garlic, 4 tablespoons Cajun-style seasoning, and 4 tablespoons seasoned salt. Boil ribs in seasoned water for 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, mix together barbeque sauce, 2 tablespoons garlic, 2 tablespoons Cajun-style seasoning, 2 tablespoons seasoned salt, and Worcestershire sauce.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place ribs in large baking dishes, and apply a generous amount of the barbeque sauce mixture to the ribs. Set aside for 10 to 15 minutes to marinate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grill the ribs for 7 to 12 minutes per side, until nicely browned.\n"
            }
        ],
