    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21060/toasted-garlic-bread/",
        "name": "Toasted Garlic Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F272203.jpg",
            "width": 3056,
            "height": 2292
        },
        "datePublished": "2000-05-23T17:13:44.000Z",
        "description": "Quick and scrumptious garlic bread that will complement any Italian meal. Melted mozzarella is optional, but delicious!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (1 pound) loaf Italian bread",
            "5 tablespoons butter, softened",
            "2 teaspoons extra virgin olive oil",
            "3 cloves garlic, crushed",
            "1 teaspoon dried oregano",
            "salt and pepper to taste",
            "1 cup shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the bread into slices 1 to 2 inches thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix butter, olive oil, garlic, oregano, salt and pepper. Spread the mixture evenly on the bread slices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a medium baking sheet, arrange the slices evenly and broil 5 minutes, or until slightly brown. Check frequently so they do not burn.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from broiler. Top with cheese and return to broiler 2 to 3 minutes, until cheese is slightly brown and melted. Serve at once.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Garlic Bread Recipes"
        ],
