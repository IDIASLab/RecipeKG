        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10444/peanut-butter-cup-brownies/",
        "name": "Peanut Butter Cup Brownies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F703623.jpg",
