    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/228738/apple-upside-down-cakes/",
        "name": "Apple Upside-Down Cakes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F984065.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2012-08-23T22:31:30.000Z",
        "description": "You'll flip over these little cakes made with slices of Golden Delicious apples, spice cake and Marzetti&reg; Old Fashioned Caramel Dip.",
        "prepTime": "P0DT0H12M",
        "cookTime": "P0DT0H22M",
        "totalTime": "P0DT0H34M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup Marzetti's\u00ae Old Fashioned Caramel Dip",
            "4 Golden Delicious apples, peeled, cored, and sliced into 1/2-inch pieces",
            "\u00bd teaspoon ground cinnamon",
            "2 tablespoons water",
            "1 (18.25 ounce) package spice cake mix",
            "1 (3.4 ounce) package instant vanilla pudding mix",
            "\u00bc cup vegetable oil",
            "\u00bc cup sour cream",
            "4 large eggs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F. Grease two large six-cup muffin tins.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a microwave safe bowl, combine apples, cinnamon and water and microwave for 3-4 minutes. The apples should be slightly tender. Drain apples and toss with 1 cup Marzetti Old Fashioned Caramel Dip.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the apples evenly into each cup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine cake mix, pudding, oil, sour cream and eggs and mix on medium speed until smooth. Spoon the batter evenly over apples.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Baked 15-18 minutes or until a toothpick inserted into the center of each cake comes out clean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool 8 minutes. Invert pan. A few apple pieces may need to be put back into place. Allow cakes to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a microwave safe container, microwave the remaining Marzetti Old Fashioned Caramel Dip for 10-30 seconds, stirring every 10 seconds. Spoon warm caramel over cakes. Serve with whipped cream or ice cream.\n"
            }
        ],
