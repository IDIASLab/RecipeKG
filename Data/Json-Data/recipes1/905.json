    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10673/seven-day-wonders/",
        "name": "Seven-Day Wonders",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Seven-Day Wonders"
        },
        "datePublished": "1998-03-11T07:59:50.000Z",
        "description": "No butter or margarine and no dairy products.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 - 3 dozen",
        "recipeIngredient": [
            "1 cup whole wheat flour",
            "\u00bd cup ground pecans",
            "\u00bd cup vegetable oil",
            "\u00bc cup honey",
            "1 teaspoon lemon zest",
            "2 cups dates, pitted and chopped",
            "\u00bc cup water",
            "\u00bd cup shredded coconut"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour and pecans and set aside.  Beat oil and honey together in a large bowl, then beat in the lemon zest.  Gradually blend in the flour and pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the mixture evenly into an ungreased 9 inch square baking pan and bake for 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While this is baking, mix together chopped dates and water in a saucepan. Cook over medium heat until mixture softens. Transfer this to a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "With a wooden spoon, break up the warm dough and turn it into the bowl containing the date mixture.  Add the coconut.  Mix all thoroughly together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form  into a log 2 inches in diameter and wrap in waxed paper or foil. (For ease in handling, dust hands with flour.) Refrigerate for a week.  To serve, cut into 1/4 inch slices.\n"
            }
        ],
