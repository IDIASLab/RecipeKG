    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14499/game-day-hamburgers/",
        "name": "Game Day Hamburgers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F411036.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-26T05:15:46.000Z",
        "description": "These are seasoned and stuffed hamburgers with a taste of potato and cheese. Serve on buns with condiments of choice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large potato, peeled and shredded",
            "1 cup shredded Swiss cheese",
            "1 cup chopped fresh mushrooms",
            "2 pounds lean ground beef",
            "2 (1 ounce) packages dry onion soup mix"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together potato, cheese, and mushrooms.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix ground beef with onion soup mix. Form into 6 large burger patties. Make a pocket in each burger, stuff with potato mixture, and seal.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to grill, brush grate with oil. Cook burgers over high heat for 5 minutes on each side, or until well done. Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Burger Recipes",
            "Hamburgers"
