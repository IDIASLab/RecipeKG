    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13012/carrot-soup-indienne-i/",
        "name": "Carrot Soup Indienne I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Carrot Soup Indienne I"
        },
        "datePublished": "1999-03-25T17:18:16.000Z",
        "description": "I developed this recipe after spending sometime in Fiji where I fell in love with the East Indian type of cooking. You can hardly tell that this soup has carrots.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "8 cups water",
            "6 carrots, chopped",
            "2 potatoes, peeled and cubed",
            "3 celery, chopped, with leaves",
            "1 large onion, chopped",
            "\u00bc cup soy sauce",
            "\u00bc teaspoon ground cumin",
            "\u00bc teaspoon cayenne pepper",
            "\u00bc teaspoon chili powder",
            "\u00bc teaspoon garlic powder",
            "3 teaspoons curry powder",
            "2\u2009\u00bd cups whole milk",
            "6 tablespoons dry potato flakes"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large cooking pot, bring water to a boil.  Add carrots, potatoes, celery and onion; return to boil.  Reduce heat, cover, and simmer for about 30 minutes, or until carrots are tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove soup from heat, and let cool for about 5 minutes.  Puree in blender or food processor in batches until smooth.  Return pureed soup back to pot.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add soy sauce, cumin, cayenne pepper, chili powder, garlic powder, curry powder; stir well.  Add whole milk, stirring constantly.  Stir in potato flakes.  Bring to a boil.  Reduce heat, and simmer for 5 minutes, stirring occasionally.  Adjust seasonings to taste.  Serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Vegetable Soup Recipes",
            "Carrot Soup Recipes"
        ],
