    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285163/coconut-key-lime-pie/",
        "name": "Coconut-Key Lime Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Coconut-Key Lime Pie"
        },
        "datePublished": "2006-11-21T02:03:23.000Z",
        "description": "I formed this recipe from a combination of many old Key lime pie recipes. It has become a staple at any family gathering, and it is relatively easy to put together.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT3H40M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "3 large eggs, separated, divided",
            "1 tablespoon grated lime zest",
            "1 (14 ounce) can sweetened condensed milk",
            "\u00be cup cream of coconut",
            "\u2154 cup fresh lime juice",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon ground nutmeg",
            "1 (9 inch) prepared graham cracker crust",
            "\u00bc cup white sugar",
            "\u00bc teaspoon cream of tartar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine egg yolks and lime zest in a bowl; beat with an electric mixer until fluffy. Gradually mix in condensed milk and cream of coconut, then beat for 3 minutes longer. Lower speed and add lime juice, vanilla, and nutmeg; mix until just combined. Pour into the crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pie is just set in the center, 14 to 18 minutes. Remove from the oven and cool on a wire rack for 1 hour. Refrigerate for at least 2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites in a glass, metal, or ceramic bowl until frothy. Add cream of tartar and beat until soft peaks form. Gradually beat in sugar, 1 tablespoon at a time, until stiff, glossy peaks form. Spread over the pie in decorative swirls.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil in the preheated oven until meringue is golden brown, 3 to 5 minutes.\n"
            }
        ],
