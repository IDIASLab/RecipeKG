    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283533/holiday-crab-puffs/",
        "name": "Holiday Crab Puffs",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Holiday Crab Puffs"
        },
        "datePublished": "2012-01-16T20:36:40.000Z",
        "description": "Tasty crab puffs... made these for the holidays and everyone loved them!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "40 servings",
        "recipeIngredient": [
            "2 (17.5 ounce) packages frozen puff pastry, thawed",
            "\u00bd cup mayonnaise",
            "\u00bd (8 ounce) package cream cheese, softened",
            "1 tablespoon lemon juice",
            "\u00bc teaspoon seafood seasoning (such as Old Bay\u00ae), or more to taste",
            "salt to taste",
            "1 pound lump crabmeat"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Line 2 baking sheets with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Use a 2-inch cookie cutter to cut puff pastry into forty rounds. Transfer to the prepared baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix mayonnaise, cream cheese, lemon juice, seafood seasoning, and salt together in a medium bowl. Fold in crab meat. Spoon about 2 teaspoons onto each pastry round.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until pastries are golden and crisp, rotating halfway through, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with additional seafood seasoning, if desired, and serve hot.\n"
            }
        ],
