    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/219863/melted-banana-bread/",
        "name": "Melted Banana Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F993199.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2012-01-17T23:07:18.000Z",
        "description": "Two or three ripe bananas and some chocolate chips are a delicious combination in this banana bread. From the kitchen of Heather Hill.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1\u2009\u2153 cups flour",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon baking powder",
            "5\u2009\u2153 tablespoons Melt\u00ae Organic Buttery Spread",
            "\u2154 cup sugar (slightly less--see directions)",
            "2 large eggs, lightly beaten",
            "1 cup mashed <a href=\"https://www.allrecipes.com/article/how-to-ripen-bananas-faster/\" aria-label=\"ripe bananas\">ripe bananas</a>",
            "1 cup milk chocolate or semi-sweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together in medium bowl the flour, salt, baking soda, and baking powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, beat on high until light in color and texture, 2-3 minutes: the Melt&reg; and a little less than 2/3 cups sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in flour mixture until blended and consistency of brown sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gradually beat in the eggs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in just until combined the bananas and chocolate chips (sometimes I use half chocolate chips and half peanut butter chips).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 50-60 minutes and when toothpick comes out clean from center it is done!\n"
            },
            {
                "@type": "HowToStep",
                "text": "Promptly remove baked banana bread from loaf pan and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve warm with an ice cold glass of milk!\n"
            }
        ],
