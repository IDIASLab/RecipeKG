        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17086/healthier-pie-crust/",
        "name": "Healthier Pie Crust",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F73813.jpg",
