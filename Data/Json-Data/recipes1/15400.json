    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284308/lower-carb-eggplant-pizza/",
        "name": "Lower-Carb Eggplant Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9233254.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-07-01T16:50:56.000Z",
        "description": "Sliced eggplant rounds give you a great crust on which to build a lower-carb pizza. You can add any pizza-type toppings that you like, just be sure they are pre-cooked if necessary.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "3 servings",
        "recipeIngredient": [
            "2 teaspoons kosher salt",
            "1 medium eggplant",
            "2 teaspoons Italian seasoning",
            "1 cup pizza sauce",
            "2 cups shredded low-moisture mozzarella cheese",
            "15 slices turkey pepperoni, or to taste",
            "2 tablespoons sliced black olives, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice ends off eggplant and cut into 3/4-inch rounds. Place on a baking sheet and sprinkle liberally on both sides with salt. Let sweat until most of the moisture seeps out, about 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rinse eggplant completely, then pat dry. Return to the baking sheet and sprinkle with Italian seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 minutes. Remove from the oven and top each slice with pizza sauce, cheese, pepperoni, and olives. Return to the oven and bake just until cheese is melted and sauce is heated through, about 5 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Eggplant"
        ],
