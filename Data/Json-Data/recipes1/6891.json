    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12657/topless-blueberry-pie/",
        "name": "Topless Blueberry Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F263792.jpg",
            "width": 2848,
            "height": 4288
        },
        "datePublished": "1999-09-18T01:16:27.000Z",
        "description": "This a no-bake recipe, so on a hot summer day you don't have to turn on the oven, other than to bake your crust. If using frozen blueberries, drain them and use juice instead of water.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "\u00be cup white sugar",
            "3 tablespoons cornstarch",
            "1 pinch salt",
            "1 cup water",
            "4 cups fresh blueberries",
            "1 tablespoon butter",
            "1 (9 inch) pie crust, baked"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan, combine sugar, cornstarch and salt. Stir in water and 1 cup of blueberries. Cook and stir over medium heat, until thick, approximately 8 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add butter and let cool about 5 minutes. Stir in remaining blueberries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into baked pie shell and cool in the refrigerator for 2 to 4 hours.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Fruit Pies",
            "Blueberry Pie Recipes"
        ],
