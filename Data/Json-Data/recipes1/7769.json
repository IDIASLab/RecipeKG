        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13809/soft-pumpkin-cookies/",
        "name": "Soft Pumpkin Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1409735.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "1999-11-01T18:04:32.000Z",
        "description": "This is a recipe my aunt made every year.  I so enjoyed the soft and tasty treat! Enjoy",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 tablespoon butter",
            "1 egg, beaten",
            "1 teaspoon vanilla extract",
            "1 cup pumpkin puree",
            "\u00bd cup chopped walnuts",
            "2 cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together sugar and butter. Add egg, vanilla, pumpkin and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour, baking powder and salt; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by the tablespoon on cookie sheet and bake for 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
