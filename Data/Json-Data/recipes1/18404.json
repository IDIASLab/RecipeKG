    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229410/giant-oreo-cookie-cake/",
        "name": "Giant OREO Cookie Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8184145.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2012-10-03T20:52:24.000Z",
        "description": "Watch everyone's eyes light up when you bring out this fantastic cake, made to look like a giant-size OREO Cookie.  The best part? It tastes as good as it looks!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 package (2-layer size) devil's food cake mix",
            "4 ounces semi-sweet baking chocolate",
            "\u00bc cup butter",
            "1 (8 ounce) package cream cheese, softened",
            "\u00bd cup white sugar",
            "2 cups thawed frozen whipped topping",
            "12 OREO Cookies, coarsely crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 350 degrees F.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare cake batter and bake in 2 (9-inch) round pans as directed on package. Cool cakes in pans 10 minutes. Invert onto wire racks; gently remove pans. Cool cakes completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave chocolate and butter in small microwaveable bowl on high 2 minutes. or until butter is melted. Stir until chocolate is completely melted. Cool 5 minutes. Meanwhile, beat cream cheese and sugar in large bowl with mixer until blended. Gently stir in whipped topping and crushed cookies.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stack cake layers on plate, filling with cream cheese mixture. Spread top with chocolate glaze; let stand 10 minutes or until firm. Keep refrigerated.\n"
            }
        ],
