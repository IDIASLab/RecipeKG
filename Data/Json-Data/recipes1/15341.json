        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9692/coconut-chocolate-cookies/",
        "name": "Coconut Chocolate Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Coconut Chocolate Cookies"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Very chewy, very chocolaty (and very yummy!) cookies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u2154 cups sweetened condensed milk",
            "3 (1 ounce) squares unsweetened chocolate",
            "2 cups shredded coconut",
            "1 pinch salt",
            "1 teaspoon vanilla extract",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine milk and chocolate in double boiler over low heat. Stir frequently until chocolate has melted. Remove from heat and pour into mixing bowl. Stir in coconut, pecans, vanilla and salt.  Mix well. Drop by teaspoonfuls onto cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for about 15 minutes, or until firm.\n"
            }
        ],
