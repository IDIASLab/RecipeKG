        "cookTime": null,
        "totalTime": "P0DT0H7M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup plain yogurt",
            "1 mango - peeled, seeded, and chopped",
            "1 tablespoon white sugar",
            "3 cups cold water",
            "1 pinch salt",
            "4 sprigs fresh mint, garnish"
