    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9526/duo-chocolate-fudge/",
        "name": "Duo-Chocolate Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F27031.jpg",
            "width": 2217,
            "height": 1478
        },
        "datePublished": "1999-12-08T03:07:22.000Z",
        "description": "This delicious fudge recipe is for all those milk chocolate lovers out there! From our family to yours...enjoy the holidays!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 pound milk chocolate",
            "1 pound semi-sweet chocolate chips",
            "2\u2009\u00bd tablespoons butter",
            "2 cups marshmallow creme",
            "2 cups chopped walnuts",
            "1 (12 fluid ounce) can evaporated milk",
            "4 cups white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Lightly butter one 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the milk chocolate, semi-sweet chocolate, butter, marshmallow cream and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan over medium heat, combine the evaporated milk and sugar. Stir constantly and bring to a boil for 4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour hot sugar mixture over chocolate mixture and stir with large spoon until well blended (do this quickly before fudge begins to harden). Once this is well blended, pour fudge into a buttered 9x13 inch baking pan. Spread out and smooth into pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set aside to cool and harden, about 6 hours. Cut into squares when ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Chocolate Dessert Recipes",
            "Milk Chocolate"
        ],
