        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13095/nigerian-peanut-soup/",
        "name": "Nigerian Peanut Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F698803.jpg",
