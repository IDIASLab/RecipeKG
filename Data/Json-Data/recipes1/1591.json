    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6721/chocolate-bread/",
        "name": "Chocolate Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F808346.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1998-05-31T03:54:05.000Z",
        "description": "This chocolate bread doesn't taste sweet.  It tastes savory, and is good with cream cheese!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "1 tablespoon sunflower seed oil",
            "2 teaspoons salt",
            "1\u2009\u00be cups warm water (110 degrees F/45 degrees C)",
            "1 (.25 ounce) package instant yeast",
            "6 cups bread flour",
            "4\u2009\u00bd tablespoons white sugar",
            "\u00be cup unsweetened cocoa powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, salt, yeast, sugar, cocoa, sunflower oil, and water in a large bowl.  Stir everything with a wooden spoon until you have a soft dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the dough on a lightly floured surface.   Knead for five minutes, until smooth and elastic.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide the dough in half, and roll each half into a ball.  Place both on a baking sheet.  Cut crisscross designs in the tops, and cover with a kitchen towel.  Let rise until double in size.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 425 degrees F (220 degrees C) for 35 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
