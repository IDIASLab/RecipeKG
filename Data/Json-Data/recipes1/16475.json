    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282642/slow-cooker-black-eyed-peas-with-bacon/",
        "name": "Slow Cooker Black-Eyed Peas with Bacon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F550144.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-01-02T02:13:59.000Z",
        "description": "Best ever Southern-style black-eyed peas made in the slow cooker with bacon! We make these annually for New Year's!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT4H15M",
        "totalTime": "P0DT4H25M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "8 slices bacon, or more to taste",
            "1 large onion, chopped",
            "4 cloves garlic, minced",
            "6 cups chicken broth",
            "4 cups dry black-eyed peas",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and crumble when cool enough to handle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat bacon drippings in the same pan over medium to medium-high heat. Add onion and garlic and saute until tender, 5 to 7 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine chicken broth, peas, salt, and pepper in the bottom of a slow cooker. Add crumbled bacon, sauteed onion and garlic mixture, and any remaining bacon drippings.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook on Low until peas are tender, 4 to 6 hours.\n"
            }
        ],
