        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10937/pumpkin-cookies-iv/",
        "name": "Pumpkin Cookies IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4676056.jpg",
            "width": 2988,
            "height": 5312
        },
        "datePublished": "1998-06-03T18:01:32.000Z",
        "description": "Gluten-free cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "1 teaspoon baking soda",
            "1 teaspoon gluten-free baking powder",
            "1 teaspoon ground cinnamon",
            "1 cup white rice flour",
            "\u00be cup potato starch",
            "\u00bd cup shortening",
            "\u00be cup white sugar",
            "1 cup canned pumpkin",
            "\u00bd cup ground walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients together.  Cream the shortening and the sugar.  Add the pumpkin.  Add the dry ingredients and nuts.  Beat until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into 1 inch balls and place on a greased cookie sheet.  Press flat with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 9 to 12 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Fruits and Vegetables",
            "Vegetables",
            "Squash"
        ],
