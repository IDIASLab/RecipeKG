        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17147/orzo-and-rice/",
        "name": "Orzo and Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F895987.jpg",
            "width": 2288,
            "height": 1520
