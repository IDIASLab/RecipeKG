        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6944/pumpkin-loaf/",
        "name": "Pumpkin Loaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6012278.jpg",
            "width": 2268,
            "height": 2268
        },
        "datePublished": "1998-08-31T20:07:19.000Z",
        "description": "The cream cheese contributes to the excellence of this loaf.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H5M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 -9x5x3 inch loaf",
        "recipeIngredient": [
            "4 ounces cream cheese, softened",
            "\u00bc cup margarine",
            "1\u2009\u00bc cups white sugar",
            "2 eggs",
            "1 cup canned pumpkin",
            "1\u2009\u00be cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bc teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc teaspoon ground cloves",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease one 9x5x3 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put softened cheese, butter, and sugar in mixing bowl. Cream together well. Beat in eggs one at a time until blended. Mix in pumpkin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In another bowl combine flour, soda, baking powder, salt, cinnamon, cloves, and walnuts. Stir until thoroughly mixed. Pour all at once over batter. Stir just enough to moisten. Turn into greased 9x5x3 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 60-70 minutes until inserted toothpick comes out clean. Cool 10 minutes in pan. Remove to rack to finish cooling.\n"
            }
        ],
