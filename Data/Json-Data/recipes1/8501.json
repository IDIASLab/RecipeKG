        "name": "Three Bean and Artichoke Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1131335.jpg",
            "width": 1280,
            "height": 960
