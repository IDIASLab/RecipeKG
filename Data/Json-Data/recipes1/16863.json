    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282746/lobster-gumbo/",
        "name": "Lobster Gumbo",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Lobster Gumbo"
        },
        "datePublished": "2011-09-19T06:11:42.000Z",
        "description": "Wonderful lobster seafood gumbo. Serve in flat-bottomed bowls over hot cooked rice.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil, divided",
            "1\u2009\u00bd tablespoons all-purpose flour",
            "1 pound shrimp, peeled and deveined",
            "2 medium onions, chopped",
            "1 cup chopped celery",
            "1 medium bell pepper, chopped",
            "1\u2009\u00bd pounds sliced fresh okra",
            "4 cups water",
            "1 (14.5 ounce) can diced tomatoes, undrained",
            "2 large bay leaves",
            "salt and ground black pepper to taste",
            "1 (6 ounce) can tomato paste",
            "\u00bd pound lobster meat",
            "1 teaspoon gumbo file powder, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 1 1/2 tablespoons oil in a saucepan over medium heat until a pinch of flour sprinkled into the oil will just begin to bubble. Whisk in flour to form a thick paste the consistency of cake frosting. Continue cooking, whisking constantly, until the flour turns the color of melted chocolate, about 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add shrimp and cook for 3 minutes. Remove from the heat and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, heat remaining oil in a large saucepan over medium heat. Add onions, celery, and bell pepper; cook until soft, about 5 minutes. Add okra; cook until it ceases to rope, about 30 minutes. Add water, undrained tomatoes, bay leaves, salt, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in shrimp mixture, cover, and bring to a simmer. Simmer for 15 minutes. Add tomato paste and simmer for 15 minutes more. Stir in lobster meat and gumbo file. Remove bay leaves before serving.\n"
            }
        ],
