    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16046/molasses-snowballs/",
        "name": "Molasses Snowballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1055418.jpg",
            "width": 4608,
            "height": 3440
        },
        "datePublished": "2000-01-13T22:36:24.000Z",
        "description": "A wonderful and delicious change from the traditional snowball cookie, and, with only six ingredients. Easy!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "\u00bc cup molasses",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "2 cups chopped walnuts",
            "\u00bd cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream the shortening, molasses and vanilla until smooth. Stir in the flour and walnuts. Roll dough into 1 inch balls and place them about 2 inches apart on an unprepared cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 minutes in the preheated oven. Roll cookies in confectioners' sugar when cool.\n"
            }
        ],
