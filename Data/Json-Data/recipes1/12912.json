    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20854/fig-preserves/",
        "name": "Fig Preserves",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1281274.jpg",
            "width": 1280,
            "height": 1116
        },
        "datePublished": "2000-08-24T17:03:50.000Z",
        "description": "Make your own fig preserves from scratch with this easy canning recipe.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H20M",
        "totalTime": "P0DT2H30M",
        "recipeYield": "64 servings",
        "recipeIngredient": [
            "16 cups fresh figs",
            "1 tablespoon baking soda",
            "12 cups boiling water",
            "8 cups white sugar",
            "4 cups water",
            "1 lemon, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl place figs and sprinkle with baking soda.  Pour the boiling water over the figs and soak for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain figs and rinse thoroughly with cold water.  In a large Dutch oven combine the sugar and the 4 cups of water; bring to a boil and cook for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the figs and lemon slices to the syrup in the Dutch oven and cook for 1 hour, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon figs into hot, sterilized jars and spoon syrup over figs, leaving 1/2 inch of head space.  Top jars with lids and screw bands on tightly.  Process for 10 minutes in a hot water bath.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Jams and Jellies Recipes"
        ],
