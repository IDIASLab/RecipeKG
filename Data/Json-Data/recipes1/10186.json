    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19113/zweiback-cheesecake/",
        "name": "Zweiback Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Zweiback Cheesecake"
        },
        "datePublished": "2000-03-13T16:35:23.000Z",
        "description": "Rich and luscious cheesecake. The crust is made with zwieback crumbs and the filling is made with dry curd cottage cheese. You may substitute ricotta if you prefer.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "1 - 9 inch springform pan",
        "recipeIngredient": [
            "1 (4 ounce) package zwieback toast",
            "\u00bd cup white sugar",
            "2 teaspoons ground cinnamon",
            "\u00bd cup margarine, melted",
            "3\u2009\u00bc cups dry small curd cottage cheese",
            "1 cup white sugar",
            "4 eggs",
            "4 tablespoons all-purpose flour",
            "1 pinch salt",
            "1 cup heavy cream",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Crush zwieback into crumbs.  In a medium bowl, combine zwieback crumbs, 1/2 cup sugar, cinnamon and melted margarine. Mix well and press into the bottom and sides of a 9 inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream cottage cheese and 1 1/4 cup sugar until smooth. Beat in eggs one at a time. Beat in the flour and salt. Mix in cream and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into crust and bake in preheated oven for 1 hour. Cool completely before removing from pan.\n"
            }
        ],
