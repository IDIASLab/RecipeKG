        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15794/graham-cracker-pudding-pie/",
        "name": "Graham Cracker Pudding Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F925985.jpg",
