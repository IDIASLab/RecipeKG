        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17770/amaretto/",
        "name": "Amaretto",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1710320.jpg",
            "width": 2448,
