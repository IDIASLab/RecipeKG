    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284029/indian-coconut-chutney/",
        "name": "Indian Coconut Chutney",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F702618.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-09-14T03:39:06.000Z",
        "description": "This delicious chutney is made mostly in the food processor and is a great side for Indian cuisine, or even chips. Freezes great.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 cup coconut milk",
            "1 cup frozen shredded coconut, thawed",
            "\u00bd cup curry leaves",
            "1 medium onion, quartered",
            "4 peppers serrano peppers, stems removed",
            "5 cloves garlic",
            "1 (1 inch) piece fresh ginger, peeled",
            "2 tablespoons lime juice",
            "2 teaspoons unsalted natural peanut butter",
            "1 teaspoon salt",
            "1 teaspoon ground cumin",
            "1 tablespoon peanut oil",
            "1 teaspoon mustard seeds",
            "4 curry leaves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine coconut milk, shredded coconut, 1/2 cup curry leaves, onion, serrano peppers, garlic, ginger, lime juice, peanut butter, salt, and cumin in a blender; puree until smooth, 2 minutes or more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a saucepan over medium heat. Add mustard seeds and cook until they pop, 10 to 20 seconds. Add 4 curry leaves and cook for 30 to 60 seconds. Add pureed mixture to the pan; it will sputter. Cook, stirring occasionally, for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove and discard curry leaves. Allow chutney to cool before serving.\n"
            }
        ],
