    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19282/applesauce-brownies-ii/",
        "name": "Applesauce Brownies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1915478.jpg",
            "width": 2560,
            "height": 1440
        },
        "datePublished": "2000-06-18T15:35:32.000Z",
        "description": "A plain cake-like brownie, moist with great chocolate taste.  Easy to make.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9x9 inch pan",
        "recipeIngredient": [
            "\u00bd cup shortening",
            "2 (1 ounce) squares unsweetened chocolate",
            "1 cup white sugar",
            "2 eggs, beaten",
            "\u00bd cup applesauce",
            "1 teaspoon vanilla extract",
            "1 cup sifted all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease and flour a 9x9 inch pan. In a microwave oven or over  a double boiler, melt shortening and chocolate together, stirring frequently until smooth. Set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, blend together the sugar, eggs, applesauce and vanilla until smooth. Stir in the chocolate mixture. Combine the flour, baking powder, baking soda and salt, gradually stir into the chocolate mixture. Spread evenly into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 35 to 40 minutes in the preheated oven, until a toothpick inserted in the center comes out clean. Cool in pan before cutting into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Apple Dessert Recipes"
        ],
