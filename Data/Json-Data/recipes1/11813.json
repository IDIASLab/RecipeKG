        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20039/teriyaki-marinade-ii/",
        "name": "Teriyaki Marinade II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6980987.jpg",
