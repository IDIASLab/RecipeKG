    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282493/vegan-broccoli-cheese-soup/",
        "name": "Vegan Broccoli-Cheese Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegan Broccoli-Cheese Soup"
        },
        "datePublished": "2013-04-10T15:30:41.000Z",
        "description": "Learning to eat vegan was both challenging and exciting. I miss cheese the most, and this recipe has all the comfort of the original broccoli-cheese soup I love. Canned pumpkin creates a creamy consistency without adding milk. And adds to the cheesy look!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds broccoli florets",
            "1 cup vegetable stock",
            "\u00bd cup canned pumpkin",
            "\u00bc cup vegan mayonnaise",
            "1 large roasted red pepper, drained and cut into strips",
            "4 cups shredded vegan Cheddar-flavored cheese (such as Daiya\u00ae)",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place a steamer insert into a saucepan and fill with water to just below the bottom of the steamer. Bring water to a boil. Add broccoli, cover, and steam until tender, 2 to 6 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "At the same time, bring vegetable stock to a simmer in a separate saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer steamed broccoli to a blender. Add pumpkin, vegan mayonnaise, and red pepper; blend.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 3 cups cheese to the blender; blend until incorporated. Slowly blend in hot stock until soup is thoroughly blended, you may not need all of it. Season with salt and pepper and garnish with remaining cheese.\n"
            }
        ],
