    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8068/heritage-jam-cake/",
        "name": "Heritage Jam Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F690421.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "1998-11-27T20:22:21.000Z",
        "description": "A butter cake with fruit and nuts. The secret ingredient is grape jam.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "1 - 2 layer cake",
        "recipeIngredient": [
            "1 cup butter, softened",
            "2 cups white sugar",
            "5 eggs",
            "1 teaspoon baking soda",
            "1 cup buttermilk",
            "3 cups all-purpose flour",
            "\u00bc teaspoon salt",
            "\u00bd teaspoon ground cinnamon",
            "1 teaspoon ground cloves",
            "1 teaspoon ground allspice",
            "1 cup dates, pitted and chopped",
            "1 cup grape jam",
            "1 cup chopped pecans",
            "1 recipe Kentucky Fruit Filling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Grease two 9 inch, round cake pans, and line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 2 1/2 cups flour, salt, and spices.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve soda in buttermilk: stir well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter or margarine.  Gradually add sugar, beating well.  Add eggs, one at a time, beating well after each addition.  Mix flour mixture into creamed mixture alternately with buttermilk mixture, beginning and ending with flour mixture.  Dredge dates and pecans in remaining 1/2 cup flour, and fold into batter.  Carefully fold grape jam into batter.  Pour batter into prepared pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 40 to 45 minutes, or until a wooden toothpick inserted in center comes out clean.  Cool in pans for 10 minutes.  Remove layers from pans, and cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread hot Kentucky Fruit Filling between the layers and on top of the cake.\n"
            }
        ],
