    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282733/robyns-parmesan-cauliflower-dip/",
        "name": "Robyn's Parmesan-Cauliflower Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Robyn's Parmesan-Cauliflower Dip"
        },
        "datePublished": "2006-11-05T18:24:41.000Z",
        "description": "This is a hot, delicious dip made with buttery, cheesy cauliflower. It warms the tummy and the heart--my mom has made this dip for years! Serve hot with Triscuits&reg; or your favorite crackers.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u00bd cup salted butter, divided",
            "\u00bc cup warm water",
            "1 large head cauliflower, cored and cut into small florets",
            "1 cup shredded Parmesan cheese",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 4 tablespoons butter in a medium saucepan. Add 1/4 cup water and bring to a boil over medium heat. Add cauliflower and cook until tender, 5 to 7 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt remaining butter in a small saucepan over low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer cauliflower to a glass baking dish. Stir in 3/4 cup Parmesan cheese, melted butter, salt, and pepper. Smooth out the mixture so it's even in the dish. Sprinkle remaining Parmesan over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until smooth and creamy with a light brown top, about 30 minutes. Transfer dip to a serving bowl or warmed fondue pot and serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Hot Cheese Dip Recipes"
        ],
