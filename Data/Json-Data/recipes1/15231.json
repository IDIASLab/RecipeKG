        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9790/orange-walnut-butter-cookies/",
        "name": "Orange-Walnut Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Orange-Walnut Butter Cookies"
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Elegant orange flavored cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "\u00be cup white sugar",
            "1 tablespoon frozen orange juice concentrate, thawed",
            "1\u2009\u00bd teaspoons orange zest",
            "\u00bd teaspoon vanilla extract",
            "1\u2009\u00bd cups self-rising flour",
            "24 walnut halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F.  Grease cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat butter until smooth.  Add sugar and mix until fluffy.  Mix in orange concentrate, orange peel and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add flour and mix until dough comes together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form 1 rounded tablespoon of dough into a ball.    Place on prepared cookie sheet and flatten to a 2-inch round.    Repeat with the remaining dough.  Press 1 walnut half firmly onto each round.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake about 12 minutes or until cookies just begin to brown.  Remove to a rack and cool.\n"
            }
        ],
