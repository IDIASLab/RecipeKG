    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10963/apricot-bars-i/",
        "name": "Apricot Bars I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3803589.jpg",
            "width": 1080,
            "height": 1920
        },
        "datePublished": "1998-06-15T18:17:32.000Z",
        "description": "A rich little bar cookie that's hard to stop with just one.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 1/2 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup butter",
            "2 egg yolks",
            "1 teaspoon vanilla extract",
            "\u00be cup packed brown sugar",
            "1 cup apricot preserves",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream flour, butter and brown sugar.  Add eggs and vanilla; mix well.  Press into a 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 minutes.  Take out and spread preserves evenly then sprinkle with nuts.  Score into bars, but leave in pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to oven at 250 degrees F (120 degrees C) for up to 15 minutes.  Cool on rack.\n"
            }
        ],
