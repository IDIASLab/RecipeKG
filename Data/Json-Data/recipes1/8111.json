        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16270/chocolate-pecan-pie-ii/",
        "name": "Chocolate Pecan Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1690959.jpg",
