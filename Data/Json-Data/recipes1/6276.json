    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15069/fast-apple-rhubarb-pie/",
        "name": "Fast Apple Rhubarb Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F381347.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-10T18:47:37.000Z",
        "description": "Simple, but sooo delicious! Serve warm with a scoop of vanilla ice cream.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell",
            "6 apple - peeled, cored, and chopped",
            "3 rhubarb, diced",
            "1 cup white sugar",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 440 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apples and rhubarb in a large bowl. Mix together sugar and cinnamon in a small bowl, then sprinkle over fruit. Toss until fruit is thoroughly coated. Spoon mixture into pastry shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 minutes.\n"
            }
        ],
