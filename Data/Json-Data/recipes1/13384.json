    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21582/easy-seafood-fettucini/",
        "name": "Easy Seafood Fettucini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F399360.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-09-28T21:02:29.000Z",
        "description": "A good, creamy sauce over fettuccini noodles.  You can use shrimp, tuna, mock crab, or whatever.  Tastes great, and isn't made with cream!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 tablespoons butter",
            "1 large onion, chopped",
            "4 cloves garlic, minced",
            "3 tablespoons all-purpose flour",
            "2 cups milk",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "4 ounces fresh shrimp, peeled and deveined",
            "4 ounces crabmeat",
            "8 bay scallops, raw",
            "16 ounces dry fettuccini noodles"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saute pan, melt butter and saute onions and garlic until onions are almost translucent.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in flour, coating all of the onions well.  Gradually pour in milk, stirring constantly. Stir in mushroom soup and seafood.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook for 10 minutes or until shrimp are pink and scallops are opaque. Serve over pasta.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Crab"
        ],
