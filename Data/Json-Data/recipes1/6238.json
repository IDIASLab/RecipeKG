    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7191/jo-jos-favorite-banana-muffins/",
        "name": "Jo Jo's Favorite Banana Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2576744.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "1999-08-09T03:06:57.000Z",
        "description": "This is my favorite recipe. I have had this recipe since I was in high school. All my friends love it. Hope you enjoy it!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "18 muffins",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup white sugar",
            "1 cup mashed ripe banana",
            "\u00bd cup margarine",
            "2 eggs",
            "6 tablespoons milk",
            "2\u2009\u00bd teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl mix flour, baking powder, baking soda, salt, and cinnamon and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl cream margarine and sugar, add eggs and mix until smooth. Stir in milk and mashed banana and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fold in flour mixture into the liquid ingredients until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill greased muffin tins two-thirds of the way full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degree F (175 degrees C) oven for 25 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
