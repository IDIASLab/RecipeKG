    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20751/dads-souper-brunch/",
        "name": "Dad's Souper Brunch",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F15634.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-08-17T12:48:28.000Z",
        "description": "My dad used to make this for us after church when we were little.  It's a simple version of eggs Benedict.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  milk",
            "8 strips Canadian-style bacon",
            "8 eggs",
            "4 English muffins, split and toasted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Split English muffins in half and toast; set aside. In a small saucepan, heat soup and milk over low heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place Canadian bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Set aside and keep warm. Add eggs to skillet and cook as desired.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place English muffin halves on serving plates. Top with Canadian bacon and then eggs. Spoon warm soup over eggs.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Meat and Seafood",
            "Bacon"
        ],
