    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13148/german-lentil-soup/",
        "name": "German Lentil Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F535085.jpg",
            "width": 4752,
            "height": 3168
        },
        "datePublished": "1999-03-26T13:31:11.000Z",
        "description": "This is a really good lentil soup recipe I created based on the favorite German Lentil Soup which has been served for years at Karl Ratzsch's in Milwaukee.  Serves a small crowd, tastes great the next day, and freezes well. Garnish with croutons.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT8H0M",
        "totalTime": "P0DT8H10M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 cups dried brown lentils, rinsed and drained",
            "3 cups chicken stock",
            "1 bay leaf",
            "1 cup chopped carrots",
            "1 cup chopped celery",
            "1 cup chopped onion",
            "1 cup cooked, cubed ham",
            "1 teaspoon Worcestershire sauce",
            "\u00bd teaspoon garlic powder",
            "\u00bc teaspoon freshly grated nutmeg",
            "5 drops hot pepper sauce",
            "\u00bc teaspoon caraway seed",
            "\u00bd teaspoon celery salt",
            "1 tablespoon chopped fresh parsley",
            "\u00bd teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place lentils in a 5 to 6 quart slow cooker. Add chicken stock, bay leaf, carrots, celery, onion, and ham. Season with Worcestershire sauce, garlic powder, nutmeg, hot pepper sauce, caraway seed, celery salt, parsley, and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover, and cook on Low for 8 to 10 hours. Remove bay leaf before serving.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas",
            "Lentil Soup Recipes"
        ],
