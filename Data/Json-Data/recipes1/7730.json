        "name": "Cherry Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cherry Cookies"
        },
        "datePublished": "1999-10-29T01:41:59.000Z",
        "description": "Chewy cherry cookies made with whole grain flours.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "20 cookies",
        "recipeIngredient": [
            "\u00bd cup packed brown sugar",
            "1 cup white sugar",
            "\u00be cup soy margarine",
            "2 egg whites",
            "1 teaspoon imitation vanilla extract",
            "1 cup white spelt flour",
            "1 cup whole wheat flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "1 cup dried cherries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C)\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the brown sugar, white sugar and soy margarine. Add egg white, and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In another bowl, stir together the spelt flour, wheat flour, baking soda and salt. Add this to the sugar mixture and mix well. Finally, stir in the dried cherries.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop cookie dough from a teaspoon onto a lightly greased cookie sheet.  Bake in preheated oven for 13 minutes.\n"
            }
        ],
