    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12042/salsa-pasta-salad/",
        "name": "Salsa Pasta Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F866065.jpg",
            "width": 1859,
            "height": 1378
        },
        "datePublished": "1999-03-25T22:56:04.000Z",
        "description": "A great summer salad served cold that tastes better the longer it sits.",
        "prepTime": "P0DT0H20M",
        "cookTime": null,
        "totalTime": "P0DT0H20M",
        "recipeYield": "11 servings",
        "recipeIngredient": [
            "2 cups dry rainbow radiatore pasta",
            "1 onion, chopped",
            "1 red bell pepper, chopped",
            "1 (6 ounce) can sliced black olives",
            "3 tomatoes, diced",
            "1 (4 ounce) can diced green chiles",
            "\u2153 cup distilled white vinegar",
            "\u00bc cup chopped fresh cilantro",
            "1\u2009\u00bd teaspoons garlic salt",
            "\u2153 cup lemon juice",
            "\u00bd cup vegetable oil",
            "1 tablespoon white sugar",
            "1 tablespoon chili powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot with boiling salted water cook radiatore pasta until al dente. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine chopped onion, bell pepper, olives, tomatoes, and green chilies in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, whisk together vinegar, cilantro, garlic salt, lemon juice, vegetable oil, sugar and chili powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix cooked pasta with vegetables. Toss with cilantro dressing and serve.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Pasta Salad",
            "Vegetarian Pasta Salad Recipes"
        ],
