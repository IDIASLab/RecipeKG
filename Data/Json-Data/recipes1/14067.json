    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23435/french-onion-soup-vi/",
        "name": "French Onion Soup VI",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5176108.jpg",
            "width": 4128,
            "height": 2322
        },
        "datePublished": "2000-10-30T21:39:56.000Z",
        "description": "A wonderful hearty soup topped with a crunchy seasoned crouton with melted mozzarella cheese.  Great for a chilly fall or winter day.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT2H10M",
        "totalTime": "P0DT2H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 pounds racked beef ribs",
            "5 red onions, sliced",
            "2 (14 ounce) cans beef broth",
            "2 teaspoons salt",
            "1 ounce soy based liquid seasoning",
            "salt and pepper to taste",
            "6 slices French bread",
            "2 tablespoons butter, melted",
            "2 teaspoons garlic powder",
            "2 teaspoons paprika",
            "\u00bd cup shredded mozzarella cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place beef ribs in a large stock pot over medium heat.  Brown on all sides.  Cover with water and bring to a boil, then cover, reduce heat and simmer 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove meat and bones to a cutting board to cool.  Introduce onions, beef broth and salt to the rib water.  Bring to a boil, then reduce heat and simmer 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the bones are cool enough to handle, remove the meat with a knife.  Finely chop the meat and add to the soup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After an hour of simmering, season the soup with the liquid seasoning and salt and pepper to taste.  Simmer 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven broiler.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut French bread in slices that will fit in your serving bowls.  Place slices on a baking sheet and brush with melted butter.  Sprinkle with garlic powder and paprika.  Place under preheated broiler until browned, 2 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Ladle soup into 6 ovenproof serving bowls.  Lay one crouton over each serving.  Top with mozzarella.  Place bowls on a sturdy baking sheet and place under broiler to melt cheese, 1 minutes.  Serve at once.\n"
            }
        ],
