        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15702/pecan-pie-iii/",
        "name": "Pecan Pie III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7369102.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-02-17T21:01:37.000Z",
        "description": "My mother's pie. I'm 78, and have tried 'em all...this is still the best!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 (9 inch) pie shell",
            "\u00be cup white sugar",
            "\u00be cup light corn syrup",
            "3 eggs, beaten",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prick pastry shell in several places with a fork and bake in preheated oven for 5 minutes. Remove and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine sugar, corn syrup, eggs, vanilla, salt, and pecans. Mix well, then pour into pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 45 minutes. Check after 30 minutes; if crust is getting too brown, cover edges with aluminum foil. When done, pie will be a little loose in center, but will set as it cools; do not overbake.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pecan Pie Recipes"
        ],
