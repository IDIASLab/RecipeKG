    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284267/fatayer-lebanese-meat-pies/",
        "name": "Fatayer (Lebanese Meat Pies)",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9218186.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-03-03T21:31:27.000Z",
        "description": "This is an old recipe my family brought with them from Lebanon at the turn of the last century.",
        "prepTime": "P0DT0H35M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "16 meat pies",
        "recipeIngredient": [
            "1 tablespoon vegetable oil",
            "2 pounds round steak, cut into small pieces",
            "2 large onion, chopped",
            "\u00bd cup snipped fresh parsley",
            "salt and ground black pepper to taste",
            "2 (1 pound) loaves frozen bread dough, thawed",
            "1 tablespoon butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line a baking sheet with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet over medium heat. Add steak pieces, onions, parsley, salt, and pepper and cook until steak is browned on all sides, about 5 minutes. Remove from the heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pat bread dough on a lightly floured surface into 4- to 5-inch rounds. Place about 2 tablespoons meat mixture on each dough round. Fold dough over the mixture to make a triangle. Pinch the edges of the dough together but make sure to leave the very center of the meat pie unsealed so steam can escape while cooking. Set on the prepared baking sheet. Repeat with remaining dough and meat mixture and cover with a clean kitchen towel. Let rise for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake fatayer in the preheated oven until lightly browned, about 25 minutes.\n"
            }
        ],
