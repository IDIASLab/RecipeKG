    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19619/chicken-on-a-stick/",
        "name": "Chicken on a Stick",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5225416.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "2000-05-22T13:12:31.000Z",
        "description": "Easy recipe for marinated chicken breast cooked on skewers on the grill. These are so simple, and your family will love them!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 skinless, boneless chicken breast halves",
            "1 teaspoon meat tenderizer",
            "\u00bd cup Italian-style salad dressing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rinse chicken breasts and pat dry. Sprinkle with the meat tenderizer and place in a sealable plastic bag. Pour the dressing in the bag and turn the chicken to coat thoroughly. Seal and marinate in the refrigerator for 30 minutes to 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for medium heat and lightly oil grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the chicken onto skewers and grill over medium heat for 5 to 10 minutes per side. Chicken is done when its juices run clear.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
