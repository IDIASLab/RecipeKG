        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1\u2009\u00bd cups white sugar",
            "1 cup quick cooking oats",
            "\u00bd cup chocolate flavored cereal puffs",
            "1 cup butter",
            "1\u2009\u00bd cups all-purpose flour",
            "3 eggs",
            "1 teaspoon vanilla extract",
            "2 teaspoons dry milk powder",
            "\u00bc cup warm water",
            "\u00bd teaspoon baking powder"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the milk in the water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the ingredients with the butter in the following order:  sugar, oatmeal,  vanilla extract, eggs, flour, baking powder, cereal and milk.  Pat into a greased 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (175 degrees C)  for 15 minutes or until golden brown.  Slice into desired sizes.\n"
            }
