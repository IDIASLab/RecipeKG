        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21485/hot-buttered-rum/",
        "name": "Hot Buttered Rum",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F26071.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-28T02:05:44.000Z",
        "description": "Always requested when friends stop in over the holidays. Oh so yummy!",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P1DT0H40M",
        "recipeYield": "100 servings",
        "recipeIngredient": [
            "2 cups butter",
            "2 cups white sugar",
            "2 cups brown sugar",
            "1 teaspoon vanilla extract",
            "1 quart vanilla ice cream, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a microwave-safe bowl, combine butter, sugar, brown sugar and vanilla. Microwave until smooth. Mix in the ice cream. Place in a sealable container and freeze.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to use, spoon 1 1/2 tablespoons of the base into a mug. Pour in 1 shot of dark rum and 1 cup of hot water for each serving.\n"
            }
        ],
