    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10738/snickerdoodles-iv/",
        "name": "Snickerdoodles IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5848391.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "1998-03-28T22:37:37.000Z",
        "description": "Cinnamon cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1\u2009\u00bd cups white sugar",
            "2 eggs",
            "2\u2009\u00bd cups all-purpose flour",
            "2 teaspoons cream of tartar",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc cup white sugar",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (205 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix the shortening , sugar, and eggs until creamy.  Mix the flour, cream of tartar, baking soda and salt  in another bowl.  Blend the wet with the dry ingredients using a wooden spoon until well-blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the dough into the size of small walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare the cinnamon and sugar mixture by mixing the cinnamon and sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll the cookie dough into the cinnamon and sugar mixture until coated and place onto an ungreased cookie sheet.  Bake 6-10 minutes watching to make sure they don't burn. They should be very lightly brown and still soft looking on top. When they cool, they will crack a little on top.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Snickerdoodle Recipes"
