        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16791/applesauce-french-toast/",
        "name": "Applesauce French Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4517329.jpg",
