    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19266/strawberries/",
        "name": "Strawberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4873861.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2000-05-16T15:18:34.000Z",
        "description": "Unique!  Beautiful cookies, really dress up a cookie tray.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup white sugar",
            "2 eggs, beaten",
            "8 ounces chopped pitted dates",
            "1 teaspoon vanilla extract",
            "1 cup sifted all-purpose flour",
            "\u00bd teaspoon salt",
            "1 cup finely chopped almonds",
            "2\u2009\u00bd cups crispy rice cereal",
            "\u00bd cup red decorator sugar",
            "1 (4.5 ounce) tube prepared green decorating icing"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium heat, combine the butter, sugar, eggs and dates. Cook for 5 minutes, or until butter is melted and sugar is completely dissolved. Gradually stir in the flour and salt, and continue cooking for 5 to 7 minutes. Remove from heat and stir in the vanilla, almonds and crispy rice cereal until well blended. Roll pieces of dough into strawberry shapes. Roll the berries in red sugar and pipe a green leaf on top using a leaf tip attachment with frosting tube or pastry bag.\n"
            }
        ],
