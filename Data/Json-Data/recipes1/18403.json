        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/229409/festive-oreo-whoopie-pies/",
        "name": "Festive OREO Whoopie Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F990835.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2012-10-03T20:51:30.000Z",
        "description": "You just might hear a 'whoopie' or two from your guests when you serve this sweet dessert. Each individual pie contains a cookie surprise!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H14M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "30 servings",
        "recipeIngredient": [
            "30 each OREO Cookies",
            "1 package (2-layer size) white cake mix with colored sprinkles",
            "1 cup marshmallow creme",
            "1\u2009\u00bd cups thawed frozen whipped topping"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place 1 cookie in each of 30 paper-lined muffin cups. Prepare cake batter as directed on package; spoon 1 heaping tablespoon over each cookie. (All batter will not be used.)\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 14 minutes or until toothpick inserted in centers comes out clean. Cool in pans 5 minutes; remove to wire racks. Cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Microwave marshmallow creme in medium microwaveable bowl on high for 15 seconds. Gradually stir in whipped topping until blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove liners from cupcakes; cut cupcakes horizontally in half. Fill with whipped topping mixture. Serve upside-down.\n"
            }
        ],
