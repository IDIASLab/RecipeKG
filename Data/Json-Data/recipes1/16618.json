        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283666/trinidad-garlic-pork-leg/",
        "name": "Trinidad Garlic Pork Leg",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Trinidad Garlic Pork Leg"
        },
        "datePublished": "2011-04-03T23:41:25.000Z",
        "description": "This Trinidad garlic pork leg is marinated in a savory blend overnight before it's fried to crispy perfection.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT8H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "2 cups white vinegar",
            "2 medium onions, chopped",
            "\u00bc pound garlic cloves",
            "1 habanero pepper, seeded and chopped",
            "1 medium lime, juiced",
            "2 tablespoons ground thyme",
            "2 teaspoons salt",
            "1 cup vegetable oil for frying, or as needed",
            "4 pounds pork legs, coarsely chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine vinegar, onions, garlic, habanero, lime juice, thyme, and salt in the bowl of an electric blender and puree until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place pork in a non-metallic bowl and pour the marinade over top. Cover and marinate in the refrigerator for 8 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drain the pork. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a 12-inch pan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry the pork in the hot oil until browned on all sides, working in batches as needed, 5 to 7 minutes per batch.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork"
        ],
