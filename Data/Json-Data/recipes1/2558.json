    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12430/candy-bar-pie-ii/",
        "name": "Candy Bar Pie II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F181314.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-09-26T17:55:29.000Z",
        "description": "Layers of chocolate, cream cheese, and nuts .... a crowd pleasing pie good for any occasion.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "1 (9 inch) pie crust, baked",
            "5 (2.16 ounce) bars chocolate-coated caramel-peanut nougat candy",
            "\u00bd cup white sugar",
            "4 (3 ounce) packages cream cheese, softened",
            "2 eggs",
            "\u2153 cup sour cream",
            "\u2153 cup creamy peanut butter",
            "3 tablespoons heavy whipping cream",
            "\u2154 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cut candy bars in half lengthwise, then into 1/4 inch pieces.  Place candy pieces over bottom of pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine sugar and cream cheese; beat until smooth.  Add eggs one at a time, beating well after each addition.  Add sour cream and peanut butter; beat until mixture is smooth.  Pour filling over candy pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 325 degrees F (165 degrees C) for 30 to 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, heat whipping cream until very warm.  Remove from heat, and stir in chocolate chips until smooth.  Spread over top of pie.  Refrigerate 2 to 3 hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies"
        ],
