    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282509/shredded-chicken-burritos/",
        "name": "Shredded Chicken Burritos",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Shredded Chicken Burritos"
        },
        "datePublished": "2004-11-15T19:12:13.000Z",
        "description": "Burritos filled with slow-cooked shredded chicken, sour cream, refried beans, and Mexican-style rice. The chicken filling is also great for quesadillas and tacos.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT5H45M",
        "totalTime": "P0DT6H5M",
        "recipeYield": "8 burritos",
        "recipeIngredient": [
            "3 (5 ounce) frozen chicken breasts",
            "\u00bc cup salsa",
            "1 medium jalapeno pepper, diced",
            "1 dried red chile pepper",
            "3 cloves garlic, minced",
            "1 (1 ounce) envelope taco seasoning mix (such as Ortega\u00ae)",
            "1 cup uncooked white rice",
            "8 (10 inch) flour tortillas",
            "1 (16 ounce) can refried beans",
            "\u00bc cup shredded Cheddar cheese, or to taste",
            "\u00bc cup sour cream, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place chicken in the bottom of a slow cooker and add just enough water to cover, 2 1/2 to 3 cups. Mix in jalapeno, dried chile, garlic, and taco seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook on Low until chicken is no longer pink in the center, juices run clear, and it shreds easily with a fork, about 5 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shred chicken and allow to cook in the water mixture on Low, about 20 more minutes. Remove chicken from the pot and set aside in a bowl. Transfer cooking water to a saucepan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add rice to the cooking water and bring to a boil. Reduce heat to medium-low, cover, and simmer until rice is tender and water has been absorbed, 20 to 25 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap tortillas in foil and place in the preheated oven to warm, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "At the same time, heat refried beans in a microwave-safe bowl in the microwave until warm, 1 to 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add beans, rice, chicken mixture, Cheddar cheese, and sour cream to each tortilla. Fold tortillas and serve.\n"
            }
        ],
