        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11241/potato-chip-cookies-iii/",
        "name": "Potato Chip Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Potato Chip Cookies III"
        },
        "datePublished": "1999-01-29T18:20:17.000Z",
        "description": "Old fashioned cookies made with crushed potato chips and rolled in sugar.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup packed brown sugar",
            "\u00bd cup shortening",
            "1 egg",
            "1 cup all-purpose flour",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon vanilla extract",
            "\u00bd cup crushed potato chips",
            "\u00bd cup chopped walnuts",
            "\u2153 cup granulated sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the shortening with the sugar. Add the egg and beat well. Stir in the vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the flour and baking soda and mix well. Stir in the crushed potato chips and chopped nuts. Shape cookies into balls and roll in white sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cookies on an ungreased cookie sheet and flatten slightly. Bake at 350 degrees F (175 degrees C) for 10 minutes.\n"
            }
        ],
