        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12561/transparent-pie/",
        "name": "Transparent Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4784134.jpg",
            "width": 960,
