        "name": "Zesty Penne, Sausage and Peppers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2490000.jpg",
            "width": 1704,
            "height": 1705
