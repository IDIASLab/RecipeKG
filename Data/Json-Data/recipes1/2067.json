    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12305/rhubarb-surprise-pie/",
        "name": "Rhubarb Surprise Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8282786.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-07-14T12:36:33.000Z",
        "description": "Tastes like strawberry shortcake! Surprise",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 pie",
        "recipeIngredient": [
            "1 cup sifted all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "2 tablespoons butter",
            "1 egg, beaten",
            "2 tablespoons milk",
            "3 cups chopped rhubarb",
            "1 (3 ounce) package strawberry flavored Jell-O\u00ae",
            "\u00bd cup all-purpose flour",
            "1 cup white sugar",
            "\u00bd teaspoon ground cinnamon",
            "\u00bc cup butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sift together 1 cup flour, baking powder, and salt.  Cut in 2 tablespoons butter or margarine.  Add egg and milk; mix.  Press mixture into a greased 9 inch pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange rhubarb in pie shell.  Sprinkle with dry gelatin mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 1/2 cup flour, sugar, cinnamon, and melted butter or margarine; sprinkle on top of pie.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 350 degrees F (175 degrees C) for 50 minutes, or until rhubarb is tender.\n"
            }
        ],
