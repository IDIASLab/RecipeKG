    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20608/super-simple-oven-barbequed-chicken/",
        "name": "Super Simple Oven Barbequed Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3145783.jpg",
            "width": 1500,
            "height": 1000
        },
        "datePublished": "2000-08-15T10:02:12.000Z",
        "description": "This dish is a good alternative to BBQ chicken and is also good for anyone on a carbohydrate restrictive diet. Lots if good flavor! This recipe can be altered to serve as little or as many as you like.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "6 bone-in chicken breast halves, with skin",
            "1 tablespoon Worcestershire sauce",
            "1 tablespoon hickory-flavored liquid smoke",
            "2 teaspoons chili powder",
            "2 teaspoons ground cumin",
            "2 teaspoons garlic powder",
            "2 teaspoons dried thyme",
            "2 teaspoons dried oregano",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange chicken pieces in a lightly greased 9x13 inch baking dish, allowing room between pieces. Drizzle with Worcestershire sauce and liquid smoke, then sprinkle with chili powder, cumin, garlic powder, thyme, oregano and salt and pepper to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover dish with aluminum foil and bake for 1 to 1 1/2 hours, or until chicken is cooked through and juices run clear.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
        "recipeCuisine": [],
        "author": [
