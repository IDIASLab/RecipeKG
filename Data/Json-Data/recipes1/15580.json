    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/278045/steaks-with-truffle-butter/",
        "name": "Steaks with Truffle Butter",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Steaks with Truffle Butter"
        },
        "datePublished": "2008-12-26T06:18:46.000Z",
        "description": "These New York strip steaks gets a touch of elegance from a homemade truffle butter.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "4 steaks",
        "recipeIngredient": [
            "\u00bd cup unsalted butter, softened",
            "2 teaspoons truffle oil",
            "\u215b truffle, thinly shaved",
            "1 pinch sea salt to taste",
            "4 (6 ounce) New York strip steaks",
            "1 pinch sea salt and ground black pepper to taste",
            "2 tablespoons olive oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix butter, truffle oil, shaved truffle, and sea salt together in a bowl. Cover and refrigerate for at least 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a baking pan in the oven and preheat to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season steaks with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a heavy skillet over medium-high heat until shimmering. Add steaks, working in batches if necessary, and sear until browned, about 3 minutes per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the hot baking pan from the oven and place steaks on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until steaks are firm and reddish-pink and juicy in the center, 8 to 10 minutes. An instant-read thermometer inserted into the center should read 130 degrees F (54 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let steaks rest for 5 minutes. To serve, dab 1 tablespoon truffle butter on top of each steak.\n"
            }
        ],
