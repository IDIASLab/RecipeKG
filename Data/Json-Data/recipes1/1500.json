        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "\u00bd teaspoon baking soda",
            "\u00bd teaspoon baking powder",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup chopped walnuts",
            "1 cup crisp rice cereal",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream margarine and sugar together.   Without sifting add remaining ingredients and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Chill dough for 1/2 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into balls and press firmly with a fork. Bake on ungreased cookie sheets for 10 to 15 minutes at 325 degrees F (165 degrees C).\n"
            }
