    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7210/broccoli-cornbread/",
        "name": "Broccoli Cornbread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1003976.jpg",
            "width": 958,
            "height": 960
        },
        "datePublished": "1999-08-19T21:41:41.000Z",
        "description": "This broccoli cornbread turns out extra moist and flavorful because of the cottage cheese. Try it tonight!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "15 servings",
        "recipeIngredient": [
            "1 cup cornmeal",
            "1 cup all-purpose flour",
            "1 teaspoon salt",
            "4 teaspoons baking powder",
            "1\u2009\u00bd teaspoons white sugar",
            "3 eggs",
            "1 (10 ounce) package frozen chopped broccoli",
            "12 ounces cottage cheese",
            "5 tablespoons margarine, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease the bottom and sides of a 9x13-inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl mix together the cornmeal, flour, salt, baking powder, and sugar. Make a well in the center of the bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, combine the eggs, thawed broccoli, cottage cheese, and melted butter or margarine.  Pour into well of flour mixture. Stir until just combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into a greased 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 350 degree F (175 degrees C) oven for 30 minutes, or until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Cornbread Recipes"
        ],
