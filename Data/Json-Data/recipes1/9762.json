    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17070/cheese-and-bacon-filling/",
        "name": "Cheese and Bacon Filling",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4460705.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2000-02-16T08:29:58.000Z",
        "description": "This is fairly quick, very easy and tasty! Bacon bits and cheese in a creamy sauce. Delicious in pancakes, crepes and over pasta, and even on a slice of toast!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "6 to 8 servings",
        "recipeIngredient": [
            "1 (3 ounce) can bacon bits",
            "2 tablespoons butter",
            "2 tablespoons all-purpose flour",
            "3 cups milk",
            "salt and pepper to taste",
            "\u00bc teaspoon mustard powder",
            "1 cup shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Fry the bacon in a large skillet over medium high heat for 10 minutes, or to desired crispiness. Drain excess fat, crumble bacon and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt the butter or margarine in a large saucepan over medium heat. Add the flour, stirring constantly, to form a roux.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly add the milk, stirring often to avoid lumps. Then stir in the salt and pepper to taste, mustard powder and the cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Let simmer for 1 minute, allowing cheese to melt some, remove from heat, add the bacon and stir well.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
