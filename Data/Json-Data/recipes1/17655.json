    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220251/asparagus-ham-and-lemon/",
        "name": "Asparagus, Ham, and Lemon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3547650.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "2012-02-15T23:20:34.000Z",
        "description": "This simple dish is fast to make and perfect for spring.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons olive oil",
            "2 ounces thinly sliced prosciutto ham, or any ham, roughly chopped or torn",
            "1 pound fresh asparagus",
            "\u00bc cup water",
            "1 clove garlic, thinly sliced",
            "salt and ground black pepper to taste",
            "1 juice of 1/2 a lemon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium-high heat. Stir in prosciutto and cook until crisp, about 3-4 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place asparagus in the skillet, and toss gently until coated with oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in water and cover with lid; cook for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove lid, stir in garlic and cook for 1 minute with lid on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove lid and cook uncovered until the spears are still crisp but tender, about 2-3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Season with salt and pepper to taste. Squeeze lemon juice over the top.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Asparagus"
        ],
