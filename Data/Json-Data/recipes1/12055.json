        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19459/key-lime-pie-ix/",
        "name": "Key Lime Pie IX",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5154132.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2000-06-26T19:24:45.000Z",
        "description": "This is the only recipe for Key Lime Pie that you'll ever need. The best restaurants use this one.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT1H35M",
        "recipeYield": "1 9-inch pie",
        "recipeIngredient": [
            "1 (14 ounce) can sweetened condensed milk",
            "\u00bd cup key lime juice",
            "1 teaspoon grated lime zest",
            "2 egg yolks",
            "1 egg",
            "1 (9 inch) prepared graham cracker crust",
            "2 egg whites",
            "4 tablespoons white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, blend together condensed milk, lime juice and zest. Mix in egg yolks and whole egg. Pour mixture into crust, then cover and refrigerate for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Make Meringue: In a large glass or metal mixing bowl, beat egg whites until foamy. Gradually add sugar, continuing to beat until whites form stiff peaks. Spread meringue over pie, covering completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 15 minutes. Chill before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies",
            "Meringue Pie Recipes"
        ],
