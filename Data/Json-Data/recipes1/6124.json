        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14577/grilled-zucchini-i/",
        "name": "Grilled Zucchini I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3412.jpg",
            "width": 3008,
            "height": 2000
