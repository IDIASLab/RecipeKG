        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22589/hanky-panky/",
        "name": "Hanky Panky",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F617686.jpg",
