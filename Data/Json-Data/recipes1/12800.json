    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20721/garlic-herbed-chicken/",
        "name": "Garlic Herbed Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F410553.jpg",
            "width": 1244,
            "height": 1242
        },
        "datePublished": "2000-08-16T03:41:10.000Z",
        "description": "Chicken is grilled with a garlic and tarragon mixture that will make your mouth water!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "6 cloves garlic, crushed",
            "2 teaspoons dried tarragon",
            "\u00bd teaspoon salt",
            "1 tablespoon freshly ground black pepper",
            "\u00bd cup olive oil",
            "4 skinless, boneless chicken breast halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, whisk together garlic, dried tarragon, salt, ground black pepper and olive oil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place each chicken breast half on a piece of foil large enough to cover the chicken. Brush chicken with the garlic mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Tightly seal chicken in the foil. Cook on the prepared grill approximately 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from grill. Discard foil and juices. Place unwrapped chicken on the grill. Brushing frequently with remaining garlic mixture and turning often, continue cooking 5 to 10 minutes per side, or to desired doneness.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
        ],
