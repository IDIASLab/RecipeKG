    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17720/crazy-crust-apple-pie/",
        "name": "Crazy Crust Apple Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F32495.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-03-28T06:38:54.000Z",
        "description": "The one that makes its own sweet, crumbly, biscuit-like crust!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9 inch deep dish pie",
        "recipeIngredient": [
            "1 cup all-purpose flour",
            "2 tablespoons white sugar",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "\u00be cup water",
            "\u2154 cup shortening",
            "1 egg",
            "1 (21 ounce) can apple pie filling",
            "1 tablespoon lemon juice",
            "\u00bd teaspoon apple pie spice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium mixing bowl, combine flour, sugar, baking powder, and salt. Mix well, then add water, shortening, and egg. Mix on low speed until ingredients are combined, then beat on medium speed 2 minutes. Spread into pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl mix together pie filling, lemon juice, and apple pie spice. Spread over crust; do not stir.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 40 to 45 minutes, until crust is browned.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Apple Pie Recipes"
        ],
