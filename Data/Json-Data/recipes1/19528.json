    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233258/smooth-double-chocolate-frosting/",
        "name": "Smooth Double Chocolate Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1027289.jpg",
            "width": 3626,
            "height": 2593
        },
        "datePublished": "2013-06-28T19:03:33.000Z",
        "description": "I've never liked the heavy texture of buttercream icing, so I came up with this as a smooth and rich alternative. It always gets rave reviews!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT0H40M",
        "recipeYield": "24 servings",
        "recipeIngredient": [
            "9 ounces semisweet chocolate chips",
            "9 ounces white chocolate chips",
            "2 cups heavy whipping cream",
            "1 cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix semisweet chocolate chips and white chocolate chips together in a large bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring cream to a boil in a small saucepan; remove from heat and pour over chip mixture. Stir mixture until chips are melted. Beat confectioners' sugar into chocolate mixture using an electric mixer until smooth and thin. Refrigerate until completely cooled and thickened, at least 30 minutes. Beat mixture again with an electric mixer until frosting is fluffy and light brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Frostings and Icings",
            "Chocolate"
        ],
