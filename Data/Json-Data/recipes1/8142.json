        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9453/cow-pies/",
        "name": "Cow Pies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F423546.jpg",
