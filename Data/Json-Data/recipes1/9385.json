    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16012/fat-tuesday-salmon/",
        "name": "Fat Tuesday Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F970105.jpg",
            "width": 1940,
            "height": 1936
        },
        "datePublished": "2000-01-23T01:49:48.000Z",
        "description": "Spicy salmon patties that are great with rice. Add as much jalapeno pepper or as little as you like.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H20M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 potato, peeled",
            "2 (14.75 ounce) cans salmon, drained",
            "\u00bd cup diced onion",
            "\u00bd cup diced green bell pepper",
            "1 jalapeno pepper, seeded and minced",
            "1 egg, beaten",
            "1 teaspoon ground black pepper",
            "\u00bd cup cracker crumbs",
            "2 tablespoons vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a saucepan of water to a boil.  Add potato and cook until tender but still firm, about 15 minutes. Drain, cool and mash.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine salmon, onion, green bell pepper, potato, jalapeno, egg and ground black pepper; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape mixture into 4 patties and coat with cracker crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium heat, warm oil and cook patties until browned on each side; serve.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Salmon"
        ],
