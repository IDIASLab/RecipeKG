    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20085/strawberry-butter-cracker-pie/",
        "name": "Strawberry Butter Cracker Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5507979.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2000-07-28T12:34:01.000Z",
        "description": "One pie never seems to be enough!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "3 egg whites",
            "1 cup white sugar",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 teaspoon vanilla extract",
            "14 buttery round crackers, crushed",
            "\u2154 cup chopped pecans",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "2 cups fresh strawberries, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9 inch pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large glass or metal mixing bowl, beat egg whites until foamy. Gradually add sugar, baking powder, salt and vanilla continuing to beat until whites form stiff peaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Gently fold  crackers and pecans into meringue mixture. Pour into pie pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 30 minutes. Cover and refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When ready to serve, completely cover pie with whipped topping, then arrange strawberry slices on top.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Fruit Pies",
            "Strawberry Pie Recipes"
        ],
