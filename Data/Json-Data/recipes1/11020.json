    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21434/shrimp-dip-iv/",
        "name": "Shrimp Dip IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4993833.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2000-04-24T23:15:54.000Z",
        "description": "This is an old recipe that my mother found in a drawer. It doesn't sound very good, but it is delicious.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed tomato soup",
            "1 cup mayonnaise",
            "1 (8 ounce) package cream cheese, softened",
            "1 envelope (1 tablespoon) unflavored gelatin",
            "2 (4 ounce) cans small shrimp, drained",
            "\u00bd cup chopped celery",
            "\u00bd cup chopped onion",
            "\u00bd cup chopped green bell pepper",
            "1 tablespoon Worcestershire sauce",
            "1 teaspoon prepared horseradish",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, heat tomato soup (undiluted) to boiling. Meanwhile, in a small bowl, soften the gelatin in 1/4 cup of water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, combine mayonnaise, softened cream cheese and softened gelatin. Mix thoroughly. Fold in shrimp, celery, onion, green pepper. Mix thoroughly. Blend in horseradish, Worcestershire sauce, salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into a 5 or 6 cup refrigerator mold, or into two smaller molds. Refrigerate for at least 2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Shrimp Dip Recipes"
        ],
