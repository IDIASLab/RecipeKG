        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233000/spicy-croutons/",
        "name": "Spicy Croutons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1997600.jpg",
