        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16735/hot-crab-dip-ii/",
        "name": "Hot Crab Dip II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9065079.jpg",
