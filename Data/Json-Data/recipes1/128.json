        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9921/toasty-macaroons/",
        "name": "Toasty Macaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Toasty Macaroons"
        },
        "datePublished": "1997-08-23T17:46:06.000Z",
        "description": "A recipe from the 70's  ... coconut and rum extract!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 (3.5 ounce) packages flaked coconut, toasted",
            "\u2154 cup sweetened condensed milk",
            "1 teaspoon rum flavored extract",
            "\u00bc teaspoon ground ginger",
            "\u00bd cup red and green candied cherries, halved"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (170 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine coconut, milk, rum extract, and ginger. Stir until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoon onto foil-lined sheet. Garnish each with 1/2 candied cherry placed upside down in center.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 15 minutes or until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from cookie sheet to wire racks. Cool completely before storing.\n"
            }
        ],
