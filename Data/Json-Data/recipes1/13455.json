    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21827/nanas-acorn-squash/",
        "name": "Nana's Acorn Squash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1857905.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-10-03T16:40:48.000Z",
        "description": "This is a recipe even veggie haters will love! People who like sweet potatoes also love it. It's a family favorite of mine.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 acorn squash, halved and seeded",
            "salt and pepper to taste",
            "4 tablespoons butter, divided",
            "4 tablespoons brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a microwave safe casserole dish pour enough water to reach 1 1/2 inches in depth.  Place the squash halves cut side down in the water, and pierce the skin with a fork a few times.  Microwave on high for 15 to 20 minutes.  Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle each half with salt and pepper to taste.  Place 2 tablespoons butter and 2 tablespoons brown sugar in each half.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil 5 minutes, or until butter is melted.  Mix the melted butter and sugar into the flesh, and serve.\n"
            }
        ],
