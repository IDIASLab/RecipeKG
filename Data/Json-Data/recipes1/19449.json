        "mainEntityOfPage": "https://www.allrecipes.com/recipe/142628/easy-red-rice/",
        "name": "Easy Red Rice",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F236119.jpg",
