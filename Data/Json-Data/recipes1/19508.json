        "recipeYield": "12 servings",
        "recipeIngredient": [
            "8 large strawberries, halved",
            "2 tablespoons white sugar",
            "7 cups water, divided",
            "1 cup white sugar",
            "2 cups freshly squeezed lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place strawberries in a blender; top with 2 tablespoons sugar. Pour 1 cup water over sugared strawberries. Blend until strawberry chunks transform into juice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine strawberry juice, 6 cups water, 1 cup sugar, and lemon juice in a large pitcher; stir until blended. Chill before serving.\n"
            }
        ],
