        "recipeIngredient": [
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup butter",
            "\u215c cup white sugar",
            "1 egg",
            "1 lemon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream the butter or margarine and sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grate and juice the lemon.  Add the lemon juice and rind to the creamed mixture.  Add flour and beaten egg.  Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Rest dough for one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Put small spoonfuls on tray.  Cook for 15-20 minutes at 350 degrees F (175 degrees C).\n"
            }
