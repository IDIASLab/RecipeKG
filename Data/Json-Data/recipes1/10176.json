    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16618/easy-fruit-dip/",
        "name": "Easy Fruit Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F73596.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-03-12T23:28:15.000Z",
        "description": "Easy and delicious fruit dip. Always a hit, and the kids LOVE IT. Slice up a selection of fruit and snacks to serve with the dip. My family likes apples, strawberries, cantaloupe, grapes, pretzels, raisin bread, or muffins.",
        "prepTime": "P0DT0H5M",
        "cookTime": null,
        "totalTime": "P0DT2H5M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "16 ounces marshmallow creme",
            "1 tablespoon maraschino cherry juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place cream cheese and marshmallow cream in a microwave-safe bowl. Place in microwave and cook on medium-high for 20 seconds increments, until softened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir cherry juice into the cream cheese and marshmallow creme mixture. Cover and refrigerate until chilled.\n"
            }
        ],
