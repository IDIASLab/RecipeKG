    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7925/blue-cheese-cheesecake/",
        "name": "Blue Cheese Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5276945.jpg",
            "width": 2256,
            "height": 2255
        },
        "datePublished": "1998-08-10T18:28:55.000Z",
        "description": "Serve this baked crustless cake - a mixture of blue cheese, cream cheese, and sour cream - as an impressive appetizer.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch cake",
        "recipeIngredient": [
            "2 (8 ounce) packages cream cheese",
            "2 cups blue cheese, crumbled",
            "1 cup sour cream",
            "\u2153 teaspoon ground white pepper",
            "3 eggs",
            "1\u2009\u00bc cups sour cream",
            "1 red bell pepper, halved and deseeded",
            "1 green onion"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat cream cheese and blue cheese in large mixing bowl until light and fluffy--about 5 minutes.  Mix in 1 cup sour cream and pepper.  Add eggs - one at a time - mixing well after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into buttered 9 inch springform pan.  Bake 60 - 65 minutes or until wooden pick inserted near center comes out clean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from oven and let stand 5 minutes.  Carefully spread 1 1/4 cup sour cream over top.  Return to oven 5 minutes.  Cool completely on wire rack.  Refrigerate several hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To serve, place on serving plate.  Carefully remove sides of pan.  Decorate top with red pepper cut into flower shapes with small cookie cutter and green onion tops as stems.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Dips and Spreads Recipes",
            "Cheese Dips and Spreads Recipes",
            "Cheese Ball Recipes"
        ],
