        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20702/cheddar-bacon-dip/",
        "name": "Cheddar Bacon Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1998997.jpg",
