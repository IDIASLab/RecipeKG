    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22793/apple-cranberry-crisp/",
        "name": "Apple Cranberry Crisp",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3362359.jpg",
            "width": 3264,
            "height": 2448
        },
        "datePublished": "2000-10-17T05:04:49.000Z",
        "description": "A great combination of apples and cranberries with a crispy topping. A favorite at Thanksgiving instead of plain cranberries.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 8x8 inch dish",
        "recipeIngredient": [
            "1\u2009\u00bd cups quick cooking oats",
            "\u00bd cup brown sugar",
            "\u2153 cup all-purpose flour",
            "1 teaspoon ground cinnamon",
            "\u2153 cup butter flavored shortening, melted",
            "1 tablespoon water",
            "1 (16 ounce) can whole berry cranberry sauce",
            "2 tablespoons cornstarch",
            "5 Granny Smith apples - peeled, cored and thinly sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375  degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the oats, brown sugar, flour, and cinnamon. Stir in the melted shortening and water to form a crumbly mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, mix together the cranberry sauce and cornstarch. Bring to a boil, and then remove from heat. Stir in the apples. Spread into an 8x8 inch glass baking dish. Crumble the oat mixture over the apples.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 to 35 minutes, or until the apples are tender. Serve warm.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cranberry Dessert Recipes"
        ],
