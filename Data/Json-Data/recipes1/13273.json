        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22652/seasoned-tortilla-chip-dip/",
        "name": "Seasoned Tortilla Chip Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F855856.jpg",
