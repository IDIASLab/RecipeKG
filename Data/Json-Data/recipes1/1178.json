    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10833/refrigerator-cookies-iii/",
        "name": "Refrigerator Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1055677.jpg",
            "width": 1600,
            "height": 1067
        },
        "datePublished": "1998-04-24T19:18:56.000Z",
        "description": "These will freeze well for several months.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "7 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "1 cup white sugar",
            "2 eggs",
            "1\u2009\u00bd teaspoons vanilla extract",
            "3 cups all-purpose flour",
            "1 teaspoon salt",
            "\u00bd teaspoon baking soda",
            "\u2153 cup colored sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Mix shortening, sugar, eggs, and vanilla together in large bowl.  Stir flour, salt, and baking soda together in a separate bowl.  Blend dry ingredients in with the shortening mixture.  Mix thoroughly with hands.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide dough into 3 parts; shape into cylinders, 1 1/2 inches in diameter and about 7 inches long.  Roll in colored sugar (or use finely chopped nuts instead).  Chill for several hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oven to 400 degrees F (205 degrees C).  Cut into 1/4 inch slices, and place on greased cookie sheets.  Bake for 8 to 10 minutes.  Cool.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
