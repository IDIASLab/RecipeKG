    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13871/easy-bread-pudding-ii/",
        "name": "Easy Bread Pudding II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F759439.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-11-21T06:25:26.000Z",
        "description": "This recipe is enjoyed by everyone I have ever served it to. There are never any leftovers because it is so creamy and rich.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "1 - 8x8 inch dish",
        "recipeIngredient": [
            "2 tablespoons butter, softened",
            "2\u2009\u00bd cups cubed day old French bread",
            "4 eggs, beaten",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract",
            "2 cups milk, scalded",
            "1 pinch ground nutmeg",
            "1 tablespoon dark brown sugar",
            "1 cup pecans, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Generously butter an 8x8 inch baking dish. Prepare a water bath for the baking dish by placing a larger dish in the oven, and partially filling it with hot water.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place bread cubes in the baking dish. In a medium bowl, beat together eggs, sugar, and vanilla. Slowly whisk in the scalded milk. Pour over the bread. Sprinkle with nutmeg, brown sugar, and pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the baking dish in the water bath. Bake for 50 to 60 minutes, or until a knife inserted in the middle comes out clean. Serve either hot or chilled.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Bread Pudding Recipes"
