    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11222/hildas-icebox-cookies/",
        "name": "Hilda's Icebox Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F937620.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-12-28T05:44:47.000Z",
        "description": "We got this recipe from a special neighbor who was like a grandmother to me.  Chill them and slice them very thin before baking.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 -7 dozen",
        "recipeIngredient": [
            "2 cups margarine",
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "3 eggs",
            "5 cups all-purpose flour",
            "1 teaspoon baking soda",
            "1 teaspoon baking powder",
            "1 pinch salt",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream the margarine and the sugars together. Add the eggs and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the flour, baking soda, baking powder, salt and chopped nuts. Shape dough into 2 inch to 2 1/2 inch diameter sized logs. Wrap logs and refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice logs very thing and place cookies on the prepared sheets. Bake at 350 degrees F (175 degrees C) for 8 to 10 minutes.\n"
            }
        ],
