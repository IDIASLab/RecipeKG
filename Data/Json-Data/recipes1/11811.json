    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21438/old-fashioned-crumb-cake/",
        "name": "Old Fashioned Crumb Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F21075.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-06-13T02:24:57.000Z",
        "description": "A simple, moist mellow spiced crumb cake that is perfect for brunch or any other time. Long time family favorite.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup white sugar",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd cup butter, cut into pieces",
            "1 teaspoon ground cinnamon",
            "\u00bd teaspoon ground cloves",
            "1 teaspoon baking soda",
            "1 pinch salt",
            "1 cup buttermilk",
            "1 egg, lightly beaten"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 7x11 inch baking pan or casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, flour and butter. Use a fork to cut butter in until mixture resembles coarse crumbs. Set aside 1/2 cup of this mixture to use as topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To the remaining flour mixture, add cinnamon, cloves, baking soda and salt. Lightly stir in the buttermilk and egg. Pour batter into prepared pan. Sprinkle cake with reserved topping.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in preheated oven for 25 minutes, until a toothpick inserted into center of the cake comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Coffee Cake Recipes"
        ],
