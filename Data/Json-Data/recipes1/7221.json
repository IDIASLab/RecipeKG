    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9386/caramel-corn-i/",
        "name": "Caramel Corn I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F579103.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-04T18:27:20.000Z",
        "description": "This caramel corn is great for Monday night football. The assortment of nuts makes it deluxe caramel corn. It takes a bit of time, but is more than worth it.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 quarts",
        "recipeIngredient": [
            "5 tablespoons vegetable oil",
            "2\u2009\u00bd cups unpopped popcorn",
            "1 cup butter",
            "2 cups packed brown sugar",
            "\u00bd cup light corn syrup",
            "1 teaspoon salt",
            "\u00bd teaspoon baking soda",
            "1 teaspoon vanilla extract",
            "1 cup peanuts",
            "1 cup chopped pecans",
            "1 cup almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 1 tablespoon of the oil to a 4 quart saucepan, and heat over high heat. When oil is hot, add 1/2 cup of popping corn. Keep pan moving constantly.  When corn stops popping, remove from heat. Place popped corn in oven to keep warm. Repeat until all corn has been popped. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt margarine or butter in a medium saucepan, and then stir in brown sugar, syrup, and salt. Bring to a rolling boil, stirring constantly; boil without stirring for 5 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir baking soda, vanilla, peanuts, pecans, and almonds into brown sugar mixture.  Quickly pour nut mixture over warm popcorn, and toss until well coated.  Spread on greased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 minutes, stirring well every 15 minutes. Cool on aluminum foil, and store in an air-tight container.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Candy Recipes",
            "Popcorn Candy Recipes"
        ],
