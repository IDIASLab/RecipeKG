    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23021/leftover-turkey-casserole/",
        "name": "Leftover Turkey Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F746281.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-10-22T00:12:46.000Z",
        "description": "This is a great way to use up some of those Thanksgiving leftovers! It's easy to prepare, as well. Top it with gravy, and revisit the feast!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 (6 ounce) package dry bread stuffing mix",
            "1 (16 ounce) container sour cream",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (10.75 ounce) can  condensed cream of celery soup",
            "1 (1 ounce) package dry onion soup mix",
            "2 (14.5 ounce) cans French-style green beans, drained",
            "2 cups cooked, chopped turkey meat"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Prepare stuffing according to package directions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix the sour cream, cream of mushroom soup, cream of celery soup and dry onion soup mix.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread the green beans in a 9x13 inch dish. Top with a layer of turkey. Pour the soup mixture over the turkey. Top with stuffing.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 30 minutes, or until browned and bubbly.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Casserole Recipes",
            "Vegetable"
        ],
