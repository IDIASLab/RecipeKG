        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20059/tater-tot-casserole-iv/",
        "name": "Tater Tot Casserole IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6281711.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2000-05-02T19:08:12.000Z",
        "description": "It's so easy, and so delicious!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 pound lean ground beef",
            "1 pound fresh, ground pork sausage",
            "1 small onion, chopped",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "1 (14.5 ounce) can French-style green beans, drained",
            "1 (32 ounce) package tater tots, thawed",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Saute onion in a large skillet until soft; add beef and sausage and cook until browned. Drain set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spread soup and beans in the bottom of a 9x13 inch baking dish. Slowly add meat mixture, then top with tater tots and season with salt to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30 to 45 minutes, or until casserole is cooked through.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Pork",
            "Ground Pork Recipes"
        ],
