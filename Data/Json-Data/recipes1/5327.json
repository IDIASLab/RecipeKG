    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13268/mediterranean-fish-soup/",
        "name": "Mediterranean Fish Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2425133.jpg",
            "width": 2448,
            "height": 2449
        },
        "datePublished": "1999-05-31T00:50:39.000Z",
        "description": "This is a very hearty soup from the Mediterranean.  If you prefer a heartier soup, add more fish.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 onion, chopped",
            "\u00bd green bell pepper, chopped",
            "2 cloves garlic, minced",
            "1 (14.5 ounce) can diced tomatoes, drained",
            "2 (14 ounce) cans chicken broth",
            "1 (8 ounce) can tomato sauce",
            "2\u2009\u00bd ounces canned mushrooms",
            "\u00bc cup sliced black olives",
            "\u00bd cup orange juice",
            "\u00bd cup dry white wine",
            "2 bay leaves",
            "1 teaspoon dried basil",
            "\u00bc teaspoon fennel seed, crushed",
            "\u215b teaspoon ground black pepper",
            "1 pound medium shrimp - peeled and deveined",
            "1 pound cod fillets, cubed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place onion, green bell pepper, garlic, tomatoes, chicken broth, tomato sauce, mushrooms, olives, orange juice, wine, bay leaves, dried basil, fennel seeds, and pepper into a  slow cooker. Cover, and cook on low 4 to 4 1/2 hours or until vegetables are crisp tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in shrimp and cod. Cover. Cook 15 to 30 minutes, or until shrimp are opaque. Remove and discard bay leaves.  Serve.\n"
            }
        ],
