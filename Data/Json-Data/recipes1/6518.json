        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16714/quick-tomato-sandwich/",
        "name": "Quick Tomato Sandwich",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9280651.jpg",
