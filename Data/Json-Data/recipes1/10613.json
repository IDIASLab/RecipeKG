    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17866/lemon-cake/",
        "name": "Lemon Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2202850.jpg",
            "width": 1688,
            "height": 1705
        },
        "datePublished": "2000-04-03T20:18:21.000Z",
        "description": "This is a wonderful easy recipe that is truly delicious. Lemon sheet cake with a cool lemony cream topping. Everyone who has tried it absolutely loves it.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H28M",
        "totalTime": "P0DT0H43M",
        "recipeYield": "1 - 10x15 inch pan",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "1 (3.4 ounce) package instant lemon pudding mix",
            "1\u2009\u00be cups water",
            "3 egg whites",
            "\u00be cup nonfat milk",
            "\u00bd teaspoon lemon extract",
            "1 (1 ounce) package instant sugar-free vanilla pudding mix",
            "1 (8 ounce) container frozen light whipped topping, thawed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Spray a 10x15 inch pan with non-stick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix together cake mix and lemon pudding mix. Pour in water and egg whites. Beat on low speed for 1 minute. Increase speed to high and beat for 4 minutes. Pour batter into prepared 10x15 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 25 to 30 minutes, or until a toothpick inserted into the center of the cake comes out clean. Allow to cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine milk, lemon extract and vanilla pudding mix. Beat on low for 2 minutes. Fold in whipped topping. Spread over cooled cake. Store cake in refrigerator.\n"
            }
        ],
