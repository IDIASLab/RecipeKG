    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21226/catfish-creole/",
        "name": "Catfish Creole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7976723.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-08-27T01:41:12.000Z",
        "description": "This dish tastes just like the authentic version except it's lower in calories and fat and is a quick recipe to make.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 cups water",
            "1 cup uncooked long-grain rice",
            "1 pound catfish fillets",
            "1 (16 ounce) can stewed tomatoes, with liquid",
            "2 teaspoons dried minced onion",
            "1 teaspoon chicken bouillon granules",
            "\u00bd teaspoon dried oregano",
            "\u00bc teaspoon garlic powder",
            "\u215b teaspoon hot pepper sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, bring the 2 cups of water to a boil. Stir in the rice and return to a boil; reduce the heat. Cover and simmer for 20 minutes or until the rice is tender and the water is absorbed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Using a very sharp knife, cut the catfish into 3/4 inch pieces; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan, combine the tomatoes (with juices), dried onions, bouillon granules, dried oregano, garlic powder and hot-pepper sauce; bring to a boil and stir in the catfish pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover and cook over medium heat for 5 to 8 minutes or until the fish flakes easily when tested with a fork and is opaque all the way through.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve the fish mixture over the rice.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Catfish"
        ],
