    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15170/cream-of-broccoli-soup-iv/",
        "name": "Cream of Broccoli Soup IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6765488.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-10-26T01:03:15.000Z",
        "description": "This soup is made with skim milk, but no one will know if you don't tell them. It is thick and creamy!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups fresh chopped broccoli, steamed and divided",
            "1 clove garlic, minced",
            "1\u2009\u00bc cups chicken broth, divided",
            "2 tablespoons margarine",
            "\u00bc cup all-purpose flour",
            "3 cups skim milk",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a food processor puree 1 cup steamed broccoli, garlic, and 1/4 cup chicken broth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large saucepan over medium heat, melt the margarine. Add the flour, and stir until mixture thickens and starts to brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slowly add remaining 1 cup chicken broth and milk, stirring often. Allow mixture to thicken, about 5 minutes. Then add the puree mixture and the remaining 1 1/2 cups steamed broccoli. Heat through, and allow to thicken for about 5 minutes. Season with salt and pepper.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Cream Soup Recipes",
            "Cream of Broccoli Soup"
        ],
