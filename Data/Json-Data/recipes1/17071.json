        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/142150/lemon-cheese-ball/",
        "name": "Lemon Cheese Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F755855.jpg",
            "width": 4000,
            "height": 3000
        },
        "datePublished": "2011-12-15T20:32:36.000Z",
        "description": "Lemonade mix flavors a sweet cheese ball to make a very refreshing snack for a spring or summer get-together. Serve with mini graham crackers.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT3H15M",
        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 (8 ounce) package cream cheese, softened",
            "\u00bc cup butter",
            "\u00bd cup confectioners' sugar",
            "1 tablespoon powdered lemonade mix",
            "\u00bc cup graham cracker crumbs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix the cream cheese, butter, confectioners' sugar and lemonade mix using an electric mixer. Refrigerate for 3 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "When the mixture is completely chilled, shape into a ball and roll in graham cracker crumbs. Serve on a platter with your favorite crackers.\n"
            }
        ],
