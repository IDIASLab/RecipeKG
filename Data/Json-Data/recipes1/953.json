        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12096/chess-pie-i/",
        "name": "Chess Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Chess Pie I"
        },
        "datePublished": "1998-03-18T18:59:22.000Z",
        "description": "This was my grandmother's recipe. This pie is always a hit.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9 inch pie",
        "recipeIngredient": [
            "4 eggs",
            "2 cups white sugar",
            "4 tablespoons butter",
            "2 tablespoons cream",
            "1 teaspoon vanilla extract",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream sugar and butter or margarine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add eggs one at a time, and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cream and vanilla to this mixture, and blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour mixture into 9 inch pie shell.  Bake at 350 degrees F (175 degrees C) for 45 minutes, or until set and brown on top.\n"
            }
        ],
