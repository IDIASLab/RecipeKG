    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8868/chicken-soup-casserole/",
        "name": "Chicken Soup Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F724391.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-06-14T04:28:54.000Z",
        "description": "This is Grandma's chicken casserole recipe. It's all in the family!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 (10.75 ounce) can  condensed cream of chicken soup",
            "1 (10.75 ounce) can  condensed cream of celery soup",
            "1 (10.5 ounce) can condensed chicken and rice soup",
            "1 cup mayonnaise",
            "1 tablespoon chopped onion",
            "1 cup shredded boiled chicken breast meat",
            "4 ounces buttery round crackers, crushed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl combine the cream of chicken soup, cream of celery soup, chicken soup with rice, mayonnaise, onion and chicken meat. Mix together well. Pour mixture into a 10x10 inch casserole dish. Sprinkle crushed cracker crumbs over the top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 30 to 35 minutes or until it starts to bubble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To Microwave: Put in microwave for 20 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Chicken Soup Recipes"
        ],
