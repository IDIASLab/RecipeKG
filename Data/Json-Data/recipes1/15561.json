    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284702/twice-baked-sweet-potato-casserole/",
        "name": "Twice-Baked Sweet Potato Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Twice-Baked Sweet Potato Casserole"
        },
        "datePublished": "2008-11-26T01:44:46.000Z",
        "description": "These twice-baked sweet potatoes are a delicious spin on an old favorite!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT2H0M",
        "totalTime": "P0DT2H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "nonstick cooking spray",
            "5 pounds sweet potatoes",
            "1 pound bacon",
            "\u00bd cup bread crumbs",
            "4 tablespoons butter, melted",
            "\u00bd cup sour cream",
            "\u00bc cup chopped green onions",
            "\u00bc cup butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C). Line a half-sheet pan with foil or parchment paper; spray lightly with nonstick cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pierce sweet potatoes well and place on the prepared baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, about 90 minutes. Remove from heat and cool until potatoes are cool enough to handle. Remove skins. Leave the oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels. Crumble.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix bread crumbs and melted butter in a small bowl and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mash potatoes well in a large bowl with a potato masher. Stir in chopped bacon, sour cream, onions, and softened butter. Stir to combine well. Spread mixture in a greased baking dish. Sprinkle crumb mixture evenly over the top of potatoes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until thoroughly heated and bread crumbs are just turning golden, about 30 minutes.\n"
            }
        ],
