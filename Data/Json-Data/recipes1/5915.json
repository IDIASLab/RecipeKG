    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15551/sweet-and-sour-meatballs/",
        "name": "Sweet and Sour Meatballs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F242199.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "1999-07-22T16:14:37.000Z",
        "description": "These meatballs are slow-cooked in a sweet and sour sauce.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT1H50M",
        "recipeYield": "2 to 3 dozen",
        "recipeIngredient": [
            "1 (12 fluid ounce) can or bottle chile sauce",
            "2 teaspoons lemon juice",
            "9 ounces grape jelly",
            "1 pound lean ground beef",
            "1 egg, beaten",
            "1 large onion, grated",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk together the chili sauce, lemon juice and grape jelly. Pour into slow cooker and simmer over low heat until warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine ground beef, egg, onion and salt. Mix well and form into 1 inch balls. Add to sauce and simmer for 1 1/2 hours.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Meat and Poultry",
