    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13180/portuguese-chicken-soup-ii/",
        "name": "Portuguese Chicken Soup II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1095313.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "1999-03-31T15:53:09.000Z",
        "description": "Elegant. Garnish with a slice of lemon and a mint leaf.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 whole bone-in chicken breast, with skin",
            "1 onion, cut into thin wedges",
            "4 sprigs fresh parsley",
            "\u00bd teaspoon lemon zest",
            "1 sprig fresh mint",
            "6 cups chicken stock",
            "\u2153 cup thin egg noodles",
            "2 tablespoons chopped fresh mint leaves",
            "salt to taste",
            "\u00bc teaspoon freshly ground white pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large saucepan, simmer chicken breast in stock with the onion, parsley, lemon zest, and mint sprig until done, about 35 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the breast, cool, then strip off the meat and cut into a julienne.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Strain the broth, return to the pot, and bring to a boil. Add pasta and chopped mint. Season to taste with salt and white pepper. Heat until the pasta is cooked al dente.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat, stir in lemon juice and chicken julienne. Ladle into soup plates and top with lemon slice and mint leaf.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Noodle Soup Recipes"
        ],
