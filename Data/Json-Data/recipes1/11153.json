        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19613/beef-marinade-i/",
        "name": "Beef Marinade I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3818721.jpg",
