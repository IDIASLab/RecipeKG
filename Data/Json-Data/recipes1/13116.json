        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22731/chinese-new-year-chocolate-candy/",
        "name": "Chinese New Year Chocolate Candy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F926251.jpg",
