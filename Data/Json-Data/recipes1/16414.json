    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285396/apple-cranberry-cornbread-stuffing/",
        "name": "Apple-Cranberry Cornbread Stuffing",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple-Cranberry Cornbread Stuffing"
        },
        "datePublished": "2010-11-25T18:29:32.000Z",
        "description": "This is an easy, sweet apple-cornbread stuffing. Can be stuffed in a turkey or baked in a casserole.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "12 servings",
        "recipeIngredient": [
            "1 cup dried cranberries",
            "2 tablespoons butter",
            "2\u2009\u00bd cups chopped, peeled apples",
            "1 cup chopped celery",
            "1 small onion, chopped",
            "6 cups cornbread stuffing mix",
            "2\u2009\u00bd cups chicken stock",
            "\u00bd cup butter, melted",
            "1 tablespoon poultry seasoning",
            "1 teaspoon dried sage",
            "1 teaspoon dried rosemary",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Pour 2 cups hot water over dried cranberries and let stand until rehydrated, about 20 minutes. Drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, preheat the oven to 375 degrees F (190 degrees C). Grease a large baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 2 tablespoons butter in a pot over medium-high heat. Saute apples, celery, and onion in the hot butter until light brown and softened, 5 to 7 minutes, adding cranberries during the last few minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer sauteed mixture to a large bowl. Add stuffing mix, chicken stock, melted butter, poultry seasoning, sage, rosemary, salt, and pepper. Mix well. Transfer to the prepared baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until golden and set, 20 to 30 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Stuffing and Dressing Recipes",
            "Cornbread Stuffing and Dressing Recipes"
        ],
