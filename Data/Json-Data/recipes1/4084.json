    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13097/wild-rice-soup-i/",
        "name": "Wild Rice Soup I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2100745.jpg",
            "width": 1224,
            "height": 1632
        },
        "datePublished": "1999-02-26T15:20:07.000Z",
        "description": "This soup has a bit of a nutty flavor. It is always a hit.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H20M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "\u2153 cup wild rice",
            "1 tablespoon vegetable oil",
            "4 cups water",
            "1 onion, chopped",
            "1 stalk celery, finely chopped",
            "1 carrot, finely chopped",
            "\u00bd cup margarine",
            "\u00bd cup all-purpose flour",
            "3 cups chicken broth",
            "2 cups half-and-half cream",
            "\u00bd teaspoon dried rosemary",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Rinse rice; drain. In a medium saucepan, combine rice, oil, and water; bring to a boil. Reduce heat, cover, and simmer for 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, in a large pot, cook onion, celery, and carrots in butter until vegetables are almost tender. Blend in flour, cook and stir for 2 minutes. Add  broth and undrained rice, bring to a boil. Cook and stir until slightly thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in cream, rosemary, and salt. Reduce heat and simmer, uncovered, about 20 minutes or until rice is tender.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Chicken Soup Recipes",
            "Chicken and Rice Soup Recipes"
        ],
