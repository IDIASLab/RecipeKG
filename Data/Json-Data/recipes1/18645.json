    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284883/red-velvet-snowflake-cookies/",
        "name": "Red Velvet Snowflake Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F938989.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2012-12-18T20:38:52.000Z",
        "description": "Red velvet cake mix cookies.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT4H55M",
        "recipeYield": "24 cookies",
        "recipeIngredient": [
            "1 (15.25 ounce) package red velvet cake mix",
            "1 cup cake flour",
            "1 cup unsalted butter, melted",
            "2 large eggs",
            "2 tablespoons evaporated milk",
            "2 drops red food coloring",
            "white decorator icing as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cake mix, cake flour, melted butter, eggs, evaporated milk, and food coloring in a large bowl. Mix with an electric mixer on slow speed until fully combined. Cover the bowl with plastic wrap and place in the refrigerator for at least 4 hours, but best if overnight. The longer the dough sits, the easier it is to roll out and cut.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C). Grease a cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough on a lightly floured piece of parchment paper to a thickness of 1/8 or 1/16 inch. Cut cookies with a snowflake-shaped cookie cutter and place on the prepared cookie sheet. Dust off any flour from the top of the cookies with a brush or paper towel.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 6 to 7 minutes. Cool on the baking sheet for 1 minute before removing to a wire rack to cool completely, 25 to 30 minutes more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Decorate fully cooled cookies as desired with icing.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies"
        ],
