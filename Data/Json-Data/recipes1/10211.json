    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18067/herman-doughnuts/",
        "name": "Herman Doughnuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Herman Doughnuts"
        },
        "datePublished": "2000-03-15T15:43:50.000Z",
        "description": "These doughnuts are made with the Herman Starter. They're tender and tangy.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "12 doughnuts",
        "recipeIngredient": [
            "8 cups vegetable oil (for frying)",
            "3 tablespoons shortening",
            "\u00bd cup white sugar",
            "2 eggs, beaten",
            "1 cup Herman Sourdough Starter",
            "\u00bd cup milk",
            "2 cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground nutmeg",
            "2 tablespoons confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, cream together shortening and sugar. Beat in the eggs, Herman starter and milk. In a separate bowl, mix together flour, baking soda, salt and nutmeg. Stir flour mixture into the starter mixture until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Turn dough out onto a well floured surface and roll out or flatten to 1/2 inch thickness. Cut out doughnuts with a doughnut cutter. Cover the donuts and let them rise for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Carefully slide doughnuts into hot oil. Fry until golden brown on both sides. Drain on paper towels, sprinkle with confectioners' sugar and serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes"
        ],
