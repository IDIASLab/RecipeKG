    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13725/pumpkin-scones/",
        "name": "Pumpkin Scones",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F698970.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-09-21T20:19:20.000Z",
        "description": "A crisp pumpkin snack to enjoy with a warm cup of tea.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 to 12 scones",
        "recipeIngredient": [
            "2 tablespoons butter",
            "6 tablespoons white sugar",
            "2 eggs, beaten",
            "2 cups mashed, cooked pumpkin",
            "4 cups self-rising flour, sifted",
            "\u00bc teaspoon ground cinnamon",
            "\u00bc teaspoon ground nutmeg",
            "\u00bd teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, beat together butter, sugar, eggs and pumpkin. Stir in flour, cinnamon, nutmeg and salt by hand.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll out into 1/2 inch thickness and cut into rounds.  Place on tray close together and bake for 15 to 18 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
