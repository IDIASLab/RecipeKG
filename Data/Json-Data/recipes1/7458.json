    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12684/bulk-pie-dough/",
        "name": "Bulk Pie Dough",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F442903.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-10-16T13:59:08.000Z",
        "description": "Is it your turn to make pie for the team? This makes more dough than you can shake a rolling pin at.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 double crust pies",
        "recipeIngredient": [
            "5 pounds sifted all-purpose flour",
            "8\u2009\u00bc cups shortening",
            "3 cups cold water",
            "2 tablespoons salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place flour in a very large mixing bowl. Add shortening and cut it into flour with a pastry blender until  the crumbs are the size of peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve salt in the water and add the water to the flour mixture a small amount at a time mixing it just until the water is absorbed and the dough holds together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover with plastic wrap and refrigerate for four hours. Use 14 oz of dough for a two crust 9 inch pie or 11oz of dough for an 8inch pie.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pie Crusts",
            "Pastry Crusts"
