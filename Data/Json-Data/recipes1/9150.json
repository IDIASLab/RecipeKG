        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15032/cheesy-potatoes-ii/",
        "name": "Cheesy Potatoes II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5109848.jpg",
