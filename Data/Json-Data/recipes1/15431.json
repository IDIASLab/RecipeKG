    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/280116/brownie-pizza/",
        "name": "Brownie Pizza",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Brownie Pizza"
        },
        "datePublished": "2008-08-11T23:44:21.000Z",
        "description": "My son requested this brownie pizza for his birthday instead of cake!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 12-inch pizza",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup unsalted butter",
            "2 (1 ounce) squares unsweetened chocolate",
            "\u00be cup all-purpose flour",
            "2  large eggs eggs, beaten",
            "1 cup confectioners' sugar",
            "\u2153 cup creamy peanut butter",
            "1\u2009\u00bd teaspoons vanilla extract",
            "2 tablespoons whole milk, or more as needed",
            "\u00be cup candy-coated milk chocolate pieces (such as M&M's\u00ae)",
            "\u00bd cup sweetened flaked coconut",
            "\u00bd cup toasted pecans, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sugar, butter, and chocolate in a saucepan over low heat; stir until melted. Remove and stir in flour until smooth. Add eggs and beat until smooth. Spread batter in a 12-inch pizza pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until top is dry and edges have started to pull away from the sides of the pan, about 15 minutes. Let cool, about 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, prepare the frosting. Beat sugar, peanut butter, and vanilla together in a bowl until smooth. Beat in milk. Spread frosting over the cooled brownie. Sprinkle chocolate pieces, coconut, and pecans over the top.\n"
            }
        ],
