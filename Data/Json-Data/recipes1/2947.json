    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11204/soft-sugar-cookies-iii/",
        "name": "Soft Sugar Cookies III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1811988.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1998-11-22T19:49:04.000Z",
        "description": "This recipe makes a soft and chewy drop style sugar cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "1 cup butter",
            "1 cup sour cream",
            "3 eggs",
            "1 teaspoon baking powder",
            "1 teaspoon baking soda",
            "3 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C). Lightly grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the butter and sugar together until light and fluffy. Add the eggs and mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add the flour, baking powder and baking soda alternately with the sour cream to the butter mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto the prepared cookie sheets and bake at 425 degrees F (220 degrees C) for 8 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Sugar Cookie Recipes"
        ],
