    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11108/christmas-cookies-i/",
        "name": "Christmas Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F422.jpg",
            "width": 1712,
            "height": 1368
        },
        "datePublished": "1998-08-30T19:12:10.000Z",
        "description": "My husband grew up with this Christmas cookie.  These cookies freeze well, and will keep a long time.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 dozen",
        "recipeIngredient": [
            "4 cups molasses",
            "2 cups butter",
            "1 teaspoon salt",
            "2 teaspoons ground cloves",
            "2 teaspoons ground allspice",
            "2 teaspoons anise oil",
            "3 cups dried currants",
            "2 cups sour milk",
            "6 cups white sugar",
            "4 eggs",
            "2 teaspoons ground cinnamon",
            "4 cups walnut halves",
            "2 teaspoons ground nutmeg",
            "3 cups raisins",
            "1 (8 ounce) package candied citron peel",
            "1 tablespoon baking soda",
            "16 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Boil molasses and sugar until the sugar dissolves.  Let the syrup cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grind nuts, raisins, currants and citron, and blend together with the butter or lard in a large bowl. Add the cooled syrup.  Mix in the spices, anise oil, eggs, salt, and the sour milk.  Mix in soda and flour.  Rest the dough in the refrigerator for several days.\n"
            },
            {
                "@type": "HowToStep",
                "text": "The dough can be rolled out and cut, or shaped into rolls and sliced.  Place on greased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 10 to 20 minutes.  Cool on wire racks.\n"
            }
        ],
