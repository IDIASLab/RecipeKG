        "review": [
            {
                "@type": "Review",
                "datePublished": "2005-01-21T16:38:15.623Z",
                "reviewBody": "I have been making this dish for years and the key is to use a 9x13 baking dish instead of the pie plate. We sometimes use pizza cheese too instead of just the cheddar.",
                "reviewRating": {
                    "@type": "Rating",
                    "worstRating": "1",
