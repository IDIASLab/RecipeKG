    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/78434/oven-roasted-greek-potatoes/",
        "name": "Oven-Roasted Greek Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1015161.jpg",
            "width": 957,
            "height": 960
        },
        "datePublished": "2004-11-15T10:47:52.000Z",
        "description": "A delicious blend of potatoes, garlic, lemon, and herbs marinated overnight in olive oil and white wine. This is a nice way to dress up the potatoes and is always a hit at my dinner parties.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT9H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 teaspoons lemon pepper",
            "\u00bd teaspoon dried marjoram",
            "1 teaspoon dried basil",
            "\u215b teaspoon dried thyme",
            "1 teaspoon dried rosemary",
            "\u00bc cup white wine",
            "1 cup water",
            "2 tablespoons olive oil",
            "2 tablespoons Italian salad dressing",
            "2 cloves garlic, minced",
            "1 lemon, juiced",
            "1 tablespoon lemon zest",
            "6 medium potatoes, peeled and quartered"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small bowl, mix the lemon pepper, marjoram, basil, thyme, and rosemary. In a separate bowl, mix the wine, water, olive oil, dressing, garlic, lemon juice, lemon zest, and 1/2 the seasoning mixture. Pour into a medium glass baking dish. Place potatoes in the dish, coat with the mixture, and sprinkle with remaining seasonings. Cover, and refrigerate 8 hours, or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake potatoes 1 hour and 15 minutes in the preheated oven, basting once half way through the bake time.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Potato",
            "Roasted Potato Recipes"
        ],
