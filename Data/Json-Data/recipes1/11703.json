        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21252/mojito/",
        "name": "Mojito",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F411996.jpg",
