        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9860/chocolate-scotcheroo-bars/",
        "name": "Chocolate Scotcheroo Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9061773.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-08-06T06:23:29.000Z",
        "description": "A very sweet bar cookie that can be made with Special K&trade; or crispy rice cereal.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "1 - 9x13 inch pan",
        "recipeIngredient": [
            "1 cup white sugar",
            "1 cup light corn syrup",
            "1 cup peanut butter",
            "6 cups crispy rice cereal",
            "1 cup semisweet chocolate chips",
            "1 cup butterscotch chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter a 9x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat, combine the sugar and corn syrup, and bring to a rolling boil. Remove from heat, and stir in peanut butter. Mix in the rice cereal until evenly coated. Press the mixture into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a glass bowl in the microwave, melt the chocolate and butterscotch chips, stirring occasionally until smooth and well blended. Spread over the top of the bars. Chill until set, then cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "No-Bake Cookie Recipes"
        ],
