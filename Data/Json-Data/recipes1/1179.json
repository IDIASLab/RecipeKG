    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10832/old-time-refrigerator-cookies/",
        "name": "Old Time Refrigerator Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Old Time Refrigerator Cookies"
        },
        "datePublished": "1998-04-24T19:28:56.000Z",
        "description": "These will freeze very well up to several months baked or unbaked.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "10 dozen",
        "recipeIngredient": [
            "2 cups butter",
            "1 cup packed brown sugar",
            "1 cup white sugar",
            "2 eggs",
            "6 cups all-purpose flour",
            "1 cup chopped pecans",
            "1 teaspoon baking soda",
            "1 teaspoon salt",
            "1 teaspoon ground cinnamon",
            "\u00bd cup candy-coated milk chocolate pieces",
            "\u00bd cup colored sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream together butter and sugars.  Add beaten eggs.  Mix and sift flour, baking soda, salt, and cinnamon in another bowl.  Add flour mixture to the creamed mixture.  Stir nuts into the dough.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape dough into 6 balls.  Roll out each ball on floured wax paper into a cylinder about the size of a silver dollar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shake chocolate decorative candies or colored sugar onto wax paper.  Roll dough into the sugars or chocolate, enough to coat the sides of the dough.  Roll cylinders in wax paper, and place in the refrigerator overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice rolls into 1/4 inch slices, and place on greased cookie sheets.  Bake at 350 degrees F (175 degrees C) for 8 to 10 minutes.\n"
            }
        ],
