    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15283/orange-cranberry-drops/",
        "name": "Orange Cranberry Drops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F519715.jpg",
            "width": 3072,
            "height": 2304
        },
        "datePublished": "1999-08-02T07:30:58.000Z",
        "description": "A delicious orange cookie with dried cranberries. Beautiful and tasty!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "\u00bd cup white sugar",
            "\u00bd cup packed brown sugar",
            "\u00bc cup butter, softened",
            "1 egg",
            "3 tablespoons orange juice",
            "\u00bd teaspoon orange extract",
            "1 teaspoon grated orange zest",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1 cup dried cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F(190 degrees C). Lightly grease cookie sheets, or line with parchment paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, cream together the white sugar, brown sugar, and butter. Stir in the egg, orange juice, orange extract, and orange zest. Sift together the flour, baking powder, baking soda, and salt; mix into the orange mixture. Stir in the dried cranberries. Drop cookie dough by heaping teaspoonfuls, 2 inches apart, on the prepared cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10 to 12 minutes, or until edges are starting to brown. Cool on baking sheets for 5 minutes, then remove to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Fruit Cookie Recipes",
            "Cranberry"
        ],
