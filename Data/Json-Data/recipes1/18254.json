    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283975/teriyaki-glazed-salmon/",
        "name": "Teriyaki-Glazed Salmon",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Teriyaki-Glazed Salmon"
        },
        "datePublished": "2012-08-10T20:47:49.000Z",
        "description": "I have been consuming salmon for many years, and this is by far my favorite salmon recipe. It's sweet, delicious, and simple to make. Much better than paying a high price at a restaurant!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "2 (5 ounce) salmon fillets",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon ground black pepper",
            "\u00bd teaspoon garlic powder",
            "\u00bd teaspoon dried dill",
            "4 tablespoons light brown sugar",
            "2 tablespoons olive oil",
            "2 tablespoons soy sauce",
            "2 tablespoons teriyaki sauce",
            "2 tablespoons honey",
            "\u00bc cup dry white wine"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Wash salmon fillets under cold water and pat dry with a paper towel. Season with salt, pepper, garlic powder, and dill.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine brown sugar, olive oil, soy sauce, teriyaki sauce, and honey in a mixing bowl. Stir until brown sugar is dissolved. Pour into a large glass or ceramic bowl. Add salmon, skin-side up. Cover the bowl with plastic wrap and marinate in the refrigerator for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Set an oven rack about 6 inches from the heat source and preheat the oven's broiler to high.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove salmon from the marinade, shake off excess, and place on a foil-lined baking sheet, skin-side down. Pour marinade into a small pan and add wine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring marinade-wine mixture to a boil. Reduce heat and simmer until reduced by half, about 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cook salmon under the preheated broiler for 5 minutes. Spoon some of the reduced sauce over the salmon and continue to cook until fish flakes easily with a fork, about 5 more minutes. Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Seafood",
            "Fish",
            "Salmon"
        ],
