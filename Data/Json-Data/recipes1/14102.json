    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23568/chili-con-carne-iii/",
        "name": "Chili con Carne III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F472416.jpg",
            "width": 2144,
            "height": 1424
        },
        "datePublished": "2000-11-03T15:38:35.000Z",
        "description": "Ground beef and kidney beans are seasoned with garlic, oregano, cumin, and chili powder to make a chili that's packed with flavor.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 large onion, finely chopped",
            "1 clove garlic, peeled and crushed",
            "2 teaspoons tomato paste",
            "2 tablespoons butter, cut into pieces",
            "2 tablespoons all-purpose flour",
            "1 teaspoon dried oregano, crushed",
            "\u00bd teaspoon ground cumin",
            "1\u2009\u00bd teaspoons chili powder",
            "1 (14.5 ounce) can whole peeled tomatoes with liquid, chopped",
            "1 pound lean ground beef",
            "1 (15.25 ounce) can kidney beans, drained and rinsed",
            "salt to taste",
            "ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large skillet over medium heat, combine onion, garlic, and tomato paste. Cover and cook for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in butter until melted. Stir in flour, oregano, cumin, chili powder, tomatoes and beef. Cook uncovered for 8 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the beans and cook for another 4 minutes, stirring occasionally. Salt and pepper to taste.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chili Recipes",
            "Beef Chili Recipes"
        ],
