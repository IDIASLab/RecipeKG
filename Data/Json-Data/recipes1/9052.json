    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15674/grilled-caribbean-chicken/",
        "name": "Grilled Caribbean Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8591743.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-01-03T23:00:59.000Z",
        "description": "Chicken breasts with a fruity marinade. This recipe is from The WEBB Cooks, articles and recipes by Robyn Webb, courtesy of the American Diabetes Association. Preparation time: 10 minutes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "3 skinless, boneless chicken breasts",
            "\u00bc cup orange juice",
            "2 tablespoons fresh lime juice",
            "2 tablespoons mango chutney",
            "2 teaspoons grated fresh ginger ",
            "1 tablespoon olive oil",
            "\u00bd teaspoon hot pepper sauce",
            "1 teaspoon minced fresh oregano",
            "2 cloves garlic, minced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a nonporous glass dish or bowl combine the orange juice, lime juice, chutney, ginger, oil, pepper sauce, oregano and cloves. Mix all together and add chicken. Toss to coat, cover dish and place in refrigerator. Marinate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat grill to medium high heat or set oven to broil\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from dish (dispose of remaining marinade) and grill or broil the chicken 6 inches from the heat source.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Breast"
