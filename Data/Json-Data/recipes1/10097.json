    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16469/blonde-brownies-iv/",
        "name": "Blonde Brownies IV",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F139370.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-03-08T23:30:59.000Z",
        "description": "Wonderful golden brownie with chocolate chips. My mom made these when I was a girl and they are still my favorite.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00bd cup margarine, melted",
            "1 cup brown sugar",
            "\u00bd cup white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bc teaspoon baking soda",
            "2 cups semisweet chocolate chips",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 325 degrees F (165 degrees C). Grease a 9x13 inch pan or two 8x8 inch pans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the melted margarine, brown sugar and white sugar until smooth. Beat in the eggs one at a time, then stir in the vanilla. Combine the flour, baking powder, and baking soda , gradually stir into the creamed mixture. Spread evenly into the prepared pan, and sprinkle the chocolate chips and pecans over the  top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 12 to 15 minutes in the preheated oven until surface of cookies appears dry. Cool, and cut into squares.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Brownie Recipes",
            "Nut Brownie Recipes"
        ],
