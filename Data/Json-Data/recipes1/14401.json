    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281462/pork-chop-foil-packets/",
        "name": "Pork Chop Foil Packets",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pork Chop Foil Packets"
        },
        "datePublished": "2004-05-05T05:13:01.000Z",
        "description": "A delicious blend of pork and vegetables topped with a creamy mushroom and onion sauce and baked in foil.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "aluminum foil",
            "1  serving cooking spray",
            "1 (10.5 ounce) can condensed cream of mushroom soup",
            "1 (1 ounce) package dry onion soup mix",
            "\u00bc teaspoon ground black pepper",
            "4 medium russet potatoes, peeled and cut into 1/4-inch slices",
            "4 (4 ounce) boneless pork chops",
            "2 carrots, peeled and cut into 1/2-inch slices",
            "1 cup mushrooms, sliced",
            "2 cups fresh green beans, trimmed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C). Tear 4 large pieces of aluminum foil, each about 14 inches long. Spray each lightly with cooking spray.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix condensed soup, onion soup mix, and pepper together in a small bowl until well blended.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dividing ingredients evenly, place some sliced potatoes on the center of each piece of foil, followed by a pork chop, carrots, mushrooms, green beans, and the condensed soup mixture. Fold the packets by bringing the 2 long ends of foil together and folding over twice to make a seal. Seal the short ends by folding each end over 2 to 3 times; this allows the meat and vegetables to steam. Place the packets on a large cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until chops are no longer pink in the center and potatoes are tender, about 30 minutes. Allow packets to sit for 5 minutes before opening.\n"
            }
        ],
