    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13701/cheesy-green-bean-casserole/",
        "name": "Cheesy Green Bean Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F25772.jpg",
            "width": 2304,
            "height": 1728
        },
        "datePublished": "1999-05-06T19:22:19.000Z",
        "description": "I am a real cheese lover, and I have found it to be a great compliment to our traditional green bean casserole!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 (10.75 ounce) cans  condensed cream of mushroom soup",
            "\u00be cup milk",
            "1 (2.8 ounce) can French fried onions",
            "2 cups shredded Cheddar cheese",
            "2 (16 ounce) packages frozen French cut green beans",
            "\u00bd pound processed cheese food (eg. Velveeta), sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease a 2 quart casserole dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, combine mushroom soup, milk, cheddar cheese, and half of the onions. Mix together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Layer the bottom of a 2 quart casserole dish with 1/2 of the green beans. Pour half of the soup mixture over beans, and add half of the processed cheese. Repeat layering, and top with remaining onions.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 to 40 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Casseroles",
            "Green Bean Casserole Recipes"
        ],
