    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7978/bookbinders-fabulous-cheesecake/",
        "name": "Bookbinder's Fabulous Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5285295.jpg",
            "width": 1936,
            "height": 1936
        },
        "datePublished": "1998-09-03T15:39:17.000Z",
        "description": "A friend gave me this delicious lemon-flavored cheesecake recipe.  Simple to make, and a favorite ending for a special meal.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 - 9 inch cheesecake",
        "recipeIngredient": [
            "2 tablespoons graham cracker crumbs",
            "4 (8 ounce) packages cream cheese, softened",
            "1 cup white sugar",
            "4 eggs",
            "1 teaspoon fresh lemon juice",
            "\u00bd teaspoon vanilla extract",
            "\u00bc teaspoon lemon zest",
            "\u2154 cup apricot preserves",
            "2 tablespoons water",
            "4 cups fresh strawberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Sprinkle the bottom of a lightly greased 9 inch springform pan with the graham cracker crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the cream cheese and sugar.  Mix at medium speed until well blended.  Add eggs, one at a time, mixing well after each addition.  Blend in the lemon juice, vanilla extract ,and lemon rind.  Pour batter into the pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated 325 degrees F (165 degrees C) for 50 minutes.  Loosen cake from rim of pan; cool before removing rim of pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine apricot preserves and water in a small saucepan.  Heat thoroughly, stirring occasionally.  Arrange strawberries on top of the cheesecake.  Spoon preserves mixture over strawberries.  Chill.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Cheesecake Recipes"
