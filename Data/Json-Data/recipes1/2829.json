        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13691/green-bean-casserole-iii/",
        "name": "Green Bean Casserole III",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4209365.jpg",
            "width": 1280,
