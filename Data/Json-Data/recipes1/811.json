        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10585/marble-bars/",
        "name": "Marble Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F226164.jpg",
