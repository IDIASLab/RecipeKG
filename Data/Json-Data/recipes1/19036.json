        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Slice the rolls in half and spread butter on both sides.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Garnish one half with a few leaves of lettuce and a slice of tomato.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top the tomato with the slices of ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut the Maille&reg; cornichons in 4, and dice them, then place them on top of the slices of ham.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle shallots and parsley to taste.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Top with other half of roll and serve.\n"
            }
