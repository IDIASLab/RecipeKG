    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16485/barbs-broccoli-cauliflower-salad/",
        "name": "Barb's Broccoli-Cauliflower Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F402749.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-01-19T17:38:33.000Z",
        "description": "This is a version of the classic broccoli salad with the addition of cauliflower. It can be prepared the day prior to serving, then served chilled or at room temperature.  Pecans can be used instead of sunflower seeds, if desired, and bacon lovers might want to add a few extra slices. . .",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "12 slices bacon",
            "1 head fresh broccoli, diced",
            "1 head cauliflower, chopped",
            "\u00bd red onion, diced",
            "\u00be cup sunflower seeds",
            "1 cup creamy salad dressing",
            "1\u2009\u00bd tablespoons white wine vinegar",
            "\u00bc cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain, crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the bacon, cauliflower, broccoli, onion and sunflower seeds or pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the salad dressing, vinegar and sugar. Pour over salad and toss to coat. Refrigerate and allow to chill before serving.\n"
            }
        ],
        "recipeCategory": [
            "Salad",
            "Vegetable Salad Recipes",
            "Broccoli Salad Recipes"
        ],
