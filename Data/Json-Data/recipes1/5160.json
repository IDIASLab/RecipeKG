        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/8239/good-for-you-cheesecake/",
        "name": "Good For You Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5201496.jpg",
            "width": 3301,
            "height": 3303
        },
        "datePublished": "1999-05-12T20:04:41.000Z",
        "description": "Quick, easy, and lower in fat than most traditional cheesecakes.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 8x8-inch pan",
        "recipeIngredient": [
            "3 (8 ounce) packages Neufchatel cheese",
            "1 cup white sugar",
            "4 eggs",
            "2 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Butter an 8x8 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix cheese and sugar until soft. Add eggs one at a time, mixing thoroughly with each addition. Add vanilla and mix to incorporate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into prepared 8x8 inch pan. If desired, sprinkle top lightly with cinnamon. Bake at 350 degrees F (175 degrees C) for 30 minutes. Cake will appear to be soft in the center. Allow to cool and it will set.\n"
            }
        ],
        "recipeCategory": [
