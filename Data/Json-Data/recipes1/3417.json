        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13070/emmas-clam-chowder/",
        "name": "Emma's Clam Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F917488.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-01-08T03:18:19.000Z",
        "description": "This is a Manhattan type clam chowder. Using your garden fresh or frozen tomatoes gives it a special flavor.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H50M",
        "totalTime": "P0DT2H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "5 (6.5 ounce) cans minced clams, drained",
            "1 large potato, diced",
            "1 onion, chopped",
            "\u00bd teaspoon salt",
            "1 pinch ground white pepper",
            "4 slices bacon",
            "1 large carrot, grated",
            "8 ounces tomato sauce",
            "2 tomatoes, chopped",
            "1 stalk celery, chopped",
            "2 cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Dice bacon, cook in large saucepan with onion and celery until crisp and onion golden. Pour off most of fat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add 2 cups water, potatoes, carrots, salt, pepper, thawed tomatoes, and clam juice from cans. Simmer 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add tomato sauce and clams. Simmer 15-20 minutes or until just boiling.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Chowders",
            "Clam Chowder Recipes"
        ],
