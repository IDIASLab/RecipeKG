        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11801/spaghetti-sauce-i/",
        "name": "Spaghetti Sauce I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F257831.jpg",
