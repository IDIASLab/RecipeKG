    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18937/ginger-lemonade/",
        "name": "Ginger Lemonade",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1012494.jpg",
            "width": 1050,
            "height": 1050
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "What a great twist on lemonade. Freeze some of this into ice cubes to use in the drinks, so it won't be diluted!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H5M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "20 servings",
        "recipeIngredient": [
            "3 cups white sugar",
            "4 quarts water",
            "14 slices fresh ginger root",
            "4 cups fresh lemon juice",
            "2 lemons, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In an 8-quart saucepan combine sugar, water and ginger root. Heat to boiling, stirring occasionally. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in lemon juice. Cool 15 minutes. Remove ginger. Refrigerate lemonade at least 1 hour, or until chilled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Serve over ice, and garnish with lemon slices.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Lemonade Recipes"
        ],
