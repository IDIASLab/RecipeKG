        "totalTime": null,
        "recipeYield": "20 chestnuts",
        "recipeIngredient": [
            "\u00bc cup shoyu (soy and wheat soy sauce)",
            "2 tablespoons white sugar",
            "2 (5 ounce) cans water chestnuts, drained",
            "10 slices bacon, cut in half"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine shoyu sauce and sugar. Stir in water chestnuts. Marinate chestnuts overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Wrap bacon slice around each chestnut. Fasten with a wooden pick. Place on a rack in shallow baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 30-35 minutes. Serve hot.\n"
            }
        ],
