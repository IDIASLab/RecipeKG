        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13213/simply-delicious-rice-soup/",
        "name": "Simply Delicious Rice Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F70165.jpg",
