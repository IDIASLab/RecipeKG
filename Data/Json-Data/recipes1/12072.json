    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18304/cucumber-slices-with-dill/",
        "name": "Cucumber Slices With Dill",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F629845.jpg",
            "width": 2288,
            "height": 1712
        },
        "datePublished": "2000-06-27T13:32:18.000Z",
        "description": "This cool cucumber salad is quick and easy to prepare. It does require at least 2 hours in the fridge for the fullest flavor so plan ahead!",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT2H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "4 large  cucumbers, sliced",
            "1 onion, thinly sliced",
            "1 tablespoon dried dill weed",
            "1 cup white sugar",
            "\u00bd cup white vinegar",
            "\u00bd cup water",
            "1 teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large serving bowl, combine cucumbers, onions and dill. In a medium size bowl combine sugar, vinegar, water and salt; stir until the sugar dissolves. Pour the liquid mixture over the cucumber mixture. Cover and refrigerate at least 2 hours before serving (the longer this dish marinates the tastier it is!).\n"
            }
        ],
