    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10629/michelles-peanut-butter-dots/",
        "name": "Michelle's Peanut Butter Dots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8092561.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-02-26T01:41:41.000Z",
        "description": "PERFECT, QUICK, and SIMPLE COOKIES. Perfect for the bachelor trying to impress a woman. Also makes a quick, yet gratifying snack.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "1 cup peanut butter",
            "\u00bc cup milk",
            "1 teaspoon vanilla extract",
            "2 cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large mixing bowl, combine sugar, butter, peanut butter, milk and vanilla.  Mix by hand about 3 minutes or until creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Thoroughly mix flour into creamy mix. Roll small balls of dough in hand, about 1 inch in diameter.  Place on greased cookie sheet, approximately 3/4 inch apart. Using a fork, make criss cross pattern on cookie  (this will flatten cookie partially). Be careful not to over flatten, dough should not touch each other.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) 18 to 25 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Peanut Butter Cookie Recipes"
        ],
