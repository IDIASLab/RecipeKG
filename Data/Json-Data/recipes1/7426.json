        "recipeYield": "5 servings",
        "recipeIngredient": [
            "\u00bd cup dry lentils",
            "1\u2009\u00bd cups water",
            "\u00bd (15 ounce) can garbanzo beans, drained",
            "2 tomatoes, chopped",
            "4 green onions, chopped",
            "2 minced hot green chile peppers",
            "1 green bell pepper, chopped",
            "\u00bd yellow bell pepper, chopped",
            "1 red bell pepper, chopped",
            "1 lime, juiced",
            "2 tablespoons olive oil",
            "\u00bc cup chopped fresh cilantro",
            "salt to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place lentils and water in a pot. Bring water to boil, reduce to simmer. Cook for 30 minutes or until tender.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium size mixing bowl combine lentils, chickpeas, tomatoes, green onions, green chilies, bell peppers, lime juice, olive oil, cilantro, and salt to taste. Toss well. Chill for 20 minutes. Serve chilled.\n"
            }
        ],
