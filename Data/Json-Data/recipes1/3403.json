        "name": "Vegetable Beef Soup I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F380829.jpg",
            "width": 250,
            "height": 250
