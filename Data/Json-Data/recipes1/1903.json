    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/6743/english-muffin-loaves/",
        "name": "English Muffin Loaves",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9296771.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-06-27T20:41:09.000Z",
        "description": "This is an easy yeast bread--no kneading required!  The texture is like English muffins, great for toasting.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "2 (.25 ounce) packages active dry yeast",
            "6 cups all-purpose flour",
            "1 tablespoon white sugar",
            "2 cups milk",
            "\u00bc teaspoon baking soda",
            "\u00bd cup water",
            "1 teaspoon salt",
            "2 tablespoons cornmeal"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small saucepan, heat the milk and water until very warm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine 3 cups flour, yeast, sugar, salt, and soda in a large bowl.   Add  the warm milk and water, and beat well.  Stir in enough of the remaining flour to make a stiff batter.  Spoon into two 9 x 5 inch loaf pans that have been greased and dusted with the cornmeal.  Cover, and let rise in a warm place for 45 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 400 degrees F (205 degrees C) for 25 minutes.  Remove from pans immediately, and let cool.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Yeast Bread Recipes",
            "White Bread Recipes"
        ],
