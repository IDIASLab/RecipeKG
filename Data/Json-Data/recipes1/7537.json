        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14790/spanish-pork-with-white-wine/",
        "name": "Spanish Pork with White Wine",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F805861.jpg",
