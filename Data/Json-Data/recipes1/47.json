        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9849/forgotten-cookies-i/",
        "name": "Forgotten Cookies I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F798253.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1997-08-04T19:43:31.000Z",
        "description": "These cookies are left overnight in the oven.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 egg whites",
            "1 teaspoon vanilla extract",
            "1 pinch salt",
            "1 cup semisweet chocolate chips",
            "\u00bc cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (170 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites with the salt until foamy.  Slowly add sugar, one tablespoonful at a time, beating after each addition  until the meringue stands in stiff peaks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in vanilla and fold in chocolate chips.  Drop by teaspoonfuls on a lined baking sheet and place in preheated oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "After 2 minutes, turn off oven.  Leave meringues in oven overnight. Remove from pan and store in cookie tin  lined with paper towel.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
