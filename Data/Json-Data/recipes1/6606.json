    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11429/brownie-cookies/",
        "name": "Brownie cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F12029.jpg",
            "width": 2560,
            "height": 1920
        },
        "datePublished": "1999-08-25T13:29:45.000Z",
        "description": "Cookies and brownies in one! Could life get any better? This dough requires chilling the dough overnight, keep this in mind while planning your cookie baking.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00be cup vegetable oil",
            "2 cups white sugar",
            "\u2154 cup cocoa powder",
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "4 eggs",
            "2 teaspoons vanilla extract",
            "\u00bd cup chopped walnuts",
            "\u00bc cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a small mixing bowl combine flour and baking powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat oil, sugar, and cocoa together in a large mixing bowl. Add eggs to oil and cocoa mixture one egg at a time, beating well after each addition. Stir in vanilla. Mix in flour mixture. Fold in nuts if desired. Chill the dough overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into walnut-size balls and roll each ball in confectioners' sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake cookies for 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
