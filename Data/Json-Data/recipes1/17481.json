        "mainEntityOfPage": "https://www.allrecipes.com/recipe/218219/chili-cream-cheese-frosting/",
        "name": "Chili Cream Cheese Frosting",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F662459.jpg",
            "width": 2640,
            "height": 1980
