    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19851/turkey-bolognese-sauce/",
        "name": "Turkey Bolognese Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1352.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-06-22T16:53:06.000Z",
        "description": "Delicious ground turkey meat recipe.  Excellent sauce.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT3H20M",
        "totalTime": "P0DT3H30M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "2 pounds ground turkey",
            "2 onions, minced",
            "4 cloves garlic, minced",
            "\u00be cup grated carrots",
            "1\u2009\u00bd teaspoons dried basil",
            "2 tablespoons minced jalapeno peppers",
            "1 cup milk",
            "1\u2009\u00bd cups white wine",
            "2 (28 ounce) cans whole peeled tomatoes",
            "1 tablespoon tomato paste",
            "1 pound spaghetti",
            "\u00bd cup grated Parmesan cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In large saucepan over medium heat, cook turkey, onion, garlic, carrot, basil and jalapeno until turkey is brown.  Pour in milk, reduce heat to low, and simmer until reduced by one-third.  Stir in wine and reduce again.  Pour in tomatoes and tomato paste and simmer 3 hours more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.  Toss with tomato sauce and top with Parmesan.  Serve.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Turkey",
            "Ground Turkey Recipes"
        ],
