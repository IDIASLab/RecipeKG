    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15377/christmas-strawberries/",
        "name": "Christmas Strawberries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F7435840.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-09-10T17:21:53.000Z",
        "description": "These strawberries are made from strawberry flavored gelatin, coconut and colored sugar crystals.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 (6 ounce) packages strawberry flavored Jell-O\u00ae",
            "1 (14 ounce) can sweetened condensed milk",
            "2 cups flaked coconut",
            "2 teaspoons vanilla extract",
            "\u00bd cup red decorator sugar",
            "\u00bc cup green decorator sugar",
            "2 ounces slivered almonds"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Blend together the gelatin and condensed milk. Add coconut and vanilla; mix well and refrigerate until chilled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll into small balls and shape into strawberries; roll in red sugar crystals.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip fat end into green sugar crystals.  Place an almond sliver into the fat end for a stem.  To prevent hardening, store in an air tight container until ready to serve.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
