    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21185/baked-salmon-i/",
        "name": "Baked Salmon I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F701355.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-05-29T18:54:31.000Z",
        "description": "A delicious, healthy dinner of salmon with herbs and rice.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 cup brown rice",
            "2\u2009\u00bd cups water",
            "1 pound salmon fillet",
            "\u00bc cup orange juice",
            "1 teaspoon dried dill weed",
            "1 teaspoon dried rosemary",
            "1 teaspoon dried basil",
            "1 teaspoon dry mustard",
            "1 teaspoon lemon pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). In a saucepan bring 2 1/2 cups of water to a boil. Add rice and stir. Reduce heat, cover and simmer for 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large pan, add enough water to just cover the bottom of the pan.  Lay the salmon fillet in the pan, pink side up. Place cooked rice around the outside of the fish. Sprinkle the orange juice over the fish and rice.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine the dill weed, rosemary, basil, mustard, lemon pepper and sprinkle over the fish and rice. Cover with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated oven for 30 to 40 minutes or until the salmon is tender and flaky.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Seafood Main Dishes",
            "Salmon",
            "Baked Salmon Recipes"
        ],
