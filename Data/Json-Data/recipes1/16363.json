    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282984/ham-and-apple-fritters/",
        "name": "Ham and Apple Fritters",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Ham and Apple Fritters"
        },
        "datePublished": "2010-10-31T19:31:21.000Z",
        "description": "These are actually an invention of my mother's. Inspired by fall season ingredients, a perfect sweet and savory snack for a football Sunday.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 medium Granny Smith apple ",
            "1 medium Macintosh apple",
            "2 cups pancake mix",
            "1\u2009\u00bc cups water",
            "1 cup diced cooked ham",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "\u00bc teaspoon ground nutmeg",
            "1 quart vegetable oil for frying, or as needed",
            "2 tablespoons powdered sugar, or as needed"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel and core both apples, then grate into a bowl. Add pancake mix, water, ham, cinnamon, salt, and nutmeg; mix until fully combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat 3 inches oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Working in batches, drop teaspoonfuls of batter into the hot oil and dry until golden brown and cooked through, 2 to 3 minutes. Try not to overcrowd the pan or let the fritters touch each other. Remove to a paper towel-lined plate and repeat to fry remaining fritters.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle with powdered sugar and serve.\n"
            }
        ],
