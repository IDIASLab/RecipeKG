    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232036/butterfinger-creme-brulee/",
        "name": "Butterfinger Creme Brulee",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1000672.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2013-03-27T19:35:47.000Z",
        "description": "Butterfinger pieces are the surprise ingredient at the bottom of these fun brulees.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT1H35M",
        "totalTime": "P0DT5H5M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "16 pieces Butterfinger\u00ae Bites, chopped, divided",
            "2 cups heavy whipping cream",
            "10 tablespoons granulated sugar, divided",
            "\u215b teaspoon salt",
            "4 large egg yolks",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F. Place about 1 tablespoon chopped Butterfinger into each of four 6-ounce ramekins or custard cups.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream, 7 tablespoons sugar and salt in medium saucepan. Cook over medium heat, stirring occasionally, for 5 to 6 minutes or until sugar is dissolved. Beat egg yolks in medium bowl; gradually whisk cream mixture into egg yolks. Stir in vanilla extract.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour cream mixture through a fine sieve into prepared cups. Place cups in 9-inch-square baking pan; fill pan with hot water to 1-inch depth. Loosely cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for about 1 hour, 30 minutes or until knife inserted in centers comes out clean. Remove to wire rack to cool slightly. Refrigerate for several hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle each creme brulee evenly with remaining sugar. Broil until sugar is melted and caramelized. Refrigerate for 10 to 15 minutes or until caramel has hardened.\n"
            }
        ],
