    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11411/buckaroons/",
        "name": "Buckaroons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F721928.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-08-31T19:54:38.000Z",
        "description": "My Grandma Lola used to make these cookies every time we would visit. They are even good the second day. Just pop them in the microwave for 30 seconds!!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking soda",
            "\u00be teaspoon salt",
            "\u00be teaspoon baking powder",
            "1\u2009\u00bd cups butter",
            "1\u2009\u00bd cups white sugar",
            "1\u2009\u00bd cups packed brown sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "3 eggs",
            "4 cups semisweet chocolate chips",
            ".3 cup wheat germ",
            "3 cups rolled oats"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium size mixing bowl sift together flour, baking soda, salt and baking powder.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl cream butter, white and brown sugars together until light and fluffy. Dribble vanilla into the creamed mixture. Beat the eggs, one at a time, into the mixture. Add the sifted ingredients to the creamed mixture. Beat until well mixed.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour chocolate chips, wheat germ and oats into the mixture; fold into dough until well combined. Drop the dough by rounded teaspoonful onto an ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 8 to 10 minutes or until the edges are just slightly brown. The cookies will look undercooked but that is the correct consistency. Cool on a wire rack.\n"
            }
        ],
