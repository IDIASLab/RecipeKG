        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12479/bacon-pie/",
        "name": "Bacon Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5275510.jpg",
            "width": 3024,
            "height": 3025
        },
        "datePublished": "1998-11-23T09:36:05.000Z",
        "description": "Good, easy to make. Welcome at breakfast, brunch, or supper.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 10-inch pie",
        "recipeIngredient": [
            "12 slices bacon",
            "1 cup shredded Swiss cheese",
            "\u2153 cup chopped onion",
            "2 cups milk",
            "4 eggs",
            "1 cup baking mix",
            "\u215b teaspoon ground black pepper"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oven to 400 degrees F (205 degrees C). Grease a 10-inch glass pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Pat dry on paper towels and crumble the bacon. Sprinkle bacon, cheese, and onion in the pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir the milk, eggs, baking mix, and pepper with fork until blended. Pour into pie plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake 35 to 40 minutes or until knife inserted in center comes out clean.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Eggs"
        ],
