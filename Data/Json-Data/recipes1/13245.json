        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21365/tasty-sweet-potatoes/",
        "name": "Tasty Sweet Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F318027.jpg",
