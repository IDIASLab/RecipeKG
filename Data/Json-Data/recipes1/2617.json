        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11105/icebox-cookies-ii/",
        "name": "Icebox Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Icebox Cookies II"
        },
        "datePublished": "1998-10-07T20:26:36.000Z",
        "description": "An easy icebox cookie recipe of my mothers. This recipe is credited to a Mrs. E.S. Cryan of Oakland, CA.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2\u2009\u215b cups shortening",
            "1 cup white sugar",
            "5 cups all-purpose flour",
            "1 tablespoon cream of tartar",
            "1 tablespoon ground cinnamon",
            "1 tablespoon vanilla extract",
            "1 cup packed brown sugar",
            "2 eggs",
            "1 teaspoon salt",
            "1 tablespoon baking soda",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream shortening with sugars.  Mix in beaten eggs.  Sift together flour, cream of tartar, baking soda, salt, and cinnamon; mix into creamed mixture.  Stir in nuts and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Make rolls about 2 inches in diameter.  Refrigerate overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice about 3/8 inch thick, and place on greased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 12 to 15 minutes.\n"
            }
        ],
