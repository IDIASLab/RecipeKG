    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11308/pecan-pie-surprise-bars/",
        "name": "Pecan Pie Surprise Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1031272.jpg",
            "width": 1320,
            "height": 1099
        },
        "datePublished": "1999-03-22T15:41:46.000Z",
        "description": "Here's your pecan pie and cookie too!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "\u00bd cup butter, melted",
            "1 egg",
            "\u00bd cup packed brown sugar",
            "1\u2009\u00bd cups dark corn syrup",
            "1 teaspoon vanilla extract",
            "3 eggs",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease one 9 x13 inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reserve 2/3 cup of the cake mix and set aside. In a large bowl combine the remaining cake mix, melted butter and 1 egg. Mix until crumbly then press mixture into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 15 to 20 minutes, until light golden brown.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the reserved 2/3 cup cake mix, brown sugar, corn syrup, vanilla and 3 eggs. Beat at medium speed for 1 to 2 minutes. Pour filling over the baked crust and sprinkle with pecans.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to oven and bake for an additional 30 to 35 minutes, until the filling is set. Cool and cut into 36 bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
