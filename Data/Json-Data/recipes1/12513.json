        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19830/fudge-bonbons/",
        "name": "Fudge Bonbons",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F74153.jpg",
            "width": 250,
            "height": 250
