        "name": "Red, White and Blue Strawberry Shortcake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3717320.jpg",
            "width": 2406,
