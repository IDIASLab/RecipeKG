        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23104/zucchini-chive-dip/",
        "name": "Zucchini Chive Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3769544.jpg",
            "width": 1944,
            "height": 1944
