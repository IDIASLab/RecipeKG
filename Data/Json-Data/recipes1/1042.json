    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10734/ginger-snaps-i/",
        "name": "Ginger Snaps I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F28025.jpg",
            "width": 2610,
            "height": 1964
        },
        "datePublished": "1998-03-30T07:33:36.000Z",
        "description": "Rolled ginger snaps.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 - 4 dozen",
        "recipeIngredient": [
            "3 cups all-purpose flour",
            "1 tablespoon ground ginger",
            "1 teaspoon salt",
            "\u00be cup shortening",
            "\u00bd cup white sugar",
            "1 cup molasses"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine flour, ginger and salt and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream shortening and sugar.  Beat in the molasses.  Gradually blend in the dry ingredients.  Cover and chill for four hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).  Lightly grease 2 cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On floured surface, roll out dough to 1/4 inch thickness. Using a 1-3/4 inch cutter, cut into rounds and place 1 inch apart on cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 10-12 minutes until lightly colored and firm to the touch.  Cool cookies on wire racks.\n"
            }
        ],
