    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9404/georgia-spiced-peaches/",
        "name": "Georgia Spiced Peaches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F40870.jpg",
            "width": 1016,
            "height": 679
        },
        "datePublished": "1999-09-02T02:29:43.000Z",
        "description": "This is an old, old recipe from relatives in Georgia.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "19 pounds",
        "recipeIngredient": [
            "19 pounds firm ripe peaches",
            "7 pounds white sugar",
            "2 cups distilled white vinegar",
            "1 quart water",
            "24 whole cloves",
            "3 tablespoons crushed cinnamon stick"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Peel peaches and set aside. In a large pot over medium high heat, boil sugar, vinegar and water until the syrup is moderately thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add cloves, cinnamon and peaches. Bring to boil stirring occasionally until the peaches can be pierced to the pit with a fork.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill sterilized canning jars with peaches.  Continue boiling syrup until heavy and add to peaches to cover.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large stock pot, pour water half way to top with boiling water. Using a holder, carefully lower jars into pot. Leave a 2-inch space between jars. Add more boiling water to cover them, about 2 inches above the tops.  Bring to a boil and cover, processing for 35 minutes. Remove jars from pot. Put jars on a wood or cloth surface, several inches apart and allow to cool. Jars will be sealed.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Peach Dessert Recipes"
        ],
