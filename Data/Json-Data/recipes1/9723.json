    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17505/simple-peanut-squash/",
        "name": "Simple Peanut Squash",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F571591.jpg",
            "width": 4272,
            "height": 2848
        },
        "datePublished": "2000-02-13T20:05:53.000Z",
        "description": "For those that don't yet have a taste for squash, here is an easy introduction. My wife once hated squash now this is her favorite.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "2 servings",
        "recipeIngredient": [
            "1 butternut squash",
            "2 tablespoons brown sugar",
            "\u00bc cup butter",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Peel and cut the peanut squash into 2 inch pieces. Place the pieces into a loaf pan or other deep and narrow baking container. Sprinkle the brown sugar and salt over the pieces and drop the  butter on top. Cover the pan with aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 375 degrees F (190 degrees C) for 35 minutes. Serve hot and enjoy!\n"
            }
        ],
