    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10846/praline-butter-cookies/",
        "name": "Praline Butter Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Praline Butter Cookies"
        },
        "datePublished": "1998-04-30T14:08:47.000Z",
        "description": "Butter cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "1 cup white sugar",
            "2 egg yolks",
            "\u00bd teaspoon vanilla extract",
            "2 tablespoons praline liqueur",
            "2 cups all-purpose flour",
            "\u00bc teaspoon salt",
            "1 cup pecan halves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter; gradually add sugar, beating well at medium speed of an electric mixer. Add egg yolks, one at a time, beating after each addition. Stir in vanilla and praline liqueur.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour, baking powder, and salt; gradually add to creamed mixture, mixing well. Shape dough into 1 inch balls; place 2 inches apart on ungreased cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press a pecan half into center of each cookie. Bake at 300 degrees F (150 degrees C) for 20 minutes or until lightly browned. Cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Specialty Dessert Recipes",
            "Praline Recipes"
        ],
