    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284324/red-white-and-blue-4th-of-july-cheesecake/",
        "name": "Red, White, and Blue 4th of July Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Red, White, and Blue 4th of July Cheesecake"
        },
        "datePublished": "2013-07-03T17:01:51.000Z",
        "description": "This is a great recipe for the 4th of July and kids love it. It is made from the best parts of a bunch of different recipes.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H55M",
        "totalTime": "P0DT10H10M",
        "recipeYield": "1 9-inch cake",
        "recipeIngredient": [
            "2 cups crushed vanilla wafers (such as Nilla\u00ae)",
            "6 tablespoons unsalted butter",
            "3 (8 ounce) packages cream cheese, at room temperature",
            "\u00bd cup white sugar",
            "1\u2009\u00bd teaspoons vanilla extract",
            "2 large eggs",
            "\u00bd cup fresh strawberries",
            "\u00bd cup fresh blueberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan. Remove from the heat and mix in wafer crumbs until combined. Press mixture into the bottom of a 9-inch springform pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the pan on a cookie sheet in the preheated oven and bake for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine cream cheese, sugar, and vanilla in a large bowl; beat with an electric mixer until smooth and creamy. Add eggs, one at a time, beating well after each addition.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove crust from the oven and pour cream cheese mixture over top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return to the oven and bake until the edges are puffed and the surface of cheesecake is firm except for a small spot in the center that will jiggle when the pan is gently shaken, about 40 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from the oven. Run the tip of a table knife around the edges of the pan, set the pan on a wire rack, and allow the cheesecake to cool at room temperature, about 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Decorate the top with strawberries and blueberries and refrigerate until completely chilled, 8 hours to overnight.\n"
            }
        ],
        "recipeCategory": [
            "Holidays and Events Recipes",
            "4th of July",
            "Desserts"
        ],
