    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20896/jalapeno-hot-sauce/",
        "name": "Jalapeno Hot Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5855823.jpg",
            "width": 960,
            "height": 960
        },
        "datePublished": "2000-08-27T19:37:30.000Z",
        "description": "Hot Sauce from scratch.  You may use other hot peppers in place of the jalapenos.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H25M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "1 teaspoon vegetable oil",
            "20 fresh jalapeno peppers, sliced",
            "3 cloves garlic, minced",
            "\u00bd cup minced onion",
            "\u00be teaspoon salt",
            "2 cups water",
            "1 cup distilled white vinegar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium glass or enamel lined sauce pan over high heat, combine oil, peppers, garlic, onion and salt; saute for 4 minutes.  Add the water and cook for 20 minutes, stirring often.  Remove from heat and allow mixture to cool to room temperature.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Transfer the mixture to a food processor and puree until smooth.  With the processor running, slowly add the vinegar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a sterilized jar with a tight lid.  This sauce will keep for 6 months when stored in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces"
        ],
