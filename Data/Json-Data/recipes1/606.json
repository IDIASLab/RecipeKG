        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10400/brownie-oat-cookies/",
        "name": "Brownie Oat Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4476467.jpg",
            "width": 2322,
            "height": 4128
