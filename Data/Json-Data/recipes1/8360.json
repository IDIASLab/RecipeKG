        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16527/dried-beef-ball/",
        "name": "Dried Beef Ball",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2892364.jpg",
