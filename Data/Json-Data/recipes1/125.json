    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9918/gumdrop-bars/",
        "name": "Gumdrop Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6037421.jpg",
            "width": 2305,
            "height": 2305
        },
        "datePublished": "1997-08-23T17:00:42.000Z",
        "description": "Great bar cookie.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "2 cups sifted all-purpose flour",
            "1 teaspoon baking powder",
            "\u00bd teaspoon salt",
            "4 eggs",
            "1 cup white sugar",
            "1 teaspoon vanilla extract",
            "1 cup gum drops, no licorice ones",
            "\u00be cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).  Grease a 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift dry ingredients and set aside\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs in large bowl until foamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat in sugar gradually.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Blend in sifted dry ingredients.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in gumdrops and walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into pan and spread evenly with a spatula.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30-35 minutes or until toothpick inserted in center comes out clean.  Cut into bars while still warm.\n"
            }
        ],
