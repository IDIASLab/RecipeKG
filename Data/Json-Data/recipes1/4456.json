        "recipeIngredient": [
            "1 cup white sugar",
            "\u2154 cup packed brown sugar",
            "3 eggs",
            "\u2153 cup butter",
            "1 cup pinto beans, cooked and mashed",
            "\u2153 cup chopped walnuts",
            "1 recipe pastry for a 9 inch single crust pie"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together butter, sugars and eggs (slightly beaten). Add beans and nuts, mix well and pour into unbaked pie shell.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 35 to 40 minutes.\n"
            }
        ],
