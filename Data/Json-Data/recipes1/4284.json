    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11303/oatmeal-butterscotch-cookies/",
        "name": "Oatmeal Butterscotch Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8647649.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1999-03-14T23:19:34.000Z",
        "description": "Great cookies!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00be cup butter, softened",
            "\u00be cup white sugar",
            "\u00be cup packed brown sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1\u2009\u00bc cups all-purpose flour",
            "1 teaspoon baking soda",
            "\u00bd teaspoon ground cinnamon",
            "\u00bd teaspoon salt",
            "3 cups rolled oats",
            "1\u2009\u2154 cups butterscotch chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl beat the butter or margarine, white sugar and brown sugar together. Add the eggs and vanilla, beating well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir together the flour, baking soda, cinnamon and salt. Gradually add the flour mixture to the butter mixture and stir until blended. Stir in the oats and the butterscotch chips. Drop by teaspoonfuls onto an ungreased cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 8 to 10 minutes in the preheated oven, until the edges begin to brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes"
        ],
