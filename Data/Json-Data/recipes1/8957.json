    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15314/chocolate-rocky-road-cookies/",
        "name": "Chocolate Rocky Road Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F609136.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-12-30T21:30:08.000Z",
        "description": "A chocolate cookie with the flavors of rocky road ice cream. These are great to eat with a big bowl of vanilla ice cream!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u00bd cup butter",
            "1 cup semisweet chocolate chips",
            "1 cup white sugar",
            "2 eggs",
            "\u00bd teaspoon vanilla extract",
            "1\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bc teaspoon salt",
            "1 cup chopped walnuts",
            "48 miniature marshmallows"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C). In a small sauce melt together the butter and 1/2 cup of the chocolate chips, stirring frequently. Remove from heat and set aside to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the melted chocolate mixture with the sugar, eggs and vanilla. Sift together the flour, baking powder and salt, stir into the chocolate mixture. Finally, stir in the chopped nuts and remaining chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop dough by rounded teaspoonfuls onto unprepared cookie sheets. Press a miniature marshmallow into the center of each cookie. Bake for 7 to 8 minutes in the preheated oven. Remove from baking sheets immediately to cool on wire racks.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
