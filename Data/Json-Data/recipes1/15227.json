    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9592/oatmeal-cinnamon-drops/",
        "name": "Oatmeal Cinnamon Drops",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F633950.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Oatmeal cookies are elevated by chocolate chips, cinnamon, and raisins to make sweet and chewy cookies.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "2 cups white sugar",
            "2 eggs",
            "1 teaspoon vanilla extract",
            "1 tablespoon molasses",
            "2 cups all-purpose flour",
            "1\u2009\u00bd teaspoons baking soda",
            "1 teaspoon ground cinnamon",
            "1 cup chopped walnuts",
            "2 cups quick cooking oats",
            "\u00bd cup semisweet chocolate chips",
            "\u2154 cup raisins"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F.  Grease cookie sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream together the butter and sugar.  Add eggs, molasses and vanilla.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, combine the flour, cinnamon, and baking soda.  Gradually add to the cream mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in oatmeal, raisins, nuts and chocolate chips.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by teaspoonfuls onto greased cookie sheet.  Bake for 12 minutes or until light brown.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Oatmeal Cookie Recipes",
            "Oatmeal Raisin Cookie Recipes"
        ],
