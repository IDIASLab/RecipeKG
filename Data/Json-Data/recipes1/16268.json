    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281459/grilled-pork-chops-with-fresh-herbs/",
        "name": "Grilled Pork Chops with Fresh Herbs",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8740267.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2010-08-18T20:13:56.000Z",
        "description": "A great tasting, easy way to cook pork chops on the grill! You can use about any cut of chop with the same great results.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "4 pork chops",
        "recipeIngredient": [
            "4 tablespoons olive oil, or more to taste",
            "4 tablespoons soy sauce",
            "4 tablespoons fresh lemon juice",
            "2 tablespoons chopped fresh parsley",
            "2 tablespoons chopped fresh rosemary",
            "2 tablespoons chopped fresh thyme",
            "1 tablespoon coriander seeds, ground",
            "1 clove garlic, minced",
            "1 lemon, zested",
            "4 (6 ounce) bone-in pork chops (1 inch thick)"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk olive oil, soy sauce, lemon juice, parsley, rosemary, thyme, ground coriander, garlic, and lemon zest together in a shallow dish. Add pork chops and toss to evenly coat. Cover the dish with plastic wrap and marinate in the refrigerator for at least 1 hour, turning chops halfway through.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chops from the marinade and gently shake off excess. Discard the remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chops on the preheated grill and reduce heat to medium. Cook until no longer pink in the center, about 8 minutes per side. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C). Let rest for 5 minutes before serving.\n"
            }
        ],
        "recipeCategory": [
            "Main Dishes",
            "Pork",
            "Pork Chop Recipes"
        ],
