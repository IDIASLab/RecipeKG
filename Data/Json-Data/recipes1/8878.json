    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15008/easy-biscotti-anisette-toast/",
        "name": "Easy Biscotti - Anisette Toast",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1923428.jpg",
            "width": 1536,
            "height": 2048
        },
        "datePublished": "1999-12-28T23:45:53.000Z",
        "description": "This is the easiest and best tasting Biscotti recipe I have ever made! Received from a co-worker back in 1993, this will make your house smell WONDERFUL!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "1 (18.25 ounce) package yellow cake mix",
            "\u00be cup vegetable oil",
            "5 eggs",
            "2 tablespoons anise seed",
            "6 tablespoons anise extract",
            "1 cup chopped almonds",
            "\u2153 cup confectioners' sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C). Grease and flour a 10x15 inch jellyroll pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, stir together the cake mix, oil, eggs, anise seeds, almonds, and anise extract. Mix well after each addition. Pour into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 20 to 30 minutes in the preheated oven, until a toothpick comes out clean.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut down the middle of the pan, then cut each half into 1/2 inch slices. Place slices on their side onto the jellyroll pan and return to the oven for 8 to 10 minutes, until toasted. Cool and dust with confectioners' sugar.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Cake Mix Cookie Recipes"
        ],
