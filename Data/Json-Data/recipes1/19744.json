    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281749/grandmas-take-on-classic-cheesecake/",
        "name": "Grandma's Take on Classic Cheesecake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Grandma's Take on Classic Cheesecake"
        },
        "datePublished": "2013-08-29T03:14:00.000Z",
        "description": "My grandma used to make this, and then my mom. Just keeping the tradition alive with a delicious and slightly different take on the classic cheesecake.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT6H0M",
        "recipeYield": "1 9-inch cheesecake",
        "recipeIngredient": [
            "1\u2009\u00bc cups graham cracker crumbs",
            "\u00bc cup white sugar",
            "\u00bc cup unsalted butter, melted",
            "2 (8 ounce) packages cream cheese, softened",
            "\u00bd cup white sugar",
            "3 large eggs",
            "\u00be teaspoon vanilla extract",
            "1 pint sour cream",
            "\u00bd cup white sugar",
            "1 teaspoon vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine graham cracker crumbs, sugar, and melted butter for crust in a bowl until well mixed. Press into the bottom and halfway up the sides of a 9-inch round pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cream cheese, sugar, eggs, and vanilla extract for filling in a large bowl; beat with an electric mixer until smooth and creamy, 2 to 3 minutes. Pour into crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until nearly set and only slightly jiggly in the center, about 18 minutes. Raise the oven temperature to 475 degrees F (245 degrees C) and keep cheesecake in for 2 minutes. Remove from the oven and cool on a wire rack for 15 minutes. Leave the oven on.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine sour cream, sugar, and vanilla extract in a bowl and mix with a spoon. Pour carefully over the cooled filling.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the oven until topping is set, about 10 minutes. Remove from the oven and let stand at room temperature for 5 to 6 hours. Refrigerate until ready to serve.\n"
            }
        ],
