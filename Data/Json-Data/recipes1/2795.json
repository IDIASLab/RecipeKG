    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13682/pumpkin-stuffing-bread/",
        "name": "Pumpkin Stuffing Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F984055.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1998-11-08T01:42:42.000Z",
        "description": "A pumpkin bread that may be used to make stuffing or try it toasted with butter.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 loaves",
        "recipeIngredient": [
            "2\u2009\u00bd cups all-purpose flour",
            "2 cups whole wheat flour",
            "2 cups white sugar",
            "1 tablespoon baking powder",
            "2 teaspoons baking soda",
            "4  large eggs eggs, beaten",
            "2 cups pumpkin puree",
            "2 teaspoons salt",
            "1 cup butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Butter two 9x5 inch loaf pans. Preheat oven to 350 degrees (175 degrees C).  Place oven rack in lower 1/3 of oven.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the white flour, whole wheat flour, sugar, baking powder and baking soda. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, combine the pumpkin, eggs and salt.  Add to flour mixture and blend in softened butter or margarine one tablespoon at a time.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Divide mixture between 2 pans.  Bake until golden and cake tester comes out clean, about 45 minutes. Invert onto racks and cool.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Pumpkin Bread Recipes"
        ],
