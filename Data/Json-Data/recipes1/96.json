    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9892/food-for-the-gods/",
        "name": "Food for the Gods",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F722616.jpg",
            "width": 2816,
            "height": 2112
        },
        "datePublished": "1997-08-15T02:00:51.000Z",
        "description": "You'll feel closer to heaven upon tasting this cookie. Makes great giveaways.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1\u2009\u00bc cups sifted all-purpose flour",
            "\u00bd teaspoon baking powder",
            "\u00bd teaspoon baking soda",
            "\u00bc teaspoon salt",
            "1 cup dates, pitted and chopped",
            "1 cup butter",
            "1 cup white sugar",
            "1 cup packed brown sugar",
            "3 eggs",
            "1 cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Grease and line a 9 x 13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine first 4 ingredients.  Take 1/2 cup of flour mixture and dredge the dates.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter and sugars until light and fluffy.  Add eggs, one at a time, and beat well after every addition.  Gradually stir in flour mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add dates and walnuts and stir until completely combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour batter into prepared pan and bake for 10 minutes.  Lower heat to 300 degrees F (150 degrees C) and bake for 30-40 minutes or until done.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cool on a wire rack for 20 minutes and cut into bars.  Wrap individually with foil or colored cellophane wrap.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Fruit Cookie Recipes",
            "Date"
        ],
