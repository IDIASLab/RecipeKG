    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14875/amazing-crab-shells/",
        "name": "Amazing Crab Shells",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6186.jpg",
            "width": 1360,
            "height": 1020
        },
        "datePublished": "1999-11-08T16:23:48.000Z",
        "description": "My family calls this 'those crab thingies' (even the adults), but they request it every holiday, and make up holidays so they can get me to make it.",
        "prepTime": "P0DT2H40M",
        "cookTime": null,
        "totalTime": "P0DT2H40M",
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "36 jumbo pasta shells",
            "2 (8 ounce) packages Neufchatel cheese",
            "1 pound imitation crabmeat",
            "6 ounces cooked tiny shrimp",
            "1 onion, minced",
            "2 stalks celery, chopped",
            "\u2153 cup mayonnaise",
            "2 tablespoons white sugar",
            "1\u2009\u00bd teaspoons salt",
            "\u00bd teaspoon ground black pepper",
            "1 teaspoon lemon juice"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of salted water to boil, and add pasta shells; boil until al dente. Drain well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, combine cream cheese, crab, shrimp, onion, celery, mayonnaise, sugar, salt, pepper and lemon juice; mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stuff cream cheese mixture into the jumbo pasta shells. Chill for at least 2 hours before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
        ],
