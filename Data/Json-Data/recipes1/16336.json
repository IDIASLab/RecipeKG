    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/281945/cheesy-chicken-and-asparagus-casserole/",
        "name": "Cheesy Chicken and Asparagus Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Cheesy Chicken and Asparagus Casserole"
        },
        "datePublished": "2010-10-08T03:10:33.000Z",
        "description": "This chicken and asparagus casserole is a quick and easy pasta dinner.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "1 (16 ounce) package whole wheat rotini pasta",
            "1 tablespoon olive oil, or to taste",
            "4 (4 ounce) skinless, boneless chicken breasts, cut into bite-size pieces",
            "1 bunch asparagus, cut into bite-sized pieces",
            "1 (8 ounce) package sliced fresh mushrooms",
            "1 medium red bell pepper, chopped",
            "1 medium yellow bell pepper, chopped",
            "1 (16 ounce) jar Alfredo sauce",
            "4 tablespoons pesto",
            "salt to taste",
            "\u2153 cup shredded Cheddar cheese, or to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bring a large pot of lightly salted water to a boil; cook rotini at a boil until tender yet firm to the bite, about 8 minutes. Drain and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While the pasta is cooking, heat oil in a large skillet over medium heat. Add chicken and cook until no longer pink in the center and the juices run clear, about 10 minutes. Add asparagus, mushrooms, and bell peppers; cook and stir until just tender, 3 to 5 minutes. Remove from heat and add rotini, Alfredo sauce, and pesto. Season with salt. Stir until well coated, transfer to a casserole dish, and top with Cheddar cheese.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until heated through and cheese has melted, 10 to 15 minutes.\n"
            }
        ],
