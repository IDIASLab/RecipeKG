    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/158398/onion-pepper-relish/",
        "name": "Onion Pepper Relish",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F871676.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2013-09-19T06:00:51.000Z",
        "description": "These pickled onions with slivers of jalapenos are delicious on hot dogs, sausages, and hamburgers. They also make great gifts for friends and family who like things hot and spicy!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 pints",
        "recipeIngredient": [
            "10 large yellow onions, cut into wedges",
            "15 fresh jalapeno peppers",
            "3 cups white vinegar",
            "2 tablespoons cracked black pepper",
            "2 tablespoons pickling salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Shred the onions and jalapeno peppers in a food processor, using the shredding blade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the onion, jalapeno pepper, vinegar, black pepper, and salt in a large pot and place over medium heat.  Stirring frequently, bring to a full boil; cook for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pack the mixture firmly into sterilized pint canning jars to within 3/4 inch of the top. Pour the cooking liquid over the onions to within 1/2 inch of the top and using a spatula, stir gently to remove all air bubbles. Wipe the rims with a clean damp cloth and adjust lids and rings firmly. Process in a boiling water bath for five minutes.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Relish Recipes"
        ],
