        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283259/homemade-condensed-cream-of-chicken-soup/",
        "name": "Homemade Condensed Cream of Chicken Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Homemade Condensed Cream of Chicken Soup"
        },
        "datePublished": "2013-08-21T16:34:42.000Z",
        "description": "Easy substitute for condensed cream of chicken soup in recipes. Change the broth, change the soup.",
        "prepTime": null,
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H15M",
        "recipeYield": "1 cup",
        "recipeIngredient": [
            "3 tablespoons salted butter",
            "3 tablespoons all-purpose flour",
            "\u00bd cup chicken stock",
            "\u00bd cup milk",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Melt butter in a small saucepan over medium heat. Add flour to melted butter and whisk rapidly until mixture is thick, about 2 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add chicken stock; whisk until smooth. Add milk and bring to a simmer. Simmer, stirring occasionally, until thickened, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove from heat and season with salt and pepper.\n"
            }
        ],
