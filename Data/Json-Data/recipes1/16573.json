    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/273306/potato-sabzi/",
        "name": "Potato Sabzi",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Potato Sabzi"
        },
        "datePublished": "2011-03-04T18:04:00.000Z",
        "description": "An authentic South Indian (Kerala) potato dish that is served in masala dosa or with chapati (Indian flatbread). A recipe that is passed down from my mother-in-law.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 medium potatoes",
            "1 tablespoon canola oil, or more as needed",
            "2 medium onions, thinly sliced",
            "1 teaspoon ground turmeric",
            "1 large tomato, chopped",
            "2 peppers green chile peppers, halved",
            "10 curry leaves",
            "salt to taste",
            "\u00bd cup water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place potatoes into a large pot and cover with salted water; bring to a boil. Reduce heat to medium-low and simmer until tender, about 20 minutes. Drain. Allow to cool slightly until potatoes can be handled. Peel and transfer to a bowl. Coarsely mash potatoes with a potato masher.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a skillet over medium heat. Add onions and cook until soft and golden brown, about 5 minutes. Add turmeric; cook and stir until well combined with the onions, 2 to 3 minutes. Mix in tomatoes, chile peppers, and curry leaves until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cover the skillet and cook, stirring from time to time, until tomatoes are well cooked, 5 to 6 minutes. Add mashed potatoes and season with salt. Mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour in water and stir thoroughly. Allow potato mixture to cook for 5 more minutes before serving.\n"
            }
        ],
