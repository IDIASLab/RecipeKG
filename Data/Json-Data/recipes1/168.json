        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/9974/chippers/",
        "name": "Chippers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6362571.jpg",
            "width": 2358,
            "height": 2358
        },
        "datePublished": "1997-09-02T00:14:45.000Z",
        "description": "Have egg whites left over?  This recipe will put them to good use.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "2 egg whites",
            "\u00bd teaspoon cream of tartar",
            "\u00bd teaspoon vanilla extract",
            "1 pinch salt",
            "\u00be cup white sugar",
            "1 cup mini semi-sweet chocolate chips",
            "1 cup chopped walnuts",
            "\u00bd teaspoon peppermint extract",
            "4 drops green food coloring"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites until glossy.  Add cream of tartar, vanilla, salt, mint extract and few drops of food coloring.  Add sugar and beat until stiff.  Fold in chocolate chips and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Line cookie sheets with foil.  Grease with oil.  Drop by teaspoonfuls onto foil.  Put cookie sheets in oven and TURN OFF OVEN.  Leave in oven overnight.\n"
            }
        ],
