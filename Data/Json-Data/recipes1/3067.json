        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12494/lemon-lush-pie/",
        "name": "Lemon Lush Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1744115.jpg",
