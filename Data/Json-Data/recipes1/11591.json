    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20961/banana-pudding-ii/",
        "name": "Banana Pudding II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4509739.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2000-05-30T00:19:20.000Z",
        "description": "An old fashioned banana pudding recipe. My husband loves when it is warm.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H30M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 eggs, beaten",
            "2\u2009\u00bd cups milk",
            "\u00bd cup white sugar",
            "2 tablespoons cornstarch",
            "\u00bc teaspoon salt",
            "1 teaspoon vanilla extract",
            "1 tablespoon margarine",
            "32 vanilla wafers",
            "4 bananas, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a double boiler over simmering water, combine eggs, milk, sugar, cornstarch and salt. Stir constantly and cook until thick, about 15 minutes. Remove from heat and stir in vanilla and margarine.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place a layer of vanilla wafers in a glass serving dish. Top with half the bananas. Top with half the pudding.  Repeat. Serve immediately or refrigerate until serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Banana Dessert Recipes"
        ],
