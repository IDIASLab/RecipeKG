    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15956/ranch-oyster-crackers/",
        "name": "Ranch Oyster Crackers",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4508517.jpg",
            "width": 1746,
            "height": 1746
        },
        "datePublished": "1999-12-12T19:43:01.000Z",
        "description": "These seasoned oyster crackers make an easy snack for any party occasion.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H45M",
        "recipeYield": "5 cups",
        "recipeIngredient": [
            "1 (1 ounce) package Ranch-style dressing mix",
            "\u00bd teaspoon dried dill weed",
            "\u00bc cup vegetable oil",
            "\u00bc teaspoon lemon pepper",
            "\u00bc teaspoon garlic powder",
            "5 cups oyster crackers"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 250 degrees F (120 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the dressing mix, dill weed, vegetable oil, lemon pepper, and garlic powder. Add oyster crackers, and toss to coat. Spread evenly on a baking sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 15 to 20 minutes in the preheated oven, stirring gently after 10 minutes. Remove from oven, and allow to cool before serving.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Cracker Recipes"
        ],
