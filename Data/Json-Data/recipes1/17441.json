        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange the apples into a baking dish with the cut sides facing down. Pour the orange beverage over the apples.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until the apples are tender, about 1 hour.\n"
            }
