    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282422/mashed-potato-donuts/",
        "name": "Mashed Potato Donuts",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Mashed Potato Donuts"
        },
        "datePublished": "2009-02-03T22:56:08.000Z",
        "description": "Amazing homemade donuts just like Grandma used to make.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT3H10M",
        "recipeYield": "2 dozen donuts",
        "recipeIngredient": [
            "1\u2009\u00bd cups milk, scalded and slightly cooled",
            "1 cup white sugar",
            "1 cup mashed potatoes",
            "\u2153 cup unsalted butter, softened",
            "2 large eggs",
            "1 teaspoon salt",
            "1 teaspoon vanilla extract",
            "2 tablespoons active dry yeast",
            "\u00bc cup warm water",
            "1 teaspoon white sugar",
            "4 cups all-purpose flour, or more as needed",
            "1 cup vegetable oil for frying, or as needed",
            "\u00bd cup water",
            "1 tablespoon cornstarch",
            "1 cup powdered sugar, or to taste",
            "\u00bc cup unsalted butter, melted"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine lukewarm milk, 1 cup white sugar, mashed potatoes, butter, eggs, salt, and vanilla extract in a bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve yeast in warm water in a separate bowl, adding 1 teaspoon sugar for action. Let rise for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine risen yeast with milk mixture. Mix in 4 cups flour, adding more as needed, until dough is only slightly sticky. Knead on a well-floured surface until elastic and pliable, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough into a ball and place in a clean, oiled bowl. Cover and let rise for 1 hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Punch down dough and let rise again for 1 hour more.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll dough out on a well-floured surface and cut with a donut-shaped cookie cutter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry donuts in the hot oil, working in batches, until light golden brown on both sides, about 5 minutes. Transfer to a rack to cool for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While donuts cool, heat water for glaze in a saucepan over medium-low heat. Add cornstarch and stir until clear and dissolved. Slowly pour in powdered sugar and butter, stirring constantly, until combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dip cooled donuts in glaze.\n"
            }
        ],
