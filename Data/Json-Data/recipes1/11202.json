    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18910/honey-glazed-carrots/",
        "name": "Honey Glazed Carrots",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F139894.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-05-07T21:20:05.000Z",
        "description": "This recipe is sweet but full of flavor and spice...Goes great with pot roast or oven baked chicken...Yum Yum",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "4 to 6 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "1 pound baby carrots",
            "\u00bd onion, chopped",
            "2 teaspoons Worcestershire sauce",
            "1 teaspoon dried oregano",
            "1 teaspoon monosodium glutamate (MSG)",
            "2 teaspoons garlic powder",
            "salt and pepper to taste",
            "3 tablespoons honey",
            "3 tablespoons butter, softened"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 325 degrees F (165 degrees C).  Lightly grease the bottom of a 9x13 inch baking dish.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Toss carrots with onion, and add Worcestershire sauce, oregano, monosodium glutamate, garlic powder, salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 minutes, or until tender.  Turn oven off and add  honey and butter, mixing thoroughly.  Place dish back in the warm oven for a few minutes.  Serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Vegetables",
            "Carrots",
            "Glazed Carrot Recipes"
        ],
