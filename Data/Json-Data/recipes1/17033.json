        "mainEntityOfPage": "https://www.allrecipes.com/recipe/139168/easy-boneless-hot-wings/",
        "name": "Easy, Boneless Hot Wings",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3302603.jpg",
            "width": 3648,
