        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10179/butterscotch-icebox-cookies/",
        "name": "Butterscotch Icebox Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1116488.jpg",
            "width": 1944,
            "height": 1944
        },
        "datePublished": "1997-11-30T05:57:48.000Z",
        "description": "Great dark brown sugar icebox cookies my southern Grandma used to make.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "2 cups packed dark brown sugar",
            "2 eggs",
            "3\u2009\u00bd cups all-purpose flour",
            "\u00bd teaspoon salt",
            "1 teaspoon baking soda",
            "1 teaspoon vanilla extract",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter, add sugar and cream well. Beat in eggs, then add sifted dry ingredients, vanilla and nuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into rolls, wrap in wax paper and freeze or put in refrigerator until firm.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice and place about an inch apart on greased cookie sheet. Bake about 12 minutes at 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "When done, remove from pan at once with a spatula. Store in a covered container and they will remain crisp.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Refrigerator Cookie Recipes"
        ],
