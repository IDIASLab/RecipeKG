    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/234465/mmmmonster-jigglers/",
        "name": "MMMMonster JIGGLERS",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1043892.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2013-09-13T19:01:12.000Z",
        "description": "These Halloween 'monsters' are fun bite-sized party treats and, if made with tonic water, they glow in the dark under a black light!",
        "prepTime": "P0DT0H10M",
        "cookTime": null,
        "totalTime": "P0DT3H10M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "2\u2009\u00bd cups boiling water, divided",
            "\u00bc cup JELL-O Brand Grape Flavor Gelatin",
            "1 (3 ounce) package JELL-O Brand Lime Flavor Gelatin",
            "32 pieces red string licorice, cut 1/2 inch each",
            "1 (1.5 ounce) tube black decorating gel"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Spray insides of 24- 1 oz. plastic shot glasses with cooking spray; set aside. Add 1-1/3 cups boiling water to lime gelatin mix in a medium bowl. Stir 2 min. until completely dissolved. Chill 12-15 min or until cool but not set.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, add remaining 1/3 cup boiling water to grape gelatin mix in small bowl; stir 2 min. until completely dissolved. Spoon about 1-1/4 tsp. into each shot glass; set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon chilled lime gelatin over partially set grape gelatin; refrigerate 3 hours or until firm. Unmold desserts onto tray just before serving. Insert licorice into each side for \"bolts\" and decorate face with decorating gel. Serve immediately.\n"
            }
        ],
