        "name": "Mary Ann's Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8135303.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1998-01-27T14:20:50.000Z",
        "description": "Molasses cookie recipe from the 1940's.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00be cup molasses",
            "1 teaspoon ground cinnamon",
            "1 cup hot water",
            "\u00be cup shortening",
            "5 cups all-purpose flour",
            "1 teaspoon ground ginger",
            "3 teaspoons baking soda"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar, shortening and molasses together.  Mix the baking soda into the hot water.  Add dry ingredients alternately with the water mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop from a spoon onto a greased cookie sheet.  Bake for 10 minutes.  DO NOT BROWN.  Frost with a vanilla frosting if desired.\n"
            }
        ],
