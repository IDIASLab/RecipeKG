    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10890/childrens-delight-cookies/",
        "name": "Children's Delight Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9172.jpg",
            "width": 1600,
            "height": 1200
        },
        "datePublished": "1998-05-18T01:57:04.000Z",
        "description": "This was a favorite of my 6 children.   It is easy to make and if the batch is too large,  the recipe is easily cut in half.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "2 cups water",
            "3 cups raisins",
            "2 cups shortening",
            "8 cups all-purpose flour",
            "2 teaspoons baking powder",
            "2 teaspoons baking soda",
            "1 teaspoon salt",
            "4 cups white sugar",
            "6 eggs",
            "2 teaspoons vanilla extract",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Boil water and raisins together for 5 minutes.  Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream sugar, shortening, eggs and vanilla until light.  Pour in the warm raisin mixture, combining well.  Stir in dry ingredients that have been combined and blend well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop by tablespoonful on a lightly oiled cookie sheet.  Bake at 350 degrees F (175 degrees C) for 12 to 15 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "Drop Cookie Recipes"
        ],
