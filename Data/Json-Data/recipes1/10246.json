    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23229/famous-peanut-caramel-candy-bars/",
        "name": "Famous Peanut Caramel Candy Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5830511.jpg",
            "width": 1280,
            "height": 960
        },
        "datePublished": "2000-03-17T14:51:30.000Z",
        "description": "Just like the real ones!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT1H40M",
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "\u00bc cup light corn syrup",
            "2 tablespoons butter",
            "1 tablespoon vanilla extract",
            "\u215b cup peanut butter",
            "1 pinch salt",
            "3 cups confectioners' sugar",
            "35 individually wrapped caramels, unwrapped",
            "1 cup dry-roasted peanuts",
            "2 cups milk chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a mixer, beat corn syrup, butter, vanilla, peanut butter and salt until smooth.  Beat in confectioners' sugar a little at a time until fully incorporated and mixture forms a thick dough.  Press into a 9x9 inch pan.  Chill in refrigerator.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium saucepan over low heat, melt caramels.  Stir in peanuts.  Pour over chilled layer and return to refrigerator until firm, 15 to 30 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small saucepan over low heat, melt chocolate chips.  Cut chilled mixture into bars and dip in melted chocolate with a fork.  Let cool on waxed paper at room temperature, or for 30 minutes in the refrigerator.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Peanut Dessert Recipes"
        ],
