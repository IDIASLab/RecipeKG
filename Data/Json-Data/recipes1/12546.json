        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23361/atlantic-corn-chowder/",
        "name": "Atlantic Corn Chowder",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F154288.jpg",
