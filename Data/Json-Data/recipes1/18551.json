        "mainEntityOfPage": "https://www.allrecipes.com/recipe/220984/pineapple-orange-fruit-salad/",
        "name": "Pineapple-Orange Fruit Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F926608.jpg",
            "width": 250,
