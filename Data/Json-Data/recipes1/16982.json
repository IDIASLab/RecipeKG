    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282489/buffalo-chicken-dip-with-blue-cheese/",
        "name": "Buffalo Chicken Dip with Blue Cheese",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Buffalo Chicken Dip with Blue Cheese"
        },
        "datePublished": "2011-11-23T21:33:49.000Z",
        "description": "Buffalo chicken dip for cheese lovers who aren't afraid of blue cheese! Serve with tortilla chips, pita chips, crackers, or crostini.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "16 servings",
        "recipeIngredient": [
            "\u00bd cup freshly shredded white Cheddar cheese",
            "\u00be cup freshly grated mozzarella cheese",
            "1\u2009\u00bd pounds shredded cooked chicken breast",
            "\u2154 cup chunky blue cheese dressing",
            "\u00bd cup Buffalo wing sauce",
            "1 (8 ounce) package cream cheese, softened and cubed",
            "\u00be cup crumbled blue cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine Cheddar and mozzarella cheeses in a small bowl. Stir chicken, blue cheese dressing, and Buffalo sauce together in a medium bowl.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place cream cheese cubes in the bottom of a 9-inch baking dish. Sprinkle 1/2 of the Cheddar-mozzarella mixture over top to cover the bottom of the dish. Pour 1/2 cup of the Buffalo mixture over the cheese. Sprinkle with crumbled blue cheese and pour remaining Buffalo mixture over top. Top with remaining Cheddar-mozzarella mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake on the middle or top rack of the preheated oven until cheese is bubbling, 15 to 20 minutes.\n"
            }
        ],
