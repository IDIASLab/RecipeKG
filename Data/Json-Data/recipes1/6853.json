    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15660/strawberry-daiquiri-pie/",
        "name": "Strawberry Daiquiri Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3473924.jpg",
            "width": 2448,
            "height": 3264
        },
        "datePublished": "1999-09-14T08:11:26.000Z",
        "description": "A great, easy pie recipe that can be made with any frozen juice for a different flavor. Additional whipped topping can be used to top pie if desired.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 pies",
        "recipeIngredient": [
            "1 (10 ounce) can frozen strawberry daiquiri mixer",
            "1 cup sweetened condensed milk",
            "1 (8 ounce) container frozen whipped topping, thawed",
            "1 (10 ounce) package <a href=\"https://www.allrecipes.com/article/how-to-freeze-strawberries/\" aria-label=\"frozen strawberries\">frozen strawberries</a>, thawed",
            "2 (9 inch)  prepared graham cracker crusts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine daiquiri mix, sweetened condensed milk, and whipped topping. Mix until smooth and creamy.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add thawed strawberries and blend into mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fill pie crusts and refrigerate for approximately 4 hours or freeze until set.\n"
            }
        ],
