    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13597/dees-date-and-nut-bread/",
        "name": "Dee's Date and Nut Bread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F973886.jpg",
            "width": 2592,
            "height": 2592
        },
        "datePublished": "1998-11-17T14:16:32.000Z",
        "description": "My mother used to make this recipe in a long loaf pan which I don't believe they make any more.  Fortunately, I have her pan and whenever I make this it sure brings back such happy memories.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 -9x5 inch loaf pan",
        "recipeIngredient": [
            "1\u2009\u00bd cups hot water",
            "1\u2009\u00bd cups dates, pitted and chopped",
            "2 teaspoons baking soda",
            "1 cup white sugar",
            "1 teaspoon salt",
            "1 tablespoon butter, melted",
            "1 egg",
            "1 teaspoon vanilla extract",
            "1 cup chopped walnuts",
            "2\u2009\u00be cups all-purpose flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Grease loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour the hot water over the dates and let stand until cooled.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sift the flour and baking soda together add the sugar and salt. Stir in the cooled dates and water. Add the melted butter, beaten egg vanilla and walnuts. Mix well. Pour batter into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 1 hour or until a toothpick insert near the center comes out clean. Let bread cool in pan for 10 minutes. Remove bread from pan and let cool on a wire rack.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Fruit Bread Recipes"
        ],
