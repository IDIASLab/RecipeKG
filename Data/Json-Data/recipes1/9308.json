    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17858/twice-baked-cheesy-potatoes/",
        "name": "Twice Baked Cheesy Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F28369.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "2000-01-17T00:38:27.000Z",
        "description": "These are great potatoes to serve with many different types of meals, and kids love them!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "5 potatoes",
            "4 ounces American cheese, sliced into strips",
            "1 tablespoon butter",
            "\u00be cup sour cream"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake potatoes for 1 hour or until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a microwave safe bowl, combine the cheese and butter. Cook in microwave for 1 1/2 minutes or until melted. Add sour cream and mix together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut potatoes in half, scoop out insides while being careful not to tear the skins. Add to cheese mixture and mix together until smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stuff potato mixture back into potato shells and bake at 350 degrees F (175 degrees C) for 10 minutes.\n"
            }
        ],
