    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20666/texas-coleslaw/",
        "name": "Texas Coleslaw",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F383252.jpg",
            "width": 4288,
            "height": 2848
        },
        "datePublished": "2000-08-28T00:51:29.000Z",
        "description": "One of my favorites! Goes great with barbecues and picnics. For a more mild salad, use less green onions.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT1H15M",
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup mayonnaise",
            "1 tablespoon lime juice",
            "1 tablespoon ground cumin",
            "1 teaspoon cayenne pepper",
            "1 teaspoon salt",
            "1 teaspoon ground black pepper",
            "1 medium head green cabbage, rinsed and very thinly sliced",
            "1 large carrot, shredded",
            "2 green onions, sliced",
            "2 radishes, sliced"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, whisk together the mayonnaise, lime juice, cumin, salt and pepper.  Add the cabbage, carrot, green onions and radishes and stir until well-combined.  Chill at least an hour before serving.\n"
            }
        ],
