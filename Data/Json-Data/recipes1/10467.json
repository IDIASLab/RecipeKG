    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21427/vienna-schnitzel/",
        "name": "Vienna Schnitzel",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3702650.jpg",
            "width": 4032,
            "height": 3024
        },
        "datePublished": "2000-03-26T22:21:27.000Z",
        "description": "Both my mother and father were Viennese and grew up there.  This was my mother's recipe.  Veal can also be used in place of pork.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 quart oil for deep frying",
            "6 (6 ounce) fillets pork sirloin",
            "1 cup cake flour",
            "2 cups dry bread crumbs",
            "2 eggs",
            "\u00bc cup milk",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat deep-fryer to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place meat on a solid, level surface and pound with a mallet until it is 1/8 to 1/4 inch thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place the flour and bread crumbs in separate dishes. Soup plates are useful. Lightly beat the eggs and add the milk.  Lightly season with salt and pepper and put this into another soup plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Coat the meat in flour, patting lightly by hand.  Using a fork to hold the meat, dip into the egg mixture and drain slightly.  Next, coat with the breadcrumbs by lightly pressing the crumbs into the meat.  When each piece of meat has been prepared, deep fry in cooking oil until golden brown.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "European",
            "Austrian"
        ],
        "recipeCuisine": [
            "European",
            "Austrian"
