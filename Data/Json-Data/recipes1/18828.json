        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/161962/creamy-yukon-mashed-potatoes/",
        "name": "Creamy Yukon Mashed Potatoes",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3892577.jpg",
