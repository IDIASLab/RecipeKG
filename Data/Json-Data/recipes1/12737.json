    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20779/chicken-adobo-i/",
        "name": "Chicken Adobo I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F396607.jpg",
            "width": 3008,
            "height": 1804
        },
        "datePublished": "2000-08-11T20:04:45.000Z",
        "description": "This is a variation of a recipe my mother taught me after I got married and wanted to make Filipino food for my husband.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "5 servings",
        "recipeIngredient": [
            "1\u2009\u00bd cups water",
            "1 cup distilled white vinegar",
            "4 tablespoons soy sauce",
            "1 teaspoon whole peppercorns",
            "4 cloves garlic, crushed",
            "2 tablespoons salt, or to taste",
            "1 (2 to 3 pound) whole chicken, cut into pieces",
            "2 tablespoons vegetable oil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot, combine the water, vinegar, soy sauce, peppercorns, garlic and salt. Mix together, add chicken pieces and simmer over low heat (uncovered) until chicken is cooked through and tender (25 to 35 minutes).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove chicken from pot and brown in oil in a large skillet over medium high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Return chicken, with oil/juices, back to pot with reserved sauce. Cover and simmer over medium heat until sauce reaches desired consistency.\n"
            }
        ],
        "recipeCategory": [
            "World Cuisine",
            "Asian",
            "Filipino"
