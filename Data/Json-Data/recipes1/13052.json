        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20851/garlic-spread/",
        "name": "Garlic Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9229901.jpg",
