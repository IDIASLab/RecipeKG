    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/236646/lime-fudge/",
        "name": "Lime Fudge",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2645540.jpg",
            "width": 5184,
            "height": 3456
        },
        "datePublished": "2012-12-29T18:46:44.000Z",
        "description": "A quick, light-tasting dessert. Great to have around the home during the holidays!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT2H45M",
        "recipeYield": "64 servings",
        "recipeIngredient": [
            "\u2153 cup unsalted butter, melted",
            "\u00bc cup sugar",
            "1\u2009\u00bc (14.4 ounce) packages graham cracker crumbs",
            "1 (14 ounce) can sweetened condensed milk",
            "1 (11 ounce) package white chocolate chips (such as Nestle\u00ae)",
            "1 (4 ounce) bar white chocolate (such as Ghirardelli\u00ae)",
            "\u00bc cup lime juice",
            "1 tablespoon grated lime zest"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C). Line an 8-inch square baking dish with parchment paper or aluminum foil, leaving a 2-inch overhang. Butter sides and bottom of paper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir butter and sugar together in a bowl until sugar is completely dissolved. Stir graham cracker crumbs into butter mixture; press mixture into the bottom of the prepared baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until edges are lightly browned, 4 to 5 minutes. Cool completely.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir condensed milk, white chocolate chips, and white chocolate together in a large saucepan over medium heat until white chocolate is completely melted. Remove saucepan from heat and stir lime juice and lime zest into chocolate mixture. Pour mixture over cooled crust, cover with plastic wrap, and chill in the refrigerator for at least two hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lift fudge from pan using the overhanging paper and cut into desired shapes. Store in a container in the refrigerator.\n"
            }
        ],
