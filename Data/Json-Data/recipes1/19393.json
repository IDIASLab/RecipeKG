        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/232904/lemon-curd-tarts/",
        "name": "Lemon Curd Tarts",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1016267.jpg",
            "width": 3005,
            "height": 3005
