        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18062/mock-chicken/",
        "name": "Mock Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F475124.jpg",
            "width": 250,
            "height": 250
