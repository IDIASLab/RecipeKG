        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10532/peanut-butter-kisses-i/",
        "name": "Peanut Butter Kisses I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2349811.jpg",
            "width": 2592,
