        "mainEntityOfPage": "https://www.allrecipes.com/recipe/143204/roasted-red-pepper-cheese-spread/",
        "name": "Roasted Red Pepper-Cheese Spread",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F276827.jpg",
