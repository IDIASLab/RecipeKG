        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7593/yellow-pound-cake/",
        "name": "Yellow Pound Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4517458.jpg",
            "width": 1704,
            "height": 1704
        },
        "datePublished": "1998-06-01T17:03:11.000Z",
        "description": "Pound cakes do not need icing, and always taste great either way.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 tube cake",
        "recipeIngredient": [
            "2\u2009\u2154 cups white sugar",
            "2 cups butter",
            "8 eggs",
            "8 tablespoons milk",
            "1 teaspoon vanilla extract",
            "3\u2009\u00bd cups cake flour"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Separate the eggs.  Beat the whites to stiff peaks, and reserve the yolks.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, cream sugar and butter or margarine.  Beat in egg yolks.  Stir in milk and vanilla.  Add flour, 1 cup at a time.  Fold in stiffly beaten egg whites.  Pour batter into well greased large tube pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 90 minutes at 325 degrees F (165 degrees C).\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cakes",
            "Pound Cake Recipes"
        ],
