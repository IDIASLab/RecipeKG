        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11357/bannys-southern-cream-cookies/",
        "name": "Banny's Southern Cream Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F91496.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-04-04T14:16:05.000Z",
        "description": "These cookies travel and freeze well.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 to 5 dozen",
        "recipeIngredient": [
            "1 cup shortening",
            "2 cups white sugar",
            "3 eggs",
            "1 teaspoon vanilla extract",
            "1 cup sour cream",
            "5 cups all-purpose flour",
            "1 teaspoon salt",
            "3 teaspoons baking powder",
            "\u00bd teaspoon baking soda",
            "1\u2009\u00bd cups chopped black walnuts",
            "3 tablespoons white sugar",
            "1 teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease baking sheets.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream the shortening with 2 cups of the white sugar until light and fluffy. Beat in the eggs, vanilla and sour cream; mixing well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in the flour, salt, baking powder and baking soda into the shortening mixture to make a stiff dough. Add the chopped black walnuts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Drop teaspoonfuls of dough onto the prepared baking sheets. Mix the 3 tablespoons with the ground cinnamon. Grease the bottom of a small glass. Dip it into the cinnamon-sugar mixture and gently press the cookies to flatten the balls of dough slightly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 10 to 15 minutes or until centers are done.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Cookies",
            "International Cookie Recipes",
            "American Cookie Recipes"
        ],
