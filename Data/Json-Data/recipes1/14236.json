    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/49338/marias-green-chicken/",
        "name": "Maria's Green Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1012919.jpg",
            "width": 1665,
            "height": 1663
        },
        "datePublished": "2002-04-17T22:32:57.000Z",
        "description": "This is a fast and easy exotic chicken recipe made with chicken breasts. It gets it's green color from ground turmeric.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT1H0M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "4 boneless, skinless chicken breast halves",
            "1 onion, chopped",
            "1 green bell pepper, seeded and chopped",
            "\u00bc cup butter",
            "2 tablespoons ground turmeric",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place chicken breasts in a single layer in a 9x13 inch baking dish. Sprinkle the chopped onion and green pepper over the breasts. Place small pieces of butter around on the vegetables. Sprinkle turmeric powder evenly over the entire dish, and season with salt and pepper to taste. Cover the dish with a lid or aluminum foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 45 to 60 minutes  in the preheated oven, or until the chicken is no longer pink, and the juices run clear.\n"
            }
        ],
