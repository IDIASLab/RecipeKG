        "totalTime": null,
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 large sweet onion, chopped",
            "1 pound lean ground beef",
            "salt and pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat grill for high heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl mix together the onion, beef, and salt and pepper to taste. Form into patties.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Lightly oil grate, and place burgers on grill. Cook for 3 to 5 minutes per side.  Remove from grill, and serve with your favorite condiments.\n"
            }
