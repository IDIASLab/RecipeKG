        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23255/great-hot-crab-dip/",
        "name": "Great Hot Crab Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F966479.jpg",
