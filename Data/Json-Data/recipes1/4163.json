        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11839/arrabbiata-sauce/",
        "name": "Arrabbiata Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3472048.jpg",
            "width": 2435,
            "height": 3020
        },
        "datePublished": "1999-03-06T15:52:51.000Z",
        "description": "Spicy and delicious. Ideal on penne pasta.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "3 cups",
        "recipeIngredient": [
            "1 teaspoon olive oil",
            "1 cup chopped onion",
            "4 cloves garlic, minced",
            "\u215c cup red wine",
            "1 tablespoon white sugar",
            "1 tablespoon chopped fresh basil",
            "1 teaspoon crushed red pepper flakes",
            "2 tablespoons tomato paste",
            "1 tablespoon lemon juice",
            "\u00bd teaspoon Italian seasoning",
            "\u00bc teaspoon ground black pepper",
            "2 (14.5 ounce) cans peeled and diced tomatoes",
            "2 tablespoons chopped fresh parsley"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large skillet or saucepan over medium heat. Saute onion and garlic in oil for 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in wine, sugar, basil, red pepper, tomato paste, lemon juice, Italian seasoning, black pepper and tomatoes; bring to a boil. Reduce heat to medium, and simmer uncovered about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in parsley. Ladle over the hot cooked pasta of your choice.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Sauces",
            "Pasta Sauces",
            "Tomato"
        ],
