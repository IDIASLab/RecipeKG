    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10083/easy-pecan-pie-bars/",
        "name": "Easy Pecan Pie Bars",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8199288.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "1997-09-21T21:46:24.000Z",
        "description": "If you like pecan pie, you'll love these bars!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 dozen",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup packed brown sugar",
            "\u00bd cup butter",
            "\u00bd cup margarine",
            "5 eggs",
            "1 cup dark corn syrup",
            "\u00be cup white sugar",
            "1 pinch salt",
            "1 teaspoon vanilla extract",
            "1 cup chopped pecans"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (180 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine flour and brown sugar.  Cut in butter and margarine until mixture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press into a 9 x 13 inch pan and bake for 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine remaining ingredients except pecans and blend well.  Stir in pecans.  Pour over baked crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Reduce oven temperature to 275 degrees F (140 degrees C).  Bake 50 minutes or until set.  Cool in pan on wire rack.  Cut into bars.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Nut Dessert Recipes",
            "Pecan Dessert Recipes"
        ],
