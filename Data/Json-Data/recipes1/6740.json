        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13404/worlds-best-potato-soup/",
        "name": "World's Best Potato Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F114574.jpg",
            "width": 2048,
            "height": 1536
        },
        "datePublished": "1999-09-04T20:04:56.000Z",
        "description": "Cheesy, hearty potato soup that's better than the restaurants.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 to 10 servings",
        "recipeIngredient": [
            "8 unpeeled potatoes, cubed",
            "1 onion, chopped",
            "2 stalks celery, diced",
            "6 cubes  chicken bouillon",
            "1 pint half-and-half cream",
            "1 pound bacon - cooked and crumbled",
            "1 (10.75 ounce) can  condensed cream of mushroom soup",
            "2 cups shredded Cheddar cheese"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large stock pot combine potatoes, onions, celery, bouillon cubes and enough water to cover all ingredients.  Bring to a boil and simmer on medium heat until potatoes are with in 15 minutes of being finished.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add half and half, bacon, cream of mushroom soup and stir until creamy.  Add cheese and stir until completely melted.  Simmer on low until potatoes are done.\n"
            }
        ],
        "recipeCategory": [
