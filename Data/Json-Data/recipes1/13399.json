        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23445/rainbow-pasta-salad-ii/",
        "name": "Rainbow Pasta Salad II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6471771.jpg",
            "width": 3024,
