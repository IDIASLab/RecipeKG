        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/11156/amariette-cookies/",
        "name": "Amariette Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2507128.jpg",
            "width": 1919,
            "height": 1919
        },
        "datePublished": "1998-12-09T01:17:44.000Z",
        "description": "This is a cookie the Italian's make on festive occasions like weddings, or Christmas.  For a festive touch, decorate the top of each cookie with a piece of candied cherry instead of the almond.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "7 egg whites",
            "2\u2009\u00bd cups white sugar",
            "2 pounds ground almonds",
            "2 tablespoons almond extract",
            "1 cup chopped almonds",
            "\u2153 cup granulated sugar for decoration"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat egg whites and sugar until fluffy. Add the ground almond and almond extract, mixing well. Shape dough into walnut sized balls, roll in sugar and put a piece of almond or candied cherry on top.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees F) until firm. Bottom should be light brown.\n"
            }
        ],
