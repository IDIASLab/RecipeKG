        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12672/jefferson-davis-pie/",
        "name": "Jefferson Davis Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F719797.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-09-02T19:09:43.000Z",
        "description": "This pie is extremely rich and should be served in small wedges. It's especially good with a dollop of bourbon whipped cream.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "2 9-inch pies",
        "recipeIngredient": [
            "1 cup butter, softened",
            "1 cup white sugar",
            "1 cup packed light brown sugar",
            "1 cup heavy cream",
            "2 eggs",
            "2 tablespoons all-purpose flour",
            "1 tablespoon vanilla extract",
            "2 unbaked pie shells",
            "1 cup heavy cream",
            "3 tablespoons white sugar",
            "2 tablespoons bourbon whiskey"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix white sugar, brown sugar, and flour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add softened butter, cream, eggs and vanilla. Mix until well blended. Pour into 2 unbaked pie shells.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 425 degrees F (220 degrees C) for 10 minutes, then reduce temperature to 350 degrees F (175 degrees C) and continue baking for 45 to 50 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bourbon Whipped Cream:  Whip cream and add sugar gradually until stiff peaks form. Once cream is whipped, gently fold in the bourbon.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Custard and Cream Pies"
        ],
