        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15934/good-dog-cookies/",
        "name": "Good Dog Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F25462.jpg",
            "width": 2304,
            "height": 1728
        },
        "datePublished": "1999-12-03T22:05:30.000Z",
        "description": "A gift idea your canine friends will beg for!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "1 dozen",
        "recipeIngredient": [
            "2\u2009\u00bd cups whole wheat flour",
            "\u00be cup nonfat dry milk powder",
            "1 egg, beaten",
            "\u00bd cup vegetable oil",
            "2 cubes beef bouillon cube",
            "\u00be cup boiling water",
            "2 tablespoons brown sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C). Lightly grease one cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve bouillon cubes in boiling water and allow to cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the flour, dry milk, egg, oil, beef broth and brown sugar. Mix well and knead dough for 1 minute.\n"
            },
            {
                "@type": "HowToStep",
                "text": "On a floured surface roll out dough to about 1/4 inch thickness. Cut out bones and place on cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 minutes and allow to cool.\n"
            }
        ],
