        "name": "Smoked Tomato Bisque",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F995574.jpg",
            "width": 2731,
            "height": 2736
