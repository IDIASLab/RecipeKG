    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/139311/shrimp-ramakis/",
        "name": "Shrimp Ramakis",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2020031.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2011-12-15T20:31:34.000Z",
        "description": "Shrimp marinated in teriyaki sauce are wrapped in bacon and broiled until crisp. Serve with white rice and steamed vegetables.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H10M",
        "totalTime": "P0DT1H30M",
        "recipeYield": "24 appetizers",
        "recipeIngredient": [
            "24 jumbo shrimp, peeled and deveined",
            "1\u2009\u00bd cups bottled teriyaki sauce",
            "1 pound bacon strips, cut in half",
            "1 (8 ounce) can whole water chestnuts, drained",
            "24 wooden toothpicks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a glass bowl, toss shrimp with teriyaki sauce. Cover with plastic wrap and let marinate in the refrigerator for one hour.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Adjust oven rack to the middle setting and set oven to preheat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove shrimp from marinade and place each on a half strip of bacon with a water chestnut. Roll and secure with a toothpick, then place onto a broiler pan approximately 1/2 inch apart from each other. Discard remaining marinade.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Broil ramakis for 5 minutes on each side, or until the shrimp is pink and the bacon is cooked (keep the oven door ajar and watch carefully). Drain on paper towels and serve immediately.\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Seafood",
            "Shrimp"
        ],
