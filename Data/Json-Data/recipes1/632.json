        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10397/peanut-butter-pinwheel-cookies/",
        "name": "Peanut Butter Pinwheel Cookies",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4580290.jpg",
            "width": 4160,
            "height": 2340
        },
        "datePublished": "1998-01-27T17:42:24.000Z",
        "description": "Pretty pinwheel cookies with peanut butter filling.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "3 dozen",
        "recipeIngredient": [
            "1 cup butter",
            "2 cups white sugar",
            "2 eggs",
            "4 cups all-purpose flour",
            "\u00be teaspoon baking soda",
            "1 teaspoon salt",
            "1 teaspoon baking powder",
            "1\u2009\u00bd teaspoons vanilla extract",
            "1 cup smooth peanut butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter or margarine and sugar;  add eggs and vanilla.  Add dry ingredients to butter mixture.  Divide dough into 2 parts.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Roll each part out, to approximate 1/8 inch thick, then spread with 1/2 of the peanut butter.  Roll up dough and refrigerate several hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Slice and bake at 400 degrees F (205 degrees C) for 5 - 8 minutes, until slightly brown.\n"
            }
        ],
