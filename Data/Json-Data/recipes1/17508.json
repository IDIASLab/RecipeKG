        "name": "Cinnamon Apple and Havarti Tea Sandwiches",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9337832.jpg",
            "width": 0,
            "height": 0
