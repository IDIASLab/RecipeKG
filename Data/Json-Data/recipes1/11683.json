    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/18128/sopapilla-dessert/",
        "name": "Sopapilla Dessert",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6490494.jpg",
            "width": 3024,
            "height": 4032
        },
        "datePublished": "2000-06-06T18:12:28.000Z",
        "description": "This is a classic fried dessert bread of Mexico. They're great hot out of the fryer and rolled in cinnamon sugar.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "32 sopapillas",
        "recipeIngredient": [
            "8 cups shortening for frying",
            "4 cups unbleached all-purpose flour",
            "4 teaspoons baking powder",
            "2 teaspoons salt",
            "2 tablespoons shortening",
            "1 cup water",
            "2 teaspoons ground cinnamon",
            "\u00bd cup white sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat 8 cups shortening in a deep-fryer to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, sift together flour, baking powder and salt. Cut in 2 tablespoons shortening until mixture resembles coarse crumbs. Gradually stir in water just until dough pulls together. Divide dough into 4 pieces and form each piece into a ball. Roll the balls into 8 inch diameter circles. Cut each one into 8 wedges. In a large bowl, mix together the cinnamon and sugar. Set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Fry the wedges until they puff up and turn light brown. Drain briefly, roll in the cinnamon sugar and serve hot.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes"
        ],
