        "mainEntityOfPage": "https://www.allrecipes.com/recipe/233250/pineapple-coconut-sauce/",
        "name": "Pineapple-Coconut Sauce",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F6934954.jpg",
            "width": 960,
            "height": 960
