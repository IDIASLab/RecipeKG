    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284345/pistachio-chocolate-friands/",
        "name": "Pistachio-Chocolate Friands",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F959640.jpg",
            "width": 1280,
            "height": 1280
        },
        "datePublished": "2013-01-24T15:06:07.000Z",
        "description": "A friand is a French small cake made with almond flour. It's delicious!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "12 friands",
        "recipeIngredient": [
            "10 tablespoons unsalted butter, plus more for greasing",
            "\u00be cup almond flour",
            "2 tablespoons almond flour",
            "\u00be cup all-purpose flour",
            "1 cup confectioners' sugar",
            "2 tablespoons confectioners' sugar",
            "\u00be cup demerara sugar",
            "\u00bc cup unsweetened cocoa powder",
            "\u00bd teaspoon baking powder",
            "5 large egg whites",
            "\u2153 cup chopped pistachios, or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 320 degrees F (160 degrees C). Grease 12 small cake moulds well with butter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Melt 10 tablespoons butter in a saucepan until it becomes golden over medium-low heat. Remove from heat and let cool.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine almond flour, all-purpose flour, confectioners' sugar, demerara sugar, cocoa powder, and baking powder in a bowl; stir together until well combined. Add unwhipped egg whites and mix until well combined. Add melted butter and pistachios. Pour batter into the prepared moulds.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center of the friands comes out clean, 30 to 35 minutes. Bon appetit!\n"
            }
        ],
