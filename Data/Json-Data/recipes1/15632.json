    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283847/italian-ricotta-cake/",
        "name": "Italian Ricotta Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Italian Ricotta Cake"
        },
        "datePublished": "2009-02-14T02:48:35.000Z",
        "description": "This is an excellent, filling dessert that feeds a crowd! I bring this cake to family get-togethers and am always asked for the recipe! It is made in a 9x13-inch pan, and has a cheesecake-like consistency. Serve it with whipped cream and fresh or frozen berries.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H15M",
        "totalTime": "P0DT6H0M",
        "recipeYield": "1 9x13-inch cake",
        "recipeIngredient": [
            "\u00bd cup unsalted butter, softened",
            "\u00bd cup white sugar",
            "2 large eggs",
            "\u00bc cup brandy",
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "3 pounds ricotta cheese",
            "5 large eggs",
            "1\u2009\u00be cups white sugar",
            "1 cup heavy cream",
            "3 large egg whites, beaten stiff",
            "\u00bd medium lemon, zested",
            "\u00bd medium lime, zested",
            "\u00bd teaspoon ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Cream butter and gradually add sugar. Beat until light and fluffy. Add eggs, one at a time, while continuing beating. Add brandy. Add flour and baking powder. Mix into a dough until the consistency is like Play-Doh&reg;. Press dough into a greased and floured 9x13-inch pan. Refrigerate for 2 1/2 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat ricotta cheese in a large bowl 4 to 5 minutes on high speed. Add eggs, 1 at a time, beating after each addition. Add sugar and heavy cream and beat until blended. Fold in egg whites.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Spoon 1/2 of the mixture into the crust. Sprinkle 1/2 of the zests and 1/2 of the cinnamon on top. Repeat with remaining filling, zest, and cinnamon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour and 15 minutes. Turn off the oven and let cake cool in the oven for 2 hours. Remove from the oven and chill completely before serving.\n"
            }
        ],
