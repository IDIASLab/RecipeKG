        "totalTime": "P0DT0H30M",
        "recipeYield": "5 dozen",
        "recipeIngredient": [
            "1 (9 ounce) package chocolate wafer cookies, crushed",
            "\u00bd pound ground almonds",
            "\u2153 cup unsweetened cocoa powder",
            "\u00bc cup white sugar",
            "2 tablespoons instant coffee powder ",
            "\u2153 cup coffee flavored liqueur",
            "\u00bd cup light corn syrup",
            "\u00bc cup white sugar",
            "2 teaspoons ground cinnamon"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large bowl, mix chocolate wafer crumbs, ground blanched almonds, unsweetened cocoa powder, and 1/4 cup sugar.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Dissolve  instant coffee in coffee liqueur and stir into crumb mixture with corn syrup.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Shape into 1/4 inch balls and roll in cinnamon sugar. To make cinnamon sugar, combine 1/4 cup sugar with 2 teaspoons cinnamon. Store in refrigerator.\n"
            }
        ],
