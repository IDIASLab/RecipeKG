        "mainEntityOfPage": "https://www.allrecipes.com/recipe/17035/tater-crisp-chicken/",
        "name": "Tater Crisp Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F9246460.jpg",
