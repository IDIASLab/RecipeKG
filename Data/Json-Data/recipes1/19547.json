    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283827/easy-creole-fried-chicken/",
        "name": "Easy Creole Fried Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Easy Creole Fried Chicken"
        },
        "datePublished": "2013-07-03T18:15:39.000Z",
        "description": "Easiest fried chicken recipe on the planet. Requires no marinating and makes no mess. Coat skinned chicken pieces in your favorite Creole seasoning and fry in peanut oil. Bake in oven a short time to ensure chicken is cooked through.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H10M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1 gallon peanut oil, or as needed",
            "1\u2009\u00bd pounds chicken wings",
            "1\u2009\u00bd pounds chicken legs",
            "1 tablespoon Creole seasoning (such as Tony Chachere's\u00ae), or more to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat oil in a deep-fryer or large saucepan to 375 degrees F (190 degrees C). Preheat the oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Liberally coat chicken pieces with Creole seasoning.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place 6 to 7 pieces chicken at a time into the hot oil. Cook until skin turns light brown and pieces float, 9 to 12 minutes. Remove from the oven and drain on paper towels before transferring to a baking sheet. Repeat to fry remaining batches.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake chicken in the preheated oven until no longer pink at the bone and the juices run clear, about 10 minutes. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C). Cool for 15 to 20 minutes before serving.\n"
            }
        ],
