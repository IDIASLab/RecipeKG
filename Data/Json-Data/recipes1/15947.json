        "name": "Nogtini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F43%2F2021%2F11%2F08%2F235721-nogtini-natalie-titanov-2x1-1.jpg",
            "width": 2000,
