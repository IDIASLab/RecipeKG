    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16019/worlds-best-pumpkin-pie/",
        "name": "World's Best Pumpkin Pie",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8764041.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "This recipe not only makes the best pumpkin pie, but it's oh so easy too! Serve with whipped topping.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "8 servings",
        "recipeIngredient": [
            "1 cup white sugar",
            "\u00bd cup packed brown sugar",
            "\u00bc teaspoon salt",
            "2 teaspoons ground cinnamon",
            "2 eggs",
            "1 (15 ounce) can pumpkin puree",
            "1\u2009\u00bc cups milk",
            "1 (9 inch) unbaked pie crust"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large mixing bowl, stir together white sugar, brown sugar, salt, and cinnamon. When these ingredients are well mixed, stir in the eggs followed by the pumpkin and milk. Transfer mixture to the pie crust.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees C) for 1 1/2 hours, or until a toothpick inserted into the pie comes out clean. Cool before serving.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Pies",
            "Pumpkin Pie Recipes"
