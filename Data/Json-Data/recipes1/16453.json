    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283132/vegetarian-thai-red-curry/",
        "name": "Vegetarian Thai Red Curry",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Vegetarian Thai Red Curry"
        },
        "datePublished": "2010-12-23T05:23:06.000Z",
        "description": "Spicy, creamy Thai red curry with vegetables and tofu.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H15M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (12 ounce) package extra-firm tofu, drained",
            "1 tablespoon vegetable oil",
            "2 cups coconut milk",
            "\u00bd cup water, or more as needed",
            "2 tablespoons Thai red curry paste",
            "1 medium onion, finely chopped",
            "2 medium potatoes, peeled and chopped",
            "3 small zucchini, chopped",
            "2 medium red bell peppers, cut into long, thin strips",
            "2 tablespoons chopped fresh basil leaves",
            "2 tablespoons lime juice",
            "3 teaspoons soft brown sugar",
            "1 (8 ounce) can bamboo shoots, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place tofu onto a plate and place another plate on top. Set a 3- to 5-pound weight on top. Press tofu for 20 to 30 minutes; drain and discard the accumulated liquid. Cut tofu into cubes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a large, nonstick skillet over medium-high heat until hot but not smoking. Add tofu and saute, stirring occasionally, until nicely colored on all sides and moisture has cooked off, 8 to 10 minutes. Remove from the skillet and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Meanwhile, combine coconut milk, 1/2 cup water, and curry paste in a large wok or pan over medium-high heat; bring to a boil, stirring occasionally. Add onion; boil for 3 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add potatoes, reduce heat to medium, and cook until potatoes are nearly cooked, about 8 minutes. Add zucchini and bell peppers; simmer for 5 minutes. Add 1/2 cup more water if curry is too thick.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut bamboo shoots in half and discard any tough ends. Add to the curry with the basil. Add brown sugar and lime juice; taste and adjust seasonings. Stir in tofu.\n"
            }
        ],
