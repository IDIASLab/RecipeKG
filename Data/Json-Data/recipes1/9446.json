    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/16798/super-orange-chicken/",
        "name": "Super Orange Chicken",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5846088.jpg",
            "width": 3024,
            "height": 3024
        },
        "datePublished": "2000-01-25T17:10:29.000Z",
        "description": "Baked chicken with a zesty orange zing. Try serving over rice with a side of broccoli done the way you like it.",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT1H5M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (6 ounce) can frozen orange juice concentrate, thawed",
            "1 packet dry onion soup mix",
            "1 teaspoon poultry seasoning",
            "1 tablespoon cornstarch",
            "4 chicken legs"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 375 degrees F (190 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, combine the orange juice, onion soup mix, poultry seasoning and cornstarch. Mix together well and set aside. Place chicken in a 9x13 inch baking dish. Pour sauce over chicken and cover with foil.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 375 degrees F (190 degrees C) for 45 minutes, remove foil, and bake for 15 more minutes.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Chicken",
            "Baked and Roasted"
        ],
