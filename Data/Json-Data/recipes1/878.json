        "mainEntityOfPage": "https://www.allrecipes.com/recipe/12084/caramel-pie-i/",
        "name": "Caramel Pie I",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4560880.jpg",
