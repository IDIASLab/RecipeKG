        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14837/best-guacamole/",
        "name": "Best Guacamole",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F363326.jpg",
