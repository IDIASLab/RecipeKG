    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/223362/peach-gelee-candy/",
        "name": "Peach Gelee Candy",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1023677.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2012-06-25T21:26:51.000Z",
        "description": "This is an old fashioned, handcrafted candy that takes a little time and finesse to pull off. It's simple and sweet, but looks and tastes like something you're only suppose to enjoy a few times a year.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H20M",
        "totalTime": "P0DT8H30M",
        "recipeYield": "25 candies",
        "recipeIngredient": [
            "1 pound ripe peaches - peeled, pitted and sliced",
            "1 tablespoon lime juice",
            "2 cups white sugar, divided",
            "3 tablespoons liquid pectin",
            "\u00bd cup white sugar, for sprinkling"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Line an 8x8-inch baking dish with plastic wrap.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine peaches and lime juice in a blender. Puree until very smooth.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour into a saucepan over medium heat, stir in 1/2 cup sugar, and bring to a boil. Cook, stirring continuously, until thickened, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir in remaining 1 1/2 cups sugar and pectin. Using a thermometer, heat to 205 degrees F (96 degrees C) and cook, stirring continuously, for another 10 minutes. Remove from heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour peach puree into the prepared baking dish. Shake gently and tap on the countertop to remove any air bubbles. Cover and refrigerate at least 8 hours or overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle about half the 1/2 cup of sugar over a silicone baking mat and invert the peach gelee on top. Remove plastic wrap and sprinkle top with sugar. Trim off any uneven edges and cut gelee into 25 squares.\n"
            }
        ],
