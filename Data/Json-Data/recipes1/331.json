        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "\u215e cup all-purpose flour",
            "\u00bd cup butter",
            "1 tablespoon white sugar",
            "1 pinch salt",
            "1 teaspoon vanilla extract",
            "1 cup ground pecans",
            "1 cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 300 degrees F (150 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cream butter and white sugar.  Add nuts and vanilla.  Blend in flour and mix well.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Form dough into small balls and bake on an ungreased sheet for 8 to 10 minutes until barely brown.  Coat in confectioner's sugar while still hot.\n"
            }
        ],
