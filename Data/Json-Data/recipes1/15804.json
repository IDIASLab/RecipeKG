        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/285348/pistachio-crusted-asparagus/",
        "name": "Pistachio-Crusted Asparagus",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F322232.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2009-08-16T21:14:47.000Z",
        "description": "Asparagus rolled in ground pistachios and baked. This is to-die-for and no one else is doing it.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "2 cups shelled pistachio nuts",
            "2 large eggs",
            "2 tablespoons water",
            "2 pounds thick asparagus, shaved using a vegetable peeler",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place pistachio nuts in a food processor and pulse until finely ground. Transfer to a plate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs with water in a shallow bowl. Roll asparagus in the egg wash, then in ground nuts. Place on a wire rack set on a sheet pan. Season with salt and pepper.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C) while pistachio crust sets, about 15 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until tender, about 15 minutes.\n"
            }
        ],
