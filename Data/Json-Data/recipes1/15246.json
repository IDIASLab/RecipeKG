    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/15073/chocolate-chip-banana-muffins/",
        "name": "Chocolate Chip Banana Muffins",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4079979.jpg",
            "width": 1706,
            "height": 960
        },
        "datePublished": "2008-06-30T23:10:59.000Z",
        "description": "Chocolate and bananas, two great tastes that go together!",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "12 muffins",
        "recipeIngredient": [
            "1\u2009\u00bd cups mashed bananas",
            "\u2154 cup sunflower seed oil",
            "1 egg, beaten",
            "1\u2009\u00bd teaspoons vanilla extract",
            "2 cups all-purpose flour",
            "\u00bd cup white sugar",
            "2 tablespoons unsweetened cocoa powder",
            "1 tablespoon baking powder",
            "\u00bd teaspoon salt",
            "1 cup semisweet chocolate chips"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 425 degrees F (220 degrees C). Lightly grease a 12-cup muffin tin.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl blend the banana, oil, egg and vanilla together.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the flour, sugar, cocoa, baking powder and salt. Stir in the banana mixture until just blended. Fold in the chocolate chips. Spoon the batter into the prepared muffin tin, filling 3/4 full.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for 15 to 20 minutes. Remove muffins to a wire rack to cool completely.\n"
            }
        ],
        "recipeCategory": [
