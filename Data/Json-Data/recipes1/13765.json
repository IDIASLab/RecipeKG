    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/22949/cranberry-nut-bread-ii/",
        "name": "Cranberry Nut Bread II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1606295.jpg",
            "width": 2592,
            "height": 1936
        },
        "datePublished": "2000-10-17T02:40:03.000Z",
        "description": "This is a fast, festive bread for the Thanksgiving or Christmas holiday dinner table. Tart, fragrant and filled with nuts!",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT1H10M",
        "totalTime": "P0DT1H25M",
        "recipeYield": "1 - loaf",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "1 cup white sugar",
            "1\u2009\u00bd teaspoons baking powder",
            "1 teaspoon salt",
            "\u00bd teaspoon baking soda",
            "\u00bc cup butter",
            "1 egg",
            "\u00be cup orange juice",
            "1 tablespoon grated orange zest",
            "1\u2009\u00bd cups fresh or frozen cranberries",
            "\u00bd cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C). Lightly grease an 8x4 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium bowl, mix together the flour, sugar, baking powder, salt and baking soda. Cut in the butter until the mixture resembles coarse crumbs.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, beat the egg, orange juice and orange zest. Blend into the dry mixture. Stir in the cranberries and walnuts. Transfer to the loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven 65 to 70 minutes, or until a wooden toothpick inserted at center comes out clean. Cool in pan 10 minutes, remove to wire rack and cool completely before serving.\n"
            }
        ],
        "recipeCategory": [
            "Bread",
            "Quick Bread Recipes",
            "Fruit Bread Recipes",
            "Cranberry Bread Recipes"
        ],
