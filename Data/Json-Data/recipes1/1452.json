        "mainEntityOfPage": "https://www.allrecipes.com/recipe/10898/no-bake-peanut-butter-cookies-ii/",
        "name": "No Bake Peanut Butter Cookies II",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1436686.jpg",
