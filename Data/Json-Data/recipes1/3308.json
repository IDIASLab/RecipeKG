    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/7044/pumpkin-bread-v/",
        "name": "Pumpkin Bread V",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F1518653.jpg",
            "width": 3088,
            "height": 2056
        },
        "datePublished": "1998-12-31T04:26:52.000Z",
        "description": "Pumpkin Bread, a tried and true recipe.  I usually make it without the raisins and walnuts, and I get tons of compliments!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H50M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 - 9 x 5 inch loaf",
        "recipeIngredient": [
            "2 cups all-purpose flour",
            "2 teaspoons baking powder",
            "\u00bd teaspoon salt",
            "\u00bd teaspoon pumpkin pie spice",
            "\u00bc teaspoon baking soda",
            "\u00bc teaspoon ground cloves",
            "1 cup packed brown sugar",
            "\u2153 cup butter flavored shortening",
            "2 eggs",
            "1 cup pumpkin puree",
            "\u00bc cup milk",
            "\u00bd cup raisins",
            "\u00bc cup chopped walnuts"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl combine flour, baking powder, salt, pumpkin pie spice, baking soda, and cloves.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a separate bowl, cream together shortening and brown sugar.  Mix in eggs, pumpkin, and milk.  Stir flour mixture into wet mixture until just combined.  Fold in raisins and walnuts if desired.  Pour into a greased and floured 9 x 5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake at 350 degrees F (175 degrees ) for 50 to 55 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Breakfast and Brunch",
            "Breakfast Bread Recipes"
        ],
