    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20656/tinas-fruit-dip/",
        "name": "Tina's Fruit Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F880563.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-05-17T14:17:39.000Z",
        "description": "I got this recipe from my mother. It's great with about any type of fruit. Strawberries, bananas, apples and grapes are the best.",
        "prepTime": "P0DT0H2M",
        "cookTime": "P0DT0H3M",
        "totalTime": "P0DT0H5M",
        "recipeYield": "2.5 cups",
        "recipeIngredient": [
            "1 cup brown sugar",
            "\u00bd cup butter",
            "1 cup sour cream",
            "3 teaspoons vanilla extract"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a saucepan over medium heat, stir together the butter and brown sugar. Remove from heat when the butter has melted.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a medium mixing bowl, whip together the butter mixture, sour cream and vanilla. Serve warm, or allow the dip to chill and thicken in the refrigerator for 2 hours.\n"
            }
        ],
