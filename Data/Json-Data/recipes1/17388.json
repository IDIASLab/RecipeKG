    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/213460/sicilian-style-steak/",
        "name": "Sicilian Style Steak",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F2415500.jpg",
            "width": 3648,
            "height": 2736
        },
        "datePublished": "2011-12-15T20:48:27.000Z",
        "description": "Any good cut of steak breaded with a mixture of bread crumbs and Parmesan cheese. You can use tenderloin fillet or porterhouse too.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "2 t-bone steaks",
        "recipeIngredient": [
            "1 cup Italian seasoned dry bread crumbs",
            "1 cup grated Parmesan cheese",
            "1 teaspoon seasoned salt",
            "1 tablespoon garlic powder",
            "\u00bd cup olive oil",
            "2 (16 ounce) t-bone steaks"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat an oven to 350 degrees F (175 degrees C). Lightly grease a broiler pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Stir the bread crumbs, Parmesan cheese, seasoned salt, and garlic powder together in a shallow dish such as a pie plate. Pour the olive oil into a separate shallow dish. Dip the t-bone steaks in the olive oil on both sides; allow excess to drip off. Press the steaks into the bread crumb mixture, and place onto the prepared broiler pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake on the middle rack in the preheated oven until the steaks have cooked to your desired degree of doneness, about 15 minutes for medium. An instant-read thermometer inserted into the center should read 140 degrees F (60 degrees C) for medium.\n"
            }
        ],
        "recipeCategory": [
            "Meat and Poultry",
            "Beef",
            "Steaks"
        ],
