    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14086/hearty-lentil-soup/",
        "name": "Hearty Lentil Soup",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F175162.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-09-15T07:37:50.000Z",
        "description": "This is a rich, satisfying vegetarian soup, wonderful for cold winter nights. Serve a loaf of warm, crusty bread on the side and you have a wonderful meal.",
        "prepTime": "P0DT0H15M",
        "cookTime": "P0DT0H40M",
        "totalTime": "P0DT0H55M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "2 tablespoons vegetable oil",
            "1 bunch green onions, chopped",
            "1 leek, sliced",
            "1 carrot, diced",
            "1 parsnip, scrubbed and diced",
            "1 potato, peeled and diced",
            "1 stalk celery, sliced",
            "1 cup dry lentils, rinsed",
            "1 (14.5 ounce) can diced tomatoes",
            "3 cups vegetable stock",
            "2 bay leaves",
            "1 dash soy sauce",
            "2 teaspoons vegetarian Worcestershire sauce",
            "\u00bd cup red wine",
            "1 bunch fresh cilantro, chopped"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Heat the oil in a large saucepan and mix in the onions, carrot, parsnip, celery, potato, and leek.  Stir over a medium heat.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Place lentils, tomatoes, stock, bay leaves, a splash of the soy and  Worcestershire sauces and wine in the saucepan. Stir and bring the mixture to a boil.  Cover and simmer for 25 to 30 minutes, or until the lentils are cooked.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove the bay leaves from the soup. Stir in the cilantro and serve.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Soup Recipes",
            "Beans and Peas",
            "Lentil Soup Recipes"
        ],
