    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284737/pineapple-and-cranberry-upside-down-cake/",
        "name": "Pineapple and Cranberry Upside-Down Cake",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Pineapple and Cranberry Upside-Down Cake"
        },
        "datePublished": "2010-11-21T20:31:04.000Z",
        "description": "This cranberry upside-down cake is a sweet/tart twist to an all-time favorite. Great for holiday entertaining. Quick, easy, and delicious! Top with whipped topping if desired.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H35M",
        "totalTime": "P0DT1H0M",
        "recipeYield": "1 9x13-inch cake",
        "recipeIngredient": [
            "1 (20 ounce) can crushed pineapple in juice, drained and juice reserved",
            "1 (20 ounce) can pineapple tidbits in juice, drained and juice reserved",
            "1 (15.25 ounce) package yellow cake mix",
            "3 large eggs",
            "\u2153 cup margarine, softened",
            "\u2154 cup packed brown sugar",
            "\u00bd cup margarine, melted",
            "1 cup fresh cranberries"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine juice from crushed pineapple and pineapple tidbits to get 1 cup. Reserve remaining juice for another use or discard.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine cake mix, 1 cup pineapple juice, eggs, and softened margarine in a bowl; mix until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Pour melted margarine into a 9x13-inch cake pan. Spread brown sugar over top and pat down. Evenly spread crushed pineapple over top, followed by pineapple tidbits. Spread cranberries over top, then pour in the prepared cake batter.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 35 to 40 minutes. Let cake cool for 15 minutes before flipping onto a plate.\n"
            }
        ],
        "recipeCategory": [
            "Desserts",
            "Fruit Desserts",
            "Cranberry Dessert Recipes"
