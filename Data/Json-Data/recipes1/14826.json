    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283185/amazing-meatless-sloppy-joes/",
        "name": "Amazing Meatless Sloppy Joes",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Amazing Meatless Sloppy Joes"
        },
        "datePublished": "2006-12-06T06:51:59.000Z",
        "description": "A vegetarian alternative that will be sure to satisfy even a diehard carnivore. Serve these tofu Sloppy Joes on hamburger buns with or without cheese. Also a great vegetarian substitute for chili fries.",
        "prepTime": "P0DT0H25M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H40M",
        "recipeYield": "4 servings",
        "recipeIngredient": [
            "1 (12 ounce) package extra-firm tofu",
            "2 tablespoons vegetable oil",
            "1 medium onion, chopped",
            "2 medium jalapeno peppers, seeded and chopped",
            "4 cloves garlic, chopped",
            "1 (14.5 ounce) can whole tomatoes, drained and chopped",
            "1 cup chopped button mushrooms",
            "\u00bd medium green bell pepper, chopped",
            "\u00bd medium yellow bell pepper, chopped",
            "\u00bd cup ketchup",
            "1 tablespoon Dijon mustard",
            "1 tablespoon Worcestershire sauce",
            "1 tablespoon white vinegar",
            "1 tablespoon chili powder, or to taste",
            "salt and ground black pepper to taste"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Drain tofu and use your hands to squeeze out as much remaining liquid as possible. Crumble and set aside.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat oil in a medium pan over medium heat. Add onion, jalapenos, and garlic; saute for 2 minutes. Add crumbled tofu; saute until browned, 5 to 10 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add tomatoes, mushrooms, bell peppers, ketchup, mustard, Worcestershire sauce, vinegar, chili powder, salt, and pepper. Stir to combine and cook until heated through, 5 to 10 minutes.\n"
            }
        ],
        "recipeCategory": [
            "Everyday Cooking",
            "Vegetarian"
        ],
