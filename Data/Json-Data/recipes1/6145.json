    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14312/mommys-summer-salad/",
        "name": "Mommy's Summer Salad",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F3218265.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "1999-08-02T17:42:51.000Z",
        "description": "A cool summer salad for hot summer nights. Goes well with sliced French bread.  You can use any shape pasta you want with this salad--bowties and rotini both work great!",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT0H25M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "3 cups rotelle pasta",
            "\u00bd pound chopped ham",
            "2 cups shredded Colby cheese",
            "\u00bd cup ranch-style salad dressing",
            "1 cup fat-free creamy salad dressing (ie: Fat Free Miracle Whip TM)",
            "1 (15 ounce) can peas, drained"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a large pot of salted boiling water, cook pasta until al dente,  rinse under cold water and drain.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a large bowl, combine the pasta, ham, cheese and peas.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Whisk together the ranch dressing and Miracle Whip &trade;. Pour over salad and toss to coat. Refrigerate until chilled and serve.\n"
            }
        ],
