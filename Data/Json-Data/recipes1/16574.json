    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/283802/california-style-carne-asada-burritos-with-fries/",
        "name": "California-Style Carne Asada Burritos with Fries",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F598391.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2011-03-05T01:43:54.000Z",
        "description": "Carne asada burritos from my hometown of San Diego. So simple and delicious that you will never eat a burrito without fries again!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H15M",
        "totalTime": "P0DT4H35M",
        "recipeYield": "4 burritos",
        "recipeIngredient": [
            "1 medium lime, juiced",
            "2 tablespoons olive oil",
            "1 tablespoon white vinegar",
            "\u00bc teaspoon garlic powder",
            "\u00bc teaspoon chili powder",
            "\u00bc teaspoon dried oregano",
            "\u00bc teaspoon ground cumin",
            "\u00bc teaspoon ground paprika",
            "\u00bc teaspoon cayenne pepper",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground black pepper",
            "1 (1 pound) flank steak",
            "1 (16 ounce) package frozen french fries",
            "4 (12 inch) flour tortillas",
            "1 cup guacamole",
            "1 cup sour cream",
            "1 cup salsa"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Whisk lime juice, olive oil, vinegar, garlic powder, chili powder, oregano, cumin, paprika, cayenne pepper, salt, and pepper together in a small bowl. Place flank steak in a glass baking dish and pour marinade over top; toss to evenly coat. Cover with plastic wrap and marinate in the refrigerator for 4 hours to overnight.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 400 degrees F (200 degrees C). Preheat an outdoor grill for high heat and lightly oil the grate.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Arrange frozen fries in a single layer on a baking sheet and bake in the preheated oven until light golden brown, 15 to 20 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "At the same time, remove flank steak from the refrigerator and place on the preheated grill. Grill until firm and reddish-pink and juicy in the center, about 4 minutes per side. An instant-read thermometer inserted into the center should read 130 degrees F (54 degrees C). Remove from the grill and chop into bite-sized pieces.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Heat tortillas in a large, dry skillet until warm, about 20 seconds per side.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine steak, fries, guacamole, sour cream, and salsa in the tortillas.\n"
            }
        ],
