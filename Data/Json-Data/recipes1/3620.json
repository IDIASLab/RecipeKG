    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/13350/thermos-bottle-stew/",
        "name": "Thermos Bottle Stew",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F61252.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "1999-01-25T19:38:47.000Z",
        "description": "My grandmother used to make this for my grandfathers lunch when he worked outside for the railroad. She put it in his thermos, and always called it thermos bottle stew.  This soup warms you from the inside out.",
        "prepTime": null,
        "cookTime": null,
        "totalTime": null,
        "recipeYield": "6 servings",
        "recipeIngredient": [
            "1\u2009\u00bd pounds lean ground beef",
            "1 cup chopped onion",
            "2 stalks celery, finely chopped",
            "1 (2 pound) package frozen mixed vegetables",
            "1 (10.75 ounce) can  condensed tomato soup",
            "1 tablespoon dried basil",
            "1 tablespoon dried parsley",
            "1 teaspoon dried oregano",
            "1 bay leaf",
            "salt and pepper to taste",
            "1 (16 ounce) can crushed tomatoes",
            "1 (8 ounce) can tomato sauce",
            "9\u2009\u00be cups water"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Using a large stock pot, brown your meat (breaking into small pieces as it cooks). Once meat is browned, add onions and celery, sauteing until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Add frozen vegetables, tomato soup, basil, parsley, oregano, bay leaves, salt and pepper, crushed tomatoes, tomato sauce and water.  Simmer for one hour and then serve. Remove bay leaves before serving.\n"
            }
        ],
        "recipeCategory": [
            "Soups, Stews and Chili Recipes",
            "Stews",
            "Beef"
        ],
