        "mainEntityOfPage": "https://www.allrecipes.com/recipe/20664/easy-nacho-dip/",
        "name": "Easy Nacho Dip",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F4575411.jpg",
