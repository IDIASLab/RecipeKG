        "recipeYield": "2 cups",
        "recipeIngredient": [
            "1 (16 ounce) jar pasteurized process cheese spread (such as Cheez Whiz\u00ae)",
            "\u00bd cup horseradish sauce",
            "1 (4 ounce) package cream cheese, softened",
            "\u00bc cup butter",
            "1 tablespoon prepared horseradish",
            "\u00bd teaspoon onion salt",
            "5 drops hot sauce"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Combine cheese spread, horseradish sauce, cream cheese, butter, prepared horseradish, onion salt, and hot sauce in a microwave-safe bowl. Heat in microwave until melted, stirring every 30 seconds, 3 to 5 minutes.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate until cold, at least 45 minutes.\n"
            }
        ],
