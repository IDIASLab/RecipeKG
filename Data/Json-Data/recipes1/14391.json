    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/282553/bacon-and-tomato-bagel-casserole/",
        "name": "Bacon and Tomato Bagel Casserole",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Bacon and Tomato Bagel Casserole"
        },
        "datePublished": "2004-03-26T14:47:44.000Z",
        "description": "Bacon, tomato, basil, and bagels combine for a chewy, tasty treat. Serve with fresh fruit for breakfast or brunch or with a green salad for dinner. Easily adaptable to your taste preferences!",
        "prepTime": "P0DT0H20M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT9H5M",
        "recipeYield": "1 9x13-inch casserole",
        "recipeIngredient": [
            "1 pound thick-cut bacon",
            "3 bagel (3\" dia)s plain bagels",
            "8 large eggs",
            "2 cups half-and-half",
            "1 cup shredded Swiss cheese",
            "\u00bd cup diced tomato",
            "2 tablespoons chopped fresh basil",
            "\u00bc teaspoon salt"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels and crumble when cool enough to handle.\n"
            },
            {
                "@type": "HowToStep",
                "text": "While bacon is frying, tear bagels into bite-sized pieces and place in the bottom of a 9x13-inch baking pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Beat eggs, half-and-half, Swiss cheese, tomato, basil, and salt together in a medium bowl until well combined.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Sprinkle crumbled bacon over bagel pieces, then pour egg mixture over top, making sure liquid is evenly distributed. Cover and refrigerate for at least 8 hours.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Remove casserole from the refrigerator and bake in the preheated oven until a knife inserted into the center comes out clean, 35 to 40 minutes.\n"
            }
        ],
