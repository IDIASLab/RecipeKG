        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21503/salmon-loaf/",
        "name": "Salmon Loaf",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F8253077.jpg",
            "width": 0,
            "height": 0
        },
        "datePublished": "2000-04-18T21:16:59.000Z",
        "description": "Salmon, cracker crumbs, milk, egg and butter, baked into a loaf.",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H45M",
        "totalTime": "P0DT0H50M",
        "recipeYield": "1 loaf",
        "recipeIngredient": [
            "1 (14.75 ounce) can salmon, undrained",
            "\u00bd cup crushed saltine crackers",
            "\u00bd cup milk",
            "1 egg, beaten",
            "salt and pepper to taste",
            "2 tablespoons melted butter"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 350 degrees F (175 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a mixing bowl, combine the salmon, cracker crumbs, milk, egg, salt, pepper, and melted butter. Mix thoroughly.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the salmon mixture into a lightly greased 9x5 inch loaf pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in a preheated oven for 45 minutes or until done.\n"
            }
        ],
