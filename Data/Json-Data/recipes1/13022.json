    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/21229/apple-butter-v/",
        "name": "Apple Butter V",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Apple Butter V"
        },
        "datePublished": "2000-08-31T06:20:34.000Z",
        "description": "Warmly spiced and easy to prepare -- this apple butter is always a tasy treat, especially on a slice of homemade bread.",
        "prepTime": "P0DT0H30M",
        "cookTime": "P0DT1H30M",
        "totalTime": "P0DT2H0M",
        "recipeYield": "1 gallon",
        "recipeIngredient": [
            "6 pounds tart apples, cored and sliced",
            "6 cups apple cider",
            "3 cups white sugar",
            "2 teaspoons ground cinnamon",
            "1 teaspoon ground cloves"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Place the apples in a Dutch oven. Pour in the cider. Bring to a boil and cook on medium heat 30 minutes, or until soft.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Press the softened mixture through a food mill. Continue cooking at a medium boil for 30 minutes, stirring occasionally.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Mix in the sugar, cinnamon and cloves. Continue cooking over low heat until the sugar dissolves, stirring frequently. Cook until the desired thickness has been attained. Refrigerate the finished apple butter in sterile containers.\n"
            }
        ],
        "recipeCategory": [
            "Side Dish",
            "Sauces and Condiments",
            "Canning and Preserving Recipes",
            "Fruit Butter Recipes"
        ],
