        "mainEntityOfPage": "https://www.allrecipes.com/recipe/23366/forever-amber/",
        "name": "Forever Amber",
        "image": {
            "@type": "ImageObject",
            "url": "https://www.allrecipes.com/img/misc/og-default.png",
            "width": null,
            "height": null,
            "caption": "Forever Amber"
        },
        "datePublished": "2000-10-29T06:01:25.000Z",
        "description": "Different and delicious!! Try 'em!!",
        "prepTime": "P0DT0H5M",
        "cookTime": "P0DT0H30M",
        "totalTime": "P0DT0H35M",
        "recipeYield": "4 dozen",
        "recipeIngredient": [
            "1 pound chopped orange slices candy",
            "2 (3.5 ounce) packages flaked coconut",
            "2 cups chopped walnuts",
            "1 tablespoon orange juice",
            "1 teaspoon vanilla extract",
            "1 cup confectioners' sugar"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat the oven to 250 degrees F (120 degrees C). Grease a 9x13 inch pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Combine the chopped orange slice candy with the coconut and chopped walnuts. Stir in the orange juice and vanilla. Press the mixture evenly into the prepared pan.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake for 30 minutes in the preheated oven. Let cool. Scoop out spoonfuls and roll into walnut sized balls. Roll the balls in confectioners' sugar.\n"
            }
        ],
