        "mainEntityOfPage": "https://www.allrecipes.com/recipe/284922/cucumber-mint-martini/",
        "name": "Cucumber-Mint Martini",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F638269.jpg",
            "width": 0,
