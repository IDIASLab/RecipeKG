        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/19167/eggless-vanilla-eggnog/",
        "name": "Eggless Vanilla Eggnog",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F759312.jpg",
            "width": 250,
            "height": 250
        },
        "datePublished": "2000-02-26T20:29:56.000Z",
        "description": "An eggnog recipe that tastes great.  Pudding may sound strange, but believe me the final product is awesome.",
        "prepTime": "P0DT0H15M",
        "cookTime": null,
        "totalTime": "P0DT12H15M",
        "recipeYield": "10 servings",
        "recipeIngredient": [
            "4 cups skim milk, divided",
            "1 (1 ounce) package instant sugar-free vanilla pudding mix",
            "1 cup egg substitute",
            "1 teaspoon vanilla extract",
            "\u00bc teaspoon salt",
            "\u00bc teaspoon ground nutmeg"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "In a medium bowl combine 2 cups milk and pudding mixture; stir well until thickened.\n"
            },
            {
                "@type": "HowToStep",
                "text": "To the pudding add remaining 2 cups milk and egg substitute; mix well.  Stir in vanilla extract, salt and nutmeg until well blended and coats a spoon.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Refrigerate overnight.\n"
            }
        ],
        "recipeCategory": [
            "Drink Recipes",
            "Eggnog Recipes"
        ],
