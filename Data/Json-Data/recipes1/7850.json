    {
        "@context": "http://schema.org",
        "@type": "Recipe",
        "mainEntityOfPage": "https://www.allrecipes.com/recipe/14827/pita-chips/",
        "name": "Pita Chips",
        "image": {
            "@type": "ImageObject",
            "url": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F5270122.jpg",
            "width": 3629,
            "height": 2721
        },
        "datePublished": "1999-11-04T21:48:19.000Z",
        "description": "These baked pita chips make a lovely accompaniment to any dip or spread.",
        "prepTime": "P0DT0H10M",
        "cookTime": "P0DT0H7M",
        "totalTime": "P0DT0H17M",
        "recipeYield": "8 dozen",
        "recipeIngredient": [
            "12 pita bread pockets",
            "\u00bd cup olive oil",
            "\u00bd teaspoon ground black pepper",
            "1 teaspoon garlic salt",
            "\u00bd teaspoon dried basil",
            "1 teaspoon dried chervil"
        ],
        "recipeInstructions": [
            {
                "@type": "HowToStep",
                "text": "Preheat oven to 400 degrees F (200 degrees C).\n"
            },
            {
                "@type": "HowToStep",
                "text": "Cut each pita bread into 8 triangles. Place triangles on lined cookie sheet.\n"
            },
            {
                "@type": "HowToStep",
                "text": "In a small bowl, combine the oil, pepper, salt, basil and chervil.  Brush each triangle with oil mixture.\n"
            },
            {
                "@type": "HowToStep",
                "text": "Bake in the preheated oven for about 7 minutes, or until lightly browned and crispy. Watch carefully, as they tend to burn easily!\n"
            }
        ],
        "recipeCategory": [
            "Appetizers and Snacks",
            "Snacks",
            "Snack Chip Recipes"
        ],
